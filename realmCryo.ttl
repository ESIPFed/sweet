#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/realmCryo/AblationZone
:AblationZone rdf:type owl:Class ;
              rdfs:subClassOf :GlacialRegion .


###  http://sweetontology.net/realmCryo/AccumulationZone
:AccumulationZone rdf:type owl:Class ;
                  rdfs:subClassOf :GlacialRegion .


###  http://sweetontology.net/realmCryo/AlpineTundra
:AlpineTundra rdf:type owl:Class ;
              rdfs:subClassOf :Tundra ;
              rdfs:comment "Alpine tundra is an ecozone that does not contain trees because it has high altitude. Alpine tundra occurs at high enough altitude at any latitude on Earth. Alpine tundra also lacks trees, but the lower part does not have permafrost, and alpine soils are generally better drained than permafrost soils. Alpine tundra transitions to subalpine forests below the tree line; stunted forests occurring at the forest-tundra ecotone are known as Krummholz. Alpine tundra occurs in an alpine zone"@en .


###  http://sweetontology.net/realmCryo/Calf
:Calf rdf:type owl:Class ;
      rdfs:subClassOf :SeaIce .


###  http://sweetontology.net/realmCryo/CongelationIce
:CongelationIce rdf:type owl:Class ;
                rdfs:subClassOf :SeaIce .


###  http://sweetontology.net/realmCryo/DriftIce
:DriftIce rdf:type owl:Class ;
          rdfs:subClassOf :SeaIce ;
          owl:disjointWith :FastIce .


###  http://sweetontology.net/realmCryo/FastIce
:FastIce rdf:type owl:Class ;
         rdfs:subClassOf :SeaIce .


###  http://sweetontology.net/realmCryo/Floe
:Floe rdf:type owl:Class ;
      owl:equivalentClass :IceFloe ;
      rdfs:subClassOf :SeaIce .


###  http://sweetontology.net/realmCryo/FrazilIce
:FrazilIce rdf:type owl:Class ;
           rdfs:subClassOf :SeaIce .


###  http://sweetontology.net/realmCryo/FrozenGround
:FrozenGround rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/realm/Land> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                owl:hasValue <http://sweetontology.net/statePhysical/Frozen>
                              ] ;
              rdfs:comment "Soil within which the moisture has predominantly changed to ice, the unfrozen portion being in vapor phase. Ice within the soil bonds (adfreezes) adjacent soil particles and renders frozen ground very hard. Permanently frozen ground is called permafrost. Dry frozen ground is relatively loose and crumbly because of the lack of bonding ice. Frozen ground is sometimes inadvisedly called frost or ground frost."@en .


###  http://sweetontology.net/realmCryo/GlacialRegion
:GlacialRegion rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/realm/PlanetaryRealm> .


###  http://sweetontology.net/realmCryo/Glacier
:Glacier rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/matrWater/Ice> .


###  http://sweetontology.net/realmCryo/GlacierTerminus
:GlacierTerminus rdf:type owl:Class ;
                 owl:equivalentClass :Snout ;
                 rdfs:subClassOf :GlacialRegion .


###  http://sweetontology.net/realmCryo/IceBase
:IceBase rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/realm/PlanetaryBoundary> .


###  http://sweetontology.net/realmCryo/IceCap
:IceCap rdf:type owl:Class ;
        rdfs:subClassOf <http://sweetontology.net/realm/PlanetaryRealm> .


###  http://sweetontology.net/realmCryo/IceCore
:IceCore rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/humanResearch/Sample> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/relaSpace/inside> ;
                           owl:someValuesFrom <http://sweetontology.net/matrWater/Ice>
                         ] .


###  http://sweetontology.net/realmCryo/IceField
:IceField rdf:type owl:Class ;
          rdfs:subClassOf :SeaIce .


###  http://sweetontology.net/realmCryo/IceFloe
:IceFloe rdf:type owl:Class .


###  http://sweetontology.net/realmCryo/IceSheet
:IceSheet rdf:type owl:Class ;
          rdfs:subClassOf :Glacier ;
          rdfs:comment "An ice sheet is a mass of glacier ice that covers surrounding terrain and is greater than 50,000 square km (19,305 square mile)." .


###  http://sweetontology.net/realmCryo/IceShelf
:IceShelf rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/matrWater/Ice> .


###  http://sweetontology.net/realmCryo/IceSnowInterface
:IceSnowInterface rdf:type owl:Class ;
                  rdfs:subClassOf <http://sweetontology.net/realm/PlanetaryBoundary> .


###  http://sweetontology.net/realmCryo/IceStream
:IceStream rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/matrWater/Ice> .


###  http://sweetontology.net/realmCryo/IceSurface
:IceSurface rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/realm/PlanetaryBoundary> .


###  http://sweetontology.net/realmCryo/Iceberg
:Iceberg rdf:type owl:Class ;
         rdfs:subClassOf :LandIce ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                           owl:someValuesFrom <http://sweetontology.net/realm/Ocean>
                         ] .


###  http://sweetontology.net/realmCryo/LandIce
:LandIce rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/matrWater/Ice> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                           owl:someValuesFrom <http://sweetontology.net/realm/Land>
                         ] .


###  http://sweetontology.net/realmCryo/Lead
:Lead rdf:type owl:Class ;
      rdfs:subClassOf :SeaIce .


###  http://sweetontology.net/realmCryo/PackIce
:PackIce rdf:type owl:Class ;
         rdfs:subClassOf :DriftIce .


###  http://sweetontology.net/realmCryo/SeaIce
:SeaIce rdf:type owl:Class ;
        rdfs:subClassOf <http://sweetontology.net/matrWater/Ice> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                          owl:someValuesFrom <http://sweetontology.net/realm/Ocean>
                        ] ;
        rdfs:comment "Specifically, ice formed by the freezing of seawater; as opposed, principally, to land ice.  Generally, any ice floating in the sea."@en .


###  http://sweetontology.net/realmCryo/SeasonalIce
:SeasonalIce rdf:type owl:Class ;
             rdfs:subClassOf :SeaIce .


###  http://sweetontology.net/realmCryo/Snout
:Snout rdf:type owl:Class .


###  http://sweetontology.net/realmCryo/TabularIceberg
:TabularIceberg rdf:type owl:Class ;
                rdfs:subClassOf :Iceberg .


###  http://sweetontology.net/realmCryo/Tundra
:Tundra rdf:type owl:Class ;
        rdfs:subClassOf <http://sweetontology.net/realm/Land> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                          owl:someValuesFrom :FrozenGround
                        ] ;
        rdfs:comment "In physical geography, tundra is an area where the tree growth is hindered by low temperatures and short growing seasons. There are two types of tundra: Arctic tundra (which also occurs in Antarctica), and alpine tundra. In tundra, the vegetation is composed of dwarf shrubs, sedges and grasses, mosses, and lichens. Scattered trees grow in some tundra. The ecotone (or ecological boundary region) between the tundra and the forest is known as the tree line or timberline."@en .


