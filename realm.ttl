@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/realm> .

<http://sweetontology.net/realm> rdf:type owl:Ontology ;
                                  owl:imports <http://sweetontology.net/matrCompound> ,
                                              <http://sweetontology.net/matrWater> ,
                                              <http://sweetontology.net/realmAstroBody> ,
                                              <http://sweetontology.net/rela> ,
                                              <http://sweetontology.net/relaChemical> ,
                                              <http://sweetontology.net/relaSpace> ,
                                              <http://sweetontology.net/reprSpaceGeometry> ,
                                              <http://sweetontology.net/reprSpaceGeometry3D> ,
                                              <http://sweetontology.net/state> ,
                                              <http://sweetontology.net/statePhysical> ;
                                  rdfs:label "SWEET Ontology" ;
                                  owl:versionInfo "3.1.0" .

#################################################################
#    Object Properties
#################################################################

###  http://sweetontology.net/rela/hasState
<http://sweetontology.net/rela/hasState> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/relaChemical/hasSubstance
<http://sweetontology.net/relaChemical/hasSubstance> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/relaSpace/hasLowerBoundary
<http://sweetontology.net/relaSpace/hasLowerBoundary> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/relaSpace/hasUpperBoundary
<http://sweetontology.net/relaSpace/hasUpperBoundary> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/realm/Atmosphere
:Atmosphere rdf:type owl:Class ;
            rdfs:subClassOf :PlanetaryRealm ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaSpace/hasLowerBoundary> ;
                              owl:allValuesFrom :PlanetarySurface
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/rela/hasState> ;
                              owl:hasValue <http://sweetontology.net/statePhysical/Gas>
                            ] ;
            rdfs:comment "An atmosphere is a layer of gases that may surround a material body of sufficient mass,[1] by the gravity of the body, and are retained for a longer duration if gravity is high and the atmosphere's temperature is low. [Wikipedia]"@en .


###  http://sweetontology.net/realm/CriticalZone
:CriticalZone rdf:type owl:Class ;
              rdfs:subClassOf :PlanetaryRealm .


###  http://sweetontology.net/realm/Cryosphere
:Cryosphere rdf:type owl:Class ;
            rdfs:subClassOf :Hydrosphere ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaChemical/hasSubstance> ;
                              owl:allValuesFrom <http://sweetontology.net/matrWater/Ice>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaSpace/hasUpperBoundary> ;
                              owl:allValuesFrom :PlanetarySurface
                            ] ;
            rdfs:comment "The cryosphere collectively describes the portions of a planetary surface in frozen form, including sea ice, lake ice, river ice, snow cover, glaciers, ice caps and ice sheets, and frozen ground (which includes permafrost).  [Wikipedia]"@en .


###  http://sweetontology.net/realm/Geosphere
:Geosphere rdf:type owl:Class ;
           rdfs:subClassOf :PlanetaryRealm ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaSpace/hasUpperBoundary> ;
                             owl:allValuesFrom :PlanetarySurface
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/rela/hasState> ;
                             owl:hasValue <http://sweetontology.net/statePhysical/Solid>
                           ] ;
           rdfs:comment "The term Geosphere is often used to refer to the densest (solid) parts of a planet, which consist mostly of rock and regolith [Wikipedia]"@en .


###  http://sweetontology.net/realm/GlobalPlanetaryLayer
:GlobalPlanetaryLayer rdf:type owl:Class ;
                      owl:equivalentClass :PlanetaryLayer .


###  http://sweetontology.net/realm/Hydrosphere
:Hydrosphere rdf:type owl:Class ;
             rdfs:subClassOf :PlanetaryRealm ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaChemical/hasSubstance> ;
                               owl:hasValue <http://sweetontology.net/matrCompound/Water>
                             ] ;
             rdfs:comment "A hydrosphere in physical geography describes the combined mass of water found on, under, and over the surface of a planet. [Wikipedia]"@en .


###  http://sweetontology.net/realm/Land
:Land rdf:type owl:Class ;
      rdfs:subClassOf :Geosphere ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/rela/hasState> ;
                        owl:hasValue <http://sweetontology.net/statePhysical/Solid>
                      ] ;
      rdfs:comment "The part of a planetary surface that is a solid"@en .


###  http://sweetontology.net/realm/Ocean
:Ocean rdf:type owl:Class ;
       rdfs:subClassOf :Hydrosphere ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://sweetontology.net/relaSpace/hasUpperBoundary> ;
                         owl:allValuesFrom :PlanetarySurface
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://sweetontology.net/rela/hasState> ;
                         owl:hasValue <http://sweetontology.net/statePhysical/Liquid>
                       ] ;
       rdfs:comment "An ocean is a major body of saline water, and a principal component of the hydrosphere. [Wikipedia]"@en .


###  http://sweetontology.net/realm/PlanetaryBoundary
:PlanetaryBoundary rdf:type owl:Class ;
                   owl:equivalentClass :PlanetarySurfaceLevel .


###  http://sweetontology.net/realm/PlanetaryLayer
:PlanetaryLayer rdf:type owl:Class ;
                rdfs:subClassOf <http://sweetontology.net/reprSpaceGeometry3D/Layer> .


###  http://sweetontology.net/realm/PlanetaryRealm
:PlanetaryRealm rdf:type owl:Class ;
                rdfs:subClassOf :Realm .


###  http://sweetontology.net/realm/PlanetaryStructure
:PlanetaryStructure rdf:type owl:Class ;
                    rdfs:subClassOf :PlanetaryRealm .


###  http://sweetontology.net/realm/PlanetarySurface
:PlanetarySurface rdf:type owl:Class ;
                  owl:equivalentClass :Subaerial ;
                  rdfs:subClassOf :PlanetarySurfaceLevel ;
                  rdfs:comment "The surface of the planet defined by solid and/or liquid layers"@en .


###  http://sweetontology.net/realm/PlanetarySurfaceLevel
:PlanetarySurfaceLevel rdf:type owl:Class ;
                       rdfs:subClassOf :CriticalZone ,
                                       <http://sweetontology.net/reprSpaceGeometry/Surface> ;
                       rdfs:comment "A qusai-horizontal surface spanning all or part of a planet that corresponds to a constant value of some parameter"@en .


###  http://sweetontology.net/realm/Realm
:Realm rdf:type owl:Class .


###  http://sweetontology.net/realm/Ring
:Ring rdf:type owl:Class ;
      rdfs:subClassOf :PlanetaryRealm ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/rela/hasState> ;
                        owl:hasValue <http://sweetontology.net/statePhysical/Solid>
                      ] .


###  http://sweetontology.net/realm/Subaerial
:Subaerial rdf:type owl:Class .


###  http://sweetontology.net/realmAstroBody/Planet
<http://sweetontology.net/realmAstroBody/Planet> rdf:type owl:Class .


###  http://sweetontology.net/reprSpaceGeometry/Surface
<http://sweetontology.net/reprSpaceGeometry/Surface> rdf:type owl:Class .


###  http://sweetontology.net/reprSpaceGeometry3D/Layer
<http://sweetontology.net/reprSpaceGeometry3D/Layer> rdf:type owl:Class .


