#################################################################

###  http://sweetontology.net/rela/hasPhenomena
<http://sweetontology.net/rela/hasPhenomena> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/relaChemical/hasSubstance
<http://sweetontology.net/relaChemical/hasSubstance> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/relaSci/hasImpact
<http://sweetontology.net/relaSci/hasImpact> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/phenReaction/Activation
:Activation rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/procChemical/ChemicalReaction> .


###  http://sweetontology.net/phenReaction/BiochemicalRelease
:BiochemicalRelease rdf:type owl:Class ;
                    rdfs:subClassOf :Release .


###  http://sweetontology.net/phenReaction/Emission
:Emission rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/procChemical/ChemicalReaction> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/rela/hasRole> ;
                            owl:hasValue <http://sweetontology.net/stateRole/ByProduct>
                          ] .


###  http://sweetontology.net/phenReaction/Eruption
:Eruption rdf:type owl:Class ;
          rdfs:subClassOf :Release .


###  http://sweetontology.net/phenReaction/Explosion
:Explosion rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/procChemical/ChemicalReaction> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                             owl:allValuesFrom <http://sweetontology.net/phenWaveNoise/Noise>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaSci/hasImpact> ;
                             owl:hasValue <http://sweetontology.net/stateRoleImpact/Hazard>
                           ] .


###  http://sweetontology.net/phenReaction/Fire
:Fire rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/procChemical/ChemicalReaction> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/relaSci/hasImpact> ;
                        owl:hasValue <http://sweetontology.net/stateRoleImpact/Hazard>
                      ] .


###  http://sweetontology.net/phenReaction/Flare
:Flare rdf:type owl:Class ;
       owl:equivalentClass :Flaring ;
       rdfs:subClassOf <http://sweetontology.net/procChemical/ChemicalReaction> .


###  http://sweetontology.net/phenReaction/Flaring
:Flaring rdf:type owl:Class .


###  http://sweetontology.net/phenReaction/FluidLeak
:FluidLeak rdf:type owl:Class ;
           rdfs:subClassOf :Leak .


###  http://sweetontology.net/phenReaction/GasFlare
:GasFlare rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/procChemical/ChemicalReaction> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaSci/hasImpact> ;
                            owl:hasValue <http://sweetontology.net/stateRoleImpact/Hazard>
                          ] .


###  http://sweetontology.net/phenReaction/GasLeak
:GasLeak rdf:type owl:Class ;
         rdfs:subClassOf :Leak .


###  http://sweetontology.net/phenReaction/IndustrialEmission
:IndustrialEmission rdf:type owl:Class ;
                    rdfs:subClassOf :Emission .


###  http://sweetontology.net/phenReaction/Leach
:Leach rdf:type owl:Class ;
       rdfs:subClassOf :Release .


###  http://sweetontology.net/phenReaction/Leak
:Leak rdf:type owl:Class ;
      rdfs:subClassOf :Release .


###  http://sweetontology.net/phenReaction/RadioactivityRelease
:RadioactivityRelease rdf:type owl:Class ;
                      rdfs:subClassOf :Release ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://sweetontology.net/relaChemical/hasSubstance> ;
                                        owl:allValuesFrom <http://sweetontology.net/matrIsotope/RadioactiveSubstance>
                                      ] .


###  http://sweetontology.net/phenReaction/Release
:Release rdf:type owl:Class ;
         rdfs:subClassOf :Emission .


###  http://sweetontology.net/phenReaction/Wildfire
:Wildfire rdf:type owl:Class ;
          rdfs:subClassOf :Fire .


###  http://sweetontology.net/phenWaveNoise/Noise
<http://sweetontology.net/phenWaveNoise/Noise> rdf:type owl:Class .


###  http://sweetontology.net/procChemical/ChemicalReaction
<http://sweetontology.net/procChemical/ChemicalReaction> rdf:type owl:Class .


