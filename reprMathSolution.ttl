@base          <http://sweetontology.net/reprMathSolution> .
@prefix sol:   <http://sweetontology.net/reprMathSolution/> .
@prefix res:   <http://sweetontology.net/humanResearch/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix math:  <http://sweetontology.net/reprMath/> .

sol:Iteration  a         owl:Class ;
        rdfs:subClassOf  sol:Solution .

sol:DivideAndConquer  a  owl:Class ;
        rdfs:subClassOf  sol:Algorithm .

sol:MapReduce  a         owl:Class ;
        rdfs:subClassOf  sol:ParallelAlgorithm .

sol:AlgebraicSolution
        a                owl:Class ;
        rdfs:subClassOf  sol:AnalyticalSolution .

sol:NewtonianRelaxation
        a                owl:Class ;
        rdfs:subClassOf  sol:NumericalSolution .

sol:HeuristicAlgorithm
        a                owl:Class ;
        rdfs:subClassOf  sol:Algorithm .

sol:Approximation  a     owl:Class ;
        rdfs:subClassOf  sol:Solution .

sol:SensitivityAnalysis
        a                owl:Class ;
        rdfs:subClassOf  res:Assessment .

sol:NumericalSolution
        a                owl:Class ;
        rdfs:subClassOf  sol:Solution .

sol:DepthFirstSearch  a  owl:Class ;
        rdfs:subClassOf  sol:Search .

sol:Solution  a          owl:Class ;
        rdfs:subClassOf  math:NumericalEntity .

sol:ExhaustiveSearch  a  owl:Class ;
        rdfs:subClassOf  sol:Search .

sol:AnalyticalSolution
        a                owl:Class ;
        rdfs:subClassOf  sol:Solution .

sol:Emulate  a  owl:Class .

sol:Simulation  a  owl:Class .

sol:Expansion  a         owl:Class ;
        rdfs:subClassOf  sol:Solution .

sol:Reduction  a         owl:Class ;
        rdfs:subClassOf  sol:Solution .

sol:TaylorSeries  a      owl:Class ;
        rdfs:subClassOf  sol:Expansion , sol:NumericalSolution .

sol:Search  a            owl:Class ;
        rdfs:subClassOf  sol:Algorithm .

sol:NumericalApproximation
        a                owl:Class ;
        rdfs:subClassOf  sol:Approximation .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <humanResearch> , <reprMath> ;
        owl:versionInfo  "3.1.0" .

sol:GeneticAlgorithm  a  owl:Class ;
        rdfs:subClassOf  sol:HeuristicAlgorithm .

sol:ParallelAlgorithm
        a                owl:Class ;
        rdfs:subClassOf  sol:Solution , sol:Algorithm .

sol:SeriesAlgorithm  a   owl:Class ;
        rdfs:subClassOf  sol:Algorithm .

sol:Simulate  a              owl:Class ;
        rdfs:subClassOf      sol:Solution ;
        owl:equivalentClass  sol:Emulate , sol:Simulation .

sol:FlowChart  a         owl:Class ;
        rdfs:subClassOf  sol:Solution .

sol:Algorithm  a         owl:Class ;
        rdfs:subClassOf  sol:Solution .

sol:VisualSolution  a    owl:Class ;
        rdfs:subClassOf  sol:Approximation , sol:Solution .

sol:Parallel  a          owl:Class ;
        rdfs:subClassOf  sol:Solution .

sol:BreadthFirstSearch
        a                owl:Class ;
        rdfs:subClassOf  sol:Search .

sol:DynamicProgramming
        a                owl:Class ;
        rdfs:subClassOf  sol:Solution .
