#################################################################

###  http://sweetontology.net/rela/hasRole
<http://sweetontology.net/rela/hasRole> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/rela/hasState
<http://sweetontology.net/rela/hasState> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/matr/Air
:Air rdf:type owl:Class ;
     rdfs:subClassOf :Mixture .


###  http://sweetontology.net/matr/BDOC
:BDOC rdf:type owl:Class ;
      owl:equivalentClass :BiodegradableDissolvedOrganicCarbon .


###  http://sweetontology.net/matr/BiodegradableDissolvedOrganicCarbon
:BiodegradableDissolvedOrganicCarbon rdf:type owl:Class ;
                                     rdfs:subClassOf :DissolvedOrganicCarbon .


###  http://sweetontology.net/matr/Chemical
:Chemical rdf:type owl:Class ;
          owl:equivalentClass :ChemicalSubstance ;
          rdfs:subClassOf :Substance ;
          rdfs:comment "Chemical substance, in chemistry, are material objects that can undergo various transformations related to artificial or natural phenomena [Wikipedia]"@en .


###  http://sweetontology.net/matr/ChemicalSubstance
:ChemicalSubstance rdf:type owl:Class .


###  http://sweetontology.net/matr/DO
:DO rdf:type owl:Class ;
    owl:equivalentClass :DissolvedOxygen .


###  http://sweetontology.net/matr/DOC
:DOC rdf:type owl:Class ;
     owl:equivalentClass :DissolvedOrganicCarbon .


###  http://sweetontology.net/matr/DissolvedGas
:DissolvedGas rdf:type owl:Class ;
              rdfs:subClassOf :DissolvedSubstance ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                owl:hasValue <http://sweetontology.net/statePhysical/Gas>
                              ] .


###  http://sweetontology.net/matr/DissolvedOrganicCarbon
:DissolvedOrganicCarbon rdf:type owl:Class ;
                        rdfs:subClassOf :DissolvedSubstance .


###  http://sweetontology.net/matr/DissolvedOxygen
:DissolvedOxygen rdf:type owl:Class ;
                 rdfs:subClassOf :DissolvedSubstance .


###  http://sweetontology.net/matr/DissolvedSolid
:DissolvedSolid rdf:type owl:Class ;
                rdfs:subClassOf :DissolvedSubstance ,
                                :SolidSubstance .


###  http://sweetontology.net/matr/DissolvedSubstance
:DissolvedSubstance rdf:type owl:Class ;
                    rdfs:subClassOf :Substance ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                      owl:hasValue <http://sweetontology.net/statePhysical/Dissolved>
                                    ] .


###  http://sweetontology.net/matr/GreenhouseGas
:GreenhouseGas rdf:type owl:Class ;
               rdfs:subClassOf :ChemicalSubstance ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                 owl:hasValue <http://sweetontology.net/statePhysical/Gas>
                               ] .


###  http://sweetontology.net/matr/Inorganic
:Inorganic rdf:type owl:Class ;
           owl:equivalentClass :InorganicSubstance ;
           rdfs:subClassOf :ChemicalSubstance .


###  http://sweetontology.net/matr/InorganicSubstance
:InorganicSubstance rdf:type owl:Class .


###  http://sweetontology.net/matr/Medium
:Medium rdf:type owl:Class ;
        rdfs:subClassOf :Substance .


###  http://sweetontology.net/matr/MixedChemicalSubstance
:MixedChemicalSubstance rdf:type owl:Class ;
                        owl:equivalentClass :Mixture .


###  http://sweetontology.net/matr/Mixture
:Mixture rdf:type owl:Class ;
         rdfs:subClassOf :ChemicalSubstance ;
         rdfs:comment "In chemistry, a mixture is when two or more different substances are mixed together but not combined chemically. The molecules of two or more different substances are mixed in the form of solutions, suspensions, and colloids. [Wikipedia]"@en .


###  http://sweetontology.net/matr/NeutralGas
:NeutralGas rdf:type owl:Class ;
            rdfs:subClassOf :ChemicalSubstance ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/rela/hasState> ;
                              owl:hasValue <http://sweetontology.net/statePhysical/Gas>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/rela/hasState> ;
                              owl:hasValue <http://sweetontology.net/statePhysical/Neutral>
                            ] .


###  http://sweetontology.net/matr/OrganicLoading
:OrganicLoading rdf:type owl:Class ;
                rdfs:subClassOf :DissolvedSubstance .


###  http://sweetontology.net/matr/SolidSubstance
:SolidSubstance rdf:type owl:Class ;
                rdfs:subClassOf :Substance ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                  owl:hasValue <http://sweetontology.net/statePhysical/Solid>
                                ] .


###  http://sweetontology.net/matr/Solution
:Solution rdf:type owl:Class ;
          rdfs:subClassOf :Mixture ;
          rdfs:comment "In chemistry, a solution is a homogeneous mixture composed of two or more substances. In such a mixture, a solute is dissolved in another substance, known as a solvent. All solutions are characterized by interactions between the solvent phase and solute molecules or ions that result in a net decrease in free energy. [Wikipedia]"@en .


###  http://sweetontology.net/matr/Substance
:Substance rdf:type owl:Class .


###  http://sweetontology.net/matr/SuspendedGas
:SuspendedGas rdf:type owl:Class ;
              rdfs:subClassOf :SuspendedSubstance ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                owl:hasValue <http://sweetontology.net/statePhysical/Gas>
                              ] .


###  http://sweetontology.net/matr/SuspendedSolid
:SuspendedSolid rdf:type owl:Class ;
                rdfs:subClassOf :SolidSubstance ,
                                :SuspendedSubstance .


###  http://sweetontology.net/matr/SuspendedSubstance
:SuspendedSubstance rdf:type owl:Class ;
                    rdfs:subClassOf :Substance ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                      owl:hasValue <http://sweetontology.net/statePhysical/Suspended>
                                    ] .


###  http://sweetontology.net/matr/TraceGas
:TraceGas rdf:type owl:Class ;
          rdfs:subClassOf :TraceSubstance ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/rela/hasState> ;
                            owl:hasValue <http://sweetontology.net/statePhysical/Gas>
                          ] .


###  http://sweetontology.net/matr/TraceSubstance
:TraceSubstance rdf:type owl:Class ;
                rdfs:subClassOf :Substance ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://sweetontology.net/rela/hasRole> ;
                                  owl:hasValue <http://sweetontology.net/stateRole/Trace>
                                ] .


