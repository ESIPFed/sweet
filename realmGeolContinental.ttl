@base          <http://sweetontology.net/realmGeolContinental> .
@prefix chem:  <http://sweetontology.net/matr/> .
@prefix crela: <http://sweetontology.net/relaChemical/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix prela: <http://sweetontology.net/relaPhysical/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sgeo:  <http://sweetontology.net/stateRoleGeographic/> .
@prefix geol:  <http://sweetontology.net/realmGeol/> .
@prefix basin: <http://sweetontology.net/realmGeolBasin/> .
@prefix crat:  <http://sweetontology.net/realmGeolContinental/> .
@prefix ign:   <http://sweetontology.net/matrRockIgneous/> .
@prefix tec:   <http://sweetontology.net/realmLandTectonic/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix screla: <http://sweetontology.net/relaSci/> .
@prefix rela:  <http://sweetontology.net/rela/> .

crat:Craton  a           owl:Class ;
        rdfs:comment     "A craton is an old and stable part of the continental crust that has survived the merging and splitting of continents and supercontinents for at least 500 million years. Some are over two billion years old. Cratons are generally found in the interiors of continents and are characteristically composed of ancient crystalline basement crust of lightweight felsic igneous rock such as granite. They have a thick crust and deep roots that extend into the mantle beneath to depths of 200 km. [Wikipedia]"@en ;
        rdfs:subClassOf  geol:GeologicProvince ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  basin:IntracratonicBasin ;
                           owl:onProperty     prela:hasPlanetaryStructure
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  ign:Kimberlite ;
                           owl:onProperty     crela:hasSubstance
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  crat:Shield ;
                           owl:onProperty     prela:hasPlanetaryStructure
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  crat:ContinentalPlatform ;
                           owl:onProperty     prela:hasPlanetaryStructure
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  crat:ContinentalCrust ;
                           owl:onProperty     rela:partOf
                         ] .

crat:Shield  a           owl:Class ;
        rdfs:comment     "The extensive central cratons of continents may consist of both shields and platforms, and the crystalline basement. A shield is that part of a craton in which the usually Precambrian basement rocks crop out extensively at the surface.  [Wikipedia]"@en ;
        rdfs:subClassOf  geol:GeologicProvince .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <stateRoleGeographic> , <relaChemical> , <realmGeol> , <matr> , <relaSci> , <matrRockIgneous> , <realmGeolBasin> , <rela> , <relaPhysical> , <realmLandTectonic> ;
        owl:versionInfo  "3.1.0" .

crat:ContinentalCrust
        a                owl:Class ;
        rdfs:comment     "The continental crust is typically from 30 km (20 mi) to 50 km (30 mi) thick, and it is mostly composed of less dense rocks, such as granite, than is the oceanic crust. The continental crust has an average composition similar to that of the igneous rock, andesite.[Wikipedia]"@en ;
        rdfs:subClassOf  geol:Continental , geol:Crust ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  crat:ContinentalLithosphere ;
                           owl:onProperty     rela:partOf
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      screla:hasRock ;
                           owl:someValuesFrom  ign:Felsic
                         ] .

crat:ActiveContinent  a  owl:Class ;
        rdfs:comment     "A distinction of whether the continental margins are active (subducting).  [Wikipedia]"@en ;
        rdfs:subClassOf  geol:GeologicProvince ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  crat:ContinentalLithosphere ;
                           owl:onProperty     rela:partOf
                         ] .

crat:ContinentalLithosphere
        a                owl:Class ;
        rdfs:comment     "[Mechanical or Seismic Definition of Structure] In the Earth, the lithosphere includes the crust and the uppermost mantle, which constitute the hard and rigid outer layer of the planet. The [Wikipedia]"@en ;
        rdfs:subClassOf  geol:Continental , geol:Lithosphere ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  crat:Craton ;
                           owl:onProperty     prela:hasPlanetaryStructure
                         ] .

crat:Supercontinent  a   owl:Class ;
        rdfs:comment     "In geology, a supercontinent is a land mass comprising more than one continental core, or craton. The assembly of cratons and accreted terranes that form Eurasia qualifies as a supercontinent today." ;
        rdfs:subClassOf  geol:GeologicProvince ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  crat:ContinentalLithosphere ;
                           owl:onProperty     rela:hasRealm
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  crat:Craton ;
                           owl:onProperty     prela:hasPlanetaryStructure
                         ] .

crat:StableContinent  a  owl:Class ;
        rdfs:comment     "A distinction of whether the continental is on a stable paltform, i.e., over millions of years. [Wikipedia]"@en ;
        rdfs:subClassOf  geol:GeologicProvince ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  crat:ContinentalLithosphere ;
                           owl:onProperty     rela:partOf
                         ] .

crat:ContinentalMargin
        a                owl:Class ;
        rdfs:comment     "The continental margin is the zone of the ocean floor that separates the thin oceanic crust from thick continental crust. [Wikipedia]"@en ;
        rdfs:subClassOf  sgeo:Margin .

crat:ContinentalRift  a  owl:Class ;
        rdfs:comment     "A long, narrow fissure in the Earth marking a zone of the lithosphere that has become thinner due to extensional forces associated with plate teconics. Continental rifts are thousands of kilometers in length and hundreds of kilometers in width, and they are associated with normal faults and with grabens. [FreeDictionary]"@en ;
        rdfs:subClassOf  tec:Rift , tec:DivergentBoundary .

crat:ContinentalPlatform
        a                owl:Class ;
        rdfs:comment     "The extensive central cratons of continents may consist of both shields and platforms, and the crystalline basement. A platform is that part of the craton for which the basement is overlain by horizontal or subhorizontal sediments. [Wikipedia]"@en ;
        rdfs:subClassOf  geol:GeologicProvince .
