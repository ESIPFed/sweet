@base          <http://sweetontology.net/rela> .
@prefix proc:  <http://sweetontology.net/proc/> .
@prefix phen:  <http://sweetontology.net/phen/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix prop:  <http://sweetontology.net/prop/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix realm: <http://sweetontology.net/realm/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix state: <http://sweetontology.net/state/> .
@prefix rela:  <http://sweetontology.net/rela/> .

rela:hasProperty  a  owl:ObjectProperty ;
        rdfs:range  prop:Property .

rela:contains  a  owl:ObjectProperty .

rela:hasRole  a     owl:ObjectProperty ;
        rdfs:range  prop:Role .

rela:hasPhenomena  a  owl:ObjectProperty ;
        rdfs:range  phen:Phenomena .

rela:hasState  a    owl:ObjectProperty ;
        rdfs:range  state:State .

rela:hasRealm  a    owl:ObjectProperty ;
        rdfs:range  realm:Realm .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <phen> , <state> , <realm> , <prop> , <proc> ;
        owl:versionInfo  "3.1.0" .

rela:hasProcess  a  owl:ObjectProperty ;
        rdfs:range  proc:Process .

rela:partOf  a  owl:TransitiveProperty , owl:ObjectProperty .
