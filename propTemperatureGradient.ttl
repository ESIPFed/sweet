###  http://sweetontology.net/propTemperatureGradient/AutoConvectiveLapseRate
:AutoConvectiveLapseRate rdf:type owl:Class ;
                         rdfs:subClassOf :EnvironmentalLapseRate ;
                         rdfs:comment "The environmental lapse rate of temperature in an atmosphere in which the density is constant with height (homogeneous atmosphere), equal to g/R, where g is the acceleration of gravity and R the gas constant."@en .


###  http://sweetontology.net/propTemperatureGradient/DryAdiabaticLapseRate
:DryAdiabaticLapseRate rdf:type owl:Class ;
                       rdfs:subClassOf :LapseRate ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                         owl:hasValue <http://sweetontology.net/stateThermodynamic/DryAdiabatic>
                                       ] ;
                       rdfs:comment "A process lapse rate of temperature, the rate of decrease of temperature with height of a parcel of dry air lifted by a reversible adiabatic process through an atmosphere in hydrostatic equilibrium."@en .


###  http://sweetontology.net/propTemperatureGradient/EnvironmentalLapseRate
:EnvironmentalLapseRate rdf:type owl:Class ;
                        rdfs:subClassOf :LapseRate ;
                        rdfs:comment "The rate of decrease of temperature with elevation"@en .


###  http://sweetontology.net/propTemperatureGradient/LapseRate
:LapseRate rdf:type owl:Class ;
           rdfs:subClassOf :TemperatureHeightDerivative .


###  http://sweetontology.net/propTemperatureGradient/TemperatureDepthDerivative
:TemperatureDepthDerivative rdf:type owl:Class ;
                            rdfs:subClassOf :TemperatureGradient ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://sweetontology.net/relaMath/derivativeWithRespectTo> ;
                                              owl:allValuesFrom <http://sweetontology.net/propSpaceHeight/Depth>
                                            ] ;
                            rdfs:comment "The derivative of temperature with respect to depth."@en .


###  http://sweetontology.net/propTemperatureGradient/TemperatureGradient
:TemperatureGradient rdf:type owl:Class ;
                     owl:equivalentClass :ThermalGradient ;
                     rdfs:subClassOf <http://sweetontology.net/propQuantity/ThermodynamicQuantity> ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://sweetontology.net/relaMath/hasGradient> ;
                                       owl:allValuesFrom <http://sweetontology.net/propTemperature/Temperature>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                                       owl:hasValue <http://sweetontology.net/reprSciUnits/kelvinPerMeter>
                                     ] ;
                     rdfs:comment "The derivative of temperature."@en .


###  http://sweetontology.net/propTemperatureGradient/TemperatureGradientRange
:TemperatureGradientRange rdf:type owl:Class ;
                          rdfs:subClassOf <http://sweetontology.net/propQuantity/IntervalQuantity> ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://sweetontology.net/relaSci/rangeOf> ;
                                            owl:someValuesFrom :TemperatureGradient
                                          ] .


###  http://sweetontology.net/propTemperatureGradient/TemperatureHeightDerivative
:TemperatureHeightDerivative rdf:type owl:Class ;
                             rdfs:subClassOf :TemperatureGradient ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty <http://sweetontology.net/relaMath/derivativeWithRespectTo> ;
                                               owl:allValuesFrom <http://sweetontology.net/propSpaceHeight/Height>
                                             ] ;
                             rdfs:comment "The derivative of temperature with respect to height."@en .


###  http://sweetontology.net/propTemperatureGradient/ThermalGradient
:ThermalGradient rdf:type owl:Class .


