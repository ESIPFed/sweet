@base          <http://sweetontology.net/matrFacility> .
@prefix infr:  <http://sweetontology.net/matrEquipment/> .
@prefix hum:   <http://sweetontology.net/relaHuman/> .
@prefix struc: <http://sweetontology.net/matrFacility/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tran:  <http://sweetontology.net/humanTransportation/> .

struc:Platform  a        owl:Class ;
        rdfs:subClassOf  struc:Structure .

struc:Meteostation  a    owl:Class ;
        rdfs:subClassOf  struc:MeteorologicalFacility .

struc:Farm  a            owl:Class ;
        rdfs:subClassOf  struc:AgriculturalProductionFacility .

struc:MeteorologicalFacility
        a                owl:Class ;
        rdfs:subClassOf  struc:Facility .

struc:Fishery  a         owl:Class ;
        rdfs:subClassOf  struc:AgriculturalProductionFacility .

struc:AirTrafficControlRadarRoom
        a                owl:Class ;
        rdfs:subClassOf  struc:AirportFacility .

struc:Airport  a         owl:Class ;
        rdfs:subClassOf  struc:AirportFacility .

struc:Plantation  a      owl:Class ;
        rdfs:subClassOf  struc:Farm .

struc:Station  a         owl:Class ;
        rdfs:subClassOf  struc:Facility .

struc:AgriculturalProductionFacility
        a                owl:Class ;
        rdfs:subClassOf  struc:Facility .

struc:CommunicationFacility
        a                owl:Class ;
        rdfs:subClassOf  struc:Facility .

struc:Runway  a          owl:Class ;
        rdfs:subClassOf  struc:AirportFacility .

struc:TransportationFacility
        a                owl:Class ;
        rdfs:subClassOf  struc:Facility .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <relaHuman> , <humanTransportation> , <matrEquipment> ;
        owl:versionInfo  "3.1.0" .

struc:Mine  a            owl:Class ;
        rdfs:subClassOf  struc:MiningFacility .

struc:MiningFacility  a  owl:Class ;
        rdfs:subClassOf  struc:Facility .

struc:Base  a            owl:Class ;
        rdfs:subClassOf  struc:Facility .

struc:AirTrafficControlCenter
        a                owl:Class ;
        rdfs:subClassOf  struc:AirportFacility .

struc:Dome  a            owl:Class ;
        rdfs:subClassOf  struc:Facility .

struc:Structure  a       owl:Class ;
        rdfs:subClassOf  infr:Infrastructure .

struc:AirTrafficControlEnRouteCenter
        a                owl:Class ;
        rdfs:subClassOf  struc:AirTrafficControlCenter .

struc:AirTransitway  a   owl:Class ;
        rdfs:subClassOf  struc:AirportFacility .

struc:CulturalFeature
        a                owl:Class ;
        rdfs:subClassOf  struc:Structure .

struc:AirportFacility
        a                owl:Class ;
        rdfs:subClassOf  struc:Facility ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  tran:AirTraffic ;
                           owl:onProperty     hum:hasHumanActivity
                         ] .

struc:Habitat  a         owl:Class ;
        rdfs:subClassOf  struc:Building .

struc:Facility  a        owl:Class ;
        rdfs:subClassOf  struc:Structure .

struc:AirTrafficControlTower
        a                owl:Class ;
        rdfs:subClassOf  struc:AirportFacility .

struc:Building  a        owl:Class ;
        rdfs:subClassOf  struc:Structure .
