#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/realmAtmo/AtmosphereLayer
:AtmosphereLayer rdf:type owl:Class ;
                 rdfs:subClassOf <http://sweetontology.net/realm/PlanetaryLayer> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                                   owl:someValuesFrom <http://sweetontology.net/realm/Atmosphere>
                                 ] .


###  http://sweetontology.net/realmAtmo/FreeTroposphere
:FreeTroposphere rdf:type owl:Class ;
                 rdfs:subClassOf :Troposphere .


###  http://sweetontology.net/realmAtmo/InversionLayer
:InversionLayer rdf:type owl:Class ;
                owl:equivalentClass :TemperatureInversionLayer ;
                rdfs:subClassOf :AtmosphereLayer ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                  owl:someValuesFrom <http://sweetontology.net/phenFluidInstability/Inversion>
                                ] .


###  http://sweetontology.net/realmAtmo/JungeAerosolLayer
:JungeAerosolLayer rdf:type owl:Class ;
                   rdfs:subClassOf :AtmosphereLayer ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://sweetontology.net/relaChemical/hasSubstance> ;
                                     owl:allValuesFrom <http://sweetontology.net/matrAerosol/Aerosol>
                                   ] ;
                   rdfs:comment "A maximum in large-particle concentrations observed in the lower stratosphere between 15 and 25 km."@en .


###  http://sweetontology.net/realmAtmo/LowerAtmosphere
:LowerAtmosphere rdf:type owl:Class ;
                 rdfs:subClassOf :AtmosphereLayer .


###  http://sweetontology.net/realmAtmo/Mesopause
:Mesopause rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/realm/PlanetaryBoundary> ;
           rdfs:comment "The top of the mesosphere and the base of the thermosphere. The mesopause is usually located at heights of 85?95 km, and is the site of the coldest temperatures in the atmosphere. Temperatures as low as 100 K (- 173?C) have been measured at the mesopause by rockets. See atmospheric shell."@en .


###  http://sweetontology.net/realmAtmo/Mesosphere
:Mesosphere rdf:type owl:Class ;
            rdfs:subClassOf :AtmosphereLayer ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaSpace/hasLowerBoundary> ;
                              owl:allValuesFrom :Stratopause
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaSpace/hasUpperBoundary> ;
                              owl:allValuesFrom :Mesopause
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaPhysical/dTdh> ;
                              owl:hasValue <http://sweetontology.net/propTemperatureGradient/NegativeSlope>
                            ] .


###  http://sweetontology.net/realmAtmo/MiddleAtmosphere
:MiddleAtmosphere rdf:type owl:Class ;
                  owl:equivalentClass :OzoneLayer ;
                  rdfs:subClassOf :AtmosphereLayer .


###  http://sweetontology.net/realmAtmo/OzoneLayer
:OzoneLayer rdf:type owl:Class .


###  http://sweetontology.net/realmAtmo/Stratopause
:Stratopause rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/realm/PlanetaryBoundary> .


###  http://sweetontology.net/realmAtmo/Stratosphere
:Stratosphere rdf:type owl:Class ;
              rdfs:subClassOf :AtmosphereLayer ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaSpace/hasLowerBoundary> ;
                                owl:allValuesFrom :Tropopause
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaSpace/hasUpperBoundary> ;
                                owl:allValuesFrom :Stratopause
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaPhysical/dTdh> ;
                                owl:hasValue <http://sweetontology.net/propTemperatureGradient/PositiveSlope>
                              ] .


###  http://sweetontology.net/realmAtmo/TemperatureInversionLayer
:TemperatureInversionLayer rdf:type owl:Class .


###  http://sweetontology.net/realmAtmo/Thermosphere
:Thermosphere rdf:type owl:Class ;
              rdfs:subClassOf :AtmosphereLayer ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaSpace/hasLowerBoundary> ;
                                owl:allValuesFrom :Mesopause
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaPhysical/dTdh> ;
                                owl:hasValue <http://sweetontology.net/propTemperatureGradient/PositiveSlope>
                              ] .


###  http://sweetontology.net/realmAtmo/Tropopause
:Tropopause rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/realm/PlanetaryBoundary> .


###  http://sweetontology.net/realmAtmo/Troposphere
:Troposphere rdf:type owl:Class ;
             rdfs:subClassOf :AtmosphereLayer ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaSpace/hasLowerBoundary> ;
                               owl:allValuesFrom <http://sweetontology.net/realm/PlanetarySurface>
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaSpace/hasUpperBoundary> ;
                               owl:allValuesFrom :Tropopause
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaPhysical/dTdh> ;
                               owl:hasValue <http://sweetontology.net/propTemperatureGradient/NegativeSlope>
                             ] .


###  http://sweetontology.net/realmAtmo/UpperAtmosphere
:UpperAtmosphere rdf:type owl:Class ;
                 rdfs:subClassOf :AtmosphereLayer .


