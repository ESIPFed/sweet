###  http://sweetontology.net/realmAstroStar/ActiveRegion
:ActiveRegion rdf:type owl:Class ;
              rdfs:subClassOf :StellarRealm .


###  http://sweetontology.net/realmAstroStar/Chromosphere
:Chromosphere rdf:type owl:Class ;
              rdfs:subClassOf :StellarAtmosphere ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                owl:hasValue <http://sweetontology.net/statePhysical/Plasma>
                              ] .


###  http://sweetontology.net/realmAstroStar/ConvectionZone
:ConvectionZone rdf:type owl:Class ;
                rdfs:subClassOf :StellarInterior ;
                owl:disjointWith :StellarCore .


###  http://sweetontology.net/realmAstroStar/Corona
:Corona rdf:type owl:Class ;
        rdfs:subClassOf :StellarAtmosphere ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/rela/hasState> ;
                          owl:hasValue <http://sweetontology.net/statePhysical/Plasma>
                        ] .


###  http://sweetontology.net/realmAstroStar/Photosphere
:Photosphere rdf:type owl:Class ;
             rdfs:subClassOf :StellarAtmosphere .


###  http://sweetontology.net/realmAstroStar/RadiativeZone
:RadiativeZone rdf:type owl:Class ;
               rdfs:subClassOf :StellarInterior .


###  http://sweetontology.net/realmAstroStar/StellarAtmosphere
:StellarAtmosphere rdf:type owl:Class ;
                   rdfs:subClassOf :StellarRealm .


###  http://sweetontology.net/realmAstroStar/StellarCore
:StellarCore rdf:type owl:Class ;
             rdfs:subClassOf :StellarInterior ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaChemical/hasChemicalReaction> ;
                               owl:someValuesFrom <http://sweetontology.net/procChemical/NuclearFusion>
                             ] .


###  http://sweetontology.net/realmAstroStar/StellarInterior
:StellarInterior rdf:type owl:Class ;
                 rdfs:subClassOf :StellarRealm .


###  http://sweetontology.net/realmAstroStar/StellarRealm
:StellarRealm rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/realm/Realm> .


###  http://sweetontology.net/realmAstroStar/Tacocline
:Tacocline rdf:type owl:Class ;
           rdfs:subClassOf :StellarInterior .


