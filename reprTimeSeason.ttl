@prefix : <http://sweetontology.net/reprTimeSeason/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/reprTimeSeason> .

<http://sweetontology.net/reprTimeSeason> rdf:type owl:Ontology ;
                                           owl:imports <http://sweetontology.net/propTemperature> ,
                                                       <http://sweetontology.net/propTime> ,
                                                       <http://sweetontology.net/rela> ,
                                                       <http://sweetontology.net/relaMath> ,
                                                       <http://sweetontology.net/relaSci> ,
                                                       <http://sweetontology.net/relaTime> ,
                                                       <http://sweetontology.net/repr> ,
                                                       <http://sweetontology.net/reprSciUnits> ,
                                                       <http://sweetontology.net/statePhysical> ;
                                           rdfs:label "SWEET Ontology" ;
                                           owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/reprTimeSeason/ColdestMonth
:ColdestMonth rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/propTime/Time> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaMath/hasMinimum> ;
                                owl:someValuesFrom <http://sweetontology.net/propTemperature/Temperature>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                                owl:hasValue <http://sweetontology.net/reprSciUnits/month>
                              ] .


###  http://sweetontology.net/reprTimeSeason/DrySeason
:DrySeason rdf:type owl:Class ;
           rdfs:subClassOf :Season ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/rela/hasState> ;
                             owl:hasValue <http://sweetontology.net/statePhysical/Dry>
                           ] .


###  http://sweetontology.net/reprTimeSeason/Equinox
:Equinox rdf:type owl:Class ;
         rdfs:subClassOf <http://www.w3.org/2006/time#Instant> .


###  http://sweetontology.net/reprTimeSeason/Season
:Season rdf:type owl:Class ;
        rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> .


###  http://sweetontology.net/reprTimeSeason/Solstice
:Solstice rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/2006/time#Instant> .


###  http://sweetontology.net/reprTimeSeason/WarmestMonth
:WarmestMonth rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/propTime/Time> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaMath/hasMaximum> ;
                                owl:someValuesFrom <http://sweetontology.net/propTemperature/Temperature>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                                owl:hasValue <http://sweetontology.net/reprSciUnits/month>
                              ] .


###  http://sweetontology.net/reprTimeSeason/WetSeason
:WetSeason rdf:type owl:Class ;
           rdfs:subClassOf :Season ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/rela/hasState> ;
                             owl:hasValue <http://sweetontology.net/statePhysical/Wet>
                           ] .


#################################################################
#    Individuals
#################################################################

###  http://sweetontology.net/reprSciUnits/month
<http://sweetontology.net/reprSciUnits/month> rdf:type owl:NamedIndividual .


###  http://sweetontology.net/reprTimeSeason/Autumn
:Autumn rdf:type owl:NamedIndividual ,
                 :Season ;
        owl:sameAs :Fall ;
        <http://www.w3.org/2006/time#hasBeginning> :AutumnalEquinox ;
        <http://www.w3.org/2006/time#hasEnd> :WinterSolstice .


###  http://sweetontology.net/reprTimeSeason/AutumnalEquinox
:AutumnalEquinox rdf:type owl:NamedIndividual ,
                          :Equinox ;
                 owl:sameAs :FallEquinox .


###  http://sweetontology.net/reprTimeSeason/DecemberSolstice
:DecemberSolstice rdf:type owl:NamedIndividual ,
                           :Solstice ;
                  <http://sweetontology.net/relaTime/dayOfYear> "355"^^xsd:nonNegativeInteger .


###  http://sweetontology.net/reprTimeSeason/Fall
:Fall rdf:type owl:NamedIndividual ,
               :Season .


###  http://sweetontology.net/reprTimeSeason/FallEquinox
:FallEquinox rdf:type owl:NamedIndividual ,
                      :Equinox .


###  http://sweetontology.net/reprTimeSeason/JuneSolstice
:JuneSolstice rdf:type owl:NamedIndividual ,
                       :Solstice ;
              <http://sweetontology.net/relaTime/dayOfYear> "172"^^xsd:nonNegativeInteger .


###  http://sweetontology.net/reprTimeSeason/Spring
:Spring rdf:type owl:NamedIndividual ,
                 :Season ;
        <http://www.w3.org/2006/time#hasBeginning> :SpringEquinox ;
        <http://www.w3.org/2006/time#hasEnd> :SummerSolstice .


###  http://sweetontology.net/reprTimeSeason/SpringEquinox
:SpringEquinox rdf:type owl:NamedIndividual ,
                        :Equinox .


###  http://sweetontology.net/reprTimeSeason/Summer
:Summer rdf:type owl:NamedIndividual ,
                 :Season ;
        <http://www.w3.org/2006/time#hasBeginning> :SummerSolstice ;
        <http://www.w3.org/2006/time#hasEnd> :FallEquinox .


###  http://sweetontology.net/reprTimeSeason/SummerSolstice
:SummerSolstice rdf:type owl:NamedIndividual ,
                         :Solstice ;
                <http://sweetontology.net/relaTime/hasNHTime> :JuneSolstice ;
                <http://sweetontology.net/relaTime/hasSHTime> :DecemberSolstice .


###  http://sweetontology.net/reprTimeSeason/Winter
:Winter rdf:type owl:NamedIndividual ,
                 :Season ;
        <http://www.w3.org/2006/time#hasBeginning> :WinterSolstice ;
        <http://www.w3.org/2006/time#hasEnd> :SpringEquinox .


###  http://sweetontology.net/reprTimeSeason/WinterSolstice
:WinterSolstice rdf:type owl:NamedIndividual ,
                         :Solstice ;
                <http://sweetontology.net/relaTime/hasNHTime> :DecemberSolstice ;
                <http://sweetontology.net/relaTime/hasSHTime> :JuneSolstice .


###  http://sweetontology.net/statePhysical/Dry
<http://sweetontology.net/statePhysical/Dry> rdf:type owl:NamedIndividual .


###  http://sweetontology.net/statePhysical/Wet
<http://sweetontology.net/statePhysical/Wet> rdf:type owl:NamedIndividual .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :FallEquinox
                        :SpringEquinox
                      )
] .


