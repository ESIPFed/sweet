                                owl:someValuesFrom <http://sweetontology.net/phenCycle/SolarCycle>
                              ] .


###  http://sweetontology.net/stateTimeCycle/SolarMinimum
:SolarMinimum rdf:type owl:Class ;
              rdfs:subClassOf :OscillationState ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaMath/minimumOf> ;
                                owl:someValuesFrom <http://sweetontology.net/phenCycle/SolarCycle>
                              ] .


###  http://sweetontology.net/stateTimeCycle/SpringTide
:SpringTide rdf:type owl:Class ;
            rdfs:subClassOf :OscillationState ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaMath/maximumOf> ;
                              owl:someValuesFrom :HighTide
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaMath/minimumOf> ;
                              owl:someValuesFrom :LowTide
                            ] .


