@base          <http://sweetontology.net/stateTimeCycle> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix pstate: <http://sweetontology.net/statePhysical/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cyc:   <http://sweetontology.net/phenCycle/> .
@prefix scyc:  <http://sweetontology.net/stateTimeCycle/> .

scyc:NeapTide  a         owl:Class ;
        rdfs:subClassOf  scyc:OscillationState ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:minimumOf ;
                           owl:someValuesFrom  scyc:HighTide
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:maximumOf ;
                           owl:someValuesFrom  scyc:LowTide
                         ] .

scyc:HighTide  a         owl:Class ;
        rdfs:subClassOf  scyc:OscillationState ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:maximumOf ;
                           owl:someValuesFrom  cyc:Tide
                         ] .

scyc:SolarMinimum  a     owl:Class ;
        rdfs:subClassOf  scyc:OscillationState ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:minimumOf ;
                           owl:someValuesFrom  cyc:SolarCycle
                         ] .

scyc:Interglacial  a     owl:Class ;
        rdfs:subClassOf  scyc:OscillationState ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:minimumOf ;
                           owl:someValuesFrom  cyc:IceAgeCycle
                         ] .

scyc:MaunderMinimum  a   owl:Class ;
        rdfs:subClassOf  scyc:SolarMinimum .

scyc:LaNina  a           owl:Class ;
        rdfs:comment     "The most common of several names given toa significant decrease in sea surface temperature (\"cold events\") in the central and eastern equatorial Pacific.  La Nina is the counterpart to the El Nino \"warm event,\" and its spatial and temporal evolution in the equatorial Pacific is, to a considerable extent, the mirror image of El Nino, although La Nina events tend to be somewhat less regular in their behavior and duration."@en ;
        rdfs:subClassOf  scyc:OscillationState ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:minimumOf ;
                           owl:someValuesFrom  cyc:ENSO
                         ] .

scyc:ElNino  a           owl:Class ;
        rdfs:comment     "A significant increase in sea surface temperature over the eastern and central equatorial Pacific that occurs at irregular intervals, generally ranging between two and seven years."@en ;
        rdfs:subClassOf  scyc:OscillationState ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:maximumOf ;
                           owl:someValuesFrom  cyc:ENSO
                         ] .

scyc:LowTide  a          owl:Class ;
        rdfs:subClassOf  scyc:OscillationState ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:minimumOf ;
                           owl:someValuesFrom  cyc:Tide
                         ] .

scyc:SolarMaximum  a     owl:Class ;
        rdfs:subClassOf  scyc:OscillationState ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:maximumOf ;
                           owl:someValuesFrom  cyc:SolarCycle
                         ] .

scyc:SpringTide  a       owl:Class ;
        rdfs:subClassOf  scyc:OscillationState ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:maximumOf ;
                           owl:someValuesFrom  scyc:HighTide
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:minimumOf ;
                           owl:someValuesFrom  scyc:LowTide
                         ] .

scyc:OscillationState
        a                owl:Class ;
        rdfs:subClassOf  pstate:PhysicalState .

scyc:IceAge  a           owl:Class ;
        rdfs:subClassOf  scyc:OscillationState ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:maximumOf ;
                           owl:someValuesFrom  cyc:IceAgeCycle
                         ] .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <relaMath> , <statePhysical> , <phenCycle> ;
        owl:versionInfo  "3.1.0" .
