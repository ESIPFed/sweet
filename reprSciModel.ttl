###  http://sweetontology.net/reprSciModel/BoussinesqApproximation
:BoussinesqApproximation rdf:type owl:Class ;
                         rdfs:subClassOf :PhysicalApproximation ;
                         rdfs:comment "An approximation to the dynamical equations of motion whereby density is assumed to be constant except in the buoyancy term of the vertical velocity equation."@en .


###  http://sweetontology.net/reprSciModel/ClimateModel
:ClimateModel rdf:type owl:Class ;
              rdfs:subClassOf :Model .


###  http://sweetontology.net/reprSciModel/EnsembleForecast
:EnsembleForecast rdf:type owl:Class ;
                  rdfs:subClassOf :Forecast ;
                  rdfs:comment "A set of different forecasts all valid at the same forecast time(s)."@en .


###  http://sweetontology.net/reprSciModel/Estimate
:Estimate rdf:type owl:Class ;
          rdfs:subClassOf :Prediction .


###  http://sweetontology.net/reprSciModel/ExtendedForecast
:ExtendedForecast rdf:type owl:Class ;
                  rdfs:subClassOf :WeatherForecast ;
                  rdfs:comment "A forecast of weather conditions for a period extending beyond three or more days from the day of issuance."@en .


###  http://sweetontology.net/reprSciModel/FiniteElement
:FiniteElement rdf:type owl:Class ;
               rdfs:subClassOf :Model .


###  http://sweetontology.net/reprSciModel/FluxCorrection
:FluxCorrection rdf:type owl:Class ;
                rdfs:subClassOf :PhysicalApproximation .


###  http://sweetontology.net/reprSciModel/Forecast
:Forecast rdf:type owl:Class ;
          rdfs:subClassOf :Prediction .


###  http://sweetontology.net/reprSciModel/GCM
:GCM rdf:type owl:Class ;
     owl:equivalentClass :GeneralCirculationModel .


###  http://sweetontology.net/reprSciModel/GeneralCirculationModel
:GeneralCirculationModel rdf:type owl:Class ;
                         rdfs:subClassOf :Model .


###  http://sweetontology.net/reprSciModel/Hypothesis
:Hypothesis rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/humanResearch/ExperimentActivity> .


###  http://sweetontology.net/reprSciModel/MixingScheme
:MixingScheme rdf:type owl:Class ;
              rdfs:subClassOf :Parameterization .


###  http://sweetontology.net/reprSciModel/Model
:Model rdf:type owl:Class ;
       owl:equivalentClass :ScientificModel ;
       rdfs:subClassOf <http://sweetontology.net/repr/Representation> .


###  http://sweetontology.net/reprSciModel/Parameterization
:Parameterization rdf:type owl:Class ;
                  rdfs:subClassOf :Model .


###  http://sweetontology.net/reprSciModel/Parcel
:Parcel rdf:type owl:Class ;
        rdfs:subClassOf :Model ;
        rdfs:comment "An imaginary volume of fluid to which may be assigned various thermodynamic and kinematic quantities."@en .


###  http://sweetontology.net/reprSciModel/PhysicalApproximation
:PhysicalApproximation rdf:type owl:Class ;
                       rdfs:subClassOf :Model .


###  http://sweetontology.net/reprSciModel/Predict
:Predict rdf:type owl:Class ;
         owl:equivalentClass :Prediction ;
         rdfs:subClassOf <http://sweetontology.net/humanResearch/ExperimentActivity> .


###  http://sweetontology.net/reprSciModel/Prediction
:Prediction rdf:type owl:Class .


###  http://sweetontology.net/reprSciModel/ReactionPathModel
:ReactionPathModel rdf:type owl:Class ;
                   rdfs:subClassOf :Model ;
                   rdfs:comment "A simulation approach to studying the chemical evolution of a (natural) system."@en .


###  http://sweetontology.net/reprSciModel/RigidLidApproximation
:RigidLidApproximation rdf:type owl:Class ;
                       rdfs:subClassOf :PhysicalApproximation .


###  http://sweetontology.net/reprSciModel/ScientificModel
:ScientificModel rdf:type owl:Class .


###  http://sweetontology.net/reprSciModel/Spectral
:Spectral rdf:type owl:Class ;
          rdfs:subClassOf :Model .


###  http://sweetontology.net/reprSciModel/SpectralElement
:SpectralElement rdf:type owl:Class ;
                 rdfs:subClassOf :Model .


###  http://sweetontology.net/reprSciModel/Theoretical
:Theoretical rdf:type owl:Class ;
             owl:equivalentClass :Theory .


###  http://sweetontology.net/reprSciModel/Theory
:Theory rdf:type owl:Class ;
        rdfs:subClassOf <http://sweetontology.net/humanResearch/ExperimentActivity> .


###  http://sweetontology.net/reprSciModel/WeatherForecast
:WeatherForecast rdf:type owl:Class ;
                 rdfs:subClassOf :Forecast ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                   owl:allValuesFrom <http://sweetontology.net/phenAtmo/Weather>
                                 ] .


