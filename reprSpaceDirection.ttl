@base          <http://sweetontology.net/reprSpaceDirection> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix mdir:  <http://sweetontology.net/propSpaceDirection/> .
@prefix srela2: <http://sweetontology.net/relaSpace/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dir:   <http://sweetontology.net/reprSpaceDirection/> .
@prefix mdir2: <http://sweetontology.net/propSpaceDirection/> .

dir:Upwind  a              mdir2:Direction ;
        srela2:oppositeTo  dir:Downstream .

dir:Downwind  a  mdir2:Direction .

dir:Parallel  a  mdir2:Direction .

dir:Shear  a    mdir2:Direction .

dir:Up  a                  mdir2:VerticalDirection ;
        srela2:oppositeTo  dir:Down ;
        owl:sameAs         dir:Upward .

dir:North  a               mdir2:HorizontalDirection ;
        srela2:oppositeTo  dir:South ;
        owl:sameAs         dir:Northward .

dir:X   a       mdir2:HorizontalDirection .

dir:Retrograde  a          mdir2:AngularDirection ;
        srela2:oppositeTo  dir:Prograde .

dir:Westward  a  mdir2:HorizontalDirection .

dir:LineOfSight  a  mdir2:AngularDirection .

dir:South  a        mdir2:HorizontalDirection ;
        owl:sameAs  dir:Southward .

dir:Seaward  a             mdir2:Direction ;
        srela2:oppositeTo  dir:Landward .

dir:Specular  a  mdir2:AngularDirection .

dir:Normal  a   mdir2:Direction .

dir:Directional  a  mdir2:AngularDirection .

dir:Emerging  a  mdir2:AngularDirection .

dir:Windward  a            mdir2:HorizontalDirection ;
        srela2:oppositeTo  dir:Leeward .

dir:Downward  a  mdir2:VerticalDirection .

dir:Eastward  a  mdir2:HorizontalDirection .

dir:Direct  a   mdir2:AngularDirection .

dir:Lee  a          mdir2:HorizontalDirection ;
        owl:sameAs  dir:Leeward .

dir:Longitudinal  a  mdir2:Direction .

dir:Prograde  a  mdir2:AngularDirection .

dir:Sunward  a  mdir2:Direction .

dir:Diffuse  a  mdir2:AngularDirection .

dir:Vertical  a                 mdir2:VerticalDirection ;
        srela2:perpendicularTo  dir:Horizontal .

dir:Downstream  a  mdir2:Direction .

dir:Clockwise  a  mdir2:AngularDirection .

dir:Horizontal  a  mdir2:HorizontalDirection .

dir:Upward  a   mdir2:VerticalDirection .

dir:Leeward  a  mdir2:HorizontalDirection .

dir:Landward  a  mdir2:Direction .

dir:Right  a               mdir2:Direction ;
        srela2:oppositeTo  dir:Left .

dir:Northward  a  mdir2:HorizontalDirection .

dir:Sinking  a  mdir2:VerticalDirection .

dir:Down  a         mdir2:VerticalDirection ;
        owl:sameAs  dir:Downward .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <relaSpace> , <propSpaceDirection> ;
        owl:versionInfo  "3.1.0" .

dir:Downslope  a  mdir2:Direction .

dir:Oblique  a             mdir2:Direction ;
        owl:differentFrom  dir:Parallel .

dir:Z   a                    mdir2:VerticalDirection ;
        srela2:orthogonalTo  dir:X .

dir:Downflow  a  mdir2:VerticalDirection .

dir:Perpendicular  a         mdir2:Direction ;
        srela2:orthogonalTo  dir:Parallel ;
        owl:sameAs           dir:Normal , dir:Transverse .

dir:Upflow  a              mdir2:VerticalDirection ;
        srela2:oppositeTo  dir:Downflow .

dir:Zenith  a   mdir2:VerticalDirection .

dir:Left  a     mdir2:Direction .

dir:Southward  a  mdir2:HorizontalDirection .

dir:East  a         mdir2:HorizontalDirection ;
        owl:sameAs  dir:Eastward .

dir:Hemispheric  a  mdir2:AngularDirection .

dir:Transverse  a  mdir2:Direction .

dir:Counterclockwise  a    mdir2:AngularDirection ;
        owl:differentFrom  dir:Clockwise .

dir:Y   a                    mdir2:HorizontalDirection ;
        srela2:orthogonalTo  dir:X .

dir:West  a                mdir2:HorizontalDirection ;
        srela2:oppositeTo  dir:East ;
        owl:sameAs         dir:Westward .

dir:Uphill  a              mdir2:Direction ;
        srela2:oppositeTo  dir:Downhill .

dir:Downhill  a  mdir2:Direction .

dir:Upslope  a             mdir2:Direction ;
        srela2:oppositeTo  dir:Downslope .

dir:Upstream  a            mdir2:Direction ;
        srela2:oppositeTo  dir:Downstream .
