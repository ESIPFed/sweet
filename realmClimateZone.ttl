#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/propSpaceThickness/PrecipitationRange
<http://sweetontology.net/propSpaceThickness/PrecipitationRange> rdf:type owl:Class .


###  http://sweetontology.net/propSpaceThickness/PrecipitationRange_cm
<http://sweetontology.net/propSpaceThickness/PrecipitationRange_cm> rdf:type owl:Class .


###  http://sweetontology.net/realmClimateZone/A
:A rdf:type owl:Class ;
   owl:equivalentClass :TropicalClimate ;
   rdfs:subClassOf :ClimateClassification ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://sweetontology.net/relaClimate/hasAverageAnnualPrecipitation> ;
                     owl:hasValue _:genid2
                   ] ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://sweetontology.net/relaClimate/hasAverageMonthlyTemperature> ;
                     owl:hasValue _:genid4
                   ] ;
   rdfs:comment "The A climate classification, also known as the Tropical climate classification, is characterized as being consistently warm with all months averaging above 18 degrees C and having annual precipitation exceeds potential evapotranspiration (POTET)."@en .

_:genid2 rdf:type <http://sweetontology.net/propSpaceThickness/PrecipitationRange> ;
          <http://sweetontology.net/relaSci/hasLowerQuantity> <http://sweetontology.net/propSpaceThickness/AnnualPotentialEvapotranspiration> .

_:genid4 rdf:type <http://sweetontology.net/propTemperature/TemperatureRange> ;
          <http://sweetontology.net/relaMath/hasLowerBound> 18 .


###  http://sweetontology.net/realmClimateZone/B
:B rdf:type owl:Class ;
   owl:equivalentClass :DryClimate ;
   rdfs:subClassOf :ClimateClassification ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://sweetontology.net/relaClimate/hasAverageAnnualPrecipitation> ;
                     owl:hasValue _:genid6
                   ] ;
   rdfs:comment "Potential evapotranspiration (POTET) exceeds precipitation in all B climates. Subdivisions are based on precipitation timing and amount and mean annual temperature."@en .

_:genid6 rdf:type <http://sweetontology.net/propSpaceThickness/PrecipitationRange> ;
          <http://sweetontology.net/relaSci/hasLowerQuantity> <http://sweetontology.net/propSpaceThickness/AnnualPotentialEvapotranspiration> .


###  http://sweetontology.net/realmClimateZone/C
:C rdf:type owl:Class ;
   owl:equivalentClass :TemperateClimate ;
   rdfs:subClassOf :ClimateClassification ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://sweetontology.net/relaClimate/hasAverageColdestMonthTemperature> ;
                     owl:hasValue _:genid8
                   ] ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://sweetontology.net/relaClimate/hasAverageWarmestMonthTemperature> ;
                     owl:hasValue _:genid10
                   ] .

_:genid8 rdf:type <http://sweetontology.net/propTemperature/TemperatureRange> ;
          <http://sweetontology.net/relaMath/hasLowerBound> 0 ;
          <http://sweetontology.net/relaMath/hasUpperBound> 18 .

_:genid10 rdf:type <http://sweetontology.net/propTemperature/TemperatureRange> ;
           <http://sweetontology.net/relaMath/hasLowerBound> 10 ;
           <http://sweetontology.net/relaMath/hasUpperBound> 22 .


###  http://sweetontology.net/realmClimateZone/ClimateClassification
:ClimateClassification rdf:type owl:Class ;
                       rdfs:subClassOf <http://sweetontology.net/propCategorical/Category> .


###  http://sweetontology.net/realmClimateZone/ClimateZoneType
:ClimateZoneType rdf:type owl:Class ;
                 rdfs:subClassOf :ClimateClassification .


###  http://sweetontology.net/realmClimateZone/D
:D rdf:type owl:Class ;
   rdfs:subClassOf :ClimateClassification ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://sweetontology.net/relaClimate/hasAverageColdestMonthTemperature> ;
                     owl:hasValue _:genid12
                   ] ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://sweetontology.net/relaClimate/hasAverageWarmestMonthTemperature> ;
                     owl:hasValue _:genid14
                   ] ;
   rdfs:comment "The D climate classification, also known as the microthermal climate classification,  is characterized by by having the warmest month of the year above 10 degree C and the coldest below 0 degree C."@en .

_:genid12 rdf:type <http://sweetontology.net/propTemperature/TemperatureRange> ;
           <http://sweetontology.net/relaMath/hasUpperBound> 0 .

_:genid14 rdf:type <http://sweetontology.net/propTemperature/TemperatureRange> ;
           <http://sweetontology.net/relaMath/hasLowerBound> 10 .


###  http://sweetontology.net/realmClimateZone/DesertClimateZone
:DesertClimateZone rdf:type owl:Class ;
                   rdfs:subClassOf :ClimateZoneType .


###  http://sweetontology.net/realmClimateZone/Doldrums
:Doldrums rdf:type owl:Class ;
          rdfs:subClassOf :ClimateZoneType ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                            owl:hasValue <http://sweetontology.net/realmRegion/LowLatitude>
                          ] ;
          rdfs:comment "A nautical term for the equatorial trough, with special reference to the light and variable nature of the winds."@en .


###  http://sweetontology.net/realmClimateZone/DryClimate
:DryClimate rdf:type owl:Class .


###  http://sweetontology.net/realmClimateZone/DryWinterClimate
:DryWinterClimate rdf:type owl:Class ;
                  rdfs:subClassOf :ClimateClassification ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://sweetontology.net/relaClimate/hasDrySeason> ;
                                    owl:hasValue <http://sweetontology.net/reprTimeSeason/Winter>
                                  ] .


###  http://sweetontology.net/realmClimateZone/E
:E rdf:type owl:Class ;
   rdfs:subClassOf :ClimateClassification .


###  http://sweetontology.net/realmClimateZone/Equator
:Equator rdf:type owl:Class ;
         rdfs:subClassOf :TropicalClimateZone .


###  http://sweetontology.net/realmClimateZone/ITCZ
:ITCZ rdf:type owl:Class ;
      owl:equivalentClass :IntertropicalConvergenceZone .


###  http://sweetontology.net/realmClimateZone/IntertropicalConvergenceZone
:IntertropicalConvergenceZone rdf:type owl:Class ;
                              rdfs:subClassOf :ClimateZoneType ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                                                owl:allValuesFrom :Equator
                                              ] .


###  http://sweetontology.net/realmClimateZone/MarineClimate
:MarineClimate rdf:type owl:Class ;
               rdfs:subClassOf :ClimateClassification ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                                 owl:someValuesFrom <http://sweetontology.net/realm/Ocean>
                               ] ;
               rdfs:comment "(Also called maritime climate, oceanic climate.) A regional climate under the predominant influence of the sea, characterized by relatively small seasonal variations and high atmospheric moisture content; the antithesis of a continental climate."@en .


###  http://sweetontology.net/realmClimateZone/RainShadow
:RainShadow rdf:type owl:Class ;
            rdfs:subClassOf :ClimateZoneType ;
            rdfs:comment "A region of sharply reduced precipitation on the lee side of an orographic barrier, as compared with regions upwind of the barrier."@en .


###  http://sweetontology.net/realmClimateZone/SubtropicalClimateZone
:SubtropicalClimateZone rdf:type owl:Class ;
                        rdfs:subClassOf :ClimateZoneType .


###  http://sweetontology.net/realmClimateZone/TemperateClimate
:TemperateClimate rdf:type owl:Class .


###  http://sweetontology.net/realmClimateZone/TropicalClimate
:TropicalClimate rdf:type owl:Class .


###  http://sweetontology.net/realmClimateZone/TropicalClimateZone
:TropicalClimateZone rdf:type owl:Class ;
                     rdfs:subClassOf :ClimateZoneType .


###  http://sweetontology.net/realmClimateZone/WetSummerClimate
:WetSummerClimate rdf:type owl:Class ;
                  rdfs:subClassOf :ClimateClassification ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://sweetontology.net/relaClimate/hasWetSeason> ;
                                    owl:hasValue <http://sweetontology.net/reprTimeSeason/Summer>
                                  ] .


###  http://sweetontology.net/realmClimateZone/WetSummerDryWinterClimate
:WetSummerDryWinterClimate rdf:type owl:Class ;
                           rdfs:subClassOf :DryWinterClimate ,
                                           :WetSummerClimate .


#################################################################
#    Individuals
#################################################################

###  http://sweetontology.net/realmClimateZone/Af
:Af rdf:type owl:NamedIndividual ,
             :A ;
    owl:sameAs :TropicalRainForestClimate ;
    <http://sweetontology.net/relaClimate/hasAverageMonthlyPrecipitation> _:genid20 ;
    rdfs:comment "The Af climate classification, also known as the tropical rain forest climate classification, is characterized by monthly precipitation in excess of 6 cm all months out of year."@en .

_:genid20 rdf:type <http://sweetontology.net/propSpaceThickness/PrecipitationRange_cm> ;
           <http://sweetontology.net/relaMath/hasLowerBound> 6 .


###  http://sweetontology.net/realmClimateZone/Am
:Am rdf:type owl:NamedIndividual ,
             :A ;
    owl:sameAs :TropicalMonsoonClimate ;
    <http://sweetontology.net/relaClimate/hasAverageDrySeasonPrecipitation> _:genid21 ;
    <http://sweetontology.net/relaClimate/hasAverageITCZDominanceDuration> _:genid22 ;
    rdfs:comment "The Am climate classification, also known as Tropical Monsoon Climate, is characterized by a short marked dry season with 1 or more months receiving less than 6 cm of precipitation, an otherwise excessively wet rainy season, and ITCZ dominance for 6 - 12 months out of the year."@en .

_:genid21 rdf:type <http://sweetontology.net/propSpaceThickness/PrecipitationRange_cm> ;
           <http://sweetontology.net/relaMath/hasUpperBound> 6 .

_:genid22 rdf:type <http://sweetontology.net/propTime/MonthRange> ;
           <http://sweetontology.net/relaMath/hasLowerBound> 6 .


