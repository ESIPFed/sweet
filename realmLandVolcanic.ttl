#################################################################
#    Object Properties
#################################################################

###  http://sweetontology.net/relaChemical/hasPrimarySubstance
<http://sweetontology.net/relaChemical/hasPrimarySubstance> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/realmLandVolcanic/Caldera
:Caldera rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/realmGeol/GeologicFeature> .


###  http://sweetontology.net/realmLandVolcanic/CentralVolcano
:CentralVolcano rdf:type owl:Class ;
                rdfs:subClassOf :Volcano ;
                rdfs:comment "A volcano constructed by the ejection of debris and lava flows from a central point, forming a more or less symmetrical volcano."@en .


###  http://sweetontology.net/realmLandVolcanic/CompositeVolcano
:CompositeVolcano rdf:type owl:Class ;
                  owl:equivalentClass :CompoundVolcano .


###  http://sweetontology.net/realmLandVolcanic/CompoundVolcano
:CompoundVolcano rdf:type owl:Class ;
                 rdfs:subClassOf :Volcano ;
                 rdfs:comment "A volcano that consists of a complex of two or more vents, or a volcano that has an associated volcanic dome, either in its crater or on its flanks."@en .


###  http://sweetontology.net/realmLandVolcanic/Fumarole
:Fumarole rdf:type owl:Class ;
          rdfs:subClassOf :Vent .


###  http://sweetontology.net/realmLandVolcanic/Geothermal
:Geothermal rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/propEnergy/Heat> ,
                            <http://sweetontology.net/realmGeol/GeologicFeature> ;
            rdfs:comment "In geology, geothermal refers to heat sources within the planet. Strictly speaking, geo-thermal necessarily refers to the Earth but the concept may be applied to other planets. Geothermal is technically an adjective (e.g., geothermal energy) but in U.S. English the word has attained frequent use as a noun (otherwise expressed as g. heat, g. source, or geotherm). The planet's internal heat was originally generated during its accretion, due to gravitational binding energy, and since then additional heat has continued to be generated by the radioactive decay of elements such as uranium, thorium, and potassium. The heat flow from the interior to the surface is only 1/20,000 as great as the energy received from the Sun."@en .


###  http://sweetontology.net/realmLandVolcanic/Geyser
:Geyser rdf:type owl:Class ;
        rdfs:subClassOf :Vent .


###  http://sweetontology.net/realmLandVolcanic/HotSpot
:HotSpot rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/realmGeol/GeologicFeature> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                           owl:allValuesFrom :LinearVolcanicChain
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/relaChemical/hasSubstance> ;
                           owl:allValuesFrom <http://sweetontology.net/matrRockIgneous/AnorogenicIgneousRock>
                         ] ;
         rdfs:comment "In geology, a hotspot is a location on the Earth's surface that has experienced active volcanism for a long period of time. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandVolcanic/LavaPlain
:LavaPlain rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/realmGeol/GeologicFeature> .


###  http://sweetontology.net/realmLandVolcanic/LinearVolcanicChain
:LinearVolcanicChain rdf:type owl:Class ;
                     rdfs:subClassOf :VolcanicChain ;
                     rdfs:comment "Where this association of flood basalts with continental rifting is observed, it is not uncommon to find linear chains of volcanic islands  [Wikipedia]"@en .


###  http://sweetontology.net/realmLandVolcanic/Monogenetic
:Monogenetic rdf:type owl:Class ;
             rdfs:subClassOf :Volcano ;
             rdfs:comment "A volcano built by a single eruption."@en .


###  http://sweetontology.net/realmLandVolcanic/ShieldVolcano
:ShieldVolcano rdf:type owl:Class ;
               rdfs:subClassOf :Volcano .


###  http://sweetontology.net/realmLandVolcanic/Stratovolcano
:Stratovolcano rdf:type owl:Class ;
               rdfs:subClassOf :Volcano ;
               rdfs:comment "A volcano composed of both lava flows and pyroclastic material."@en .


###  http://sweetontology.net/realmLandVolcanic/SubaqueousVolcano
:SubaqueousVolcano rdf:type owl:Class ;
                   owl:equivalentClass :SubmarineVolcano ;
                   rdfs:subClassOf :Volcano ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                                     owl:allValuesFrom <http://sweetontology.net/realm/Ocean>
                                   ] .


###  http://sweetontology.net/realmLandVolcanic/SubmarineVolcano
:SubmarineVolcano rdf:type owl:Class .


###  http://sweetontology.net/realmLandVolcanic/Tumulus
:Tumulus rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/realmLandform/Landform> ;
         rdfs:comment "A doming or small mound on the crest of a lava flow caused by pressure due to the difference in the rate of flow between the cooler crust and the more fluid lava below."@en .


###  http://sweetontology.net/realmLandVolcanic/Vent
:Vent rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/phenSolid/Crack> ,
                      <http://sweetontology.net/realmGeol/GeologicFeature> .


###  http://sweetontology.net/realmLandVolcanic/VolcanicArc
:VolcanicArc rdf:type owl:Class ;
             owl:equivalentClass :VolcanicChain .


###  http://sweetontology.net/realmLandVolcanic/VolcanicChain
:VolcanicChain rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/realmGeol/GeologicProvince> ;
               rdfs:comment "Island arcs that develop along the edges of a continent (for example, large parts of the Andes/ Central American/ Canadian mountain chain) may be known as a volcanic arc or volcanic chain. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandVolcanic/VolcanicField
:VolcanicField rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/realmGeol/GeologicProvince> ;
               rdfs:comment "A volcanic field is a spot of the earth's crust that is prone to localized volcanic activity. They usually contain 10 to 100 volcanoes, such as cinder cones and are usually in clusters. Lava flows may also occur. They may occur as a monogenetic volcanic field or a polygenetic volcanic field. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandVolcanic/VolcanicSystem
:VolcanicSystem rdf:type owl:Class ;
                rdfs:subClassOf <http://sweetontology.net/realmGeol/GeologicFeature> .


###  http://sweetontology.net/realmLandVolcanic/Volcano
:Volcano rdf:type owl:Class ;
         rdfs:subClassOf :VolcanicField ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/relaChemical/hasPrimarySubstance> ;
                           owl:someValuesFrom <http://sweetontology.net/matrRockIgneous/Magma>
                         ] ;
         rdfs:comment "A volcano is an opening, or rupture, in a planet's surface or crust, which allows hot, molten rock, ash, and gases to escape from below the surface. Volcanic activity involving the extrusion of rock tends to form mountains or features like mountains over a period of time. [Wikipedia]"@en .


