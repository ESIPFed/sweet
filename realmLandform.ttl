@prefix : <http://sweetontology.net/realmLandform/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/realmLandform> .

<http://sweetontology.net/realmLandform> rdf:type owl:Ontology ;
                                          owl:imports <http://sweetontology.net/relaPhysical> ,
                                                      <http://sweetontology.net/relaSpace> ,
                                                      <http://sweetontology.net/reprSpaceGeometry> ,
                                                      <http://sweetontology.net/reprSpaceGeometry3D> ,
                                                      <http://sweetontology.net/stateVisibility> ;
                                          rdfs:label "SWEET Ontology" ;
                                          owl:versionInfo "3.1.0" .

#################################################################
#    Object Properties
#################################################################

###  http://sweetontology.net/relaPhysical/hasVisibility
<http://sweetontology.net/relaPhysical/hasVisibility> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/relaSpace/hasSpatialConfiguration
<http://sweetontology.net/relaSpace/hasSpatialConfiguration> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/realmLandform/CentralCreekCrater
:CentralCreekCrater rdf:type owl:Class ;
                    rdfs:subClassOf :Crater .


###  http://sweetontology.net/realmLandform/ComplexCrater
:ComplexCrater rdf:type owl:Class ;
               rdfs:subClassOf :Crater .


###  http://sweetontology.net/realmLandform/Continent
:Continent rdf:type owl:Class ;
           rdfs:subClassOf :LandRegion ;
           rdfs:comment "A continent is one of several large landmasses. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandform/Crater
:Crater rdf:type owl:Class ;
        rdfs:subClassOf :Landform ,
                        <http://sweetontology.net/reprSpaceGeometry3D/Cavity> .


###  http://sweetontology.net/realmLandform/DebrisBasin
:DebrisBasin rdf:type owl:Class ;
             rdfs:subClassOf :LandRegion .


###  http://sweetontology.net/realmLandform/DeflationZone
:DeflationZone rdf:type owl:Class ;
               rdfs:subClassOf :LandRegion .


###  http://sweetontology.net/realmLandform/Dryland
:Dryland rdf:type owl:Class ;
         rdfs:subClassOf :LandRegion .


###  http://sweetontology.net/realmLandform/Field
:Field rdf:type owl:Class ;
       rdfs:subClassOf :Landform ;
       rdfs:comment "Field (agriculture), an area of land used to cultivate crops, or to keep livestock [Wikipedia]"@en .


###  http://sweetontology.net/realmLandform/Highland
:Highland rdf:type owl:Class ;
          rdfs:subClassOf :Landform ;
          owl:disjointWith :Lowland .


###  http://sweetontology.net/realmLandform/ImpactBasin
:ImpactBasin rdf:type owl:Class ;
             rdfs:subClassOf :Landform .


###  http://sweetontology.net/realmLandform/ImpactCrater
:ImpactCrater rdf:type owl:Class ;
              rdfs:subClassOf :Crater .


###  http://sweetontology.net/realmLandform/Lamination
:Lamination rdf:type owl:Class ;
            rdfs:subClassOf :Landform .


###  http://sweetontology.net/realmLandform/Land
:Land rdf:type owl:Class ;
      owl:equivalentClass :LandRegion ,
                          :LandSurface ,
                          :SurfaceRegion ;
      rdfs:subClassOf <http://sweetontology.net/reprSpaceGeometry/Region> .


###  http://sweetontology.net/realmLandform/LandCover
:LandCover rdf:type owl:Class ;
           rdfs:subClassOf :Landform ;
           rdfs:comment "Land cover is the physical material at the surface of the earth. Land covers include grass, asphalt, trees, bare ground, water, etc. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandform/LandRegion
:LandRegion rdf:type owl:Class .


###  http://sweetontology.net/realmLandform/LandSurface
:LandSurface rdf:type owl:Class .


###  http://sweetontology.net/realmLandform/Landform
:Landform rdf:type owl:Class ;
          rdfs:subClassOf :LandRegion ;
          rdfs:comment "A landform comprises a geomorphological unit, and is largely defined by its surface form and location in the landscape, as part of the terrain, and as such, is typically an element of topography. Landforms are categorised by features such as elevation, slope, orientation, stratification, rock exposure, and soil type. They include berms, mounds, hills, cliffs, valleys, rivers and numerous other elements."@en .


###  http://sweetontology.net/realmLandform/Landscape
:Landscape rdf:type owl:Class ;
           rdfs:subClassOf :Landform ;
           rdfs:comment "A landscape comprises the visible features of an area of land, including physical elements such as landforms, living elements of flora and fauna, abstract elements such as lighting and weather conditions, and human elements, for instance human activity or the built environment. Landscape may also signify the objects around one in a building."@en .


###  http://sweetontology.net/realmLandform/Lowland
:Lowland rdf:type owl:Class ;
         rdfs:subClassOf :Landform .


###  http://sweetontology.net/realmLandform/Mare
:Mare rdf:type owl:Class ;
      rdfs:subClassOf :Plain ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/relaPhysical/hasVisibility> ;
                        owl:hasValue <http://sweetontology.net/stateVisibility/Dark>
                      ] .


###  http://sweetontology.net/realmLandform/PedestalCrater
:PedestalCrater rdf:type owl:Class ;
                rdfs:subClassOf :Crater .


###  http://sweetontology.net/realmLandform/PitCrater
:PitCrater rdf:type owl:Class ;
           rdfs:subClassOf :Crater .


###  http://sweetontology.net/realmLandform/Plain
:Plain rdf:type owl:Class ;
       rdfs:subClassOf :Landform ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://sweetontology.net/relaSpace/hasSpatialConfiguration> ;
                         owl:hasValue <http://sweetontology.net/stateSpaceConfiguration/Flat>
                       ] .


###  http://sweetontology.net/realmLandform/Prarie
:Prarie rdf:type owl:Class ;
        rdfs:subClassOf :Plain .


###  http://sweetontology.net/realmLandform/RockyPitFloorCrater
:RockyPitFloorCrater rdf:type owl:Class ;
                     rdfs:subClassOf :PitCrater .


###  http://sweetontology.net/realmLandform/ShatterCone
:ShatterCone rdf:type owl:Class ;
             rdfs:subClassOf :Landform .


###  http://sweetontology.net/realmLandform/Spall
:Spall rdf:type owl:Class ;
       rdfs:subClassOf :Landform .


###  http://sweetontology.net/realmLandform/SurfaceRegion
:SurfaceRegion rdf:type owl:Class .


###  http://sweetontology.net/reprSpaceGeometry/Region
<http://sweetontology.net/reprSpaceGeometry/Region> rdf:type owl:Class .


###  http://sweetontology.net/reprSpaceGeometry3D/Cavity
<http://sweetontology.net/reprSpaceGeometry3D/Cavity> rdf:type owl:Class .


