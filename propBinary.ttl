@base          <http://sweetontology.net/propBinary> .
@prefix cate:  <http://sweetontology.net/propCategorical/> .
@prefix func:  <http://sweetontology.net/reprMathFunction/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix bin:   <http://sweetontology.net/propBinary/> .
@prefix prop:  <http://sweetontology.net/prop/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

bin:Free  a              owl:Class ;
        rdfs:subClassOf  bin:BinaryProperty .

bin:BinaryProperty  a    owl:Class ;
        rdfs:subClassOf  func:BinaryFunction , prop:Property .

bin:Polarity  a          owl:Class ;
        rdfs:subClassOf  bin:BinaryProperty , prop:ChemicalProperty .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <propCategorical> , <reprMathFunction> , <prop> ;
        owl:versionInfo  "3.1.0" .
