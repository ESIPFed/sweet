@prefix : <http://sweetontology.net/stateTime/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/stateTime> .

<http://sweetontology.net/stateTime> rdf:type owl:Ontology ;
                                      owl:imports <http://sweetontology.net/propOrdinal> ,
                                                  <http://sweetontology.net/propTime> ,
                                                  <http://sweetontology.net/relaSci> ,
                                                  <http://sweetontology.net/relaTime> ,
                                                  <http://sweetontology.net/reprSciUnits> ,
                                                  <http://sweetontology.net/reprTime> ;
                                      rdfs:label "SWEET Ontology" ;
                                      owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/stateTime/Age
:Age rdf:type owl:Class ;
     rdfs:subClassOf :GeologicTimeUnit ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <http://sweetontology.net/relaTime/temporalPartOf> ;
                       owl:someValuesFrom :Epoch
                     ] ;
     rdfs:comment "Epochs are divided into ages [millions of years] [Wikipedia]"@en .


###  http://sweetontology.net/stateTime/Eon
:Eon rdf:type owl:Class ;
     rdfs:subClassOf :GeologicTimeUnit ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <http://sweetontology.net/relaTime/temporalPartOf> ;
                       owl:someValuesFrom :Supereon
                     ] ;
     rdfs:comment "Supereons are divided into eons.  [billions of years] [Wikipedia]"@en .


###  http://sweetontology.net/stateTime/Epoch
:Epoch rdf:type owl:Class ;
       rdfs:subClassOf :GeologicTimeUnit ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://sweetontology.net/relaTime/temporalPartOf> ;
                         owl:someValuesFrom :Period
                       ] ;
       rdfs:comment "Periods are divided into epochs [tens of millions of years] [Wikipedia]"@en .


###  http://sweetontology.net/stateTime/Era
:Era rdf:type owl:Class ;
     rdfs:subClassOf :GeologicTimeUnit ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <http://sweetontology.net/relaTime/temporalPartOf> ;
                       owl:someValuesFrom :Eon
                     ] ;
     rdfs:comment "Eons are divided into eras [several hundred million years] [Wikipedia]"@en .


###  http://sweetontology.net/stateTime/GeologicTimeUnit
:GeologicTimeUnit rdf:type owl:Class ;
                  rdfs:subClassOf <http://sweetontology.net/propTime/Duration> ;
                  rdfs:comment "The subdivisions of geologic time. The table of geologic time spans are dates and nomenclature defined by the International Commission on Stratigraphy. [Wikipedia]"@en .


###  http://sweetontology.net/stateTime/NeotectonicPeriod
:NeotectonicPeriod rdf:type owl:Class ;
                   rdfs:subClassOf <http://sweetontology.net/reprTime/TimeReference> .


###  http://sweetontology.net/stateTime/Period
:Period rdf:type owl:Class ;
        rdfs:subClassOf :GeologicTimeUnit ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/relaTime/temporalPartOf> ;
                          owl:someValuesFrom :Era
                        ] ;
        rdfs:comment "Eras are divided into periods. [Wikipedia]"@en .


###  http://sweetontology.net/stateTime/Supereon
:Supereon rdf:type owl:Class ;
          rdfs:subClassOf :GeologicTimeUnit ;
          rdfs:comment "The largest defined unit of time is the supereon, composed of eons.] [Wikipedia]"@en .


