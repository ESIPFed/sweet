@base          <http://sweetontology.net/reprDataModel> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix :      <http://sweetontology.net/reprDataModel> .
@prefix obj3d: <http://sweetontology.net/reprSpaceGeometry3D/> .
@prefix mod:   <http://sweetontology.net/reprSciModel/> .
@prefix data:  <http://sweetontology.net/reprDataProduct/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix space: <http://sweetontology.net/reprSpace/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix obj:   <http://sweetontology.net/reprSpaceGeometry/> .
@prefix time:  <http://sweetontology.net/reprTime/> .

<reprDataModel/TemporalRepresentation>
        a                owl:Class ;
        rdfs:subClassOf  repr:Representation ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  time:Time ;
                           owl:onProperty     mrela:representationOf
                         ] .

<reprDataModel/Resolution>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/Increment> .

:       a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <repr> , <relaSpace> , <reprSpaceGeometry> , <reprSpaceGeometry3D> , <reprSpace> , <reprSciModel> , <reprTime> , <reprDataProduct> , <relaMath> ;
        owl:versionInfo  "3.1.0" .

<reprDataModel/Parameter>
        a                owl:Class ;
        rdfs:subClassOf  repr:Dimension .

<reprDataModel/Vector>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/DataModel> .

<reprDataModel/Field>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/DataModel> .

<reprDataModel/Raster>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/DataModel> .

<reprDataModel/Column>
        a                owl:Class ;
        rdfs:subClassOf  repr:Dimension .

<reprDataModel/DataStructure>
        a                owl:Class ;
        rdfs:subClassOf  repr:Representation .

<reprDataModel/Swath>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/DataModel> .

<reprDataModel/Point>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/DataModel> ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  obj:Point ;
                           owl:onProperty     srela:hasGeometricalObject
                         ] .

<reprDataModel/Swath_3D>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/Swath> ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  obj3d:GeometricalObject_3D ;
                           owl:onProperty     srela:hasGeometricalObject
                         ] .

<reprDataModel/SpatialRepresentation>
        a                owl:Class ;
        rdfs:subClassOf  repr:Representation ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  space:Space ;
                           owl:onProperty     mrela:representationOf
                         ] .

<reprDataModel/Grid>  a  owl:Class ;
        rdfs:subClassOf  <reprDataModel/Raster> , <reprDataModel/DataModel> ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      srela:hasGeometricalObject ;
                           owl:someValuesFrom  obj:Rectangle
                         ] .

<reprDataModel/ReducedGrid>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/Grid> .

<reprDataModel/Swath_2D>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/Swath> ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  obj:GeometricalObject_2D ;
                           owl:onProperty     srela:hasGeometricalObject
                         ] .

<reprDataModel/Structure>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/DataStructure> .

<reprDataModel/DataModel>
        a                owl:Class ;
        rdfs:subClassOf  mod:Model ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  data:Data ;
                           owl:onProperty     mrela:representationOf
                         ] .

<reprDataModel/Array>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/DataStructure> .

<reprDataModel/Row>  a   owl:Class ;
        rdfs:subClassOf  repr:Dimension .

<reprDataModel/Character>
        a                owl:Class ;
        rdfs:subClassOf  repr:Representation .

<reprDataModel/Increment>
        a                owl:Class ;
        rdfs:subClassOf  repr:Scale .

<reprDataModel/Delimiter>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/Character> .

<reprDataModel/Cell>  a  owl:Class ;
        rdfs:subClassOf  <reprDataModel/DataModel> .

<reprDataModel/Trajectory>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataModel/DataModel> .
