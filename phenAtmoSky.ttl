@prefix : <http://sweetontology.net/phenAtmoSky/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/phenAtmoSky> .

<http://sweetontology.net/phenAtmoSky> rdf:type owl:Ontology ;
                                        owl:imports <http://sweetontology.net/phen> ,
                                                    <http://sweetontology.net/phenAtmo> ,
                                                    <http://sweetontology.net/phenAtmoFog> ,
                                                    <http://sweetontology.net/phenEnvirImpact> ,
                                                    <http://sweetontology.net/procWave> ,
                                                    <http://sweetontology.net/rela> ;
                                        rdfs:label "SWEET Ontology" ;
                                        owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/phenAtmoSky/AirPollution
:AirPollution rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/phenAtmo/AtmosphericPhenomena> ,
                              <http://sweetontology.net/phenEnvirImpact/Pollution> ;
              rdfs:comment "The presence of substances in the atmosphere, particularly those that do not occur naturally" ;
              rdfs:label "Air Pollution" .


###  http://sweetontology.net/phenAtmoSky/AirQuality
:AirQuality rdf:type owl:Class ;
            rdfs:subClassOf :AirPollution .


###  http://sweetontology.net/phenAtmoSky/Airglow
:Airglow rdf:type owl:Class ;
         rdfs:subClassOf :AtmosphericLightEmission .


###  http://sweetontology.net/phenAtmoSky/AtmosphericLightEmission
:AtmosphericLightEmission rdf:type owl:Class ;
                          rdfs:subClassOf <http://sweetontology.net/phenAtmo/AtmosphericPhenomena> ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                            owl:allValuesFrom <http://sweetontology.net/procWave/Emission>
                                          ] .


###  http://sweetontology.net/phenAtmoSky/Aurora
:Aurora rdf:type owl:Class ;
        rdfs:subClassOf :AtmosphericLightEmission ;
        rdfs:comment "The sporadic radiant emission from the upper atmosphere over the middle and high latitudes"@en .


###  http://sweetontology.net/phenAtmoSky/AuroralSubstorm
:AuroralSubstorm rdf:type owl:Class ;
                 owl:equivalentClass :MagneticSubstorm ;
                 rdfs:subClassOf :AtmosphericLightEmission .


###  http://sweetontology.net/phenAtmoSky/IndoorAirQuality
:IndoorAirQuality rdf:type owl:Class ;
                  rdfs:subClassOf :AirQuality .


###  http://sweetontology.net/phenAtmoSky/MagneticSubstorm
:MagneticSubstorm rdf:type owl:Class .


###  http://sweetontology.net/phenAtmoSky/Obscuration
:Obscuration rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/phenAtmoFog/Fog> .


###  http://sweetontology.net/phenAtmoSky/OzoneDepletion
:OzoneDepletion rdf:type owl:Class ;
                owl:equivalentClass :OzoneHole .


###  http://sweetontology.net/phenAtmoSky/OzoneHole
:OzoneHole rdf:type owl:Class ;
           rdfs:subClassOf :AirPollution .


###  http://sweetontology.net/phenAtmoSky/PhotochemicalAirPollution
:PhotochemicalAirPollution rdf:type owl:Class ;
                           rdfs:subClassOf :AirPollution ;
                           rdfs:comment "Type of air pollution, such as Los Angeles smog, associated with the buildup of oxidation products formed from the degradation of hydrocarbons, etc."@en .


###  http://sweetontology.net/phenAtmoSky/Rainbow
:Rainbow rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/phenAtmo/MeteorologicalPhenomena> .


###  http://sweetontology.net/phenAtmoSky/Sunlight
:Sunlight rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/phen/RadiativeHeating> ;
          rdfs:comment "Sunlight, in the broad sense, is the total spectrum of the electromagnetic radiation given off by the Sun. On Earth, sunlight is filtered through the atmosphere, and the solar radiation is obvious as daylight when the Sun is above the horizon. This is usually during the hours known as day. Near the poles in summer, sunlight also occurs during the hours known as night and in the winter at the poles sunlight may not occur at any time. When the direct radiation is not blocked by clouds, it is experienced as sunshine, a combination of bright light and heat. Radiant heat directly produced by the radiation of the sun is different from the increase in atmospheric temperature due to the radiative heating of the atmosphere by the sun's radiation."@en .


###  http://sweetontology.net/phenAtmoSky/Sunlit
:Sunlit rdf:type owl:Class ;
        rdfs:subClassOf <http://sweetontology.net/phenAtmo/WeatherCondition> .


###  http://sweetontology.net/phenAtmoSky/Whiteout
:Whiteout rdf:type owl:Class ;
          rdfs:subClassOf :Obscuration ;
          rdfs:comment "An atmospheric optical phenomenon in which the observer appears to be engulfed in a uniformly white glow."@en .


###  http://sweetontology.net/phenAtmoSky/ZodiacalLight
:ZodiacalLight rdf:type owl:Class ;
               rdfs:subClassOf :AtmosphericLightEmission .


###  http://sweetontology.net/phenEnvirImpact/Pollution
<http://sweetontology.net/phenEnvirImpact/Pollution> rdf:type owl:Class .


###  http://sweetontology.net/procWave/Emission
<http://sweetontology.net/procWave/Emission> rdf:type owl:Class .


