#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/phenAtmoSky/AirPollution
sky:AirPollution rdf:type owl:Class ;
                 rdfs:subClassOf atmo:AtmosphericPhenomena ,
                                 eimpa:Pollution .


###  http://sweetontology.net/phenAtmoSky/AirQuality
sky:AirQuality rdf:type owl:Class ;
               rdfs:subClassOf sky:AirPollution .


###  http://sweetontology.net/phenAtmoSky/Airglow
sky:Airglow rdf:type owl:Class ;
            rdfs:subClassOf sky:AtmosphericLightEmission .


###  http://sweetontology.net/phenAtmoSky/AtmosphericLightEmission
sky:AtmosphericLightEmission rdf:type owl:Class ;
                             rdfs:subClassOf atmo:AtmosphericPhenomena ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty rela:hasPhenomena ;
                                               owl:allValuesFrom waves:Emission
                                             ] .


###  http://sweetontology.net/phenAtmoSky/Aurora
sky:Aurora rdf:type owl:Class ;
           rdfs:subClassOf sky:AtmosphericLightEmission .


###  http://sweetontology.net/phenAtmoSky/AuroralSubstorm
sky:AuroralSubstorm rdf:type owl:Class ;
                    owl:equivalentClass sky:MagneticSubstorm ;
                    rdfs:subClassOf sky:AtmosphericLightEmission .


###  http://sweetontology.net/phenAtmoSky/IndoorAirQuality
sky:IndoorAirQuality rdf:type owl:Class ;
                     rdfs:subClassOf sky:AirQuality .


###  http://sweetontology.net/phenAtmoSky/MagneticSubstorm
sky:MagneticSubstorm rdf:type owl:Class .


###  http://sweetontology.net/phenAtmoSky/Obscuration
sky:Obscuration rdf:type owl:Class ;
                rdfs:subClassOf fog:Fog .


###  http://sweetontology.net/phenAtmoSky/OzoneDepletion
sky:OzoneDepletion rdf:type owl:Class ;
                   owl:equivalentClass sky:OzoneHole .


###  http://sweetontology.net/phenAtmoSky/OzoneHole
sky:OzoneHole rdf:type owl:Class ;
              rdfs:subClassOf sky:AirPollution .


###  http://sweetontology.net/phenAtmoSky/PhotochemicalAirPollution
sky:PhotochemicalAirPollution rdf:type owl:Class ;
                              rdfs:subClassOf sky:AirPollution .


###  http://sweetontology.net/phenAtmoSky/Rainbow
sky:Rainbow rdf:type owl:Class ;
            rdfs:subClassOf atmo:MeteorologicalPhenomena .


###  http://sweetontology.net/phenAtmoSky/Sunlight
sky:Sunlight rdf:type owl:Class ;
             rdfs:subClassOf phen:RadiativeHeating .


###  http://sweetontology.net/phenAtmoSky/Sunlit
sky:Sunlit rdf:type owl:Class ;
           rdfs:subClassOf atmo:WeatherCondition .


###  http://sweetontology.net/phenAtmoSky/Whiteout
sky:Whiteout rdf:type owl:Class ;
             rdfs:subClassOf sky:Obscuration .


###  http://sweetontology.net/phenAtmoSky/ZodiacalLight
sky:ZodiacalLight rdf:type owl:Class ;
                  rdfs:subClassOf sky:AtmosphericLightEmission .


###  http://sweetontology.net/phenEnvirImpact/Pollution
eimpa:Pollution rdf:type owl:Class .


###  http://sweetontology.net/procWave/Emission
waves:Emission rdf:type owl:Class .


