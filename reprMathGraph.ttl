@base          <http://sweetontology.net/reprMathGraph> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix mod:   <http://sweetontology.net/reprSciModel/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix math:  <http://sweetontology.net/reprMath/> .
@prefix geom:  <http://sweetontology.net/reprSpaceGeometry/> .
@prefix graph: <http://sweetontology.net/reprMathGraph/> .

graph:Node  a   owl:Class .

graph:Edge  a            owl:Class ;
        rdfs:subClassOf  graph:StructuralComponent ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   mrela:hasUnorderedVertexPair
                         ] .

graph:Arc  a             owl:Class ;
        rdfs:subClassOf  graph:StructuralComponent ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   mrela:hasOrderedVertexPair
                         ] .

graph:WeightedEdge  a    owl:Class ;
        rdfs:subClassOf  graph:Edge ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   mrela:hasWeight
                         ] .

graph:Forest  a          owl:Class ;
        rdfs:subClassOf  graph:Graph .

graph:Tree  a            owl:Class ;
        rdfs:subClassOf  graph:Graph .

graph:Vertice  a  owl:Class .

graph:StructuralModel
        a                owl:Class ;
        rdfs:subClassOf  mod:Model .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <reprSpaceGeometry> , <relaMath> , <reprMath> , <reprSciModel> ;
        owl:versionInfo  "3.1.0" .

graph:IsolatedVertex  a  owl:Class ;
        rdfs:subClassOf  graph:Vertex .

graph:StructuralComponent
        a                owl:Class ;
        rdfs:subClassOf  graph:StructuralModel .

graph:OrderedVertexPair
        a                owl:Class ;
        rdfs:subClassOf  math:OrderedPair ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  graph:Vertex ;
                           owl:onProperty     mrela:eachElementHasType
                         ] .

graph:Cycle  a               owl:Class ;
        rdfs:subClassOf      geom:Path , graph:Path ;
        owl:equivalentClass  graph:Circuit .

graph:LeafNode  a        owl:Class ;
        rdfs:subClassOf  graph:Node .

graph:Circuit  a  owl:Class .

graph:Graph  a           owl:Class ;
        rdfs:subClassOf  graph:StructuralModel ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      mrela:hasEdge
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      mrela:hasVertex
                         ] .

graph:Network  a         owl:Class ;
        rdfs:subClassOf  graph:Graph ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      mrela:hasWeightedEdge
                         ] .

graph:Loop  a            owl:Class ;
        rdfs:subClassOf  graph:Edge .

graph:ConnectedGraph  a  owl:Class ;
        rdfs:subClassOf  graph:Graph .

graph:DirectedGraph  a       owl:Class ;
        rdfs:subClassOf      graph:Graph ;
        rdfs:subClassOf      [ a                   owl:Restriction ;
                               owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                               owl:onProperty      mrela:hasArc
                             ] ;
        owl:equivalentClass  graph:Digraph .

graph:UnorderedVertexPair
        a                owl:Class ;
        rdfs:subClassOf  math:UnorderedPair ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  graph:Vertex ;
                           owl:onProperty     mrela:eachElementHasType
                         ] .

graph:Vertex  a              owl:Class ;
        rdfs:subClassOf      math:Point , graph:StructuralComponent ;
        owl:equivalentClass  graph:Node , graph:Vertice .

graph:UndirectedGraph
        a                 owl:Class ;
        rdfs:subClassOf   graph:Graph ;
        owl:disjointWith  graph:DirectedGraph .

graph:Digraph  a  owl:Class .

graph:Path  a            owl:Class ;
        rdfs:subClassOf  math:Array ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  graph:Edge ;
                           owl:onProperty     mrela:eachElementHasType
                         ] .
