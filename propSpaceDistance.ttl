#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/propSpaceDistance/AttenuationCoefficient
:AttenuationCoefficient rdf:type owl:Class ;
                        owl:equivalentClass :ExtinctionCoefficient .


###  http://sweetontology.net/propSpaceDistance/CompactionCoefficient
:CompactionCoefficient rdf:type owl:Class ;
                       rdfs:subClassOf :PerMeterQuantity .


###  http://sweetontology.net/propSpaceDistance/CyclotronRadius
:CyclotronRadius rdf:type owl:Class ;
                 owl:equivalentClass :Gyroradius .


###  http://sweetontology.net/propSpaceDistance/DebyeLength
:DebyeLength rdf:type owl:Class ;
             owl:equivalentClass :DebyeScreeningLength ;
             rdfs:subClassOf :Distance .


###  http://sweetontology.net/propSpaceDistance/DebyeScreeningLength
:DebyeScreeningLength rdf:type owl:Class .


###  http://sweetontology.net/propSpaceDistance/DispersionCoeffienct
:DispersionCoeffienct rdf:type owl:Class ;
                      rdfs:subClassOf <http://sweetontology.net/propMass/MaterialProperty> ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://sweetontology.net/relaSci/measureOf> ;
                                        owl:someValuesFrom :Dispersivity
                                      ] .


###  http://sweetontology.net/propSpaceDistance/Dispersivity
:Dispersivity rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/propMass/MaterialProperty> .


###  http://sweetontology.net/propSpaceDistance/Distance
:Distance rdf:type owl:Class ;
          rdfs:subClassOf :Length .


###  http://sweetontology.net/propSpaceDistance/DistanceRange
:DistanceRange rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/propQuantity/IntervalQuantity> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaSci/rangeOf> ;
                                 owl:someValuesFrom :Distance
                               ] .


###  http://sweetontology.net/propSpaceDistance/DistanceRange_km
:DistanceRange_km rdf:type owl:Class ;
                  rdfs:subClassOf :DistanceRange ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                                    owl:hasValue <http://sweetontology.net/reprSciUnits/kilometer>
                                  ] .


###  http://sweetontology.net/propSpaceDistance/ExtinctionCoefficient
:ExtinctionCoefficient rdf:type owl:Class ;
                       rdfs:subClassOf :SpatialFrequency ;
                       rdfs:comment "The extinction coefficient for a particular substance is a measure of how well it scatters and absorbs electromagnetic radiation (EM waves). If the EM wave can pass through very easily, the material has a low extinction coefficient. Conversely, if the radiation hardly penetrates the material, but rather quickly becomes extinct within it, the extinction coefficient is high." .


###  http://sweetontology.net/propSpaceDistance/Fetch
:Fetch rdf:type owl:Class ;
       rdfs:subClassOf :Distance .


###  http://sweetontology.net/propSpaceDistance/GrainSize
:GrainSize rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/propMass/ConsistenceProperty> .


###  http://sweetontology.net/propSpaceDistance/Gyroradius
:Gyroradius rdf:type owl:Class ;
            owl:equivalentClass :LarmorRadius ;
            rdfs:subClassOf :Distance .


###  http://sweetontology.net/propSpaceDistance/LarmorRadius
:LarmorRadius rdf:type owl:Class .


###  http://sweetontology.net/propSpaceDistance/Length
:Length rdf:type owl:Class ;
        rdfs:subClassOf <http://sweetontology.net/propQuantity/PhysicalQuantity> ,
                        <http://sweetontology.net/propSpace/SpatialExtent> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/relaMath/hasDimension> ;
                          owl:hasValue 1
                        ] .


###  http://sweetontology.net/propSpaceDistance/MeanFreePath
:MeanFreePath rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/prop/MotionProperty> ,
                              :Distance ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/rela/hasProcess> ;
                                owl:someValuesFrom <http://sweetontology.net/procPhysical/Collision>
                              ] .


###  http://sweetontology.net/propSpaceDistance/MixingLength
:MixingLength rdf:type owl:Class ;
              rdfs:subClassOf :Distance .


###  http://sweetontology.net/propSpaceDistance/ObukhovLength
:ObukhovLength rdf:type owl:Class ;
               rdfs:subClassOf :Distance .


###  http://sweetontology.net/propSpaceDistance/PerMeterQuantity
:PerMeterQuantity rdf:type owl:Class ;
                  rdfs:subClassOf <http://sweetontology.net/propQuantity/Quantity> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                                    owl:hasValue <http://sweetontology.net/reprSciUnits/perMeter>
                                  ] .


###  http://sweetontology.net/propSpaceDistance/PulseLength
:PulseLength rdf:type owl:Class ;
             rdfs:subClassOf :Length .


###  http://sweetontology.net/propSpaceDistance/Range
:Range rdf:type owl:Class ;
       rdfs:subClassOf :Distance .


###  http://sweetontology.net/propSpaceDistance/RossbyRadiusOfDeformation
:RossbyRadiusOfDeformation rdf:type owl:Class ;
                           rdfs:subClassOf :Distance .


###  http://sweetontology.net/propSpaceDistance/ScatteringCoefficient
:ScatteringCoefficient rdf:type owl:Class ;
                       rdfs:subClassOf :SpatialFrequency ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://sweetontology.net/rela/hasProcess> ;
                                         owl:allValuesFrom <http://sweetontology.net/procWave/Scattering>
                                       ] ;
                       rdfs:comment "A measure of the extinction due to scattering of monochromatic radiation as it traverses a medium containing scattering particles."@en .


###  http://sweetontology.net/propSpaceDistance/Size
:Size rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/propSpace/SpatialExtent> .


###  http://sweetontology.net/propSpaceDistance/SizeRange
:SizeRange rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/propSpace/SpatialExtent> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaSci/rangeOf> ;
                             owl:someValuesFrom :Size
                           ] .


###  http://sweetontology.net/propSpaceDistance/SizeRange_micron
:SizeRange_micron rdf:type owl:Class ;
                  rdfs:subClassOf :SizeRange ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                                    owl:hasValue <http://sweetontology.net/reprSciUnits/micron>
                                  ] .


###  http://sweetontology.net/propSpaceDistance/SpatialFrequency
:SpatialFrequency rdf:type owl:Class ;
                  rdfs:subClassOf <http://sweetontology.net/propQuantity/RadiationMediumInteractionQuantity> ,
                                  :PerMeterQuantity .


###  http://sweetontology.net/propSpaceDistance/Visibility
:Visibility rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/prop/RadiationMediumInteractionProperty> ,
                            :Distance ;
            rdfs:comment "The greatest distance in a given direction at which it is just possible to see and identify with the unaided eye"@en .


###  http://sweetontology.net/propSpaceDistance/WaveNumber
:WaveNumber rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/prop/WaveProperty> ,
                            :PerMeterQuantity .


###  http://sweetontology.net/propSpaceDistance/Wavelength
:Wavelength rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/prop/WaveProperty> ,
                            :Distance .


###  http://sweetontology.net/propSpaceDistance/WavelengthBand
:WavelengthBand rdf:type owl:Class ;
                rdfs:subClassOf <http://sweetontology.net/propQuantity/IntervalQuantity> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://sweetontology.net/relaSci/rangeOf> ;
                                  owl:allValuesFrom :Wavelength
                                ] .


###  http://sweetontology.net/propSpaceDistance/WavelengthBand_nm
:WavelengthBand_nm rdf:type owl:Class ;
                   rdfs:subClassOf :WavelengthBand ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                                     owl:hasValue <http://sweetontology.net/reprSciUnits/nanometer>
                                   ] .


###  http://sweetontology.net/propSpaceDistance/Wavelength_cm
:Wavelength_cm rdf:type owl:Class ;
               rdfs:subClassOf :Wavelength ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                                 owl:hasValue <http://sweetontology.net/reprSciUnits/centimeter>
                               ] .


###  http://sweetontology.net/propSpaceDistance/Wavelength_nm
:Wavelength_nm rdf:type owl:Class ;
               rdfs:subClassOf :Wavelength ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                                 owl:hasValue <http://sweetontology.net/reprSciUnits/nanometer>
                               ] .


