@base          <http://sweetontology.net/propSpaceDistance> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix proc:  <http://sweetontology.net/procPhysical/> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix mass:  <http://sweetontology.net/propMass/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix wavep: <http://sweetontology.net/procWave/> .
@prefix space: <http://sweetontology.net/propSpace/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix prop:  <http://sweetontology.net/prop/> .
@prefix rela:  <http://sweetontology.net/rela/> .
@prefix screla: <http://sweetontology.net/relaSci/> .
@prefix xten:  <http://sweetontology.net/propSpaceDistance/> .

xten:Size  a             owl:Class ;
        rdfs:subClassOf  space:SpatialExtent .

xten:Visibility  a       owl:Class ;
        rdfs:comment     "The greatest distance in a given direction at which it is just possible to see and identify with the unaided eye"@en ;
        rdfs:subClassOf  xten:Distance , prop:RadiationMediumInteractionProperty .

xten:SpatialFrequency
        a                owl:Class ;
        rdfs:subClassOf  quan:RadiationMediumInteractionQuantity , xten:PerMeterQuantity .

xten:Distance  a         owl:Class ;
        rdfs:subClassOf  xten:Length .

xten:SizeRange_micron
        a                owl:Class ;
        rdfs:subClassOf  xten:SizeRange ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:micron ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

xten:PerMeterQuantity
        a                owl:Class ;
        rdfs:subClassOf  quan:Quantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:perMeter ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

xten:DispersionCoeffienct
        a                owl:Class ;
        rdfs:subClassOf  mass:MaterialProperty ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      screla:measureOf ;
                           owl:someValuesFrom  xten:Dispersivity
                         ] .

xten:ObukhovLength  a    owl:Class ;
        rdfs:subClassOf  xten:Distance .

xten:AttenuationCoefficient
        a       owl:Class .

xten:DebyeLength  a          owl:Class ;
        rdfs:subClassOf      xten:Distance ;
        owl:equivalentClass  xten:DebyeScreeningLength .

xten:GrainSize  a        owl:Class ;
        rdfs:subClassOf  mass:ConsistenceProperty .

xten:Fetch  a            owl:Class ;
        rdfs:subClassOf  xten:Distance .

xten:Dispersivity  a     owl:Class ;
        rdfs:subClassOf  mass:MaterialProperty .

xten:LarmorRadius  a  owl:Class .

xten:DebyeScreeningLength
        a       owl:Class .

xten:Length  a           owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity , space:SpatialExtent ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    1 ;
                           owl:onProperty  mrela:hasDimension
                         ] .

xten:MixingLength  a     owl:Class ;
        rdfs:subClassOf  xten:Distance .

xten:DistanceRange  a    owl:Class ;
        rdfs:subClassOf  quan:IntervalQuantity ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      screla:rangeOf ;
                           owl:someValuesFrom  xten:Distance
                         ] .

xten:RossbyRadiusOfDeformation
        a                owl:Class ;
        rdfs:subClassOf  xten:Distance .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <propSpace> , <prop> , <reprSciUnits> , <propQuantity> , <procWave> , <relaMath> , <rela> , <relaSci> , <propMass> , <procPhysical> ;
        owl:versionInfo  "3.1.0" .

xten:SizeRange  a        owl:Class ;
        rdfs:subClassOf  space:SpatialExtent ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      screla:rangeOf ;
                           owl:someValuesFrom  xten:Size
                         ] .

xten:Gyroradius  a           owl:Class ;
        rdfs:subClassOf      xten:Distance ;
        owl:equivalentClass  xten:LarmorRadius , xten:CyclotronRadius .

xten:ScatteringCoefficient
        a                owl:Class ;
        rdfs:comment     "A measure of the extinction due to scattering of monochromatic radiation as it traverses a medium containing scattering particles."@en ;
        rdfs:subClassOf  xten:SpatialFrequency ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  wavep:Scattering ;
                           owl:onProperty     rela:hasProcess
                         ] .

xten:WaveNumber  a       owl:Class ;
        rdfs:subClassOf  prop:WaveProperty , xten:PerMeterQuantity .

xten:ExtinctionCoefficient
        a                    owl:Class ;
        rdfs:comment         "The extinction coefficient for a particular substance is a measure of how well it scatters and absorbs electromagnetic radiation (EM waves). If the EM wave can pass through very easily, the material has a low extinction coefficient. Conversely, if the radiation hardly penetrates the material, but rather quickly becomes extinct within it, the extinction coefficient is high." ;
        rdfs:subClassOf      xten:SpatialFrequency ;
        owl:equivalentClass  xten:AttenuationCoefficient .

xten:Range  a            owl:Class ;
        rdfs:subClassOf  xten:Distance .

xten:DistanceRange_km
        a                owl:Class ;
        rdfs:subClassOf  xten:DistanceRange ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:kilometer ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

xten:Wavelength  a       owl:Class ;
        rdfs:subClassOf  xten:Distance , prop:WaveProperty .

xten:WavelengthBand_nm
        a                owl:Class ;
        rdfs:subClassOf  xten:WavelengthBand ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:nanometer ;
                           owl:onProperty  screla:hasUnit
                         ] .

xten:CompactionCoefficient
        a                owl:Class ;
        rdfs:subClassOf  xten:PerMeterQuantity .

xten:PulseLength  a      owl:Class ;
        rdfs:subClassOf  xten:Length .

xten:MeanFreePath  a     owl:Class ;
        rdfs:subClassOf  xten:Distance , prop:MotionProperty ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      rela:hasProcess ;
                           owl:someValuesFrom  proc:Collision
                         ] .

xten:Wavelength_cm  a    owl:Class ;
        rdfs:subClassOf  xten:Wavelength ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:centimeter ;
                           owl:onProperty  screla:hasUnit
                         ] .

xten:Wavelength_nm  a    owl:Class ;
        rdfs:subClassOf  xten:Wavelength ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:nanometer ;
                           owl:onProperty  screla:hasUnit
                         ] .

xten:WavelengthBand  a   owl:Class ;
        rdfs:subClassOf  quan:IntervalQuantity ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xten:Wavelength ;
                           owl:onProperty     screla:rangeOf
                         ] .

xten:CyclotronRadius  a  owl:Class .
