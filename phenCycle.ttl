@base          <http://sweetontology.net/phenCycle> .
@prefix trela: <http://sweetontology.net/relaTime/> .
@prefix star:  <http://sweetontology.net/phenStar/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix planet: <http://sweetontology.net/realm/> .
@prefix prela: <http://sweetontology.net/relaPhysical/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix spac:  <http://sweetontology.net/stateSpaceScale/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sys:   <http://sweetontology.net/phenSystem/> .
@prefix sfreq: <http://sweetontology.net/stateTimeFrequency/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rela:  <http://sweetontology.net/rela/> .
@prefix osc:   <http://sweetontology.net/phenCycle/> .

osc:ElNinoSouthernOscillation
        a                    owl:Class ;
        rdfs:comment         "A significant increase in sea surface temperature over the eastern and central equatorial Pacific that occurs at irregular intervals, generally ranging between two and seven years."@en ;
        rdfs:subClassOf      osc:EarthOscillation ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  planet:Ocean ;
                               owl:onProperty     rela:hasRealm
                             ] ;
        owl:equivalentClass  osc:ENSO .

osc:PDO  a      owl:Class .

osc:AntarcticOscillation
        a                owl:Class ;
        rdfs:subClassOf  osc:EarthOscillation .

osc:QBO  a      owl:Class .

osc:AnnualCycle  a       owl:Class ;
        rdfs:subClassOf  osc:GlobalCycle ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    sfreq:Annual ;
                           owl:onProperty  trela:hasFrequency
                         ] .

osc:IceAgeCycle  a       owl:Class ;
        rdfs:subClassOf  osc:EarthOscillation .

osc:MaddenJulianOscillation
        a                owl:Class ;
        rdfs:subClassOf  osc:EarthOscillation ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    sfreq:Decadal ;
                           owl:onProperty  trela:hasFrequency
                         ] .

osc:ArcticOscillation
        a                owl:Class ;
        rdfs:subClassOf  osc:EarthOscillation .

osc:EarthOscillation  a  owl:Class ;
        rdfs:subClassOf  osc:PlanetaryOscillation ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    planet:Earth ;
                           owl:onProperty  prela:hasAstronomicalBody
                         ] .

osc:Tide  a              owl:Class ;
        rdfs:subClassOf  osc:PlanetaryOscillation .

osc:PoleTide  a          owl:Class ;
        rdfs:subClassOf  osc:Tide .

osc:GlobalCycle  a       owl:Class ;
        rdfs:subClassOf  sys:Oscillation ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    spac:Global ;
                           owl:onProperty  srela:hasSpatialScale
                         ] .

osc:PlanetaryOscillation
        a                owl:Class ;
        rdfs:subClassOf  osc:GlobalCycle .

osc:PacificNorthAmericanPattern
        a                owl:Class ;
        rdfs:subClassOf  osc:EarthOscillation .

osc:ENSO  a     owl:Class .

osc:QuasiBiennialOscillation
        a                    owl:Class ;
        rdfs:subClassOf      osc:EarthOscillation ;
        rdfs:subClassOf      [ a               owl:Restriction ;
                               owl:hasValue    sfreq:Biannual ;
                               owl:onProperty  trela:hasFrequency
                             ] ;
        owl:equivalentClass  osc:QBO .

osc:BodyTide  a          owl:Class ;
        rdfs:subClassOf  osc:Tide .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <stateTimeFrequency> , <relaSpace> , <relaTime> , <rela> , <stateSpaceScale> , <phenSystem> , <phenStar> , <realm> , <relaPhysical> ;
        owl:versionInfo  "3.1.0" .

osc:ChandlerWobble  a    owl:Class ;
        rdfs:subClassOf  osc:EarthOscillation .

osc:SolarCycle  a        owl:Class ;
        rdfs:subClassOf  osc:GlobalCycle ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  star:SolarActivity ;
                           owl:onProperty     rela:hasPhenomena
                         ] .

osc:NAO  a      owl:Class .

osc:DiurnalCycle  a      owl:Class ;
        rdfs:subClassOf  osc:GlobalCycle ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    sfreq:Daily ;
                           owl:onProperty  trela:hasFrequency
                         ] .

osc:NorthAtlanticOscillation
        a                    owl:Class ;
        rdfs:subClassOf      osc:EarthOscillation ;
        owl:equivalentClass  osc:NAO .

osc:SouthernOscillation
        a                    owl:Class ;
        rdfs:subClassOf      osc:EarthOscillation ;
        owl:equivalentClass  osc:ENSO .

osc:PacificDecadalOscillation
        a                    owl:Class ;
        rdfs:subClassOf      osc:EarthOscillation ;
        owl:equivalentClass  osc:PDO .

osc:PolarWander  a       owl:Class ;
        rdfs:subClassOf  osc:EarthOscillation .

osc:MilankovitchCycle
        a                owl:Class ;
        rdfs:subClassOf  osc:EarthOscillation .
