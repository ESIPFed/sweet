#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/stateStorm/BeaufortScale
:BeaufortScale rdf:type owl:Class ;
               rdfs:subClassOf :WindScale .


###  http://sweetontology.net/stateStorm/EnhancedFujitaScale
:EnhancedFujitaScale rdf:type owl:Class ;
                     rdfs:subClassOf :TornadoScale .


###  http://sweetontology.net/stateStorm/FujitaPearsonScale
:FujitaPearsonScale rdf:type owl:Class ;
                    rdfs:subClassOf :TornadoScale .


###  http://sweetontology.net/stateStorm/HurricaneScale
:HurricaneScale rdf:type owl:Class ;
                rdfs:subClassOf :WindScale ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                  owl:allValuesFrom <http://sweetontology.net/phenAtmoPrecipitation/Hurricane>
                                ] .


###  http://sweetontology.net/stateStorm/SaffirSimpsonScale
:SaffirSimpsonScale rdf:type owl:Class ;
                    rdfs:subClassOf :HurricaneScale .


###  http://sweetontology.net/stateStorm/TornadoScale
:TornadoScale rdf:type owl:Class ;
              rdfs:subClassOf :WindScale ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                owl:allValuesFrom <http://sweetontology.net/phenAtmoPrecipitation/Tornado>
                              ] .


###  http://sweetontology.net/stateStorm/WindScale
:WindScale rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/repr/OrdinalScale> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaSci/measureOf> ;
                             owl:allValuesFrom <http://sweetontology.net/phenAtmoWind/Wind>
                           ] .


