@base          <http://sweetontology.net/propSpaceLocation> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix loca:  <http://sweetontology.net/propSpaceLocation/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix sstate: <http://sweetontology.net/stateSystem/> .
@prefix obj:   <http://sweetontology.net/reprSpaceGeometry/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rela:  <http://sweetontology.net/rela/> .
@prefix space: <http://sweetontology.net/propSpace/> .

loca:Nadir  a            owl:Class ;
        rdfs:subClassOf  loca:Location .

loca:SubsolarPoint  a    owl:Class ;
        rdfs:subClassOf  loca:Location .

loca:CenterOfMass  a     owl:Class ;
        rdfs:subClassOf  loca:Center .

loca:Focus  a                owl:Class ;
        rdfs:subClassOf      loca:Location ;
        owl:equivalentClass  loca:Focii .

loca:Location  a             owl:Class ;
        rdfs:subClassOf      space:SpatialProperty , obj:Point ;
        rdfs:subClassOf      [ a                   owl:Restriction ;
                               owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                               owl:onProperty      mrela:hasCoordinates
                             ] ;
        owl:equivalentClass  loca:Position .

loca:Epicenter  a        owl:Class ;
        rdfs:subClassOf  loca:Center .

loca:Focii  a   owl:Class .

loca:Position  a  owl:Class .

loca:ControlPoint  a     owl:Class ;
        rdfs:subClassOf  loca:Location .

loca:Center  a           owl:Class ;
        rdfs:subClassOf  loca:Location .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <rela> , <relaMath> , <propSpace> , <stateSystem> , <reprSpaceGeometry> ;
        owl:versionInfo  "3.1.0" .

loca:MirrorPoint  a      owl:Class ;
        rdfs:subClassOf  loca:Location .

loca:LagrangianPoint  a  owl:Class ;
        rdfs:subClassOf  loca:Location ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    sstate:Equilibrium ;
                           owl:onProperty  rela:hasState
                         ] .
