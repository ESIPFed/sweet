#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/matrCompound/ChemicalCompound
:ChemicalCompound rdf:type owl:Class ;
                  owl:equivalentClass :Compound .


###  http://sweetontology.net/matrCompound/Clathrate
:Clathrate rdf:type owl:Class ;
           rdfs:subClassOf :InorganicCompound .


###  http://sweetontology.net/matrCompound/Compound
:Compound rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/matr/Chemical> ;
          rdfs:comment "A chemical compound is a pure chemical substance consisting of two or more different chemical elements[1][2][3] that can be separated into simpler substances by chemical reactions[4] and that have a unique and defined chemical structure. Chemical compounds consist of a fixed ratio of atoms[3] that are hold together in a defined spatial arrangement by chemical bonds. [Wikipedia]"@en .


###  http://sweetontology.net/matrCompound/InorganicAcid
:InorganicAcid rdf:type owl:Class ;
               rdfs:subClassOf :InorganicCompound ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                 owl:hasValue <http://sweetontology.net/stateChemical/Acid>
                               ] .


###  http://sweetontology.net/matrCompound/InorganicCompound
:InorganicCompound rdf:type owl:Class ;
                   rdfs:subClassOf :Compound .


###  http://sweetontology.net/matrCompound/Perchlorate
:Perchlorate rdf:type owl:Class ;
             rdfs:subClassOf :InorganicAcid .


###  http://sweetontology.net/matrCompound/Peroxide
:Peroxide rdf:type owl:Class ;
          rdfs:subClassOf :InorganicCompound .


