@base          <http://sweetontology.net/humanCommerce> .
@prefix huma:  <http://sweetontology.net/human/> .
@prefix infr:  <http://sweetontology.net/matrEquipment/> .
@prefix comm:  <http://sweetontology.net/humanCommerce/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix hrela: <http://sweetontology.net/relaHuman/> .
@prefix cons:  <http://sweetontology.net/humanEnvirConservation/> .

comm:Construction  a     owl:Class ;
        rdfs:subClassOf  comm:Production .

comm:OnSite  a  owl:Class .

comm:ConsumptiveUse  a   owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

comm:Packaging  a        owl:Class ;
        rdfs:subClassOf  comm:Production .

comm:Productivity  a     owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <matrEquipment> , <humanEnvirConservation> , <human> , <relaHuman> ;
        owl:versionInfo  "3.1.0" .

comm:Grading  a          owl:Class ;
        rdfs:subClassOf  comm:Processing .

comm:Demand  a           owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

comm:StripMining  a      owl:Class ;
        rdfs:subClassOf  comm:Mining .

comm:ManagementSystem
        a                owl:Class ;
        rdfs:subClassOf  huma:HumanActivity ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  comm:Management ;
                           owl:onProperty     hrela:hasHumanActivity
                         ] .

comm:FossilFuelBurning
        a                owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

comm:Exploration  a      owl:Class ;
        rdfs:subClassOf  comm:EngineeringActivity .

comm:ClearCutting  a     owl:Class ;
        rdfs:subClassOf  comm:Forestry .

comm:EngineeringActivity
        a                owl:Class ;
        rdfs:subClassOf  comm:Production .

comm:Assembly  a         owl:Class ;
        rdfs:subClassOf  comm:Production .

comm:Industrialization
        a                owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

comm:Production  a       owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

comm:Mining  a           owl:Class ;
        rdfs:subClassOf  comm:Extraction .

comm:Manafacturing  a    owl:Class ;
        rdfs:subClassOf  comm:Production .

comm:CapacityBuiiding
        a                owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

comm:ResourceExtraction
        a       owl:Class .

comm:Management  a       owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

comm:LandUse  a          owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

comm:Processing  a       owl:Class ;
        rdfs:subClassOf  comm:Production .

comm:Dig  a              owl:Class ;
        rdfs:subClassOf  comm:Extraction .

comm:Urbanization  a     owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

comm:Forestry  a         owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

comm:Drill  a            owl:Class ;
        rdfs:subClassOf  comm:Extraction .

comm:InSitu  a               owl:Class ;
        rdfs:subClassOf      huma:HumanActivity ;
        owl:equivalentClass  comm:OnSite .

comm:SustainableDevelopment
        a                owl:Class ;
        rdfs:subClassOf  comm:Development ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  cons:Sustainable ;
                           owl:onProperty     hrela:hasHumanActivity
                         ] .

comm:Prospecting  a      owl:Class ;
        rdfs:subClassOf  comm:Exploration .

comm:Extraction  a           owl:Class ;
        rdfs:subClassOf      comm:EngineeringActivity ;
        owl:equivalentClass  comm:ResourceExtraction .

comm:Development  a      owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .
