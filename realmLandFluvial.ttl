@base <http://sweetontology.net/realmLandFluvial> .

<http://sweetontology.net/realmLandFluvial> rdf:type owl:Ontology ;
                                             owl:imports <http://sweetontology.net/phen> ,
                                                         <http://sweetontology.net/phenGeol> ,
                                                         <http://sweetontology.net/phenGeolGeomorphology> ,
                                                         <http://sweetontology.net/realmLandform> ,
                                                         <http://sweetontology.net/rela> ,
                                                         <http://sweetontology.net/reprSpaceGeometry3D> ;
                                             rdfs:label "SWEET Ontology" ;
                                             owl:versionInfo "3.1.0" .

#################################################################
#    Object Properties
#################################################################

###  http://sweetontology.net/rela/contains
<http://sweetontology.net/rela/contains> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/rela/hasPhenomena
<http://sweetontology.net/rela/hasPhenomena> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/realmLandFluvial/Alluvial
:Alluvial rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/phenGeolGeomorphology/SedimentFormation> ;
          rdfs:comment "The process of depositing soil or sediments by a river or other running water. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandFluvial/AlluvialFan
:AlluvialFan rdf:type owl:Class ;
             rdfs:subClassOf :PiedmontSlope .


###  http://sweetontology.net/realmLandFluvial/AlluvialPlain
:AlluvialPlain rdf:type owl:Class ;
               rdfs:subClassOf :FluvialPlain ,
                               :PiedmontSlope .


###  http://sweetontology.net/realmLandFluvial/Bajada
:Bajada rdf:type owl:Class ;
        rdfs:subClassOf :PiedmontSlope .


###  http://sweetontology.net/realmLandFluvial/Bedform
:Bedform rdf:type owl:Class ;
         rdfs:subClassOf :FluvialLandform .


###  http://sweetontology.net/realmLandFluvial/Canyon
:Canyon rdf:type owl:Class ;
        rdfs:subClassOf :FluvialLandform ;
        rdfs:comment "A canyon, or gorge, is a deep valley between cliffs often carved from the landscape by a river. Most canyons were formed by a process of long-time erosion from a plateau level. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandFluvial/Catchment
:Catchment rdf:type owl:Class ;
           rdfs:subClassOf :Watershed ;
           rdfs:comment "A catchment, or drainage basin, is an extent of land where water from precipitation drains into a body of water [Wikipedia]"@en .


###  http://sweetontology.net/realmLandFluvial/Cave
:Cave rdf:type owl:Class ;
      rdfs:subClassOf :FluvialLandform ,
                      <http://sweetontology.net/reprSpaceGeometry3D/Cavity> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/rela/contains> ;
                        owl:someValuesFrom :Tunnel
                      ] .


###  http://sweetontology.net/realmLandFluvial/DrainageBasin
:DrainageBasin rdf:type owl:Class ;
               owl:equivalentClass :Watershed .


###  http://sweetontology.net/realmLandFluvial/DryLake
:DryLake rdf:type owl:Class ;
         owl:equivalentClass :Playa .


###  http://sweetontology.net/realmLandFluvial/FloodPlain
:FloodPlain rdf:type owl:Class ;
            rdfs:subClassOf :FluvialPlain .


###  http://sweetontology.net/realmLandFluvial/FluvialLandform
:FluvialLandform rdf:type owl:Class ;
                 rdfs:subClassOf <http://sweetontology.net/realmLandform/Landform> .


###  http://sweetontology.net/realmLandFluvial/FluvialPlain
:FluvialPlain rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/realmLandform/Plain> .


###  http://sweetontology.net/realmLandFluvial/Gorge
:Gorge rdf:type owl:Class ;
       rdfs:subClassOf :Canyon .


###  http://sweetontology.net/realmLandFluvial/Gulley
:Gulley rdf:type owl:Class ;
        rdfs:subClassOf :FluvialLandform .


###  http://sweetontology.net/realmLandFluvial/KarstLandform
:KarstLandform rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/realmLandform/Landform> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                 owl:someValuesFrom <http://sweetontology.net/phenGeolGeomorphology/Karst>
                               ] ;
               rdfs:comment "Karst topography is a landscape shaped by the dissolution of a layer or layers of soluble bedrock, usually carbonate rock such as limestone or dolomite. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandFluvial/LacustrinePlain
:LacustrinePlain rdf:type owl:Class ;
                 rdfs:subClassOf :FluvialPlain .


###  http://sweetontology.net/realmLandFluvial/PiedmontSlope
:PiedmontSlope rdf:type owl:Class ;
               rdfs:subClassOf :FluvialLandform .


###  http://sweetontology.net/realmLandFluvial/Playa
:Playa rdf:type owl:Class ;
       owl:equivalentClass :SaltLake ;
       rdfs:subClassOf :FluvialPlain .


###  http://sweetontology.net/realmLandFluvial/Ripple
:Ripple rdf:type owl:Class ;
        rdfs:subClassOf :Bedform .


###  http://sweetontology.net/realmLandFluvial/RiverBed
:RiverBed rdf:type owl:Class ;
          rdfs:subClassOf :Bedform .


###  http://sweetontology.net/realmLandFluvial/Sabkha
:Sabkha rdf:type owl:Class ;
        rdfs:subClassOf :SaltFlat .


###  http://sweetontology.net/realmLandFluvial/SaltFlat
:SaltFlat rdf:type owl:Class ;
          rdfs:subClassOf :FluvialLandform .


###  http://sweetontology.net/realmLandFluvial/SaltLake
:SaltLake rdf:type owl:Class .


###  http://sweetontology.net/realmLandFluvial/ScrollPlain
:ScrollPlain rdf:type owl:Class ;
             rdfs:subClassOf :FluvialPlain .


###  http://sweetontology.net/realmLandFluvial/Trench
:Trench rdf:type owl:Class ;
        rdfs:subClassOf :Canyon ;
        rdfs:comment "A trench is a type of excavation or depression in the ground. Trenches are generally defined by being deeper than they are wide (as opposed to a wider gully or ditch), and by being narrow compared to their length (as opposed to a simple hole). Trenches are a natural feature in many landscapes. Some are created by rivers in flow (which may have long since fallen dry), others are features created by geological movement, such as oceanic trenches. The latter form is relatively deep, linear and narrow, and is formed by plate subduction. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandFluvial/Tunnel
:Tunnel rdf:type owl:Class ;
        rdfs:subClassOf :FluvialLandform ,
                        <http://sweetontology.net/reprSpaceGeometry3D/Tube> .


###  http://sweetontology.net/realmLandFluvial/Turbidite
:Turbidite rdf:type owl:Class ;
           rdfs:subClassOf :FluvialLandform .


###  http://sweetontology.net/realmLandFluvial/Valley
:Valley rdf:type owl:Class ;
        rdfs:subClassOf :FluvialLandform ;
        rdfs:comment "A valley (also called a vale, dale, glen or strath and near or in Appalachia, a draw) is a depression with predominant extent in one direction. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandFluvial/Wall
:Wall rdf:type owl:Class ;
      rdfs:subClassOf :FluvialLandform .


###  http://sweetontology.net/realmLandFluvial/Wash
:Wash rdf:type owl:Class ;
      rdfs:subClassOf :PiedmontSlope .


###  http://sweetontology.net/realmLandFluvial/Watershed
:Watershed rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/realmLandform/LandRegion> ;
           rdfs:comment "A watershed refers to a divide that separates one drainage area from another drainage area. However, in the US and Canada, the term is often used to mean a drainage basin or catchment area itself. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandform/LandRegion
<http://sweetontology.net/realmLandform/LandRegion> rdf:type owl:Class .


