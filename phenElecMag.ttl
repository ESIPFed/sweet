@base          <http://sweetontology.net/phenElecMag> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix obj3d: <http://sweetontology.net/reprSpaceGeometry3D/> .
@prefix proc:  <http://sweetontology.net/procPhysical/> .
@prefix func:  <http://sweetontology.net/reprMathFunction/> .
@prefix phen:  <http://sweetontology.net/phen/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix char:  <http://sweetontology.net/propCharge/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix emf:   <http://sweetontology.net/phenElecMag/> .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <reprSpaceGeometry3D> , <procPhysical> , <reprMathFunction> , <propCharge> , <relaSpace> , <relaMath> , <phen> ;
        owl:versionInfo  "3.1.0" .

emf:MagneticDipole  a    owl:Class ;
        rdfs:subClassOf  emf:MagneticField , obj3d:Dipole .

emf:MagneticField  a     owl:Class ;
        rdfs:subClassOf  func:VectorField ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      srela:hasSpatialDistribution ;
                           owl:someValuesFrom  char:MagneticMoment
                         ] .

emf:GeomagneticField  a  owl:Class ;
        rdfs:subClassOf  emf:MagneticField .

emf:ElectricField  a     owl:Class ;
        rdfs:subClassOf  func:VectorField ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      srela:hasSpatialDistribution ;
                           owl:someValuesFrom  char:Charge
                         ] .

emf:Gyroresonance  a     owl:Class ;
        rdfs:subClassOf  emf:MagneticFieldPhenomena .

emf:ElectricDipole  a    owl:Class ;
        rdfs:subClassOf  emf:ElectricField , obj3d:Dipole .

emf:CurrentSheet  a      owl:Class ;
        rdfs:subClassOf  emf:ElectricField .

emf:MagneticFieldPhenomena
        a                owl:Class ;
        rdfs:subClassOf  phen:Phenomena ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  emf:MagneticField ;
                           owl:onProperty     mrela:hasField
                         ] .

emf:MagneticMirror  a    owl:Class ;
        rdfs:subClassOf  emf:MagneticFieldPhenomena .
