@base          <http://sweetontology.net/phenAtmoTransport> .
@prefix chem:  <http://sweetontology.net/matr/> .
@prefix crela: <http://sweetontology.net/relaChemical/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix boun:  <http://sweetontology.net/realmAtmoBoundaryLayer/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sys:   <http://sweetontology.net/phenSystem/> .
@prefix cstate: <http://sweetontology.net/stateChemical/> .
@prefix phen:  <http://sweetontology.net/phen/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix atmt:  <http://sweetontology.net/phenAtmoTransport/> .
@prefix rela:  <http://sweetontology.net/rela/> .
@prefix trans: <http://sweetontology.net/phenFluidTransport/> .
@prefix fog:   <http://sweetontology.net/phenAtmoFog/> .

atmt:ScavengingByPrecipitation
        a                owl:Class ;
        rdfs:comment     "Removal of pollutants from the air by either rain or snow.  Rainout (or snowout), which is the in-cloud capture of particulates as condensation nuclei, is one form of scavenging."@en ;
        rdfs:subClassOf  atmt:WetDeposition .

atmt:NucleationScavenging
        a                owl:Class ;
        rdfs:comment     "Water vapor that removes particulate matter from the atmosphere"@en ;
        rdfs:subClassOf  atmt:ScavengingByPrecipitation .

atmt:WetDeposition  a    owl:Class ;
        rdfs:comment     "The removal of atmospheric gases or particles through their incorporation into hydrometeors, which are then lost by precipitation."@en ;
        rdfs:subClassOf  phen:AtmosphereLandExchange ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  phen:Precipitation ;
                           owl:onProperty     rela:hasPhenomena
                         ] .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <phenFluidTransport> , <rela> , <phenSystem> , <relaChemical> , <phenAtmoFog> , <stateChemical> , <realmAtmoBoundaryLayer> , <phen> , <matr> ;
        owl:versionInfo  "3.1.0" .

atmt:Rainout  a              owl:Class ;
        rdfs:subClassOf      atmt:ScavengingByPrecipitation ;
        owl:equivalentClass  atmt:Washout .

atmt:AcidRain  a         owl:Class ;
        rdfs:comment     "A popular expression for the deposition by rainfall of various airborne pollutants (especially SO2 and NO2) that have harmful effects on vegetation, soils, buildings and other external structures."@en ;
        rdfs:subClassOf  atmt:WetDeposition ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    cstate:Acid ;
                           owl:onProperty  crela:hasSubstance
                         ] .

atmt:Washout  a  owl:Class .

atmt:AcidHaze  a  owl:Class .

atmt:AcidFog  a              owl:Class ;
        rdfs:comment         "Occurrence of fog or haze in which considerable amounts of acidic material have been taken up from the gas phase, resulting in pH values less than approximately 3 in the liquid phase." ;
        rdfs:label           "Acid Fog" ;
        rdfs:subClassOf      fog:Fog ;
        rdfs:subClassOf      [ a               owl:Restriction ;
                               owl:hasValue    cstate:Acid ;
                               owl:onProperty  crela:hasSubstance
                             ] ;
        owl:equivalentClass  atmt:AcidHaze .

atmt:MixedLayerVenting
        a                owl:Class ;
        rdfs:comment     "Removal of pollutants out of the top of the atmospheric boundary layer through the mixed-layer capping inversion. Normally pollutants cannot escape through the capping inversion. However, penetrating cumulus clouds, thunderstorms, mountain circulations, and frontal circulations can force polluted air through the inversion to vent pollutants into the free atmosphere."@en ;
        rdfs:subClassOf  sys:Removal ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  boun:MixedLayer ;
                           owl:onProperty     rela:hasPhenomena
                         ] .
