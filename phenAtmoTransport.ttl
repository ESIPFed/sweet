###  http://sweetontology.net/phenAtmoTransport/AcidFog
atmt:AcidFog rdf:type owl:Class ;
             owl:equivalentClass atmt:AcidHaze ;
             rdfs:subClassOf fog:Fog ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty crela:hasSubstance ;
                               owl:hasValue cstate:Acid
                             ] .


###  http://sweetontology.net/phenAtmoTransport/AcidHaze
atmt:AcidHaze rdf:type owl:Class .


###  http://sweetontology.net/phenAtmoTransport/AcidRain
atmt:AcidRain rdf:type owl:Class ;
              rdfs:subClassOf atmt:WetDeposition ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty crela:hasSubstance ;
                                owl:hasValue cstate:Acid
                              ] .


###  http://sweetontology.net/phenAtmoTransport/MixedLayerVenting
atmt:MixedLayerVenting rdf:type owl:Class ;
                       rdfs:subClassOf sys:Removal ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty rela:hasPhenomena ;
                                         owl:allValuesFrom boun:MixedLayer
                                       ] .


###  http://sweetontology.net/phenAtmoTransport/NucleationScavenging
atmt:NucleationScavenging rdf:type owl:Class ;
                          rdfs:subClassOf atmt:ScavengingByPrecipitation .


###  http://sweetontology.net/phenAtmoTransport/Rainout
atmt:Rainout rdf:type owl:Class ;
             owl:equivalentClass atmt:Washout ;
             rdfs:subClassOf atmt:ScavengingByPrecipitation .


###  http://sweetontology.net/phenAtmoTransport/ScavengingByPrecipitation
atmt:ScavengingByPrecipitation rdf:type owl:Class ;
                               rdfs:subClassOf atmt:WetDeposition .


###  http://sweetontology.net/phenAtmoTransport/Washout
atmt:Washout rdf:type owl:Class .


###  http://sweetontology.net/phenAtmoTransport/WetDeposition
atmt:WetDeposition rdf:type owl:Class ;
                   rdfs:subClassOf phen:AtmosphereLandExchange ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty rela:hasPhenomena ;
                                     owl:allValuesFrom phen:Precipitation
                                   ] .


###  http://sweetontology.net/phenSystem/Removal
sys:Removal rdf:type owl:Class .


