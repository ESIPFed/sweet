@base          <http://sweetontology.net/propSpaceThickness> .
@prefix crela: <http://sweetontology.net/relaChemical/> .
@prefix pres:  <http://sweetontology.net/propPressure/> .
@prefix trela: <http://sweetontology.net/relaTime/> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix ht:    <http://sweetontology.net/propSpaceHeight/> .
@prefix water: <http://sweetontology.net/matrWater/> .
@prefix seas:  <http://sweetontology.net/reprTimeSeason/> .
@prefix cstate: <http://sweetontology.net/statePhysical/> .
@prefix func:  <http://sweetontology.net/propFunction/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix prop:  <http://sweetontology.net/prop/> .
@prefix thic:  <http://sweetontology.net/propSpaceThickness/> .
@prefix screla: <http://sweetontology.net/relaSci/> .
@prefix rela:  <http://sweetontology.net/rela/> .

thic:Snowpack  a         owl:Class ;
        rdfs:subClassOf  thic:SnowCover .

thic:SWE  a     owl:Class .

thic:StreamStage  a      owl:Class ;
        rdfs:subClassOf  thic:StageHeight .

thic:GaugeHeight  a      owl:Class ;
        rdfs:subClassOf  thic:WaterHeight .

thic:AverageDrySeasonPrecipitation
        a                owl:Class ;
        rdfs:subClassOf  func:AnnualAverage , thic:Precipitation ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      trela:hasTime ;
                           owl:someValuesFrom  seas:DrySeason
                         ] .

thic:LiquidWaterEquivalent
        a                    owl:Class ;
        rdfs:subClassOf      thic:WaterContent ;
        owl:equivalentClass  thic:LWE .

thic:OpticalDepth  a         owl:Class ;
        rdfs:comment         "The optical thickness measured vertically above some given altitude.  Optical depth is dimensionless and may be used to specify many different radiative characteristics of the atmosphere."@en ;
        rdfs:subClassOf      quan:RadiationMediumInteractionQuantity ;
        rdfs:subClassOf      [ a               owl:Restriction ;
                               owl:hasValue    units:dimensionlessUnit ;
                               owl:onProperty  screla:hasDefaultUnit
                             ] ;
        owl:equivalentClass  thic:OpticalThickness .

thic:StageHeight  a      owl:Class ;
        rdfs:subClassOf  thic:WaterHeight .

thic:EquivalentThickness
        a                owl:Class ;
        rdfs:subClassOf  thic:Thickness .

thic:PressureThickness
        a                owl:Class ;
        rdfs:subClassOf  thic:Thickness ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  pres:Pressure ;
                           owl:onProperty     rela:hasProcess
                         ] .

thic:SnowWaterEquivalent
        a                    owl:Class ;
        rdfs:subClassOf      thic:LiquidWaterEquivalent ;
        owl:equivalentClass  thic:SWE .

thic:AnnualHalfPotentialEvapotranspiration
        a       thic:HalfPotentialEvapotranspiration .

thic:LWE  a     owl:Class .

thic:AverageMonthlyPrecipitation
        a                owl:Class ;
        rdfs:subClassOf  func:MonthlyAverage , thic:Precipitation .

thic:TotalHead  a        owl:Class ;
        rdfs:subClassOf  thic:FluidHead .

thic:Thickness  a        owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity , ht:VerticalExtent .

thic:HalfPotentialEvapotranspiration
        a                owl:Class ;
        rdfs:subClassOf  thic:Evapotranspiration .

thic:Precipitation  a    owl:Class ;
        rdfs:subClassOf  thic:WaterContent .

thic:Evapotranspiration
        a                owl:Class ;
        rdfs:subClassOf  thic:Thickness .

thic:HydraulicHead  a    owl:Class ;
        rdfs:subClassOf  thic:FluidHead ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    cstate:Liquid ;
                           owl:onProperty  rela:hasState
                         ] .

thic:StaticHead  a       owl:Class ;
        rdfs:subClassOf  thic:FluidHead .

thic:OpticalThickness
        a       owl:Class .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <reprSciUnits> , <propQuantity> , <propSpaceHeight> , <reprTimeSeason> , <relaChemical> , <propFunction> , <matrWater> , <statePhysical> , <rela> , <relaTime> , <prop> , <relaSci> , <propPressure> ;
        owl:versionInfo  "3.1.0" .

thic:SnowCover  a        owl:Class ;
        rdfs:comment     "The depth of snow that has fallen."@en ;
        rdfs:subClassOf  thic:Thickness ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  water:Snow ;
                           owl:onProperty     crela:hasSubstance
                         ] .

thic:FluidHead  a        owl:Class ;
        rdfs:subClassOf  thic:Thickness , prop:FluidProperty .

thic:PrecipitableWater
        a                owl:Class ;
        rdfs:comment     "The total atmospheric water vapor contained in a vertical column of unit cross-sectional area extending between any two specified levels."@en ;
        rdfs:subClassOf  thic:WaterContent .

thic:LakeStage  a        owl:Class ;
        rdfs:subClassOf  thic:StageHeight .

thic:PotentialEvapotranspiration
        a                owl:Class ;
        rdfs:subClassOf  thic:Evapotranspiration .

thic:AverageAnnualPrecipitation
        a                owl:Class ;
        rdfs:subClassOf  func:AnnualAverage , thic:Precipitation .

thic:PrecipitationRange
        a                owl:Class ;
        rdfs:subClassOf  thic:Precipitation , ht:HeightRange .

thic:PrecipitationRange_cm
        a                owl:Class ;
        rdfs:subClassOf  thic:PrecipitationRange ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:centimeter ;
                           owl:onProperty  screla:hasUnit
                         ] .

thic:AnnualPotentialEvapotranspiration
        a       thic:PotentialEvapotranspiration .

thic:WaterHeight  a      owl:Class ;
        rdfs:subClassOf  thic:Thickness ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  water:LiquidWater ;
                           owl:onProperty     crela:hasSubstance
                         ] .

thic:WaterContent  a     owl:Class ;
        rdfs:subClassOf  thic:Thickness ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  water:LiquidWater ;
                           owl:onProperty     crela:hasChemical
                         ] .
