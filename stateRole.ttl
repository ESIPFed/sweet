#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( role:Desirable
                        role:Undesirable
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( role:Negative
                        role:Positive
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( role:Secure
                        role:Vulnerable
                      )
] .


