@base          <http://sweetontology.net/realmAstroBody> .
@prefix obj3d: <http://sweetontology.net/reprSpaceGeometry3D/> .
@prefix chem:  <http://sweetontology.net/matr/> .
@prefix astro: <http://sweetontology.net/realmAstroBody/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix prela: <http://sweetontology.net/relaPhysical/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix realm: <http://sweetontology.net/realm/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time:  <http://sweetontology.net/propTime/> .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <relaPhysical> , <propTime> , <matr> , <realm> , <reprSpaceGeometry3D> ;
        owl:versionInfo  "3.1.0" .

astro:Satellite  a       owl:Class ;
        rdfs:subClassOf  astro:AstronomicalBody .

astro:Moon  a            owl:Class ;
        rdfs:subClassOf  astro:AstronomicalBody ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  astro:Planet ;
                           owl:onProperty     prela:revolvesAround
                         ] .

astro:Comet  a           owl:Class ;
        rdfs:subClassOf  astro:AstronomicalBody .

astro:Galaxy  a          owl:Class ;
        rdfs:subClassOf  astro:AstronomicalBody .

astro:Asteroid  a        owl:Class ;
        rdfs:subClassOf  astro:AstronomicalBody .

astro:AstronomicalBody
        a                owl:Class ;
        rdfs:subClassOf  obj3d:Body .

astro:Star  a            owl:Class ;
        rdfs:subClassOf  astro:AstronomicalBody .

astro:Extraterrestrial
        a                owl:Class ;
        rdfs:comment     "A body or substance which does not originate from Earth. [Wikipedia]"@en ;
        rdfs:subClassOf  astro:AstronomicalBody .

astro:OuterSpace  a      owl:Class ;
        rdfs:subClassOf  realm:Realm .

astro:Planet  a          owl:Class ;
        rdfs:subClassOf  astro:AstronomicalBody ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  astro:Star ;
                           owl:onProperty     prela:revolvesAround
                         ] .
