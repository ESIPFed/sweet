#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/reprMathFunctionOrthogonal/Coefficient
:Coefficient rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/reprMath/Scalar> .


###  http://sweetontology.net/reprMathFunctionOrthogonal/Degree
:Degree rdf:type owl:Class ;
        rdfs:subClassOf :Harmonic .


###  http://sweetontology.net/reprMathFunctionOrthogonal/Fourier
:Fourier rdf:type owl:Class ;
         owl:equivalentClass :FourierFunction ;
         rdfs:subClassOf <http://sweetontology.net/reprMathFunction/ExponentialFunction> .


###  http://sweetontology.net/reprMathFunctionOrthogonal/FourierFunction
:FourierFunction rdf:type owl:Class .


###  http://sweetontology.net/reprMathFunctionOrthogonal/Harmonic
:Harmonic rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/repr/Coordinate> .


###  http://sweetontology.net/reprMathFunctionOrthogonal/Mode
:Mode rdf:type owl:Class ;
      rdfs:subClassOf :Harmonic .


###  http://sweetontology.net/reprMathFunctionOrthogonal/Order
:Order rdf:type owl:Class ;
       rdfs:subClassOf :Harmonic .


###  http://sweetontology.net/reprMathFunctionOrthogonal/OrthogonalFunction
:OrthogonalFunction rdf:type owl:Class ;
                    rdfs:subClassOf <http://sweetontology.net/reprMathFunction/BasisFunction> .


###  http://sweetontology.net/reprMathFunctionOrthogonal/SphericalHarmonic
:SphericalHarmonic rdf:type owl:Class ;
                   owl:equivalentClass :SphericalHarmonicFunction ;
                   rdfs:subClassOf <http://sweetontology.net/reprMathFunction/TranscendentalFunction> ,
                                   :OrthogonalFunction ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://sweetontology.net/relaMath/hasDegree> ;
                                     owl:cardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://sweetontology.net/relaMath/hasOrder> ;
                                     owl:cardinality "1"^^xsd:nonNegativeInteger
                                   ] .


###  http://sweetontology.net/reprMathFunctionOrthogonal/SphericalHarmonicFunction
:SphericalHarmonicFunction rdf:type owl:Class .


###  http://sweetontology.net/reprMathFunctionOrthogonal/WaveletFunction
:WaveletFunction rdf:type owl:Class ;
                 rdfs:subClassOf :OrthogonalFunction .


