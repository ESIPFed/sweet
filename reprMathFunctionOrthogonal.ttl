@base          <http://sweetontology.net/reprMathFunctionOrthogonal> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix orth:  <http://sweetontology.net/reprMathFunctionOrthogonal/> .
@prefix func:  <http://sweetontology.net/reprMathFunction/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix math:  <http://sweetontology.net/reprMath/> .

orth:Harmonic  a         owl:Class ;
        rdfs:subClassOf  repr:Coordinate .

orth:SphericalHarmonicFunction
        a       owl:Class .

orth:OrthogonalFunction
        a                owl:Class ;
        rdfs:subClassOf  func:BasisFunction .

orth:SphericalHarmonic
        a                    owl:Class ;
        rdfs:subClassOf      func:TranscendentalFunction , orth:OrthogonalFunction ;
        rdfs:subClassOf      [ a                owl:Restriction ;
                               owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                               owl:onProperty   mrela:hasDegree
                             ] ;
        rdfs:subClassOf      [ a                owl:Restriction ;
                               owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                               owl:onProperty   mrela:hasOrder
                             ] ;
        owl:equivalentClass  orth:SphericalHarmonicFunction .

orth:WaveletFunction  a  owl:Class ;
        rdfs:subClassOf  orth:OrthogonalFunction .

orth:Fourier  a              owl:Class ;
        rdfs:subClassOf      func:ExponentialFunction ;
        owl:equivalentClass  orth:FourierFunction .

orth:Order  a            owl:Class ;
        rdfs:subClassOf  orth:Harmonic .

orth:FourierFunction  a  owl:Class .

orth:Coefficient  a      owl:Class ;
        rdfs:subClassOf  math:Scalar .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <repr> , <relaMath> , <reprMath> ;
        owl:versionInfo  "3.1.0" .

orth:Degree  a           owl:Class ;
        rdfs:subClassOf  orth:Harmonic .

orth:Mode  a             owl:Class ;
        rdfs:subClassOf  orth:Harmonic .
