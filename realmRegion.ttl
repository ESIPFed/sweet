@base          <http://sweetontology.net/realmRegion> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix sreg:  <http://sweetontology.net/realmRegion/> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix realm: <http://sweetontology.net/realm/> .
@prefix spac:  <http://sweetontology.net/reprSpaceGeometry/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix screla: <http://sweetontology.net/relaSci/> .

sreg:Austral  a          owl:Class ;
        rdfs:subClassOf  sreg:SouthLatitudeBand .

sreg:Tropics  a  sreg:LatitudeBand .

sreg:NorthernHemisphere
        a                owl:Class ;
        rdfs:subClassOf  sreg:NorthLatitudeBand ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    90 ;
                           owl:onProperty  mrela:hasUpperBound
                         ] .

sreg:Equatorial  a             sreg:LatitudeBand ;
        srela:hasLatitudeBand  [ a                    sreg:SouthLatitudeBand ;
                                 mrela:hasLowerBound  -20 ;
                                 mrela:hasUpperBound  20
                               ] .

sreg:LatitudeBand  a     owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity , spac:Region ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:degrees ;
                           owl:onProperty  screla:hasUnit
                         ] .

sreg:NorthLatitudeLine
        a                owl:Class ;
        rdfs:subClassOf  sreg:LatitudeLine .

sreg:Polar  a                  sreg:LatitudeBand ;
        srela:hasLatitudeBand  [ a                    sreg:NorthLatitudeBand ;
                                 mrela:hasLowerBound  70 ;
                                 mrela:hasUpperBound  90
                               ] ;
        srela:hasLatitudeBand  [ a                    sreg:SouthLatitudeBand ;
                                 mrela:hasLowerBound  -90 ;
                                 mrela:hasUpperBound  -70
                               ] .

sreg:Subtropics  a             sreg:LatitudeBand ;
        srela:hasLatitudeBand  [ a                    sreg:NorthLatitudeBand ;
                                 mrela:hasLowerBound  20 ;
                                 mrela:hasUpperBound  40
                               ] ;
        srela:hasLatitudeBand  [ a                    sreg:SouthLatitudeBand ;
                                 mrela:hasLowerBound  -40 ;
                                 mrela:hasUpperBound  -20
                               ] ;
        owl:sameAs             sreg:Subtropical .

sreg:Equator  a                sreg:LatitudeLine ;
        srela:hasLatitudeLine  [ a                       sreg:LatitudeLine ;
                                 screla:hasNumericValue  "0."^^xsd:double
                               ] .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <reprSpaceGeometry> , <relaSpace> , <reprSciUnits> , <relaMath> , <realm> , <propQuantity> , <relaSci> ;
        owl:versionInfo  "3.1.0" .

sreg:HighLatitude  a           sreg:LatitudeBand ;
        srela:hasLatitudeBand  [ a                    sreg:NorthLatitudeBand ;
                                 mrela:hasLowerBound  55 ;
                                 mrela:hasUpperBound  90
                               ] ;
        srela:hasLatitudeBand  [ a                    sreg:SouthLatitudeBand ;
                                 mrela:hasLowerBound  -90 ;
                                 mrela:hasUpperBound  -55
                               ] .

sreg:SouthLatitudeLine
        a                owl:Class ;
        rdfs:subClassOf  sreg:LatitudeLine .

sreg:Subtropical  a  sreg:LatitudeBand .

sreg:Pole  a                   sreg:LatitudeLine ;
        srela:hasLatitudeLine  [ a                       sreg:NorthLatitudeLine ;
                                 screla:hasNumericValue  "90."^^xsd:double
                               ] ;
        srela:hasLatitudeLine  [ a                       sreg:SouthLatitudeLine ;
                                 screla:hasNumericValue  "-90."^^xsd:double
                               ] .

sreg:Tropical  a  sreg:LatitudeBand .

sreg:Extratropics  a       sreg:LatitudeBand ;
        owl:differentFrom  sreg:Subtropics , sreg:Tropics .

sreg:MidLatitude  a            sreg:LatitudeBand ;
        srela:hasLatitudeBand  [ a                    sreg:NorthLatitudeBand ;
                                 mrela:hasLowerBound  25 ;
                                 mrela:hasUpperBound  55
                               ] ;
        srela:hasLatitudeBand  [ a                    sreg:SouthLatitudeBand ;
                                 mrela:hasLowerBound  -55 ;
                                 mrela:hasUpperBound  -25
                               ] .

sreg:NorthLatitudeBand
        a                owl:Class ;
        rdfs:subClassOf  sreg:LatitudeBand ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    0 ;
                           owl:onProperty  mrela:hasLowerBound
                         ] .

sreg:SouthLatitudeBand
        a                owl:Class ;
        rdfs:subClassOf  sreg:LatitudeBand ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    0 ;
                           owl:onProperty  mrela:hasUpperBound
                         ] .

sreg:LatitudeLine  a     owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity , spac:Region ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:degrees ;
                           owl:onProperty  screla:hasUnit
                         ] .

sreg:LowLatitude  a            sreg:LatitudeBand ;
        srela:hasLatitudeBand  [ a                    sreg:LatitudeBand ;
                                 mrela:hasLowerBound  -25 ;
                                 mrela:hasUpperBound  25
                               ] ;
        owl:sameAs             sreg:LowLatitude , sreg:Tropics , sreg:Tropical .

sreg:SouthernHemisphere
        a                owl:Class ;
        rdfs:subClassOf  sreg:SouthLatitudeBand ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    -90 ;
                           owl:onProperty  mrela:hasLowerBound
                         ] .

sreg:Global  a                 sreg:LatitudeBand ;
        srela:hasLatitudeBand  [ a                    sreg:SouthLatitudeBand ;
                                 mrela:hasLowerBound  -90 ;
                                 mrela:hasUpperBound  90
                               ] .

sreg:Borealis  a         owl:Class ;
        rdfs:subClassOf  sreg:NorthLatitudeBand .
