@prefix : <http://sweetontology.net/stateSystem/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/stateSystem> .

<http://sweetontology.net/stateSystem> rdf:type owl:Ontology ;
                                        owl:imports <http://sweetontology.net/propOrdinal> ,
                                                    <http://sweetontology.net/rela> ,
                                                    <http://sweetontology.net/relaSci> ,
                                                    <http://sweetontology.net/state> ;
                                        rdfs:label "SWEET Ontology" ;
                                        owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/stateSystem/Coherent
:Coherent rdf:type owl:Class ;
          rdfs:subClassOf :SystemState .


###  http://sweetontology.net/stateSystem/Complex
:Complex rdf:type owl:Class ;
         rdfs:subClassOf :SystemState ;
         rdfs:comment "A complex system is a system composed of interconnected parts that as a whole exhibit one or more properties (behavior among the possible properties) not obvious from the properties" .


###  http://sweetontology.net/stateSystem/Deficit
:Deficit rdf:type owl:Class ;
         rdfs:subClassOf :SystemState ;
         owl:disjointWith :Excess .


###  http://sweetontology.net/stateSystem/EquilibriumState
:EquilibriumState rdf:type owl:Class ;
                  rdfs:subClassOf :SystemState ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                    owl:hasValue :Equilibrium
                                  ] .


###  http://sweetontology.net/stateSystem/Excess
:Excess rdf:type owl:Class ;
        rdfs:subClassOf :SystemState .


###  http://sweetontology.net/stateSystem/Homeostasis
:Homeostasis rdf:type owl:Class ;
             rdfs:subClassOf :SystemState .


###  http://sweetontology.net/stateSystem/Instability
:Instability rdf:type owl:Class ;
             owl:equivalentClass :Unstable .


###  http://sweetontology.net/stateSystem/Overflow
:Overflow rdf:type owl:Class ;
          rdfs:subClassOf :SystemState .


###  http://sweetontology.net/stateSystem/Resilient
:Resilient rdf:type owl:Class ;
           rdfs:subClassOf :SystemState .


###  http://sweetontology.net/stateSystem/Stability
:Stability rdf:type owl:Class ;
           owl:equivalentClass :Stable ;
           owl:disjointWith :Unstable .


###  http://sweetontology.net/stateSystem/Stable
:Stable rdf:type owl:Class ;
        rdfs:subClassOf :SystemState ;
        rdfs:comment "The characteristic of a system if sufficiently small disturbances have only small effects, either decreasing in amplitude or oscillating periodically; it is asymptotically stable if the effect of small disturbances vanishes for long time periods."@en .


###  http://sweetontology.net/stateSystem/SystemState
:SystemState rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/state/State> .


###  http://sweetontology.net/stateSystem/Unstable
:Unstable rdf:type owl:Class ;
          rdfs:subClassOf :SystemState ;
          rdfs:comment "property describing unstable conditions, such as in the atmosphere" .


