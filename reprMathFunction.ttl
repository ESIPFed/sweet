@base          <http://sweetontology.net/reprMathFunction> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix func:  <http://sweetontology.net/reprMathFunction/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix oper:  <http://sweetontology.net/reprMathOperation/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix math:  <http://sweetontology.net/reprMath/> .

func:Logarithmic  a      owl:Class ;
        rdfs:subClassOf  func:TranscendentalFunction .

func:NormalizedFunction
        a                owl:Class ;
        rdfs:subClassOf  func:Function .

func:Linear  a               owl:Class ;
        rdfs:subClassOf      func:Polynomial ;
        rdfs:subClassOf      [ a               owl:Restriction ;
                               owl:hasValue    1 ;
                               owl:onProperty  mrela:hasDegree
                             ] ;
        owl:equivalentClass  func:Affine .

func:Dirac  a            owl:Class ;
        rdfs:subClassOf  func:Function .

func:Quadratic  a        owl:Class ;
        rdfs:subClassOf  func:Polynomial ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    2 ;
                           owl:onProperty  mrela:hasDegree
                         ] .

func:Well  a             owl:Class ;
        rdfs:subClassOf  func:Function .

func:DiscreteFunction
        a       owl:Class .

func:ExponentialDecay
        a                owl:Class ;
        rdfs:subClassOf  func:ExponentialFunction .

func:Cosine  a           owl:Class ;
        rdfs:subClassOf  func:TrigonometricFunction .

func:Polynomial  a       owl:Class ;
        rdfs:subClassOf  func:AlgebraicFunction .

func:Nonlinear  a         owl:Class ;
        rdfs:subClassOf   func:Function ;
        owl:disjointWith  func:Linear .

func:AlgebraicFunction
        a                owl:Class ;
        rdfs:subClassOf  func:Function .

func:CubicSpline  a      owl:Class ;
        rdfs:subClassOf  func:Cubic .

func:Affine  a  owl:Class .

func:ProbabilityDensityFunction
        a                    owl:Class ;
        rdfs:subClassOf      func:NormalizedFunction ;
        owl:equivalentClass  func:pdf .

func:pdf  a     owl:Class .

func:VectorField  a      owl:Class ;
        rdfs:subClassOf  func:VectorFunction , func:Field .

func:BinaryFunction  a   owl:Class ;
        rdfs:subClassOf  func:DiscreteFunction .

func:BasisFunction  a    owl:Class ;
        rdfs:subClassOf  func:Function .

func:Quartic  a          owl:Class ;
        rdfs:subClassOf  func:Polynomial ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    4 ;
                           owl:onProperty  mrela:hasDegree
                         ] .

func:MaxwellBoltzmannDistribution
        a                owl:Class ;
        rdfs:subClassOf  func:Function .

func:ScalarField  a      owl:Class ;
        rdfs:subClassOf  func:ScalarFunction .

func:UtilityFunction  a  owl:Class ;
        rdfs:subClassOf  func:OptimizationFunction .

func:SquareWave  a  owl:Class .

func:RadialBasisFunction
        a                owl:Class ;
        rdfs:subClassOf  func:BasisFunction .

func:PowerLaw  a         owl:Class ;
        rdfs:subClassOf  func:PowerFunction .

func:Continuous  a           owl:Class ;
        rdfs:subClassOf      func:Function ;
        owl:equivalentClass  func:Continuity .

func:Logistic  a         owl:Class ;
        rdfs:subClassOf  func:TranscendentalFunction .

func:Filter  a           owl:Class ;
        rdfs:subClassOf  func:Function .

func:Bivariate  a        owl:Class ;
        rdfs:subClassOf  func:Function .

func:DistributionFunction
        a                owl:Class ;
        rdfs:subClassOf  func:NormalizedFunction .

func:Step  a             owl:Class ;
        rdfs:subClassOf  func:Function .

func:Normal  a               owl:Class ;
        rdfs:subClassOf      func:DistributionFunction ;
        owl:equivalentClass  func:NormalDistribution .

func:Kernel  a           owl:Class ;
        rdfs:subClassOf  func:Filter .

func:Discontinuity  a  owl:Class .

func:Function  a         owl:Class ;
        rdfs:subClassOf  oper:Operation ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  repr:DependentVariable ;
                           owl:onProperty     mrela:hasOutput
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  repr:IndependentVariable ;
                           owl:onProperty     mrela:hasInput
                         ] .

func:ProbabilityDistributionFunction
        a                    owl:Class ;
        rdfs:subClassOf      func:NormalizedFunction ;
        owl:equivalentClass  func:cdf .

func:Field  a            owl:Class ;
        rdfs:subClassOf  func:Function ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  quan:Quantity ;
                           owl:onProperty     srela:hasSpatialDistribution
                         ] .

func:TrigonometricFunction
        a                owl:Class ;
        rdfs:subClassOf  func:TranscendentalFunction .

func:ExponentialFunction
        a                owl:Class ;
        rdfs:subClassOf  func:TranscendentalFunction .

func:ExponentialGrowth
        a                owl:Class ;
        rdfs:subClassOf  func:ExponentialFunction .

func:Sin  a              owl:Class ;
        rdfs:subClassOf  func:TrigonometricFunction .

func:BoxCar  a               owl:Class ;
        rdfs:subClassOf      func:AlgebraicFunction ;
        owl:equivalentClass  func:SquareWave .

func:NormalDistribution
        a       owl:Class .

func:TensorField  a      owl:Class ;
        rdfs:subClassOf  func:TensorFunction , func:Field .

func:Discrete  a             owl:Class ;
        rdfs:subClassOf      func:Function ;
        owl:disjointWith     func:Continuous ;
        owl:equivalentClass  func:DiscreteFunction .

func:VectorQuantity  a   owl:Class ;
        rdfs:subClassOf  func:Function ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  math:Vector ;
                           owl:onProperty     mrela:hasOutput
                         ] .

func:cdf  a     owl:Class .

func:Horizon  a  owl:Class .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <propQuantity> , <repr> , <reprMathOperation> , <relaSpace> , <relaMath> , <reprMath> ;
        owl:versionInfo  "3.1.0" .

func:TensorFunction  a   owl:Class ;
        rdfs:subClassOf  func:Function ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  math:Tensor ;
                           owl:onProperty     mrela:hasInput
                         ] .

func:Profile  a              owl:Class ;
        rdfs:subClassOf      func:Function ;
        owl:equivalentClass  func:VerticalProfile , func:Horizon .

func:PowerFunction  a    owl:Class ;
        rdfs:subClassOf  func:Function .

func:BSpline  a          owl:Class ;
        rdfs:subClassOf  func:CubicSpline .

func:ScalarFunction  a   owl:Class ;
        rdfs:subClassOf  func:Function ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  math:Scalar ;
                           owl:onProperty     mrela:hasOutput
                         ] .

func:Cubic  a            owl:Class ;
        rdfs:subClassOf  func:Polynomial ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    3 ;
                           owl:onProperty  mrela:hasDegree
                         ] .

func:Discontinuous  a        owl:Class ;
        rdfs:comment         "A point in the range of a function at which it is undefined or not continuous [Wiktionary]"@en ;
        rdfs:subClassOf      func:Function ;
        owl:disjointWith     func:Continuous ;
        owl:equivalentClass  func:Discontinuity .

func:ChiSquareDistribution
        a       owl:Class .

func:OptimizationFunction
        a                owl:Class ;
        rdfs:subClassOf  func:Function .

func:ChiSquare  a            owl:Class ;
        rdfs:subClassOf      func:DistributionFunction ;
        owl:equivalentClass  func:ChiSquareDistribution .

func:TranscendentalFunction
        a                 owl:Class ;
        rdfs:subClassOf   func:Function ;
        owl:disjointWith  func:AlgebraicFunction .

func:VectorFunction  a   owl:Class ;
        rdfs:subClassOf  func:Function ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  math:Vector ;
                           owl:onProperty     mrela:hasInput
                         ] .

func:VerticalProfile  a  owl:Class .

func:Continuity  a  owl:Class .
