@base          <http://sweetontology.net/relaTime> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix owlt:  <http://www.w3.org/2006/time#> .
@prefix trela: <http://sweetontology.net/relaTime/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix freq:  <http://sweetontology.net/propTimeFrequency/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time:  <http://sweetontology.net/propTime/> .
@prefix rela:  <http://sweetontology.net/relaMath/> .
@prefix trep:  <http://sweetontology.net/reprTime/> .

owlt:hasEnd  a              owl:ObjectProperty ;
        rdfs:subPropertyOf  trela:hasEndTime .

trela:endOf  a              owl:ObjectProperty ;
        rdfs:subPropertyOf  owlt:after .

trela:hasTimeReference
        a                       owl:ObjectProperty ;
        rdfs:range              trep:TimeReference ;
        owl:equivalentProperty  trela:hasTemporalReference .

trela:moreFrequentThan
        a                   owl:ObjectProperty ;
        rdfs:domain         freq:Frequency ;
        rdfs:range          freq:Frequency ;
        rdfs:subPropertyOf  rela:greaterThan .

trela:hasFrequency  a  owl:ObjectProperty ;
        rdfs:range  freq:Frequency .

trela:hasStartTime  a           owl:ObjectProperty ;
        rdfs:range              time:StartTime ;
        rdfs:subPropertyOf      trela:hasTime ;
        owl:equivalentProperty  trela:hasStart , trela:start , trela:hasBeginning , trela:beginning .

trela:beginning  a  owl:ObjectProperty .

trela:dayOfYear  a  owl:DatatypeProperty .

trela:hasStart  a  owl:ObjectProperty .

trela:hasEnd  a  owl:ObjectProperty .

trela:hasPeriod  a  owl:ObjectProperty ;
        rdfs:range  time:Period .

owlt:after  a               owl:ObjectProperty ;
        rdfs:subPropertyOf  rela:greaterThan .

trela:hasAverageDuration
        a                   owl:ObjectProperty ;
        rdfs:subPropertyOf  trela:hasDuration .

trela:hasSHTime  a          owl:ObjectProperty ;
        rdfs:subPropertyOf  srela:inSH , trela:hasTime .

trela:lessFrequentThan
        a              owl:ObjectProperty ;
        owl:inverseOf  trela:moreFrequentThan .

trela:halfLife  a           owl:ObjectProperty ;
        rdfs:subPropertyOf  trela:hasDuration .

trela:end  a    owl:ObjectProperty .

trela:youngerThan  a   owl:ObjectProperty ;
        owl:inverseOf  trela:olderThan .

trela:hasDuration  a  owl:ObjectProperty ;
        rdfs:range  time:Duration .

trela:hasTime  a    owl:ObjectProperty ;
        rdfs:range  time:Time .

trela:hasBeginning  a  owl:ObjectProperty .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <propTimeFrequency> , <reprTime> , <propTime> , <relaSpace> , <relaMath> ;
        owl:versionInfo  "3.1.0" .

trela:start  a  owl:ObjectProperty .

trela:hasEndTime  a             owl:ObjectProperty ;
        rdfs:range              time:EndTime ;
        rdfs:subPropertyOf      trela:hasTime ;
        owl:equivalentProperty  trela:hasEnd , trela:end .

trela:startOf  a            owl:ObjectProperty ;
        rdfs:subPropertyOf  owlt:before .

trela:temporalPartOf  a     owl:ObjectProperty ;
        rdfs:domain         time:Time ;
        rdfs:range          time:Time ;
        rdfs:subPropertyOf  rela:subsetOf .

owlt:hasBeginning  a        owl:ObjectProperty ;
        rdfs:subPropertyOf  trela:hasStartTime .

owlt:before  a              owl:ObjectProperty ;
        rdfs:subPropertyOf  rela:lessThan .

trela:olderThan  a          owl:ObjectProperty ;
        rdfs:domain         time:Time ;
        rdfs:range          time:Time ;
        rdfs:subPropertyOf  rela:greaterThan .

trela:hasNHTime  a          owl:ObjectProperty ;
        rdfs:subPropertyOf  srela:inNH , trela:hasTime .

trela:hasTemporalReference
        a       owl:ObjectProperty .
