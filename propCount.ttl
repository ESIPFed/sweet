@base          <http://sweetontology.net/propCount> .
@prefix star:  <http://sweetontology.net/phenStar/> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix count: <http://sweetontology.net/propCount/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix math:  <http://sweetontology.net/reprMathFunction/> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix screla: <http://sweetontology.net/relaSci/> .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <relaSci> , <reprSciUnits> , <phenStar> , <propQuantity> , <reprMathFunction> ;
        owl:versionInfo  "3.1.0" .

count:Population  a      owl:Class ;
        rdfs:subClassOf  count:Count .

count:SunspotNumber  a   owl:Class ;
        rdfs:subClassOf  count:Count ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  star:SolarActivity ;
                           owl:onProperty     screla:measureOf
                         ] .

count:TotalElectronContent
        a                owl:Class ;
        rdfs:subClassOf  count:NumberDensity .

count:Count  a           owl:Class ;
        rdfs:subClassOf  math:Discrete .

count:NumberDensity  a   owl:Class ;
        rdfs:subClassOf  quan:Quantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:perMeterCubed ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

count:EnergyLevel  a     owl:Class ;
        rdfs:subClassOf  count:Count .

count:Fluence  a         owl:Class ;
        rdfs:subClassOf  quan:Quantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:perMeterSquared ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

count:CarryingCapacity
        a                owl:Class ;
        rdfs:subClassOf  count:Population .

count:AtomicNeutralNumberDensity
        a                owl:Class ;
        rdfs:subClassOf  count:NeutralNumberDensity .

count:NeutralNumberDensity
        a                owl:Class ;
        rdfs:subClassOf  count:NumberDensity .

count:MolecularNeutralNumberDensity
        a                owl:Class ;
        rdfs:subClassOf  count:NeutralNumberDensity .
