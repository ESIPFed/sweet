###  http://sweetontology.net/propCount/AtomicNeutralNumberDensity
count:AtomicNeutralNumberDensity rdf:type owl:Class ;
                                 rdfs:subClassOf count:NeutralNumberDensity .


###  http://sweetontology.net/propCount/CarryingCapacity
count:CarryingCapacity rdf:type owl:Class ;
                       rdfs:subClassOf count:Population .


###  http://sweetontology.net/propCount/Count
count:Count rdf:type owl:Class ;
            rdfs:subClassOf math:Discrete .


###  http://sweetontology.net/propCount/EnergyLevel
count:EnergyLevel rdf:type owl:Class ;
                  rdfs:subClassOf count:Count .


###  http://sweetontology.net/propCount/Fluence
count:Fluence rdf:type owl:Class ;
              rdfs:subClassOf quan:Quantity ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty screla:hasDefaultUnit ;
                                owl:hasValue units:perMeterSquared
                              ] .


###  http://sweetontology.net/propCount/MolecularNeutralNumberDensity
count:MolecularNeutralNumberDensity rdf:type owl:Class ;
                                    rdfs:subClassOf count:NeutralNumberDensity .


###  http://sweetontology.net/propCount/NeutralNumberDensity
count:NeutralNumberDensity rdf:type owl:Class ;
                           rdfs:subClassOf count:NumberDensity .


###  http://sweetontology.net/propCount/NumberDensity
count:NumberDensity rdf:type owl:Class ;
                    rdfs:subClassOf quan:Quantity ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty screla:hasDefaultUnit ;
                                      owl:hasValue units:perMeterCubed
                                    ] .


###  http://sweetontology.net/propCount/Population
count:Population rdf:type owl:Class ;
                 rdfs:subClassOf count:Count .


###  http://sweetontology.net/propCount/SunspotNumber
count:SunspotNumber rdf:type owl:Class ;
                    rdfs:subClassOf count:Count ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty screla:measureOf ;
                                      owl:allValuesFrom star:SolarActivity
                                    ] .


###  http://sweetontology.net/propCount/TotalElectronContent
count:TotalElectronContent rdf:type owl:Class ;
                           rdfs:subClassOf count:NumberDensity .


###  http://sweetontology.net/propQuantity/Quantity
quan:Quantity rdf:type owl:Class .


###  http://sweetontology.net/reprMathFunction/Discrete
math:Discrete rdf:type owl:Class .


