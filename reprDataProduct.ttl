@base          <http://sweetontology.net/reprDataProduct> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix solu:  <http://sweetontology.net/reprMathSolution/> .
@prefix :      <http://sweetontology.net/reprDataProduct> .
@prefix res:   <http://sweetontology.net/humanResearch/> .
@prefix func:  <http://sweetontology.net/reprMathFunction/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

<reprDataProduct/Image>
        a                owl:Class ;
        rdfs:subClassOf  repr:Representation .

:       a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <reprMathFunction> , <reprMathSolution> , <repr> , <humanResearch> ;
        owl:versionInfo  "3.1.0" .

<reprDataProduct/Offset>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/Value> .

<reprDataProduct/Curate>
        a                    owl:Class ;
        rdfs:subClassOf      <reprDataProduct/Collection> ;
        owl:equivalentClass  <reprDataProduct/Curation> .

<reprDataProduct/Collection>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/Data> .

<reprDataProduct/CDR>
        a       owl:Class .

<reprDataProduct/MaximumValid>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/Value> .

<reprDataProduct/Archiving>
        a       owl:Class .

<reprDataProduct/Ensemble>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/Collection> .

<reprDataProduct/ScaleFactor>
        a                    owl:Class ;
        rdfs:subClassOf      <reprDataProduct/Value> ;
        owl:equivalentClass  <reprDataProduct/Scaling> .

<reprDataProduct/Version>
        a                owl:Class ;
        rdfs:subClassOf  solu:Iteration .

<reprDataProduct/Scaling>
        a       owl:Class .

<reprDataProduct/DataProduct>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/Collection> .

<reprDataProduct/Archive>
        a                    owl:Class ;
        rdfs:subClassOf      <reprDataProduct/Collection> ;
        owl:equivalentClass  <reprDataProduct/Archiving> .

<reprDataProduct/ClimateDataRecord>
        a                    owl:Class ;
        rdfs:subClassOf      <reprDataProduct/Collection> ;
        owl:equivalentClass  <reprDataProduct/CDR> .

<reprDataProduct/FillValue>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/Value> .

<reprDataProduct/EarthScienceDataRecord>
        a                    owl:Class ;
        rdfs:subClassOf      <reprDataProduct/Collection> ;
        owl:equivalentClass  <reprDataProduct/ESDR> .

<reprDataProduct/Value>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/Data> .

<reprDataProduct/Dataset>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/DataProduct> .

<reprDataProduct/ESDR>
        a       owl:Class .

<reprDataProduct/Profile>
        a                owl:Class ;
        rdfs:subClassOf  func:Profile , repr:Representation .

<reprDataProduct/Data>
        a                owl:Class ;
        rdfs:subClassOf  res:Realization .

<reprDataProduct/MinimumValid>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/Value> .

<reprDataProduct/Layer>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/DataProduct> .

<reprDataProduct/Granule>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/Data> .

<reprDataProduct/Curation>
        a       owl:Class .

<reprDataProduct/MissingValue>
        a                owl:Class ;
        rdfs:subClassOf  <reprDataProduct/Value> .
