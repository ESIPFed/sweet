@base          <http://sweetontology.net/reprSpaceGeometry> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix mathp: <http://sweetontology.net/reprMathFunction/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix obj:   <http://sweetontology.net/reprSpaceGeometry/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix math:  <http://sweetontology.net/reprMath/> .

obj:Trajectory  a            owl:Class ;
        rdfs:comment         "Curve in space tracing the points successivley occupied by a particle in motion"@en ;
        rdfs:subClassOf      obj:Curve ;
        owl:equivalentClass  obj:Path .

obj:Cluster  a               owl:Class ;
        rdfs:subClassOf      math:Collection ;
        owl:equivalentClass  obj:Swarm , obj:Family .

obj:Lattice  a           owl:Class ;
        rdfs:subClassOf  mathp:Discrete , obj:GeometricalObject .

obj:Conic  a                 owl:Class ;
        rdfs:subClassOf      obj:GeometricalObject_2D ;
        owl:equivalentClass  obj:ConicSection .

obj:GeometricalObject_1D
        a                owl:Class ;
        rdfs:subClassOf  repr:1D , obj:GeometricalObject .

obj:Swarm  a    owl:Class .

obj:Circle  a            owl:Class ;
        rdfs:subClassOf  obj:ConicSection .

obj:Parabola  a          owl:Class ;
        rdfs:subClassOf  obj:ConicSection .

obj:Polygon  a           owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject_2D ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  obj:Point ;
                           owl:onProperty     srela:hasVertices
                         ] .

obj:Contour  a           owl:Class ;
        rdfs:subClassOf  obj:Surface .

obj:Family  a   owl:Class .

obj:Crescent  a          owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject_2D .

obj:Region  a            owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject .

obj:Arc  a               owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject_1D .

obj:Circular  a          owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject_2D .

obj:Patch  a             owl:Class ;
        rdfs:subClassOf  obj:Surface .

obj:Curve  a             owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject_1D .

obj:Plane  a             owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject_2D , obj:Surface .

obj:BoundingBox  a       owl:Class ;
        rdfs:subClassOf  obj:Rectangle .

obj:Square  a            owl:Class ;
        rdfs:subClassOf  obj:Rectangle .

obj:Spiral  a            owl:Class ;
        rdfs:subClassOf  obj:Arc .

obj:Zone  a              owl:Class ;
        rdfs:subClassOf  obj:Region .

obj:SmallCircle  a        owl:Class ;
        rdfs:subClassOf   obj:Circle ;
        owl:disjointWith  obj:GreatCircle .

obj:GeometricalObject_0D
        a                    owl:Class ;
        rdfs:subClassOf      obj:GeometricalObject ;
        rdfs:subClassOf      [ a               owl:Restriction ;
                               owl:hasValue    0 ;
                               owl:onProperty  mrela:hasDimension
                             ] ;
        owl:equivalentClass  obj:Point .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <repr> , <relaSpace> , <relaMath> , <reprMathFunction> , <reprMath> ;
        owl:versionInfo  "3.1.0" .

obj:Ridge  a             owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject .

obj:GeometricalObject_2D
        a                owl:Class ;
        rdfs:subClassOf  repr:2D , obj:GeometricalObject .

obj:Hyperbola  a         owl:Class ;
        rdfs:subClassOf  obj:ConicSection .

obj:Rectangle  a         owl:Class ;
        rdfs:subClassOf  obj:Polygon .

obj:Star  a              owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject_2D .

obj:Route  a             owl:Class ;
        rdfs:subClassOf  obj:Path .

obj:Helical  a           owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject_2D .

obj:Surface  a           owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject .

obj:Peak  a              owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject .

obj:Trough  a            owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject .

obj:GreatCircle  a       owl:Class ;
        rdfs:subClassOf  obj:Circle .

obj:ConicSection  a  owl:Class .

obj:Hyperplane  a        owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject .

obj:GeometricalObject
        a                owl:Class ;
        rdfs:subClassOf  math:NumericalEntity .

obj:Path  a     owl:Class .

obj:Line  a              owl:Class ;
        rdfs:subClassOf  obj:Curve .

obj:Ellipse  a           owl:Class ;
        rdfs:subClassOf  obj:ConicSection .

obj:Depression  a        owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject .

obj:Parabolic  a         owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject_2D .

obj:Point  a             owl:Class ;
        rdfs:subClassOf  obj:GeometricalObject_0D .
