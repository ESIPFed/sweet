###  http://sweetontology.net/phenEnergy/ActiveSolarEnergy
:ActiveSolarEnergy rdf:type owl:Class ;
                   rdfs:subClassOf :SolarEnergy ,
                                   :ThermalCollection .


###  http://sweetontology.net/phenEnergy/BinaryCycle
:BinaryCycle rdf:type owl:Class ;
             rdfs:subClassOf :GeothermalPowerProcess ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaPhysical/hasEnergySource> ;
                               owl:allValuesFrom <http://sweetontology.net/matrEnergy/HeatExchanger>
                             ] .


###  http://sweetontology.net/phenEnergy/Biopower
:Biopower rdf:type owl:Class ;
          rdfs:subClassOf :RenewableEnergySource ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaPhysical/hasEnergyProduct> ;
                            owl:someValuesFrom :Electricity
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaPhysical/hasEnergySource> ;
                            owl:allValuesFrom <http://sweetontology.net/matrEnergy/Biomass>
                          ] .


###  http://sweetontology.net/phenEnergy/ConcentratingSolarPower
:ConcentratingSolarPower rdf:type owl:Class ;
                         rdfs:subClassOf :SolarEnergy .


###  http://sweetontology.net/phenEnergy/DirectUse
:DirectUse rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/procPhysical/PhysicalProcess> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaPhysical/hasEnergyProduct> ;
                             owl:allValuesFrom <http://sweetontology.net/propEnergy/Heat>
                           ] ;
           owl:disjointWith :EnergyConversionProcess .


###  http://sweetontology.net/phenEnergy/DrySteam
:DrySteam rdf:type owl:Class ;
          rdfs:subClassOf :GeothermalPowerProcess ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaPhysical/hasEnergySource> ;
                            owl:allValuesFrom <http://sweetontology.net/matrWater/Steam>
                          ] .


###  http://sweetontology.net/phenEnergy/Electricity
:Electricity rdf:type owl:Class ;
             owl:equivalentClass :PowerSource ;
             rdfs:subClassOf :EnergyForm .


###  http://sweetontology.net/phenEnergy/ElectricityProduction
:ElectricityProduction rdf:type owl:Class ;
                       rdfs:subClassOf :EnergyConversionProcess ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://sweetontology.net/relaPhysical/hasEnergyProduct> ;
                                         owl:allValuesFrom :Electricity
                                       ] .


###  http://sweetontology.net/phenEnergy/EnergyCollection
:EnergyCollection rdf:type owl:Class ;
                  rdfs:subClassOf :EnergyForm .


###  http://sweetontology.net/phenEnergy/EnergyConservation
:EnergyConservation rdf:type owl:Class ;
                    rdfs:subClassOf <http://sweetontology.net/humanEnvirConservation/Conservation> .


###  http://sweetontology.net/phenEnergy/EnergyConversionProcess
:EnergyConversionProcess rdf:type owl:Class ;
                         rdfs:subClassOf <http://sweetontology.net/procPhysical/PhysicalProcess> .


###  http://sweetontology.net/phenEnergy/EnergyForm
:EnergyForm rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/propEnergy/Energy> .


###  http://sweetontology.net/phenEnergy/EnergySource
:EnergySource rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/propEnergy/Energy> .


###  http://sweetontology.net/phenEnergy/FlashSteam
:FlashSteam rdf:type owl:Class ;
            rdfs:subClassOf :GeothermalPowerProcess ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaPhysical/hasEnergySource> ;
                              owl:allValuesFrom <http://sweetontology.net/matrWater/LiquidWater>
                            ] .


###  http://sweetontology.net/phenEnergy/Gasification
:Gasification rdf:type owl:Class ;
              rdfs:subClassOf :EnergyConversionProcess ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaPhysical/hasEnergyProduct> ;
                                owl:allValuesFrom <http://sweetontology.net/matrEnergy/Fuel>
                              ] .


###  http://sweetontology.net/phenEnergy/Geothermal
:Geothermal rdf:type owl:Class ;
            rdfs:subClassOf :RenewableEnergySource .


###  http://sweetontology.net/phenEnergy/GeothermalPowerProcess
:GeothermalPowerProcess rdf:type owl:Class ;
                        rdfs:subClassOf :EnergyConversionProcess ,
                                        :RenewableEnergySource .


###  http://sweetontology.net/phenEnergy/PassiveSolarEnergy
:PassiveSolarEnergy rdf:type owl:Class ;
                    rdfs:subClassOf :SolarEnergy .


###  http://sweetontology.net/phenEnergy/Photovoltaics
:Photovoltaics rdf:type owl:Class ;
               rdfs:subClassOf :SolarPower .


###  http://sweetontology.net/phenEnergy/PowerSource
:PowerSource rdf:type owl:Class .


###  http://sweetontology.net/phenEnergy/RenewableEnergySource
:RenewableEnergySource rdf:type owl:Class ;
                       rdfs:subClassOf :EnergySource ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://sweetontology.net/rela/hasRole> ;
                                         owl:allValuesFrom <http://sweetontology.net/humanEnvirConservation/Sustainability>
                                       ] .


###  http://sweetontology.net/phenEnergy/SolarEnergy
:SolarEnergy rdf:type owl:Class ;
