###  http://sweetontology.net/phen/AirSeaExchange
:AirSeaExchange rdf:type owl:Class ;
                owl:equivalentClass :AtmosphereOceanExchange ;
                rdfs:subClassOf :Phenomena ,
                                <http://sweetontology.net/phenSystem/Exchange> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://sweetontology.net/relaSci/hasSource> ;
                                  owl:someValuesFrom <http://sweetontology.net/realm/Atmosphere>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://sweetontology.net/relaSci/hasSource> ;
                                  owl:someValuesFrom <http://sweetontology.net/realm/Ocean>
                                ] .


###  http://sweetontology.net/phen/AtmosphereLandExchange
:AtmosphereLandExchange rdf:type owl:Class ;
                        rdfs:subClassOf :Phenomena ,
                                        <http://sweetontology.net/phenSystem/Exchange> ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://sweetontology.net/relaSci/hasSource> ;
                                          owl:someValuesFrom <http://sweetontology.net/realm/Atmosphere>
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://sweetontology.net/relaSci/hasSource> ;
                                          owl:someValuesFrom <http://sweetontology.net/realm/Land>
                                        ] .


