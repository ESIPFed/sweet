                        fstate:Turbulent
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( fstate:Newtonian
                        fstate:NonNewtonian
                      )
] .


[ rdf:type owl:AllDifferent ;
