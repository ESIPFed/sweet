                                       rdfs:label "SWEET Ontology" ;
                                       owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/stateFluid/FluidEquilibriumState
:FluidEquilibriumState rdf:type owl:Class ;
                       rdfs:subClassOf :FluidState ,
                                       <http://sweetontology.net/stateSystem/EquilibriumState> .


###  http://sweetontology.net/stateFluid/FluidState
:FluidState rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/statePhysical/PhysicalState> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/rela/hasState> ;
                              owl:hasValue <http://sweetontology.net/statePhysical/Fluid>
                            ] .


###  http://sweetontology.net/statePhysical/PhysicalState
<http://sweetontology.net/statePhysical/PhysicalState> rdf:type owl:Class .


###  http://sweetontology.net/stateSystem/EquilibriumState
<http://sweetontology.net/stateSystem/EquilibriumState> rdf:type owl:Class .


