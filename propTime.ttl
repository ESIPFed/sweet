@prefix : <http://sweetontology.net/propTime/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/propTime> .

<http://sweetontology.net/propTime> rdf:type owl:Ontology ;
                                     owl:imports <http://sweetontology.net/phenHydro> ,
                                                 <http://sweetontology.net/propFunction> ,
                                                 <http://sweetontology.net/propQuantity> ,
                                                 <http://sweetontology.net/realmClimateZone> ,
                                                 <http://sweetontology.net/rela> ,
                                                 <http://sweetontology.net/relaSci> ,
                                                 <http://sweetontology.net/relaTime> ,
                                                 <http://sweetontology.net/reprMath> ,
                                                 <http://sweetontology.net/reprSciUnits> ,
                                                 <http://sweetontology.net/reprTime> ,
                                                 <http://sweetontology.net/reprTimeSeason> ;
                                     rdfs:label "SWEET Ontology" ;
                                     owl:versionInfo "3.1.0" .

#################################################################
#    Object Properties
#################################################################

###  http://sweetontology.net/relaTime/hasEndTime
<http://sweetontology.net/relaTime/hasEndTime> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/relaTime/hasStartTime
<http://sweetontology.net/relaTime/hasStartTime> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/propTime/Age
:Age rdf:type owl:Class ;
     rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> .


###  http://sweetontology.net/propTime/AverageITCZDominanceDuration
:AverageITCZDominanceDuration rdf:type owl:Class ;
                              rdfs:subClassOf <http://sweetontology.net/propFunction/AnnualAverage> ,
                                              :ITCZDominanceDuration .


###  http://sweetontology.net/propTime/AverageSnowCoverDuration
:AverageSnowCoverDuration rdf:type owl:Class ;
                          rdfs:subClassOf <http://sweetontology.net/propFunction/AnnualAverage> ,
                                          :SnowCoverDuration .


###  http://sweetontology.net/propTime/AverageWinterDuration
:AverageWinterDuration rdf:type owl:Class ;
                       rdfs:subClassOf <http://sweetontology.net/propFunction/AnnualAverage> ,
                                       :WinterDuration .


###  http://sweetontology.net/propTime/CurrentTime
:CurrentTime rdf:type owl:Class ;
             rdfs:subClassOf <http://www.w3.org/2006/time#Instant> .


###  http://sweetontology.net/propTime/Cycle
:Cycle rdf:type owl:Class ;
       rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://sweetontology.net/relaTime/hasTime> ;
                         owl:allValuesFrom :Period
                       ] .


###  http://sweetontology.net/propTime/Delay
:Delay rdf:type owl:Class ;
       rdfs:subClassOf <http://www.w3.org/2006/time#Interval> .


###  http://sweetontology.net/propTime/DoublingTime
:DoublingTime rdf:type owl:Class ;
              rdfs:subClassOf :Duration .


###  http://sweetontology.net/propTime/Duration
:Duration rdf:type owl:Class ;
          owl:equivalentClass :DurationDescription ,
                              :TimeInterval ;
          rdfs:subClassOf :TemporalRange ,
                          <http://sweetontology.net/reprMath/Interval> .


###  http://sweetontology.net/propTime/DurationDescription
:DurationDescription rdf:type owl:Class .


###  http://sweetontology.net/propTime/EFoldingTime
:EFoldingTime rdf:type owl:Class ;
              rdfs:subClassOf :Duration .


###  http://sweetontology.net/propTime/EndTime
:EndTime rdf:type owl:Class ;
         rdfs:subClassOf <http://www.w3.org/2006/time#Instant> .


###  http://sweetontology.net/propTime/Episode
:Episode rdf:type owl:Class ;
         owl:equivalentClass :Episodic ;
         rdfs:subClassOf :Exposure .


###  http://sweetontology.net/propTime/Episodic
:Episodic rdf:type owl:Class .


###  http://sweetontology.net/propTime/Exposure
:Exposure rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/2006/time#Interval> .


###  http://sweetontology.net/propTime/GeologicTime
:GeologicTime rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/reprTime/TimeReference> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                                owl:hasValue <http://sweetontology.net/reprSciUnits/MYA>
                              ] ;
              rdfs:comment "The geologic time scale is a chronologic schema (or idealized model) relating stratigraphy to time that is used by geologists, paleontologists and other earth scientists to describe the timing and relationships between events that have occurred during the history of the Earth.  [Wikipedia]"@en .


###  http://sweetontology.net/propTime/Hydroperiod
:Hydroperiod rdf:type owl:Class ;
             rdfs:subClassOf :Period .


###  http://sweetontology.net/propTime/ITCZDominanceDuration
:ITCZDominanceDuration rdf:type owl:Class ;
                       rdfs:subClassOf :Duration ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                         owl:someValuesFrom <http://sweetontology.net/realmClimateZone/ITCZ>
                                       ] .


###  http://sweetontology.net/propTime/Latency
:Latency rdf:type owl:Class ;
         rdfs:subClassOf :Delay .


###  http://sweetontology.net/propTime/MonthRange
:MonthRange rdf:type owl:Class ;
            rdfs:subClassOf :Duration ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                              owl:hasValue <http://sweetontology.net/reprSciUnits/month>
                            ] .


###  http://sweetontology.net/propTime/Period
:Period rdf:type owl:Class ;
        rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> .


###  http://sweetontology.net/propTime/Repose
:Repose rdf:type owl:Class ;
        rdfs:subClassOf <http://www.w3.org/2006/time#Interval> ;
        rdfs:comment "The interval of time between volcanic eruptions."@en .


###  http://sweetontology.net/propTime/ResidenceTime
:ResidenceTime rdf:type owl:Class ;
               rdfs:subClassOf :Duration .


###  http://sweetontology.net/propTime/SnowCoverDuration
:SnowCoverDuration rdf:type owl:Class ;
                   rdfs:subClassOf :Duration ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                     owl:someValuesFrom <http://sweetontology.net/phenHydro/SnowCover>
                                   ] .


###  http://sweetontology.net/propTime/StartTime
:StartTime rdf:type owl:Class ;
           rdfs:subClassOf <http://www.w3.org/2006/time#Instant> .


###  http://sweetontology.net/propTime/TemporalExtent
:TemporalExtent rdf:type owl:Class ;
                rdfs:subClassOf <http://sweetontology.net/reprTime/TemporalEntity> .


###  http://sweetontology.net/propTime/TemporalRange
:TemporalRange rdf:type owl:Class ;
               owl:equivalentClass :TimeRange ;
               rdfs:subClassOf <http://sweetontology.net/propQuantity/IntervalQuantity> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaSci/rangeOf> ;
                                 owl:allValuesFrom :Time
                               ] .


###  http://sweetontology.net/propTime/Time
:Time rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/propQuantity/PhysicalQuantity> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                        owl:hasValue <http://sweetontology.net/reprSciUnits/second>
                      ] ;
      rdfs:comment "This is a reference to time as a dependent variable"@en .


###  http://sweetontology.net/propTime/TimeInterval
:TimeInterval rdf:type owl:Class .


###  http://sweetontology.net/propTime/TimePeriod
:TimePeriod rdf:type owl:Class ;
            rdfs:subClassOf :Duration ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaTime/hasEndTime> ;
                              owl:allValuesFrom <http://sweetontology.net/reprTime/Time>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaTime/hasStartTime> ;
                              owl:allValuesFrom :Time
                            ] .


###  http://sweetontology.net/propTime/TimeRange
:TimeRange rdf:type owl:Class .


###  http://sweetontology.net/propTime/TimeStep
:TimeStep rdf:type owl:Class ;
          rdfs:subClassOf :Duration .


###  http://sweetontology.net/propTime/Timestamp
:Timestamp rdf:type owl:Class ;
           rdfs:subClassOf <http://www.w3.org/2006/time#Instant> .


###  http://sweetontology.net/propTime/WinterDuration
:WinterDuration rdf:type owl:Class ;
                rdfs:subClassOf :Duration ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://sweetontology.net/relaTime/hasTime> ;
                                  owl:hasValue <http://sweetontology.net/reprTimeSeason/Winter>
                                ] .


###  http://sweetontology.net/realmClimateZone/ITCZ
<http://sweetontology.net/realmClimateZone/ITCZ> rdf:type owl:Class .


###  http://sweetontology.net/reprMath/Interval
<http://sweetontology.net/reprMath/Interval> rdf:type owl:Class .


###  http://sweetontology.net/reprTime/TemporalEntity
<http://sweetontology.net/reprTime/TemporalEntity> rdf:type owl:Class .


###  http://sweetontology.net/reprTime/TimeReference
<http://sweetontology.net/reprTime/TimeReference> rdf:type owl:Class .


###  http://www.w3.org/2006/time#Instant
<http://www.w3.org/2006/time#Instant> rdf:type owl:Class .


###  http://www.w3.org/2006/time#Interval
<http://www.w3.org/2006/time#Interval> rdf:type owl:Class ;
                                       rdfs:subClassOf :TimeRange .


###  http://www.w3.org/2006/time#ProperInterval
<http://www.w3.org/2006/time#ProperInterval> rdf:type owl:Class ;
                                             rdfs:subClassOf :TimeRange .


