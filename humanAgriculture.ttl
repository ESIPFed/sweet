@base          <http://sweetontology.net/humanAgriculture> .
@prefix agri:  <http://sweetontology.net/humanAgriculture/> .
@prefix comp:  <http://sweetontology.net/matrCompound/> .
@prefix comm:  <http://sweetontology.net/humanCommerce/> .
@prefix crela: <http://sweetontology.net/relaChemical/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix anim:  <http://sweetontology.net/matrAnimal/> .
@prefix screla: <http://sweetontology.net/relaSci/> .

agri:Overfishing  a      owl:Class ;
        rdfs:subClassOf  agri:Fishing .

agri:AnimalProduction
        a                owl:Class ;
        rdfs:subClassOf  agri:Agriculture .

agri:CropProduction  a   owl:Class ;
        rdfs:subClassOf  agri:PlantProduction .

agri:ShellfishProduction
        a                owl:Class ;
        rdfs:subClassOf  agri:Aquaculture , agri:AnimalProduction .

agri:Logging  a          owl:Class ;
        rdfs:subClassOf  comm:Production .

agri:Cultivation  a      owl:Class ;
        rdfs:subClassOf  agri:Agriculture .

agri:Horticulture  a     owl:Class ;
        rdfs:comment     "Horticulture is the art and science of the cultivation of plants"@en ;
        rdfs:subClassOf  agri:Agriculture .

agri:Agriculture  a      owl:Class ;
        rdfs:subClassOf  comm:LandUse , comm:Production .

agri:Breeding  a         owl:Class ;
        rdfs:subClassOf  agri:AnimalProduction .

agri:Beekeeping  a           owl:Class ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  anim:Bee ;
                               owl:onProperty     screla:hasSpecies
                             ] ;
        owl:equivalentClass  agri:Apiculture .

agri:PlantProduction  a  owl:Class ;
        rdfs:subClassOf  agri:Agriculture .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <relaChemical> , <relaSci> , <matrAnimal> , <humanCommerce> , <matrCompound> ;
        owl:versionInfo  "3.1.0" .

agri:Fishing  a          owl:Class ;
        rdfs:subClassOf  comm:Production .

agri:FishProduction  a   owl:Class ;
        rdfs:subClassOf  agri:Aquaculture , agri:AnimalProduction .

agri:Apiculture  a       owl:Class ;
        rdfs:subClassOf  agri:AnimalProduction .

agri:Sericulture  a      owl:Class ;
        rdfs:subClassOf  agri:AnimalProduction ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  anim:Silkworm ;
                           owl:onProperty     screla:hasSpecies
                         ] .

agri:SoilCultivation  a  owl:Class ;
        rdfs:subClassOf  agri:Agriculture .

agri:Harvesting  a       owl:Class ;
        rdfs:subClassOf  agri:PlantProduction .

agri:DripIrrigation  a   owl:Class ;
        rdfs:subClassOf  agri:Agriculture .

agri:Irrigation  a       owl:Class ;
        rdfs:subClassOf  agri:SoilCultivation ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    comp:Water ;
                           owl:onProperty  crela:hasSubstance
                         ] .

agri:Aquaculture  a      owl:Class ;
        rdfs:subClassOf  agri:Agriculture ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  anim:MarineAnimal ;
                           owl:onProperty     screla:hasSpecies
                         ] .
