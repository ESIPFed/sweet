@prefix : <http://sweetontology.net/repr/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/repr> .

<http://sweetontology.net/repr> rdf:type owl:Ontology ;
                                 owl:imports <http://sweetontology.net/relaMath> ,
                                             <http://sweetontology.net/reprMath> ;
                                 rdfs:label "SWEET Ontology" ;
                                 owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/repr/CardinalScale
:CardinalScale rdf:type owl:Class ;
               rdfs:subClassOf :QuantitativeScale .


###  http://sweetontology.net/repr/CategoricalScale
:CategoricalScale rdf:type owl:Class ;
                  rdfs:subClassOf :MeasurementScale ;
                  owl:disjointWith :OrdinalScale ,
                                   :QuantitativeScale .


###  http://sweetontology.net/repr/Component
:Component rdf:type owl:Class ;
           owl:equivalentClass :Coordinate .


###  http://sweetontology.net/repr/Coordinate
:Coordinate rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/reprMath/Scalar> .


###  http://sweetontology.net/repr/CoordinateSystem
:CoordinateSystem rdf:type owl:Class ;
                  rdfs:subClassOf :ReferenceFrame .


###  http://sweetontology.net/repr/Coordinates
:Coordinates rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/reprMath/Array> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaMath/eachElementHasType> ;
                               owl:allValuesFrom :Coordinate
                             ] .


###  http://sweetontology.net/repr/DependentVariable
:DependentVariable rdf:type owl:Class ;
                   rdfs:subClassOf <http://sweetontology.net/reprMath/Variable> ;
                   rdfs:comment "Any variable considered as a function of other variables, the latter being called independent."@en .


###  http://sweetontology.net/repr/Dimension
:Dimension rdf:type owl:Class ;
           rdfs:subClassOf :Representation ;
           rdfs:comment "In common usage, a dimension is a parameter or measurement used to describe some relevant characteristic of an object. The most commonly used dimensions are the parameters describing the size of an object: length, width, and height, but dimensions can also be other physical parameters such as the mass and electric charge of an object, or even, in a context where cost is relevant, an economic parameter such as its price." .


###  http://sweetontology.net/repr/IndependentVariable
:IndependentVariable rdf:type owl:Class ;
                     rdfs:subClassOf <http://sweetontology.net/reprMath/Variable> .


###  http://sweetontology.net/repr/IntervalScale
:IntervalScale rdf:type owl:Class ;
               rdfs:subClassOf :QuantitativeScale .


###  http://sweetontology.net/repr/Knowledge
:Knowledge rdf:type owl:Class ;
           rdfs:subClassOf :Representation .


###  http://sweetontology.net/repr/LogarithmicScale
:LogarithmicScale rdf:type owl:Class ;
                  rdfs:subClassOf :QuantitativeScale .


###  http://sweetontology.net/repr/Measurement
:Measurement rdf:type owl:Class ;
             rdfs:subClassOf :Representation .


###  http://sweetontology.net/repr/MeasurementScale
:MeasurementScale rdf:type owl:Class ;
                  owl:equivalentClass :Scale .


###  http://sweetontology.net/repr/OrdinalScale
:OrdinalScale rdf:type owl:Class ;
              rdfs:subClassOf :MeasurementScale ;
              owl:disjointWith :QuantitativeScale .


###  http://sweetontology.net/repr/Qualitative
:Qualitative rdf:type owl:Class ;
             rdfs:subClassOf :MeasurementScale .


###  http://sweetontology.net/repr/QuantitativeScale
:QuantitativeScale rdf:type owl:Class ;
                   rdfs:subClassOf :MeasurementScale .


###  http://sweetontology.net/repr/RatioScale
:RatioScale rdf:type owl:Class ;
            rdfs:subClassOf :MeasurementScale .


###  http://sweetontology.net/repr/ReferenceFrame
:ReferenceFrame rdf:type owl:Class ;
                rdfs:subClassOf <http://sweetontology.net/reprMath/NumericalEntity> .


###  http://sweetontology.net/repr/Representation
:Representation rdf:type owl:Class .


###  http://sweetontology.net/repr/Scale
:Scale rdf:type owl:Class ;
       rdfs:subClassOf :Representation .


###  http://sweetontology.net/repr/1D
:D rdf:type owl:Class ;
   rdfs:subClassOf :Representation ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://sweetontology.net/relaMath/hasDimension> ;
                     owl:hasValue 1
                   ] .


###  http://sweetontology.net/repr/2D
:D rdf:type owl:Class ;
   rdfs:subClassOf :Representation ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://sweetontology.net/relaMath/hasDimension> ;
                     owl:hasValue 2
                   ] .


###  http://sweetontology.net/repr/3D
:D rdf:type owl:Class ;
   rdfs:subClassOf :Representation ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://sweetontology.net/relaMath/hasDimension> ;
                     owl:hasValue 3
                   ] .


###  http://sweetontology.net/reprMath/Array
<http://sweetontology.net/reprMath/Array> rdf:type owl:Class .


###  http://sweetontology.net/reprMath/NumericalEntity
<http://sweetontology.net/reprMath/NumericalEntity> rdf:type owl:Class .


###  http://sweetontology.net/reprMath/Scalar
<http://sweetontology.net/reprMath/Scalar> rdf:type owl:Class .


###  http://sweetontology.net/reprMath/Variable
<http://sweetontology.net/reprMath/Variable> rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://sweetontology.net/repr/Barycentric
:Barycentric rdf:type owl:NamedIndividual ,
                      :ReferenceFrame .


###  http://sweetontology.net/repr/Eulerian
:Eulerian rdf:type owl:NamedIndividual ,
                   :ReferenceFrame .


###  http://sweetontology.net/repr/Geocentric
:Geocentric rdf:type owl:NamedIndividual ,
                     :ReferenceFrame .


###  http://sweetontology.net/repr/Geodetic
:Geodetic rdf:type owl:NamedIndividual ,
                   :ReferenceFrame .


###  http://sweetontology.net/repr/Heliocentric
:Heliocentric rdf:type owl:NamedIndividual ,
                       :ReferenceFrame .


###  http://sweetontology.net/repr/Lagrangian
:Lagrangian rdf:type owl:NamedIndividual ,
                     :ReferenceFrame .


###  http://sweetontology.net/repr/Spectral
:Spectral rdf:type owl:NamedIndividual ,
                   :ReferenceFrame .


###  http://sweetontology.net/repr/Synoptic
:Synoptic rdf:type owl:NamedIndividual ,
                   :ReferenceFrame .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Eulerian
                        :Lagrangian
                      )
] .


