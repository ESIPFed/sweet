@base          <http://sweetontology.net/realmLandOrographic> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix land:  <http://sweetontology.net/realmLandform/> .
@prefix realm: <http://sweetontology.net/realm/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ht:    <http://sweetontology.net/propSpaceHeight/> .
@prefix rela:  <http://sweetontology.net/rela/> .
@prefix space: <http://sweetontology.net/reprSpaceGeometry/> .
@prefix orog:  <http://sweetontology.net/realmLandOrographic/> .

orog:Orographic  a  owl:Class .

orog:MountainRange  a    owl:Class ;
        rdfs:subClassOf  space:Cluster ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  orog:Mountain ;
                           owl:onProperty     srela:clusterOf
                         ] .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <propSpaceHeight> , <relaSpace> , <rela> , <realm> , <reprSpaceGeometry> , <realmLandform> ;
        owl:versionInfo  "3.1.0" .

orog:Pass  a             owl:Class ;
        rdfs:subClassOf  orog:MountainRegion .

orog:TopographicalRegion
        a                owl:Class ;
        rdfs:subClassOf  land:SurfaceRegion .

orog:Alpine  a           owl:Class ;
        rdfs:comment     "Alpine region is one above the tree line. [Wikipedia]"@en ;
        rdfs:subClassOf  orog:MountainRegion .

orog:Cliff  a            owl:Class ;
        rdfs:subClassOf  orog:Hill .

orog:Foothill  a         owl:Class ;
        rdfs:subClassOf  orog:MountainRegion .

orog:Orography  a            owl:Class ;
        rdfs:subClassOf      land:Landform ;
        owl:equivalentClass  orog:Orographic , orog:Mountain .

orog:Hill  a             owl:Class ;
        rdfs:comment     "A hill is a landform that extends above the surrounding terrain, in a limited area. Hills often have a distinct summit, although in areas with scarp/dip topography a hill may refer to a particular section of scarp slope without a well-defined summit (e.g. Box Hill). A hillock is a small hill. [Wikipedia]"@en ;
        rdfs:subClassOf  land:Landform .

orog:Plateau  a          owl:Class ;
        rdfs:subClassOf  orog:MountainRegion .

orog:Mesa  a             owl:Class ;
        rdfs:subClassOf  orog:MountainRegion .

orog:Protrusion  a       owl:Class ;
        rdfs:subClassOf  orog:Ridge .

orog:Ubac  a             owl:Class ;
        rdfs:comment     "The shady (usually poleward) side of a mountain." ;
        rdfs:subClassOf  orog:MountainRegion .

orog:Summit  a           owl:Class ;
        rdfs:subClassOf  orog:MountainRegion .

orog:Mountain  a         owl:Class ;
        rdfs:subClassOf  land:Landform .

orog:Ridge  a            owl:Class ;
        rdfs:subClassOf  orog:MountainRegion .

orog:NonOrographic  a     owl:Class ;
        rdfs:subClassOf   land:Landform ;
        owl:disjointWith  orog:Orographic .

orog:Talus  a            owl:Class ;
        rdfs:comment     "A slope formed at the base of a steeper slope, made of fallen and disintegrated materials."@en ;
        rdfs:subClassOf  orog:MountainRegion .

orog:MountainRegion  a   owl:Class ;
        rdfs:subClassOf  land:LandRegion ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  orog:Mountain ;
                           owl:onProperty     rela:hasRealm
                         ] .

orog:Bluff  a            owl:Class ;
        rdfs:subClassOf  orog:Hill .

orog:Butte  a            owl:Class ;
        rdfs:subClassOf  orog:Hill .

orog:Hummock  a          owl:Class ;
        rdfs:subClassOf  land:Landform .
