#################################################################

###  http://sweetontology.net/rela/hasRealm
<http://sweetontology.net/rela/hasRealm> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/relaSpace/clusterOf
<http://sweetontology.net/relaSpace/clusterOf> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/realmLandOrographic/Alpine
:Alpine rdf:type owl:Class ;
        rdfs:subClassOf :MountainRegion ;
        rdfs:comment "Alpine region is one above the tree line. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandOrographic/Bluff
:Bluff rdf:type owl:Class ;
       rdfs:subClassOf :Hill .


###  http://sweetontology.net/realmLandOrographic/Butte
:Butte rdf:type owl:Class ;
       rdfs:subClassOf :Hill .


###  http://sweetontology.net/realmLandOrographic/Cliff
:Cliff rdf:type owl:Class ;
       rdfs:subClassOf :Hill .


###  http://sweetontology.net/realmLandOrographic/Foothill
:Foothill rdf:type owl:Class ;
          rdfs:subClassOf :MountainRegion .


###  http://sweetontology.net/realmLandOrographic/Hill
:Hill rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/realmLandform/Landform> ;
      rdfs:comment "A hill is a landform that extends above the surrounding terrain, in a limited area. Hills often have a distinct summit, although in areas with scarp/dip topography a hill may refer to a particular section of scarp slope without a well-defined summit (e.g. Box Hill). A hillock is a small hill. [Wikipedia]"@en .


###  http://sweetontology.net/realmLandOrographic/Hummock
:Hummock rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/realmLandform/Landform> .


###  http://sweetontology.net/realmLandOrographic/Mesa
:Mesa rdf:type owl:Class ;
      rdfs:subClassOf :MountainRegion .


###  http://sweetontology.net/realmLandOrographic/Mountain
:Mountain rdf:type owl:Class ;
          owl:equivalentClass :Orography ;
          rdfs:subClassOf <http://sweetontology.net/realmLandform/Landform> .


###  http://sweetontology.net/realmLandOrographic/MountainRange
:MountainRange rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/reprSpaceGeometry/Cluster> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaSpace/clusterOf> ;
                                 owl:allValuesFrom :Mountain
                               ] .


###  http://sweetontology.net/realmLandOrographic/MountainRegion
:MountainRegion rdf:type owl:Class ;
                rdfs:subClassOf <http://sweetontology.net/realmLandform/LandRegion> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                                  owl:allValuesFrom :Mountain
                                ] .


###  http://sweetontology.net/realmLandOrographic/NonOrographic
:NonOrographic rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/realmLandform/Landform> ;
               owl:disjointWith :Orographic .


###  http://sweetontology.net/realmLandOrographic/Orographic
:Orographic rdf:type owl:Class ;
            owl:equivalentClass :Orography .


###  http://sweetontology.net/realmLandOrographic/Orography
:Orography rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/realmLandform/Landform> .


###  http://sweetontology.net/realmLandOrographic/Pass
:Pass rdf:type owl:Class ;
      rdfs:subClassOf :MountainRegion .


###  http://sweetontology.net/realmLandOrographic/Plateau
:Plateau rdf:type owl:Class ;
         rdfs:subClassOf :MountainRegion .


###  http://sweetontology.net/realmLandOrographic/Protrusion
:Protrusion rdf:type owl:Class ;
            rdfs:subClassOf :Ridge .


###  http://sweetontology.net/realmLandOrographic/Ridge
:Ridge rdf:type owl:Class ;
       rdfs:subClassOf :MountainRegion .


###  http://sweetontology.net/realmLandOrographic/Summit
:Summit rdf:type owl:Class ;
        rdfs:subClassOf :MountainRegion .


###  http://sweetontology.net/realmLandOrographic/Talus
:Talus rdf:type owl:Class ;
       rdfs:subClassOf :MountainRegion ;
       rdfs:comment "A slope formed at the base of a steeper slope, made of fallen and disintegrated materials."@en .


###  http://sweetontology.net/realmLandOrographic/TopographicalRegion
:TopographicalRegion rdf:type owl:Class ;
                     rdfs:subClassOf <http://sweetontology.net/realmLandform/SurfaceRegion> .


###  http://sweetontology.net/realmLandOrographic/Ubac
:Ubac rdf:type owl:Class ;
      rdfs:subClassOf :MountainRegion ;
      rdfs:comment "The shady (usually poleward) side of a mountain." .


###  http://sweetontology.net/reprSpaceGeometry/Cluster
<http://sweetontology.net/reprSpaceGeometry/Cluster> rdf:type owl:Class .


