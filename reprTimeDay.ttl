@prefix trela: <http://sweetontology.net/relaTime/> .
@base <http://sweetontology.net/reprTimeDay> .

<http://sweetontology.net/reprTimeDay> rdf:type owl:Ontology ;
                                        owl:imports <http://sweetontology.net/relaTime> ;
                                        rdfs:label "SWEET Ontology" ;
                                        owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/reprTimeDay/Dawn
day:Dawn rdf:type owl:Class ;
         rdfs:subClassOf day:Twilight .


###  http://sweetontology.net/reprTimeDay/Day
day:Day rdf:type owl:Class ;
        rdfs:subClassOf owlt:ProperInterval .


###  http://sweetontology.net/reprTimeDay/Daytime
day:Daytime rdf:type owl:Class ;
            rdfs:subClassOf owlt:ProperInterval .


###  http://sweetontology.net/reprTimeDay/Dusk
day:Dusk rdf:type owl:Class ;
         rdfs:subClassOf day:Twilight .


###  http://sweetontology.net/reprTimeDay/Evening
day:Evening rdf:type owl:Class ;
            rdfs:subClassOf day:Nighttime .


###  http://sweetontology.net/reprTimeDay/Midnight
day:Midnight rdf:type owl:Class ;
             rdfs:subClassOf owlt:Instant .


###  http://sweetontology.net/reprTimeDay/Morning
day:Morning rdf:type owl:Class ;
            rdfs:subClassOf day:Daytime .


###  http://sweetontology.net/reprTimeDay/Nighttime
day:Nighttime rdf:type owl:Class ;
              owl:equivalentClass day:Nocturnal ;
              rdfs:subClassOf owlt:ProperInterval .


###  http://sweetontology.net/reprTimeDay/Nocturnal
day:Nocturnal rdf:type owl:Class .


###  http://sweetontology.net/reprTimeDay/Noon
day:Noon rdf:type owl:Class ;
         rdfs:subClassOf owlt:Instant .


###  http://sweetontology.net/reprTimeDay/Sunrise
day:Sunrise rdf:type owl:Class ;
            rdfs:subClassOf owlt:Instant ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty trela:startOf ;
                              owl:someValuesFrom day:Morning
                            ] .


###  http://sweetontology.net/reprTimeDay/Sunset
day:Sunset rdf:type owl:Class ;
           rdfs:subClassOf owlt:Instant ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty trela:startOf ;
                             owl:someValuesFrom day:Evening
                           ] .


###  http://sweetontology.net/reprTimeDay/Twilight
day:Twilight rdf:type owl:Class ;
             rdfs:subClassOf owlt:ProperInterval .


