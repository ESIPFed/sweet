@prefix : <http://sweetontology.net/reprTimeDay/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/reprTimeDay> .

<http://sweetontology.net/reprTimeDay> rdf:type owl:Ontology ;
                                        owl:imports <http://sweetontology.net/relaTime> ;
                                        rdfs:label "SWEET Ontology" ;
                                        owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/reprTimeDay/Dawn
:Dawn rdf:type owl:Class ;
      rdfs:subClassOf :Twilight .


###  http://sweetontology.net/reprTimeDay/Day
:Day rdf:type owl:Class ;
     rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> .


###  http://sweetontology.net/reprTimeDay/Daytime
:Daytime rdf:type owl:Class ;
         rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> .


###  http://sweetontology.net/reprTimeDay/Dusk
:Dusk rdf:type owl:Class ;
      rdfs:subClassOf :Twilight .


###  http://sweetontology.net/reprTimeDay/Evening
:Evening rdf:type owl:Class ;
         rdfs:subClassOf :Nighttime .


###  http://sweetontology.net/reprTimeDay/Midnight
:Midnight rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/2006/time#Instant> .


###  http://sweetontology.net/reprTimeDay/Morning
:Morning rdf:type owl:Class ;
         rdfs:subClassOf :Daytime .


###  http://sweetontology.net/reprTimeDay/Nighttime
:Nighttime rdf:type owl:Class ;
           owl:equivalentClass :Nocturnal ;
           rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> .


###  http://sweetontology.net/reprTimeDay/Nocturnal
:Nocturnal rdf:type owl:Class .


###  http://sweetontology.net/reprTimeDay/Noon
:Noon rdf:type owl:Class ;
      rdfs:subClassOf <http://www.w3.org/2006/time#Instant> .


###  http://sweetontology.net/reprTimeDay/Sunrise
:Sunrise rdf:type owl:Class ;
         rdfs:subClassOf <http://www.w3.org/2006/time#Instant> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/relaTime/startOf> ;
                           owl:someValuesFrom :Morning
                         ] .


###  http://sweetontology.net/reprTimeDay/Sunset
:Sunset rdf:type owl:Class ;
        rdfs:subClassOf <http://www.w3.org/2006/time#Instant> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/relaTime/startOf> ;
                          owl:someValuesFrom :Evening
                        ] .


###  http://sweetontology.net/reprTimeDay/Twilight
:Twilight rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> .


