@base          <http://sweetontology.net/propFunction> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix stat:  <http://sweetontology.net/propFunction/> .
@prefix mod:   <http://sweetontology.net/reprSciModel/> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix diff:  <http://sweetontology.net/propDifference/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix prop:  <http://sweetontology.net/prop/> .
@prefix time:  <http://sweetontology.net/reprTime/> .
@prefix math:  <http://sweetontology.net/reprMath/> .
@prefix screla: <http://sweetontology.net/relaSci/> .

stat:Disturbance  a      owl:Class ;
        rdfs:subClassOf  prop:Error .

stat:Minimum  a          owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary .

stat:StandardDeviation
        a                owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary , stat:Spread .

stat:Moment  a           owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary .

stat:Variance  a         owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary , stat:Spread .

stat:Kurtosis  a         owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary .

stat:Mode  a             owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary .

stat:Skewness  a         owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary .

stat:Average  a  owl:Class .

stat:ErrorBar  a         owl:Class ;
        rdfs:subClassOf  quan:IntervalQuantity ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  prop:Error ;
                           owl:onProperty     screla:rangeOf
                         ] .

stat:Percentile  a       owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary .

stat:Threshold  a        owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary .

stat:CriticalPoint  a        owl:Class ;
        rdfs:subClassOf      stat:Threshold ;
        owl:equivalentClass  stat:CriticalLevel .

stat:StandardError  a    owl:Class ;
        rdfs:subClassOf  mod:Estimate ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  stat:StandardDeviation ;
                           owl:onProperty     mrela:estimateOf
                         ] .

stat:TemporalMean  a  owl:Class .

stat:Mean  a                 owl:Class ;
        rdfs:subClassOf      stat:Moment ;
        owl:equivalentClass  stat:Average .

stat:TimeAverage  a  owl:Class .

stat:Maximum  a          owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary .

stat:Median  a           owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary .

stat:Decile  a           owl:Class ;
        rdfs:subClassOf  stat:Percentile .

stat:Quartile  a         owl:Class ;
        rdfs:subClassOf  stat:Percentile .

stat:Peak  a             owl:Class ;
        rdfs:subClassOf  stat:Maximum .

stat:MidRange  a         owl:Class ;
        rdfs:subClassOf  prop:StatisticalSummary .

stat:CriticalLevel  a  owl:Class .

stat:TimeMean  a             owl:Class ;
        rdfs:subClassOf      stat:Average ;
        rdfs:subClassOf      [ a                   owl:Restriction ;
                               owl:onProperty      mrela:averageOver ;
                               owl:someValuesFrom  time:Time
                             ] ;
        owl:equivalentClass  stat:TimeAverage , stat:TemporalMean , stat:TemporalAverage .

stat:AnnualAverage  a    owl:Class ;
        rdfs:subClassOf  stat:TimeMean ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:year ;
                           owl:onProperty  mrela:averageOver
                         ] .

stat:TemporalAverage  a  owl:Class .

stat:FlippingPoint  a  owl:Class .

stat:Quintile  a         owl:Class ;
        rdfs:subClassOf  stat:Percentile .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <reprTime> , <relaSci> , <reprMath> , <prop> , <propQuantity> , <propDifference> , <reprSciModel> , <reprSciUnits> , <relaMath> ;
        owl:versionInfo  "3.1.0" .

stat:Spread  a           owl:Class ;
        rdfs:subClassOf  diff:Deviation .

stat:TippingPoint  a         owl:Class ;
        rdfs:subClassOf      stat:CriticalPoint ;
        owl:equivalentClass  stat:FlippingPoint .

stat:MonthlyAverage  a   owl:Class ;
        rdfs:subClassOf  stat:TimeMean ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:month ;
                           owl:onProperty  mrela:averageOver
                         ] .
