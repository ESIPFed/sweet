<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY comm "http://sweet.jpl.nasa.gov/2.0/humanCommerce.owl">
  <!ENTITY human "http://sweet.jpl.nasa.gov/2.0/human.owl">
  <!ENTITY cat "http://sweet.jpl.nasa.gov/2.0/sciCategorical.owl">
  <!ENTITY top "http://sweet.jpl.nasa.gov/2.0/top.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&comm;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:cat="&cat;"
         xmlns:top="&top;"
         xmlns:comm="&comm;"
         xmlns:human="&human;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.0">
     <owl:imports rdf:resource="&human;"/>
     <owl:imports rdf:resource="&cat;"/>
     <owl:imports rdf:resource="&top;"/>
  </owl:Ontology>

<!-- Classes -->
  <owl:Class rdf:about="#Assembly">
    <rdfs:subClassOf rdf:resource="#Production"/>
  </owl:Class>

  <owl:Class rdf:about="#Construction">
    <rdfs:subClassOf rdf:resource="#Production"/>
  </owl:Class>

  <owl:Class rdf:about="#Development">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#Grading">
    <rdfs:subClassOf rdf:resource="#Processing"/>
  </owl:Class>

  <owl:Class rdf:about="#Industrialization">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#LandUse">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#Logging">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#Manafacturing">
    <rdfs:subClassOf rdf:resource="#Production"/>
  </owl:Class>

  <owl:Class rdf:about="#Management">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#ManagementSystem">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Management"/>
        <owl:onProperty rdf:resource="&human;#hasActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Processing">
    <rdfs:subClassOf rdf:resource="#Production"/>
  </owl:Class>

  <owl:Class rdf:about="#Product">
    <rdfs:subClassOf rdf:resource="#MaterialThing"/>
  </owl:Class>

  <owl:Class rdf:about="#Material">
    <rdfs:subClassOf rdf:resource="&top;#Matter"/>
    <owl:equivalentClass rdf:resource="#MaterialThing"/>
  </owl:Class>
  <owl:Class rdf:about="#MaterialThing"/>

  <owl:Class rdf:about="#Production">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#Packaging">
    <rdfs:subClassOf rdf:resource="#Production"/>
  </owl:Class>

  <owl:Class rdf:about="#EngineeringActivity">
    <rdfs:subClassOf rdf:resource="#Production"/>
  </owl:Class>

  <owl:Class rdf:about="#ForestProduct">
    <rdfs:subClassOf rdf:resource="#Product"/>
  </owl:Class>

  <owl:Class rdf:about="#IndustrialProduct">
    <rdfs:subClassOf rdf:resource="#Product"/>
  </owl:Class>

  <owl:Class rdf:about="#Productivity">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
  </owl:Class>

  <owl:Class rdf:about="#StandarIndustrialClassification">
    <rdfs:subClassOf rdf:resource="&cat;#Classification"/>
  </owl:Class>

  <owl:Class rdf:about="#Supply">
    <rdfs:subClassOf rdf:resource="#Product"/>
  </owl:Class>

  <owl:Class rdf:about="#Yield">
    <rdfs:subClassOf rdf:resource="#Product"/>
  </owl:Class>

<!-- Human activities -->
  <owl:Class rdf:about="#CapacityBuliding">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
  </owl:Class>

<!-- Equipment-->
  <owl:Class rdf:about="#ComputerHardware">
    <rdfs:subClassOf rdf:resource="#Equipment"/>
  </owl:Class>

  <owl:Class rdf:about="#Equipment">
    <rdfs:subClassOf rdf:resource="#Infrastructure"/>
  </owl:Class>

  <owl:Class rdf:about="#Infrastructure">
    <rdfs:subClassOf rdf:resource="#MaterialThing"/>
  </owl:Class>

  <owl:Class rdf:about="#InSitu">
    <rdfs:subClassOf rdf:resource="&human;#HumanActivity"/>
    <owl:equivalentClass rdf:resource="#OnSite"/>
  </owl:Class>
  <owl:Class rdf:about="#OnSite"/>

  <owl:Class rdf:about="#Sink">
    <rdfs:subClassOf rdf:resource="#Equipment"/>
  </owl:Class>

  <owl:ObjectProperty rdf:about="#produces">
    <rdfs:range rdf:resource="#Product"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasInfrastructure">
    <rdfs:range rdf:resource="#Infrastructure"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#readinessOf">
    <rdfs:range rdf:resource="#Infrastructure"/>
  </owl:ObjectProperty>

</rdf:RDF>
