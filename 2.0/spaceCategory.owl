<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY ordi "http://sweet.jpl.nasa.gov/2.0/sciOrdinal.owl">
  <!ENTITY res "http://sweet.jpl.nasa.gov/2.0/sciResearch.owl">
  <!ENTITY rela "http://sweet.jpl.nasa.gov/2.0/mathRelation.owl">
  <!ENTITY space "http://sweet.jpl.nasa.gov/2.0/space.owl">  
  <!ENTITY cate "http://sweet.jpl.nasa.gov/2.0/spaceCategory.owl">  
  <!ENTITY xten "http://sweet.jpl.nasa.gov/2.0/spaceExtent.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&cate;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:rela="&rela;"
         xmlns:xten="&xten;"
         xmlns:cate="&cate;"
         xmlns:res="&res;"
         xmlns:ordi="&ordi;"
         xmlns:space="&space;"
>

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.0">
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&xten;"/>
     <owl:imports rdf:resource="&res;"/>
     <owl:imports rdf:resource="&ordi;"/>
     <owl:imports rdf:resource="&space;"/>
  </owl:Ontology>
  
  <owl:Class rdf:about="#Big">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Little"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Coarse">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Fine"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Deep">
    <rdfs:subClassOf rdf:resource="#SpatialCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Shallow"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:about="#DistanceCategory">
    <rdfs:subClassOf rdf:resource="&ordi;#OrderCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&xten;#Distance"/>
        <owl:onProperty rdf:resource="&res;#measureOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Far">
    <rdfs:subClassOf rdf:resource="#DistanceCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Near"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Fine">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#High">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Low"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Isolated">
    <rdfs:subClassOf rdf:resource="#DistanceCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Nearby"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Large">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Small"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Little">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#Low">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#Lower">
    <rdfs:subClassOf rdf:resource="#SpatialCategory"/>
  </owl:Class>
  
  <owl:Class rdf:about="#Macro">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Micro"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Micro">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#Near">
    <rdfs:subClassOf rdf:resource="#DistanceCategory"/>
    <owl:equivalentClass rdf:resource="#Nearby"/>
    <owl:equivalentClass rdf:resource="#Close"/>
  </owl:Class>
  <owl:Class rdf:about="#Nearby"/>
  <owl:Class rdf:about="#Close"/>

  <owl:Class rdf:about="#Remote">
    <rdfs:subClassOf rdf:resource="#DistanceCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Near"/>
        <owl:onProperty rdf:resource="&rela;#greaterThan"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Shallow">
    <rdfs:subClassOf rdf:resource="#SpatialCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#Size">
    <rdfs:subClassOf rdf:resource="&space;#SpatialProperty"/>
  </owl:Class>

  <owl:Class rdf:about="#SizeCategory">
    <rdfs:subClassOf rdf:resource="&ordi;#OrderCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Size"/>
        <owl:onProperty rdf:resource="&res;#measureOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Small">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
  </owl:Class>

  <owl:Class rdf:about="#SpatialCategory">
    <rdfs:subClassOf rdf:resource="&ordi;#OrderCategory"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&space;#Space"/>
        <owl:onProperty rdf:resource="&res;#measureOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Tiny">
    <rdfs:subClassOf rdf:resource="#SizeCategory"/>
  </owl:Class>
  
  <owl:Class rdf:about="#Upper">
    <rdfs:subClassOf rdf:resource="#SpatialCategory"/>
  </owl:Class>

</rdf:RDF>
