@base          <http://sweetontology.net/propSpaceMultidimensional> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix dist:  <http://sweetontology.net/propSpace/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix screla: <http://sweetontology.net/relaSci/> .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <propQuantity> , <propSpace> , <repr> , <reprSciUnits> , <relaSci> , <relaMath> ;
        owl:versionInfo  "3.1.0" .

<propSpaceMultidimensional/Area>
        a                owl:Class ;
        rdfs:subClassOf  dist:SpatialMeasure ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:meterSquared ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    2 ;
                           owl:onProperty  mrela:hasDimension
                         ] .

<propSpaceMultidimensional/FieldOfView>
        a                owl:Class ;
        rdfs:subClassOf  <propSpaceMultidimensional/AngularExtent> .

<propSpaceMultidimensional/Volume>
        a                owl:Class ;
        rdfs:subClassOf  dist:SpatialMeasure ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:meterCubed ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    3 ;
                           owl:onProperty  mrela:hasDimension
                         ] .

<propSpaceMultidimensional/Resolution>
        a                owl:Class ;
        rdfs:subClassOf  <propSpaceMultidimensional/AngularExtent> .

<propSpaceMultidimensional/AngularExtent>
        a                owl:Class ;
        rdfs:subClassOf  dist:SpatialMeasure ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:radian ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

<propSpaceMultidimensional/Footprint>
        a                owl:Class ;
        rdfs:subClassOf  <propSpaceMultidimensional/Area> .

<propSpaceMultidimensional/Capacity>
        a                owl:Class ;
        rdfs:subClassOf  <propSpaceMultidimensional/Volume> .

<propSpaceMultidimensional/CrossSection>
        a                    owl:Class ;
        rdfs:subClassOf      <propSpaceMultidimensional/Area> ;
        owl:equivalentClass  <propSpaceMultidimensional/CrossSectionalArea> .

<propSpaceMultidimensional/CarbonFootprint>
        a                owl:Class ;
        rdfs:subClassOf  <propSpaceMultidimensional/Footprint> .

<propSpaceMultidimensional/CrossSectionalArea>
        a       owl:Class .

<propSpaceMultidimensional/RadarCrossSection>
        a                owl:Class ;
        rdfs:comment     "Radar cross section (RCS) describes the extent to which an object reflects an incident electromagnetic wave. It is a measure of the strength of the radar signal backscattered from a target object for a given incident wave power.."@en ;
        rdfs:subClassOf  quan:RadiationMediumInteractionQuantity , <propSpaceMultidimensional/CrossSection> .
