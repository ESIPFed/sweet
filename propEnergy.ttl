###  http://sweetontology.net/propEnergy/AvailablePotentialEnergy
:AvailablePotentialEnergy rdf:type owl:Class ;
                          rdfs:subClassOf :PotentialEnergy ;
                          rdfs:comment "That portion of the total potential energy that may be converted to kinetic energy in an adiabatically enclosed system."@en .


###  http://sweetontology.net/propEnergy/BindingEnergy
:BindingEnergy rdf:type owl:Class ;
               rdfs:subClassOf :ChemicalEnergy ,
                               :PotentialEnergy .


###  http://sweetontology.net/propEnergy/ChemicalEnergy
:ChemicalEnergy rdf:type owl:Class ;
                rdfs:subClassOf <http://sweetontology.net/prop/ChemicalProperty> ,
                                :Energy .


###  http://sweetontology.net/propEnergy/ConvectiveInhibition
:ConvectiveInhibition rdf:type owl:Class ;
                      rdfs:subClassOf :Energy ;
                      rdfs:comment "The energy needed to lift an air parcel vertically and pseudoadiabatically from its originating level to its level of free convection (LFC)."@en .


###  http://sweetontology.net/propEnergy/Energy
:Energy rdf:type owl:Class ;
        rdfs:subClassOf <http://sweetontology.net/propQuantity/ExtensiveProperty> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                          owl:hasValue <http://sweetontology.net/reprSciUnits/joule>
                        ] .


###  http://sweetontology.net/propEnergy/EnergyBudget
:EnergyBudget rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/propQuantity/Budget> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/rela/hasProperty> ;
                                owl:allValuesFrom :Energy
                              ] .


###  http://sweetontology.net/propEnergy/EnergyDensity
:EnergyDensity rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/propQuantity/IntensiveProperty> ,
                               <http://sweetontology.net/reprMathOperation/Ratio> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaMath/hasFirstOperand> ;
                                 owl:allValuesFrom :Energy
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaMath/hasSecondOperand> ;
                                 owl:allValuesFrom <http://sweetontology.net/propSpaceMultidimensional/Area>
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                                 owl:hasValue <http://sweetontology.net/reprSciUnits/joulePerMeterSquared>
                               ] ;
               rdfs:comment "Energy per unit area."@en .


###  http://sweetontology.net/propEnergy/Enthalpy
:Enthalpy rdf:type owl:Class ;
          rdfs:subClassOf :ChemicalEnergy .


###  http://sweetontology.net/propEnergy/Entropy
:Entropy rdf:type owl:Class ;
         rdfs:subClassOf :Energy .


###  http://sweetontology.net/propEnergy/EvaporativeAvailablePotentialEnergy
:EvaporativeAvailablePotentialEnergy rdf:type owl:Class ;
                                     rdfs:subClassOf :AvailablePotentialEnergy ,
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty <http://sweetontology.net/relaPhysical/hasStateChange> ;
                                                       owl:allValuesFrom <http://sweetontology.net/procStateChange/Evaporation>
                                                     ] ;
                                     rdfs:comment "Analogous to convective available potential energy, except that it is related to the negative buoyancy associated with evaporative cooling of liquid water within a sinking cloudy air parcel."@en .


###  http://sweetontology.net/propEnergy/Geopotential
:Geopotential rdf:type owl:Class ;
              rdfs:subClassOf :GravitationalPotential .


###  http://sweetontology.net/propEnergy/GibbsFreeEnergy
:GibbsFreeEnergy rdf:type owl:Class ;
                 rdfs:subClassOf :ChemicalEnergy .


###  http://sweetontology.net/propEnergy/GravitationalPotential
:GravitationalPotential rdf:type owl:Class ;
                        rdfs:subClassOf <http://sweetontology.net/propQuantity/ScalarPotential> ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://sweetontology.net/rela/hasProcess> ;
                                          owl:allValuesFrom <http://sweetontology.net/procPhysical/Gravity>
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                                          owl:hasValue <http://sweetontology.net/reprSciUnits/meterSquaredPerSecondSquared>
                                        ] .


###  http://sweetontology.net/propEnergy/Hamiltonian
:Hamiltonian rdf:type owl:Class ;
             rdfs:subClassOf :Energy .


###  http://sweetontology.net/propEnergy/Heat
:Heat rdf:type owl:Class ;
      owl:equivalentClass :HeatEnergy ,
                          :ThermalEnergy ;
      rdfs:subClassOf :Energy ,
                      <http://sweetontology.net/propQuantity/ThermodynamicQuantity> .


###  http://sweetontology.net/propEnergy/HeatBudget
:HeatBudget rdf:type owl:Class ;
            rdfs:subClassOf :EnergyBudget ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/rela/hasProperty> ;
                              owl:allValuesFrom :Heat
                            ] .


###  http://sweetontology.net/propEnergy/HeatEnergy
:HeatEnergy rdf:type owl:Class .


###  http://sweetontology.net/propEnergy/InternalEnergy
:InternalEnergy rdf:type owl:Class ;
