@base          <http://sweetontology.net/propConductivity> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix mass:  <http://sweetontology.net/propMass/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix cond:  <http://sweetontology.net/propConductivity/> .
@prefix screla: <http://sweetontology.net/relaSci/> .

cond:Impedance  a        owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:ohm ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

cond:ThermalConductivity
        a                owl:Class ;
        rdfs:subClassOf  cond:Conductivity , quan:ThermodynamicQuantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:wattPerMeterPerKelvin ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

cond:Resistivity  a      owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:ohmMeter ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

cond:PedersenConductivity
        a                owl:Class ;
        rdfs:subClassOf  cond:ElectricalConductivity .

cond:Resistance  a       owl:Class ;
        rdfs:subClassOf  cond:Impedance .

cond:ElectricalConductivity
        a                owl:Class ;
        rdfs:subClassOf  cond:Conductivity , quan:ElectromagneticQuantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:siemensPerMeter ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

cond:SpecificConductance
        a                owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:siemensPerKilogram ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

cond:HydraulicConductivity
        a                owl:Class ;
        rdfs:subClassOf  mass:ConsistenceProperty , cond:Conductivity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:meterPerSecond ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

cond:HallConductivity
        a                owl:Class ;
        rdfs:subClassOf  cond:ElectricalConductivity .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <propMass> , <propQuantity> , <relaSci> , <reprSciUnits> ;
        owl:versionInfo  "3.1.0" .

cond:Conductivity  a     owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity .

cond:Conductance  a      owl:Class ;
        rdfs:subClassOf  quan:ElectromagneticQuantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:siemens ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .
