@base          <http://sweetontology.net/propChemical> .
@prefix elem:  <http://sweetontology.net/matrElement/> .
@prefix chemp: <http://sweetontology.net/propChemical/> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix comp:  <http://sweetontology.net/matrCompound/> .
@prefix chem:  <http://sweetontology.net/matr/> .
@prefix crela: <http://sweetontology.net/relaChemical/> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix indx:  <http://sweetontology.net/propIndex/> .
@prefix pstate: <http://sweetontology.net/statePhysical/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix ion:   <http://sweetontology.net/matrIon/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix conc:  <http://sweetontology.net/propMass/> .
@prefix cstate: <http://sweetontology.net/stateChemical/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix prop:  <http://sweetontology.net/prop/> .
@prefix rela:  <http://sweetontology.net/rela/> .
@prefix screla: <http://sweetontology.net/relaSci/> .

chemp:Acidity  a         owl:Class ;
        rdfs:subClassOf  prop:ChemicalProperty .

chemp:Basicity  a  owl:Class .

chemp:SAR  a    owl:Class .

chemp:DissolvedLoad  a   owl:Class ;
        rdfs:subClassOf  chem:DissolvedSolid , chemp:Load .

chemp:Alkalinity  a          owl:Class ;
        rdfs:subClassOf      prop:ChemicalProperty ;
        owl:equivalentClass  chemp:Alkaline , chemp:Alkalic , chemp:Basic , chemp:Basicity .

chemp:CarbonateAlkalinity
        a                owl:Class ;
        rdfs:subClassOf  chemp:Alkalinity .

chemp:Solubility  a      owl:Class ;
        rdfs:comment     "The total amount of solute species that will remain indefinitely in a solution maintained at constant temperature and pressure in contact with the solid crystals from which the solutes were derived."@en ;
        rdfs:subClassOf  prop:ReactionProperty .

chemp:pH  a              owl:Class ;
        rdfs:comment     "pH is a measure of the acidity or basicity of a solution. It is defined as the cologarithm of the activity of dissolved hydrogen ions (H+). Hydrogen ion activity coefficients cannot be measured experimentally, so they are based on theoretical calculations. The pH scale is not an absolute scale; it is relative to a set of standard solutions whose pH is established by international agreement. [Wikipedia]"@en ;
        rdfs:subClassOf  quan:ChemicalQuantity .

chemp:SodiumAdsorptionRatio
        a                    owl:Class ;
        rdfs:subClassOf      indx:WaterQualityIndex ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  chemp:Sodicity ;
                               owl:onProperty     screla:measureOf
                             ] ;
        owl:equivalentClass  chemp:SAR .

chemp:Reactivity  a      owl:Class ;
        rdfs:subClassOf  prop:ReactionProperty .

chemp:Sodicity  a        owl:Class ;
        rdfs:subClassOf  conc:MassConcentration ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    elem:Sodium ;
                           owl:onProperty  crela:hasSubstance
                         ] .

chemp:SuspensionLoad  a  owl:Class .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <matrElement> , <propMass> , <statePhysical> , <reprSciUnits> , <repr> , <matrCompound> , <rela> , <prop> , <relaSci> , <matrIon> , <stateChemical> , <propIndex> , <relaChemical> , <matr> , <propQuantity> ;
        owl:versionInfo  "3.1.0" .

chemp:TotalDissolvedSolids
        a                owl:Class ;
        rdfs:subClassOf  conc:Density ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  chem:DissolvedSolid ;
                           owl:onProperty     crela:hasSubstance
                         ] .

chemp:BiologicalOxygenDemand
        a                    owl:Class ;
        rdfs:subClassOf      chemp:OxygenDemand ;
        owl:equivalentClass  chemp:BOD , chemp:BiochemicalOxygenDemand .

chemp:TotalAlkalinity
        a                owl:Class ;
        rdfs:subClassOf  chemp:Alkalinity .

chemp:BiochemicalOxygenDemand
        a       owl:Class .

chemp:ChemicalOxygenDemand
        a                    owl:Class ;
        rdfs:subClassOf      chemp:OxygenDemand ;
        owl:equivalentClass  chemp:COD .

chemp:OxygenDemand  a    owl:Class ;
        rdfs:subClassOf  conc:Density .

chemp:SaturationProperty
        a                owl:Class ;
        rdfs:subClassOf  prop:Property ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    cstate:Saturation ;
                           owl:onProperty  rela:hasProperty
                         ] .

chemp:Abundance  a       owl:Class ;
        rdfs:subClassOf  chemp:Composition .

chemp:Alkaline  a  owl:Class .

chemp:Alkalic  a  owl:Class .

chemp:Purity  a          owl:Class ;
        rdfs:subClassOf  prop:ChemicalProperty .

chemp:Salinity  a        owl:Class ;
        rdfs:subClassOf  conc:MassConcentration ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:perMil ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  ion:SaltIon ;
                           owl:onProperty     crela:hasSubstance
                         ] .

chemp:WaterConcentration
        a                owl:Class ;
        rdfs:subClassOf  conc:MassConcentration ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    comp:Water ;
                           owl:onProperty  crela:hasSubstance
                         ] .

chemp:TotalSuspendedSolids
        a                owl:Class ;
        rdfs:subClassOf  conc:Density ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  chem:SuspendedSolid ;
                           owl:onProperty     crela:hasSubstance
                         ] .

chemp:Moisture  a        owl:Class ;
        rdfs:subClassOf  chemp:WaterConcentration .

chemp:Constituent  a     owl:Class ;
        rdfs:subClassOf  repr:Component , conc:ConsistenceProperty .

chemp:WashLoad  a         owl:Class ;
        rdfs:subClassOf   chemp:Load ;
        rdfs:subClassOf   [ a               owl:Restriction ;
                            owl:hasValue    pstate:Floating ;
                            owl:onProperty  rela:hasState
                          ] ;
        owl:disjointWith  chemp:SuspendedLoad .

chemp:SuspendedLoad  a       owl:Class ;
        rdfs:subClassOf      chem:SuspendedSolid , chemp:Load ;
        owl:equivalentClass  chemp:SuspensionLoad .

chemp:HplusConcentration
        a                owl:Class ;
        rdfs:subClassOf  quan:ChemicalQuantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    ion:Hplus ;
                           owl:onProperty  crela:hasSubstance
                         ] .

chemp:Composition  a     owl:Class ;
        rdfs:subClassOf  prop:ChemicalProperty .

chemp:COD  a    owl:Class .

chemp:Basic  a  owl:Class .

chemp:BOD  a    owl:Class .

chemp:BedLoad  a          owl:Class ;
        rdfs:subClassOf   chemp:Load ;
        owl:disjointWith  chemp:SuspendedLoad .

chemp:SaturationPoint
        a                owl:Class ;
        rdfs:subClassOf  chemp:SaturationProperty .

chemp:Zoomass  a         owl:Class ;
        rdfs:subClassOf  conc:DryWeight .

chemp:Load  a            owl:Class ;
        rdfs:subClassOf  conc:Density .

chemp:IonComposition  a  owl:Class ;
        rdfs:subClassOf  chemp:Composition ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    pstate:IonizedState ;
                           owl:onProperty  rela:hasState
                         ] .
