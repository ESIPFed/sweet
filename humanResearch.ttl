@base          <http://sweetontology.net/humanResearch> .
@prefix huma:  <http://sweetontology.net/human/> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix res:   <http://sweetontology.net/humanResearch/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

res:Evidence  a          owl:Class ;
        rdfs:subClassOf  res:Assessment .

res:Variable  a          owl:Class ;
        rdfs:subClassOf  repr:Representation .

res:ExperimentActivity
        a                owl:Class ;
        rdfs:subClassOf  res:Experiment .

res:Validation  a        owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

res:MissionTestedEnvironment
        a                 owl:Class ;
        rdfs:subClassOf   res:ResearchSetting ;
        owl:disjointWith  res:LaboratoryEnvironment .

res:Observation  a           owl:Class ;
        rdfs:subClassOf      res:Investigation ;
        owl:equivalentClass  res:Observe .

res:ProofOfConcept  a    owl:Class ;
        rdfs:subClassOf  res:Research , res:Proof .

res:Realization  a       owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

res:Research  a          owl:Class ;
        rdfs:subClassOf  huma:HumanActivity .

res:OperationalEnvironment
        a                 owl:Class ;
        rdfs:subClassOf   res:ResearchSetting ;
        owl:disjointWith  res:LaboratoryEnvironment .

res:Investigate  a  owl:Class .

res:WeightOfEvidence  a  owl:Class ;
        rdfs:subClassOf  res:Assessment .

res:ResearchSetting  a   owl:Class ;
        rdfs:subClassOf  res:Research .

res:Sample  a            owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

res:Result  a            owl:Class ;
        rdfs:subClassOf  res:Research .

res:Monitoring  a  owl:Class .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <repr> , <human> ;
        owl:versionInfo  "3.1.0" .

res:Investigation  a         owl:Class ;
        rdfs:subClassOf      res:Research ;
        owl:equivalentClass  res:Investigate .

res:HypothesisTest  a    owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

res:Experiment  a        owl:Class ;
        rdfs:subClassOf  res:Investigation .

res:Proof  a             owl:Class ;
        rdfs:subClassOf  res:Validation .

res:Observe  a  owl:Class .

res:AppliedResearch  a   owl:Class ;
        rdfs:subClassOf  res:Research .

res:Difference  a        owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

res:Campaign  a          owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

res:EndToEndEnvironment
        a                 owl:Class ;
        rdfs:subClassOf   res:ResearchSetting ;
        owl:disjointWith  res:LaboratoryEnvironment .

res:Project  a           owl:Class ;
        rdfs:subClassOf  res:Research .

res:Monitor  a               owl:Class ;
        rdfs:subClassOf      res:Research ;
        owl:equivalentClass  res:Monitoring .

res:RepresentativeEnvironment
        a                 owl:Class ;
        rdfs:subClassOf   res:ResearchSetting ;
        owl:disjointWith  res:LaboratoryEnvironment .

res:Publication  a       owl:Class ;
        rdfs:subClassOf  res:Research .

res:LaboratoryEnvironment
        a                owl:Class ;
        rdfs:subClassOf  res:ResearchSetting .

res:Correction  a        owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

res:Assessment  a        owl:Class ;
        rdfs:subClassOf  res:Investigation .

res:Residual  a          owl:Class ;
        rdfs:subClassOf  res:Research .

res:Analysis  a          owl:Class ;
        rdfs:subClassOf  res:Research .

res:VerifiedValidatedEnvironment
        a                 owl:Class ;
        rdfs:subClassOf   res:ResearchSetting ;
        owl:disjointWith  res:LaboratoryEnvironment .
