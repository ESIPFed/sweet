@base          <http://sweetontology.net/reprSpaceGeometry3D> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix obj3d: <http://sweetontology.net/reprSpaceGeometry3D/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix obj:   <http://sweetontology.net/reprSpaceGeometry/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

obj3d:Striation  a       owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Sphere  a          owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Layer  a           owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Strata  a          owl:Class ;
        rdfs:subClassOf  obj3d:Layer .

obj3d:GeometricalObject_3D
        a                owl:Class ;
        rdfs:subClassOf  repr:3D , obj:GeometricalObject .

obj3d:Tube  a            owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Envelope  a        owl:Class ;
        rdfs:subClassOf  obj3d:Layer .

obj3d:Dome  a            owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Spheroid  a        owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:AirSpace  a        owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Polyhedron  a      owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Shell  a           owl:Class ;
        rdfs:subClassOf  obj3d:Layer .

obj3d:Cone  a            owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Ellipsoid  a       owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Body  a            owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Hemisphere  a      owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Dipole  a          owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Corkscrew  a       owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:FluxTube  a        owl:Class ;
        rdfs:subClassOf  obj3d:Tube .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <reprSpaceGeometry> , <repr> ;
        owl:versionInfo  "3.1.0" .

obj3d:Bowl  a            owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:ReferenceEllipsoid
        a                owl:Class ;
        rdfs:subClassOf  obj3d:Ellipsoid .

obj3d:Cylinder  a        owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Torus  a           owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Ring  a            owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Cavity  a          owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .

obj3d:Belt  a            owl:Class ;
        rdfs:subClassOf  obj3d:GeometricalObject_3D .
