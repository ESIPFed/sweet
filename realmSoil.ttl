@base          <http://sweetontology.net/realmSoil> .
@prefix cate:  <http://sweetontology.net/propCategorical/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix cstate: <http://sweetontology.net/statePhysical/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix realm: <http://sweetontology.net/realm/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mix:   <http://sweetontology.net/phenMixing/> .
@prefix rela:  <http://sweetontology.net/rela/> .

<realmSoil/ParentRock>
        a                    owl:Class ;
        rdfs:subClassOf      <realmSoil/SoilLayer> ;
        owl:equivalentClass  <realmSoil/C_Horizon> .

<realmSoil/SoilHorizon>
        a                owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilLayer> .

<realmSoil/Alfisol>  a   owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/O_Horizon>
        a       owl:Class .

<realmSoil/Substratum>
        a       owl:Class .

<realmSoil/Regolith>  a  owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilLayer> .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <realm> , <rela> , <relaSpace> , <phenMixing> , <statePhysical> , <propCategorical> ;
        owl:versionInfo  "3.1.0" .

<realmSoil/Aridisol>  a  owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/ActiveLayer>
        a                owl:Class ;
        rdfs:comment     "In environments containing permafrost, the active layer is the top layer of soil that thaws during the summer and freezes again during the autumn."@en ;
        rdfs:subClassOf  <realmSoil/SoilLayer> ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      rela:partOf ;
                           owl:someValuesFrom  <realmSoil/Permafrost>
                         ] .

<realmSoil/Topsoil>  a       owl:Class ;
        rdfs:subClassOf      <realmSoil/SoilLayer> ;
        rdfs:subClassOf      [ a                   owl:Restriction ;
                               owl:onProperty      srela:above ;
                               owl:someValuesFrom  <realmSoil/Subsoil>
                             ] ;
        owl:equivalentClass  <realmSoil/SurfaceSoil> , <realmSoil/A_Horizon> .

<realmSoil/Mollisol>  a  owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/A_Horizon>
        a       owl:Class .

<realmSoil/Permafrost>
        a                owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilLayer> ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    cstate:Frozen ;
                           owl:onProperty  rela:hasState
                         ] .

<realmSoil/Andisol>  a   owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/Subsoil>  a       owl:Class ;
        rdfs:subClassOf      <realmSoil/SoilLayer> ;
        rdfs:subClassOf      [ a                   owl:Restriction ;
                               owl:onProperty      srela:above ;
                               owl:someValuesFrom  <realmSoil/ParentRock>
                             ] ;
        owl:equivalentClass  <realmSoil/B_Horizon> .

<realmSoil/Ultisol>  a   owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/C_Horizon>
        a       owl:Class .

<realmSoil/Ground>  a    owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilLayer> ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    cstate:Solid ;
                           owl:onProperty  rela:hasState
                         ] .

<realmSoil/Paleosol>  a  owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/Vertisol>  a  owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/OrganicMatter>
        a                    owl:Class ;
        rdfs:subClassOf      <realmSoil/SoilLayer> ;
        rdfs:subClassOf      [ a                   owl:Restriction ;
                               owl:onProperty      srela:above ;
                               owl:someValuesFrom  <realmSoil/P_Horizon>
                             ] ;
        rdfs:subClassOf      [ a                   owl:Restriction ;
                               owl:onProperty      srela:above ;
                               owl:someValuesFrom  <realmSoil/Topsoil>
                             ] ;
        owl:equivalentClass  <realmSoil/O_Horizon> .

<realmSoil/Histisol>  a  owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/P_Horizon>
        a                owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilLayer> .

<realmSoil/Gelisol>  a   owl:Class ;
        rdfs:comment     "Gelisols are soils of very cold climates which are defined as containing permafrost within two metres of the soil surface. The word Gelisol comes from the Latin gelare meaning to freeze, a reference to the process of cryoturbation that occurs from the alternating thawing and freezing characteristic of Gelisols."@en ;
        rdfs:subClassOf  <realmSoil/SoilOrder> ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  mix:Cryoturbation ;
                           owl:onProperty     rela:hasProcess
                         ] .

<realmSoil/Talik>  a     owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilLayer> ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  <realmSoil/Permafrost> ;
                           owl:onProperty     rela:partOf
                         ] .

<realmSoil/Entisol>  a   owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/Substrata>
        a                    owl:Class ;
        rdfs:subClassOf      <realmSoil/SoilLayer> ;
        owl:equivalentClass  <realmSoil/Substratum> .

<realmSoil/Spodosol>  a  owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/SoilLayer>
        a                owl:Class ;
        rdfs:subClassOf  realm:PlanetaryLayer .

<realmSoil/Cryosol>  a       owl:Class ;
        owl:equivalentClass  <realmSoil/Gelisol> .

<realmSoil/B_Horizon>
        a       owl:Class .

<realmSoil/SurfaceSoil>
        a       owl:Class .

<realmSoil/Inceptisol>
        a                owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/Oxisol>  a    owl:Class ;
        rdfs:subClassOf  <realmSoil/SoilOrder> .

<realmSoil/SoilOrder>
        a                owl:Class ;
        rdfs:subClassOf  cate:Classification .
