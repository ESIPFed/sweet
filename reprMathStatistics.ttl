#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/reprMathStatistics/ANOVA
:ANOVA rdf:type owl:Class ;
       owl:equivalentClass :AnalysisOfVariance .


###  http://sweetontology.net/reprMathStatistics/AnalysisOfVariance
:AnalysisOfVariance rdf:type owl:Class ;
                    rdfs:subClassOf :HypothesisTest .


###  http://sweetontology.net/reprMathStatistics/ChiSquareTest
:ChiSquareTest rdf:type owl:Class ;
               rdfs:subClassOf :HypothesisTest ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaMath/hasStatisticalDistribution> ;
                                 owl:allValuesFrom <http://sweetontology.net/reprMathFunction/ChiSquare>
                               ] .


###  http://sweetontology.net/reprMathStatistics/GeneralLinearModel
:GeneralLinearModel rdf:type owl:Class ;
                    rdfs:subClassOf :Regression .


###  http://sweetontology.net/reprMathStatistics/GoodnessOfFit
:GoodnessOfFit rdf:type owl:Class ;
               rdfs:subClassOf :Statistics .


###  http://sweetontology.net/reprMathStatistics/HypothesisTest
:HypothesisTest rdf:type owl:Class ;
                rdfs:subClassOf :StatisticalInference .


###  http://sweetontology.net/reprMathStatistics/OLS
:OLS rdf:type owl:Class ;
     owl:equivalentClass :OrdinaryLeastSquares .


###  http://sweetontology.net/reprMathStatistics/OrdinaryLeastSquares
:OrdinaryLeastSquares rdf:type owl:Class ;
                      rdfs:subClassOf :Regression .


###  http://sweetontology.net/reprMathStatistics/Regression
:Regression rdf:type owl:Class ;
            rdfs:subClassOf :StatisticalOperation .


###  http://sweetontology.net/reprMathStatistics/SampleStatistics
:SampleStatistics rdf:type owl:Class ;
                  rdfs:subClassOf :Statistics .


###  http://sweetontology.net/reprMathStatistics/Sampling
:Sampling rdf:type owl:Class ;
          rdfs:subClassOf :StatisticalOperation .


###  http://sweetontology.net/reprMathStatistics/StatisticalDistribution
:StatisticalDistribution rdf:type owl:Class ;
                         rdfs:subClassOf <http://sweetontology.net/reprMathFunction/DistributionFunction> .


###  http://sweetontology.net/reprMathStatistics/StatisticalInference
:StatisticalInference rdf:type owl:Class ;
                      rdfs:subClassOf :StatisticalOperation .


###  http://sweetontology.net/reprMathStatistics/StatisticalOperation
:StatisticalOperation rdf:type owl:Class ;
                      rdfs:subClassOf <http://sweetontology.net/reprMathOperation/Operation> .


###  http://sweetontology.net/reprMathStatistics/StatisticalSample
:StatisticalSample rdf:type owl:Class ;
                   rdfs:subClassOf <http://sweetontology.net/humanResearch/Sample> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                     owl:hasValue <http://sweetontology.net/stateSystem/Random>
                                   ] .


###  http://sweetontology.net/reprMathStatistics/Statistics
:Statistics rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/reprMath/NumericalEntity> .


###  http://sweetontology.net/reprMathStatistics/ZTest
:ZTest rdf:type owl:Class ;
       rdfs:subClassOf :HypothesisTest ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://sweetontology.net/relaMath/hasStatisticalDistribution> ;
                         owl:allValuesFrom <http://sweetontology.net/reprMathFunction/Normal>
                       ] .


###  http://sweetontology.net/reprMathStatistics/tTest
:tTest rdf:type owl:Class ;
       rdfs:subClassOf :HypothesisTest .


