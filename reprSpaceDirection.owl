<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY mdir "http://sweetontology.net/propSpaceDirection.owl">  
  <!ENTITY mdir2 "http://sweetontology.net/propSpaceDirection.owl#">  
  <!ENTITY dir "http://sweetontology.net/reprSpaceDirection.owl">  
  <!ENTITY srela "http://sweetontology.net/relaSpace.owl">  
  <!ENTITY srela2 "http://sweetontology.net/relaSpace.owl#">  
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&dir;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:srela="&srela;"
         xmlns:srela2="&srela2;"
         xmlns:dir="&dir;"
         xmlns:mdir="&mdir;"
         xmlns:mdir2="&mdir2;"
>
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.4">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&mdir;"/>
     <owl:imports rdf:resource="&srela;"/>
  </owl:Ontology>

<!-- Directional values or modifiers -->
  <mdir2:AngularDirection rdf:about="#Clockwise">
  </mdir2:AngularDirection>

  <mdir2:AngularDirection rdf:about="#Counterclockwise">
    <owl:differentFrom rdf:resource="#Clockwise"/>
  </mdir2:AngularDirection>

  <mdir2:AngularDirection rdf:about="#Direct">
  </mdir2:AngularDirection>

  <mdir2:AngularDirection rdf:about="#Directional">
  </mdir2:AngularDirection>

  <mdir2:AngularDirection rdf:about="#Emerging">
  </mdir2:AngularDirection>

  <mdir2:AngularDirection rdf:about="#Hemispheric">
  </mdir2:AngularDirection>

  <mdir2:HorizontalDirection rdf:about="#East">
    <owl:sameAs rdf:resource="#Eastward"/>
  </mdir2:HorizontalDirection>
  <mdir2:HorizontalDirection rdf:about="#Eastward"/>

  <mdir2:VerticalDirection rdf:about="#Down">
    <owl:sameAs rdf:resource="#Downward"/>
  </mdir2:VerticalDirection>
  <mdir2:VerticalDirection rdf:about="#Downward"/>

  <mdir2:VerticalDirection rdf:about="#Downflow">
  </mdir2:VerticalDirection>

  <mdir2:VerticalDirection rdf:about="#Sinking">
  </mdir2:VerticalDirection>

  <mdir2:Direction rdf:about="#Left">
  </mdir2:Direction>

  <mdir2:AngularDirection rdf:about="#LineOfSight">
  </mdir2:AngularDirection>

  <mdir2:HorizontalDirection rdf:about="#South">
    <owl:sameAs rdf:resource="#Southward"/>
  </mdir2:HorizontalDirection>
  <mdir2:HorizontalDirection rdf:about="#Southward"/>

  <mdir2:HorizontalDirection rdf:about="#North">
    <owl:sameAs rdf:resource="#Northward"/>
    <srela2:oppositeTo rdf:resource="#South"/>
  </mdir2:HorizontalDirection>
  <mdir2:HorizontalDirection rdf:about="#Northward"/>

  <mdir2:Direction rdf:about="#Longitudinal">
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Oblique">
    <owl:differentFrom rdf:resource="#Parallel"/>
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Parallel">
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Perpendicular">
    <srela2:orthogonalTo rdf:resource="#Parallel"/>
    <owl:sameAs rdf:resource="#Transverse"/>
    <owl:sameAs rdf:resource="#Normal"/>
  </mdir2:Direction>
  <mdir2:Direction rdf:about="#Transverse"/>
  <mdir2:Direction rdf:about="#Normal"/>

  <mdir2:Direction rdf:about="#Right">
    <srela2:oppositeTo rdf:resource="#Left"/>
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Shear">
  </mdir2:Direction>

  <mdir2:VerticalDirection rdf:about="#Up">
    <owl:sameAs rdf:resource="#Upward"/>
    <srela2:oppositeTo rdf:resource="#Down"/>
  </mdir2:VerticalDirection>
  <mdir2:VerticalDirection rdf:about="#Upward"/>

  <mdir2:VerticalDirection rdf:about="#Upflow">
    <srela2:oppositeTo rdf:resource="#Downflow"/>
  </mdir2:VerticalDirection>

  <mdir2:VerticalDirection rdf:about="#Vertical">
    <srela2:perpendicularTo rdf:resource="#Horizontal"/>
  </mdir2:VerticalDirection>

  <mdir2:HorizontalDirection rdf:about="#Horizontal">
  </mdir2:HorizontalDirection>

  <mdir2:HorizontalDirection rdf:about="#West">
    <owl:sameAs rdf:resource="#Westward"/>
    <srela2:oppositeTo rdf:resource="#East"/>
  </mdir2:HorizontalDirection>
  <mdir2:HorizontalDirection rdf:about="#Westward"/>

  <mdir2:HorizontalDirection rdf:about="#X">
  </mdir2:HorizontalDirection>

  <mdir2:HorizontalDirection rdf:about="#Y">
    <srela2:orthogonalTo rdf:resource="#X"/>
  </mdir2:HorizontalDirection>

  <mdir2:VerticalDirection rdf:about="#Z">
    <srela2:orthogonalTo rdf:resource="#X"/>
  </mdir2:VerticalDirection>

  <mdir2:VerticalDirection rdf:about="#Zenith">
  </mdir2:VerticalDirection>

<!-- Process related -->
  <mdir2:AngularDirection rdf:about="#Diffuse">
  </mdir2:AngularDirection>

  <mdir2:Direction rdf:about="#Downhill">
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Downstream">
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Downslope">
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Downwind">
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Downstream">
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Landward">
  </mdir2:Direction>

  <mdir2:HorizontalDirection rdf:about="#Lee">
    <owl:sameAs rdf:resource="#Leeward"/>
  </mdir2:HorizontalDirection>
  <mdir2:HorizontalDirection rdf:about="#Leeward"/>

  <mdir2:AngularDirection rdf:about="#Retrograde">
    <srela2:oppositeTo rdf:resource="#Prograde"/>
  </mdir2:AngularDirection>

  <mdir2:AngularDirection rdf:about="#Prograde">
  </mdir2:AngularDirection>

  <mdir2:Direction rdf:about="#Seaward">
    <srela2:oppositeTo rdf:resource="#Landward"/>
  </mdir2:Direction>

  <mdir2:AngularDirection rdf:about="#Specular">
  </mdir2:AngularDirection>

  <mdir2:Direction rdf:about="#Sunward">
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Upslope">
    <srela2:oppositeTo rdf:resource="#Downslope"/>
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Uphill">
    <srela2:oppositeTo rdf:resource="#Downhill"/>
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Upstream">
    <srela2:oppositeTo rdf:resource="#Downstream"/>
  </mdir2:Direction>

  <mdir2:Direction rdf:about="#Upwind">
    <srela2:oppositeTo rdf:resource="#Downstream"/>
  </mdir2:Direction>

  <mdir2:HorizontalDirection rdf:about="#Windward">
    <srela2:oppositeTo rdf:resource="#Leeward"/>
  </mdir2:HorizontalDirection>

</rdf:RDF>
