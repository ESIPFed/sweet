###  http://sweetontology.net/matrWater/BlackWater
:BlackWater rdf:type owl:Class ;
            rdfs:subClassOf :WaterMixture .


###  http://sweetontology.net/matrWater/BrackishWater
:BrackishWater rdf:type owl:Class ;
               rdfs:subClassOf :SaltWater ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                 owl:hasValue <http://sweetontology.net/stateChemical/Brackish>
                               ] ;
               rdfs:comment "Brackish water is water that has more salinity than fresh water, but not as much as seawater. It may result from mixing of seawater with fresh water, as in estuaries, or it may occur in brackish fossil aquifers."@en .


###  http://sweetontology.net/matrWater/Brine
:Brine rdf:type owl:Class ;
       rdfs:subClassOf :SaltWater ;
       owl:disjointWith :SalineWater .


###  http://sweetontology.net/matrWater/CloudDrop
:CloudDrop rdf:type owl:Class ;
           rdfs:subClassOf :Drop ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaSpace/diameter> ;
                             owl:hasValue _:genid3
                           ] .

_:genid3 rdf:type <http://sweetontology.net/propSpaceDistance/SizeRange_micron> ;
          <http://sweetontology.net/relaMath/hasLowerBound> "200.0"^^xsd:double ;
          <http://sweetontology.net/relaMath/hasUpperBound> "500.0"^^xsd:double .


###  http://sweetontology.net/matrWater/DepthHoar
:DepthHoar rdf:type owl:Class ;
           rdfs:subClassOf :IceCrystal ;
           rdfs:comment "Depth hoares are large crystals occurring at the base of a snowpack that form due to the fact that a snow crystal can grow over time as moisture freezes onto the crystal from vapor that is rising in the snowpack."@en .


###  http://sweetontology.net/matrWater/Dew
:Dew rdf:type owl:Class ;
     rdfs:subClassOf :LiquidWater .


###  http://sweetontology.net/matrWater/DrinkingWater
:DrinkingWater rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/human/HumanNeed> ,
                               :WaterMixture .


###  http://sweetontology.net/matrWater/DrizzleDrop
:DrizzleDrop rdf:type owl:Class ;
             rdfs:subClassOf :Drop ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaSpace/diameter> ;
                               owl:hasValue _:genid5
                             ] .

_:genid5 rdf:type <http://sweetontology.net/propSpaceDistance/SizeRange_micron> ;
          <http://sweetontology.net/relaMath/hasUpperBound> "200.0"^^xsd:double .


###  http://sweetontology.net/matrWater/Drop
:Drop rdf:type owl:Class ;
      rdfs:subClassOf :LiquidWater .


###  http://sweetontology.net/matrWater/Effluent
:Effluent rdf:type owl:Class ;
          rdfs:subClassOf :WaterMixture .


###  http://sweetontology.net/matrWater/Firn
:Firn rdf:type owl:Class ;
      rdfs:subClassOf :IceCrystal ;
      rdfs:comment "Firn is partially-compacted névé, a type of snow that has been left over from past seasons and has been recrystallized into a substance denser than névé. It is ice that is at an intermediate stage between snow and glacial ice. Firn has the appearance of wet sugar, but has a hardness that makes it extremely resistant to shovelling. It generally has a density greater than 550 kg/m³ and is often found underneath the snow that accumulates at the head of a glacier."@en .


###  http://sweetontology.net/matrWater/FreshWater
:FreshWater rdf:type owl:Class ;
            rdfs:subClassOf :WaterSubstance ;
            owl:disjointWith :SaltWater .


###  http://sweetontology.net/matrWater/Frost
:Frost rdf:type owl:Class ;
       rdfs:subClassOf :WaterSubstance ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://sweetontology.net/rela/hasState> ;
                         owl:hasValue <http://sweetontology.net/statePhysical/Frozen>
                       ] .


###  http://sweetontology.net/matrWater/GranularIce
:GranularIce rdf:type owl:Class ;
             rdfs:subClassOf :Ice .


###  http://sweetontology.net/matrWater/GrayWater
:GrayWater rdf:type owl:Class ;
           rdfs:subClassOf :WaterMixture .


###  http://sweetontology.net/matrWater/Groundwater
:Groundwater rdf:type owl:Class ;
             rdfs:subClassOf :LiquidWater ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaSpace/below> ;
                               owl:allValuesFrom <http://sweetontology.net/realm/PlanetarySurface>
                             ] .


###  http://sweetontology.net/matrWater/HoarCrystal
:HoarCrystal rdf:type owl:Class ;
             rdfs:subClassOf :IceCrystal .


###  http://sweetontology.net/matrWater/Hydrate
:Hydrate rdf:type owl:Class ;
         rdfs:subClassOf :WaterSubstance .


###  http://sweetontology.net/matrWater/Ice
:Ice rdf:type owl:Class ;
     rdfs:subClassOf :WaterSubstance ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <http://sweetontology.net/rela/hasState> ;
                       owl:hasValue <http://sweetontology.net/statePhysical/Solid>
                     ] .


###  http://sweetontology.net/matrWater/IceCrystal
:IceCrystal rdf:type owl:Class ;
            owl:equivalentClass :IceParticle ;
            rdfs:subClassOf :Ice ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/rela/hasState> ;
                              owl:hasValue <http://sweetontology.net/stateSolid/Crystal>
                            ] .


###  http://sweetontology.net/matrWater/IceParticle
:IceParticle rdf:type owl:Class .


###  http://sweetontology.net/matrWater/LiquidWater
:LiquidWater rdf:type owl:Class ;
             rdfs:subClassOf :WaterSubstance ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/rela/hasState> ;
                               owl:hasValue <http://sweetontology.net/statePhysical/Liquid>
                             ] .


###  http://sweetontology.net/matrWater/RainDrop
:RainDrop rdf:type owl:Class ;
          rdfs:subClassOf :Drop ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaSpace/diameter> ;
                            owl:hasValue _:genid12
                          ] .

_:genid12 rdf:type <http://sweetontology.net/propSpaceDistance/SizeRange_micron> ;
           <http://sweetontology.net/relaMath/hasLowerBound> "500.0"^^xsd:double .


###  http://sweetontology.net/matrWater/Rainwater
:Rainwater rdf:type owl:Class ;
           rdfs:subClassOf :LiquidWater ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaProvenance/hasSource> ;
                             owl:allValuesFrom <http://sweetontology.net/phenAtmoPrecipitation/Rainfall>
                           ] .


###  http://sweetontology.net/matrWater/Rime
:Rime rdf:type owl:Class ;
      rdfs:subClassOf :Ice ;
      rdfs:comment "A white or milky and opaque granular deposit of ice formed by the rapid freezing of supercooled water drops as they impinge upon an exposed object."@en .


###  http://sweetontology.net/matrWater/SalineWater
:SalineWater rdf:type owl:Class ;
             rdfs:subClassOf :SaltWater .


###  http://sweetontology.net/matrWater/SaltWater
:SaltWater rdf:type owl:Class ;
           rdfs:subClassOf :WaterSubstance ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/rela/hasState> ;
                             owl:hasValue <http://sweetontology.net/stateChemical/Saline>
                           ] .


###  http://sweetontology.net/matrWater/SeaWater
:SeaWater rdf:type owl:Class ;
          rdfs:subClassOf :SaltWater .


###  http://sweetontology.net/matrWater/Sewage
:Sewage rdf:type owl:Class ;
        rdfs:subClassOf :Effluent .


###  http://sweetontology.net/matrWater/Snow
:Snow rdf:type owl:Class ;
      rdfs:subClassOf :WaterSubstance ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/rela/hasState> ;
                        owl:hasValue <http://sweetontology.net/statePhysical/Solid>
                      ] .


###  http://sweetontology.net/matrWater/SnowFacies
:SnowFacies rdf:type owl:Class ;
            rdfs:subClassOf :IceCrystal .


###  http://sweetontology.net/matrWater/SnowGrain
:SnowGrain rdf:type owl:Class ;
           rdfs:subClassOf :Snow .


###  http://sweetontology.net/matrWater/Steam
:Steam rdf:type owl:Class ;
       owl:equivalentClass :WaterVapor .


###  http://sweetontology.net/matrWater/Wastewater
:Wastewater rdf:type owl:Class ;
            rdfs:subClassOf :Effluent .


###  http://sweetontology.net/matrWater/WaterMixture
:WaterMixture rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/matr/Mixture> ,
