@base          <http://sweetontology.net/matrEnergy> .
@prefix infr:  <http://sweetontology.net/matrEquipment/> .
@prefix comp:  <http://sweetontology.net/reprSciComponent/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix prela: <http://sweetontology.net/relaPhysical/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mener: <http://sweetontology.net/matrEnergy/> .
@prefix pener: <http://sweetontology.net/phenEnergy/> .

mener:PowerPlant  a      owl:Class ;
        rdfs:subClassOf  mener:EnergyConversionDevice .

mener:HydrogenEnergy  a  owl:Class ;
        rdfs:subClassOf  mener:EnergyStorage .

mener:HeatPump  a        owl:Class ;
        rdfs:subClassOf  mener:EnergyDistributionSystem .

mener:FuelCell  a        owl:Class ;
        rdfs:subClassOf  mener:EnergyStorage .

mener:PowerTransmissionFacility
        a                owl:Class ;
        rdfs:subClassOf  infr:Infrastructure .

mener:CompundParabolicConcentrator
        a                owl:Class ;
        rdfs:subClassOf  mener:EnergyCollector .

mener:EnergyCollector
        a                owl:Class ;
        rdfs:subClassOf  infr:Infrastructure .

mener:TubularCollector
        a                owl:Class ;
        rdfs:subClassOf  mener:EnergyCollector .

mener:FlatPlateCollector
        a                owl:Class ;
        rdfs:subClassOf  mener:EnergyCollector .

mener:Ethanol  a         owl:Class ;
        rdfs:subClassOf  mener:Biofuel .

mener:Biomass  a         owl:Class ;
        rdfs:subClassOf  mener:EnergyStorage .

mener:ParabolicTrough
        a                owl:Class ;
        rdfs:subClassOf  mener:EnergyCollector .

mener:EnergyStorage  a   owl:Class ;
        rdfs:subClassOf  comp:Storage .

mener:Refrigerator  a    owl:Class ;
        rdfs:subClassOf  mener:EnergyConversionDevice .

mener:Heater  a          owl:Class ;
        rdfs:subClassOf  mener:EnergyConversionDevice .

mener:EnergyConversionDevice
        a                owl:Class ;
        rdfs:subClassOf  infr:Infrastructure .

mener:Battery  a         owl:Class ;
        rdfs:subClassOf  mener:EnergyStorage .

mener:Biofuel  a         owl:Class ;
        rdfs:subClassOf  mener:Fuel ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  mener:Biomass ;
                           owl:onProperty     prela:hasEnergySource
                         ] .

mener:GeothermalPowerPlant
        a                owl:Class ;
        rdfs:subClassOf  mener:PowerPlant ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  pener:Geothermal ;
                           owl:onProperty     prela:hasEnergySource
                         ] .

mener:SmartGrid  a       owl:Class ;
        rdfs:subClassOf  mener:PowerGrid .

mener:PhotovoltaicCell
        a                owl:Class ;
        rdfs:subClassOf  mener:EnergyCollector ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  pener:SolarPower ;
                           owl:onProperty     prela:hasEnergySource
                         ] .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <relaPhysical> , <phenEnergy> , <reprSciComponent> , <matrEquipment> ;
        owl:versionInfo  "3.1.0" .

mener:HeatExchanger  a   owl:Class ;
        rdfs:subClassOf  mener:EnergyDistributionSystem .

mener:Fuel  a            owl:Class ;
        rdfs:subClassOf  mener:EnergyStorage .

mener:PowerGrid  a       owl:Class ;
        rdfs:subClassOf  mener:EnergyDistributionSystem .

mener:Biodiesel  a       owl:Class ;
        rdfs:subClassOf  mener:Biofuel .

mener:EnergyDistributionSystem
        a                owl:Class ;
        rdfs:subClassOf  infr:Infrastructure .
