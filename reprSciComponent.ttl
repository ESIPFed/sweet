:Background rdf:type owl:NamedIndividual ,
                     :Component .


###  http://sweetontology.net/reprSciComponent/Connate
:Connate rdf:type owl:NamedIndividual ,
                  :Component .


###  http://sweetontology.net/reprSciComponent/Direct
:Direct rdf:type owl:NamedIndividual ,
                 :Component .


###  http://sweetontology.net/reprSciComponent/Explicit
:Explicit rdf:type owl:NamedIndividual ,
                   :Component .


###  http://sweetontology.net/reprSciComponent/External
:External rdf:type owl:NamedIndividual ,
                   :Component .


###  http://sweetontology.net/reprSciComponent/Gross
:Gross rdf:type owl:NamedIndividual ,
                :Component .


###  http://sweetontology.net/reprSciComponent/Implicit
:Implicit rdf:type owl:NamedIndividual ,
                   :Component .


###  http://sweetontology.net/reprSciComponent/Indirect
:Indirect rdf:type owl:NamedIndividual ,
                   :Component .


###  http://sweetontology.net/reprSciComponent/Internal
:Internal rdf:type owl:NamedIndividual ,
                   :Component .


###  http://sweetontology.net/reprSciComponent/Net
:Net rdf:type owl:NamedIndividual ,
              :Component .


###  http://sweetontology.net/reprSciComponent/Segment
:Segment rdf:type owl:NamedIndividual ,
                  :Component .


###  http://sweetontology.net/reprSciComponent/Sensible
:Sensible rdf:type owl:NamedIndividual ,
                   :Component .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Direct
                        :Indirect
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Explicit
                        :Implicit
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :External
                        :Internal
                      )
] .


