@base          <http://sweetontology.net/phenOceanCoastal> .
@prefix coast: <http://sweetontology.net/phenOceanCoastal/> .
@prefix ocea:  <http://sweetontology.net/phenOcean/> .
@prefix coastr: <http://sweetontology.net/realmLandCoastal/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rela:  <http://sweetontology.net/rela/> .
@prefix wave:  <http://sweetontology.net/phenWave/> .
@prefix osc:   <http://sweetontology.net/phenCycle/> .

coast:Breaker  a         owl:Class ;
        rdfs:subClassOf  coast:BreakingWave .

coast:Backwash  a        owl:Class ;
        rdfs:subClassOf  coast:CoastalPhenomena .

coast:BeachDepletion  a  owl:Class ;
        rdfs:subClassOf  coast:CoastalPhenomena .

coast:CoastalPhenomena
        a                owl:Class ;
        rdfs:subClassOf  ocea:OceanPhenomena ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  coastr:CoastalRegion ;
                           owl:onProperty     rela:hasRealm
                         ] .

coast:OceanTide  a       owl:Class ;
        rdfs:comment     "The periodic rising and falling of the earth's oceans and atmosphere.\n    It results from the tide-producing forces of the moon and sun acting upon the rotating earth. This disturbance actually propagates as a wave through the atmosphere and along the surface of the waters of the earth. Atmospheric tides are always so designated, whereas the term “tide“ alone commonly implies the oceanic variety. Sometimes, the consequent horizontal movement of water along the coastlines is also called “tide,” but it is preferable to designate the latter as tidal current, reserving the name tide for the vertical wavelike movement." ;
        rdfs:subClassOf  coast:CoastalPhenomena , osc:Tide .

coast:Swell  a           owl:Class ;
        rdfs:comment     "Surface gravity waves on the ocean that are not growing or being sustained any longer by the wind."@en ;
        rdfs:subClassOf  wave:GravityWave , coast:CoastalPhenomena .

coast:LongshoreDrift  a  owl:Class .

coast:LittoralDrift  a       owl:Class ;
        rdfs:subClassOf      coast:CoastalPhenomena ;
        owl:equivalentClass  coast:LongshoreDrift .

coast:SpillingBreaker
        a                owl:Class ;
        rdfs:subClassOf  coast:Breaker .

coast:SurgingBreaker  a  owl:Class ;
        rdfs:subClassOf  coast:Breaker .

coast:Attrition  a       owl:Class ;
        rdfs:subClassOf  coast:CoastalPhenomena .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <phenCycle> , <rela> , <phenOcean> , <phenWave> , <realmLandCoastal> ;
        owl:versionInfo  "3.1.0" .

coast:PlungingBreaker
        a                owl:Class ;
        rdfs:subClassOf  coast:Breaker .

coast:BreakingWave  a    owl:Class ;
        rdfs:subClassOf  coast:CoastalPhenomena .

coast:SurfBeat  a        owl:Class ;
        rdfs:subClassOf  coast:CoastalPhenomena .

coast:StormSurge  a      owl:Class ;
        rdfs:subClassOf  coast:CoastalPhenomena .
