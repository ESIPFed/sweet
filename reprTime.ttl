@prefix : <http://sweetontology.net/reprTime/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/reprTime> .

<http://sweetontology.net/reprTime> rdf:type owl:Ontology ;
                                     owl:imports <http://sweetontology.net/realmRegion> ,
                                                 <http://sweetontology.net/relaSci> ,
                                                 <http://sweetontology.net/repr> ,
                                                 <http://sweetontology.net/reprMath> ,
                                                 <http://sweetontology.net/reprSciUnits> ;
                                     rdfs:label "SWEET Ontology" ;
                                     owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/reprTime/Duration
:Duration rdf:type owl:Class ;
          owl:equivalentClass :TemporalEntity .


###  http://sweetontology.net/reprTime/Local
:Local rdf:type owl:Class ;
       rdfs:subClassOf :LocalTime .


###  http://sweetontology.net/reprTime/LocalTime
:LocalTime rdf:type owl:Class ;
           rdfs:subClassOf :TimeZone .


###  http://sweetontology.net/reprTime/SolarTime
:SolarTime rdf:type owl:Class ;
           rdfs:subClassOf :LocalTime .


###  http://sweetontology.net/reprTime/TemporalEntity
:TemporalEntity rdf:type owl:Class ;
                rdfs:subClassOf <http://sweetontology.net/reprMath/NumericalEntity> .


###  http://sweetontology.net/reprTime/TemporalReference
:TemporalReference rdf:type owl:Class ;
                   owl:equivalentClass :TimeReference ;
                   rdfs:subClassOf :TemporalEntity .


###  http://sweetontology.net/reprTime/Time
:Time rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/repr/Dimension> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                        owl:hasValue <http://sweetontology.net/reprSciUnits/second>
                      ] ;
      rdfs:comment "This concept is of time as an independent variable"@en .


###  http://sweetontology.net/reprTime/TimeFrame
:TimeFrame rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/repr/ReferenceFrame> ,
                           :Time .


###  http://sweetontology.net/reprTime/TimeReference
:TimeReference rdf:type owl:Class .


###  http://sweetontology.net/reprTime/TimeZone
:TimeZone rdf:type owl:Class ;
          rdfs:subClassOf :TemporalReference .


###  http://www.w3.org/2006/time#Instant
<http://www.w3.org/2006/time#Instant> rdf:type owl:Class ;
                                      rdfs:subClassOf <http://sweetontology.net/reprMath/Point> ,
                                                      :Time .


###  http://www.w3.org/2006/time#TemporalEntity
<http://www.w3.org/2006/time#TemporalEntity> rdf:type owl:Class ;
                                             rdfs:subClassOf :Time .


#################################################################
#    Individuals
#################################################################

###  http://sweetontology.net/reprSciUnits/second
<http://sweetontology.net/reprSciUnits/second> rdf:type owl:NamedIndividual .


###  http://sweetontology.net/reprTime/Sidereal
:Sidereal rdf:type owl:NamedIndividual ,
                   :TimeFrame .


###  http://sweetontology.net/reprTime/Synodic
:Synodic rdf:type owl:NamedIndividual ,
                  :TimeFrame .


###  http://sweetontology.net/reprTime/UT
:UT rdf:type owl:NamedIndividual ,
             :TimeZone ;
    owl:sameAs :UniversalTime .


###  http://sweetontology.net/reprTime/UniversalTime
:UniversalTime rdf:type owl:NamedIndividual ,
                        :TimeZone .


