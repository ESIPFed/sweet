#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/reprTime/Duration
time:Duration rdf:type owl:Class ;
              owl:equivalentClass time:TemporalEntity .


###  http://sweetontology.net/reprTime/Local
time:Local rdf:type owl:Class ;
           rdfs:subClassOf time:LocalTime .


###  http://sweetontology.net/reprTime/LocalTime
time:LocalTime rdf:type owl:Class ;
               rdfs:subClassOf time:TimeZone .


###  http://sweetontology.net/reprTime/SolarTime
time:SolarTime rdf:type owl:Class ;
               rdfs:subClassOf time:LocalTime .


###  http://sweetontology.net/reprTime/TemporalEntity
time:TemporalEntity rdf:type owl:Class ;
                    rdfs:subClassOf math:NumericalEntity .


###  http://sweetontology.net/reprTime/TemporalReference
time:TemporalReference rdf:type owl:Class ;
                       owl:equivalentClass time:TimeReference ;
                       rdfs:subClassOf time:TemporalEntity .


###  http://sweetontology.net/reprTime/Time
time:Time rdf:type owl:Class ;
          rdfs:subClassOf repr:Dimension ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty screla:hasDefaultUnit ;
                            owl:hasValue units:second
                          ] .


###  http://sweetontology.net/reprTime/TimeFrame
time:TimeFrame rdf:type owl:Class ;
               rdfs:subClassOf repr:ReferenceFrame ,
                               time:Time .


###  http://sweetontology.net/reprTime/TimeReference
time:TimeReference rdf:type owl:Class .


###  http://sweetontology.net/reprTime/TimeZone
time:TimeZone rdf:type owl:Class ;
              rdfs:subClassOf time:TemporalReference .


###  http://www.w3.org/2006/time#Instant
owlt:Instant rdf:type owl:Class ;
             rdfs:subClassOf math:Point ,
                             time:Time .


###  http://www.w3.org/2006/time#TemporalEntity
owlt:TemporalEntity rdf:type owl:Class ;
                    rdfs:subClassOf time:Time .


#################################################################
