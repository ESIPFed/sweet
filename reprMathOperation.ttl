@prefix : <http://sweetontology.net/reprMathOperation/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/reprMathOperation> .

<http://sweetontology.net/reprMathOperation> rdf:type owl:Ontology ;
                                              owl:imports <http://sweetontology.net/proc> ,
                                                          <http://sweetontology.net/relaMath> ,
                                                          <http://sweetontology.net/reprMath> ,
                                                          <http://sweetontology.net/reprMathFunction> ;
                                              rdfs:label "SWEET Ontology" ;
                                              owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/reprMathOperation/AreaIntegral
:AreaIntegral rdf:type owl:Class ;
              rdfs:subClassOf :MultipleIntegral .


###  http://sweetontology.net/reprMathOperation/Biharmonic
:Biharmonic rdf:type owl:Class ;
            rdfs:subClassOf :VectorFieldOperation .


###  http://sweetontology.net/reprMathOperation/BinaryOperation
:BinaryOperation rdf:type owl:Class ;
                 rdfs:subClassOf :Operation ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://sweetontology.net/relaMath/hasInput> ;
                                   owl:allValuesFrom <http://sweetontology.net/reprMath/OrderedPair>
                                 ] .


###  http://sweetontology.net/reprMathOperation/CrossProduct
:CrossProduct rdf:type owl:Class ;
              rdfs:subClassOf :Product ,
                              :VectorFieldOperation ,
                              :VectorValuedOperation .


###  http://sweetontology.net/reprMathOperation/DefiniteIntegral
:DefiniteIntegral rdf:type owl:Class ;
                  rdfs:subClassOf :Integral ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://sweetontology.net/relaMath/hasInterval> ;
                                    owl:cardinality "1"^^xsd:nonNegativeInteger
                                  ] .


###  http://sweetontology.net/reprMathOperation/Derivative
:Derivative rdf:type owl:Class ;
            owl:equivalentClass :Differentiation ;
            rdfs:subClassOf :OperationOnFunction ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaMath/derivativeWithRespectTo> ;
                              owl:allValuesFrom <http://sweetontology.net/reprMath/Variable>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaMath/hasOutput> ;
                              owl:allValuesFrom <http://sweetontology.net/reprMathFunction/Function>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaMath/isDerivativeOf> ;
                              owl:allValuesFrom <http://sweetontology.net/reprMathFunction/Function>
                            ] .


###  http://sweetontology.net/reprMathOperation/Difference
:Difference rdf:type owl:Class ;
            owl:equivalentClass :Subtraction .


###  http://sweetontology.net/reprMathOperation/Differentiation
:Differentiation rdf:type owl:Class .


###  http://sweetontology.net/reprMathOperation/Division
:Division rdf:type owl:Class ;
          owl:equivalentClass :Quotient ;
          rdfs:subClassOf :BinaryOperation .


###  http://sweetontology.net/reprMathOperation/DotProduct
:DotProduct rdf:type owl:Class ;
            rdfs:subClassOf :Product ,
                            :ScalarValuedOperation ,
                            :VectorFieldOperation .


###  http://sweetontology.net/reprMathOperation/Double
:Double rdf:type owl:Class ;
        rdfs:subClassOf :UnaryOperation .


###  http://sweetontology.net/reprMathOperation/Fraction
:Fraction rdf:type owl:Class ;
          rdfs:subClassOf :Division .


###  http://sweetontology.net/reprMathOperation/Gradient
:Gradient rdf:type owl:Class ;
          rdfs:subClassOf :Derivative ,
                          :VectorValuedOperation .


###  http://sweetontology.net/reprMathOperation/Half
:Half rdf:type owl:Class ;
      rdfs:subClassOf :UnaryOperation .


###  http://sweetontology.net/reprMathOperation/IndefiniteIntegral
:IndefiniteIntegral rdf:type owl:Class ;
                    rdfs:subClassOf :Integral ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://sweetontology.net/relaMath/hasInterval> ;
                                      owl:cardinality "0"^^xsd:nonNegativeInteger
                                    ] .


###  http://sweetontology.net/reprMathOperation/InflectionPoint
:InflectionPoint rdf:type owl:Class ;
                 rdfs:subClassOf :Operation .


###  http://sweetontology.net/reprMathOperation/Integral
:Integral rdf:type owl:Class ;
          owl:equivalentClass :Integration ;
          rdfs:subClassOf :OperationOnFunction ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaMath/hasFunction> ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaMath/withRespectTo> ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] .


###  http://sweetontology.net/reprMathOperation/Integration
:Integration rdf:type owl:Class .


###  http://sweetontology.net/reprMathOperation/Laplacian
:Laplacian rdf:type owl:Class ;
           rdfs:subClassOf :VectorFieldOperation .


###  http://sweetontology.net/reprMathOperation/LineIntegral
:LineIntegral rdf:type owl:Class ;
              rdfs:subClassOf :Integral .


###  http://sweetontology.net/reprMathOperation/Logarithm
:Logarithm rdf:type owl:Class ;
           rdfs:subClassOf :UnaryOperation .


###  http://sweetontology.net/reprMathOperation/MathematicalProcess
:MathematicalProcess rdf:type owl:Class ;
                     rdfs:subClassOf <http://sweetontology.net/proc/Process> .


###  http://sweetontology.net/reprMathOperation/MultipleIntegral
:MultipleIntegral rdf:type owl:Class ;
                  rdfs:subClassOf :OperationOnFunction ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://sweetontology.net/relaMath/hasFunction> ;
                                    owl:someValuesFrom :Integral
                                  ] .


###  http://sweetontology.net/reprMathOperation/Multiplication
:Multiplication rdf:type owl:Class ;
                owl:equivalentClass :Product ;
                rdfs:subClassOf :BinaryOperation .


###  http://sweetontology.net/reprMathOperation/Normalization
:Normalization rdf:type owl:Class ;
               rdfs:subClassOf :Operation .


###  http://sweetontology.net/reprMathOperation/Operation
:Operation rdf:type owl:Class ;
           rdfs:subClassOf :MathematicalProcess ;
           rdfs:comment "An operation is an action or procedure which produces a new value from one or more input values.  [Wikipedia]"@en .


###  http://sweetontology.net/reprMathOperation/OperationOnFunction
:OperationOnFunction rdf:type owl:Class ;
                     rdfs:subClassOf :Operation ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://sweetontology.net/relaMath/hasInput> ;
                                       owl:allValuesFrom <http://sweetontology.net/reprMathFunction/Function>
                                     ] .


###  http://sweetontology.net/reprMathOperation/PartialDerivative
:PartialDerivative rdf:type owl:Class ;
                   rdfs:subClassOf :Derivative ,
                                   :VectorFieldOperation .


###  http://sweetontology.net/reprMathOperation/Product
:Product rdf:type owl:Class .


###  http://sweetontology.net/reprMathOperation/Quotient
:Quotient rdf:type owl:Class .


###  http://sweetontology.net/reprMathOperation/Ratio
:Ratio rdf:type owl:Class ;
       rdfs:subClassOf :Division .


###  http://sweetontology.net/reprMathOperation/SaddlePoint
:SaddlePoint rdf:type owl:Class ;
             rdfs:subClassOf :Operation .


###  http://sweetontology.net/reprMathOperation/ScalarFieldOperation
:ScalarFieldOperation rdf:type owl:Class ;
                      rdfs:subClassOf :OperationOnFunction ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://sweetontology.net/relaMath/hasInput> ;
                                        owl:allValuesFrom <http://sweetontology.net/reprMathFunction/ScalarField>
                                      ] .


###  http://sweetontology.net/reprMathOperation/ScalarValuedOperation
:ScalarValuedOperation rdf:type owl:Class ;
                       rdfs:subClassOf :OperationOnFunction ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://sweetontology.net/relaMath/hasOutput> ;
                                         owl:allValuesFrom <http://sweetontology.net/reprMathFunction/ScalarField>
                                       ] .


###  http://sweetontology.net/reprMathOperation/Slope
:Slope rdf:type owl:Class ;
       rdfs:subClassOf :Derivative .


###  http://sweetontology.net/reprMathOperation/Square
:Square rdf:type owl:Class ;
        rdfs:subClassOf :UnaryOperation .


###  http://sweetontology.net/reprMathOperation/SquareRoot
:SquareRoot rdf:type owl:Class ;
            rdfs:subClassOf :UnaryOperation .


###  http://sweetontology.net/reprMathOperation/Subtraction
:Subtraction rdf:type owl:Class ;
             rdfs:subClassOf :BinaryOperation .


###  http://sweetontology.net/reprMathOperation/Sum
:Sum rdf:type owl:Class ;
     owl:equivalentClass :Summation .


###  http://sweetontology.net/reprMathOperation/Summation
:Summation rdf:type owl:Class ;
           rdfs:subClassOf :BinaryOperation .


###  http://sweetontology.net/reprMathOperation/SurfaceIntegral
:SurfaceIntegral rdf:type owl:Class ;
                 rdfs:subClassOf :MultipleIntegral .


###  http://sweetontology.net/reprMathOperation/TensorFieldOperation
:TensorFieldOperation rdf:type owl:Class ;
