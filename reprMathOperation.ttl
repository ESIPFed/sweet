@base          <http://sweetontology.net/reprMathOperation> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix proc:  <http://sweetontology.net/proc/> .
@prefix func:  <http://sweetontology.net/reprMathFunction/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix oper:  <http://sweetontology.net/reprMathOperation/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix math:  <http://sweetontology.net/reprMath/> .

oper:SquareRoot  a       owl:Class ;
        rdfs:subClassOf  oper:UnaryOperation .

oper:Quotient  a  owl:Class .

oper:PartialDerivative
        a                owl:Class ;
        rdfs:subClassOf  oper:VectorFieldOperation , oper:Derivative .

oper:Difference  a  owl:Class .

oper:MultipleIntegral
        a                owl:Class ;
        rdfs:subClassOf  oper:OperationOnFunction ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      mrela:hasFunction ;
                           owl:someValuesFrom  oper:Integral
                         ] .

oper:TernaryOperation
        a                owl:Class ;
        rdfs:subClassOf  oper:Operation ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  math:OrderedTriple ;
                           owl:onProperty     mrela:hasInput
                         ] .

oper:Biharmonic  a       owl:Class ;
        rdfs:subClassOf  oper:VectorFieldOperation .

oper:Sum  a     owl:Class .

oper:CrossProduct  a     owl:Class ;
        rdfs:subClassOf  oper:VectorValuedOperation , oper:VectorFieldOperation , oper:Product .

oper:ScalarFieldOperation
        a                owl:Class ;
        rdfs:subClassOf  oper:OperationOnFunction ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  func:ScalarField ;
                           owl:onProperty     mrela:hasInput
                         ] .

oper:Derivative  a           owl:Class ;
        rdfs:subClassOf      oper:OperationOnFunction ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  func:Function ;
                               owl:onProperty     mrela:hasOutput
                             ] ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  func:Function ;
                               owl:onProperty     mrela:isDerivativeOf
                             ] ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  math:Variable ;
                               owl:onProperty     mrela:derivativeWithRespectTo
                             ] ;
        owl:equivalentClass  oper:Differentiation .

oper:BinaryOperation  a  owl:Class ;
        rdfs:subClassOf  oper:Operation ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  math:OrderedPair ;
                           owl:onProperty     mrela:hasInput
                         ] .

oper:AreaIntegral  a     owl:Class ;
        rdfs:subClassOf  oper:MultipleIntegral .

oper:Half  a             owl:Class ;
        rdfs:subClassOf  oper:UnaryOperation .

oper:Gradient  a         owl:Class ;
        rdfs:subClassOf  oper:VectorValuedOperation , oper:Derivative .

oper:Integration  a  owl:Class .

oper:Ratio  a            owl:Class ;
        rdfs:subClassOf  oper:Division .

oper:IndefiniteIntegral
        a                owl:Class ;
        rdfs:subClassOf  oper:Integral ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onProperty   mrela:hasInterval
                         ] .

oper:SurfaceIntegral  a  owl:Class ;
        rdfs:subClassOf  oper:MultipleIntegral .

oper:Normalization  a    owl:Class ;
        rdfs:subClassOf  oper:Operation .

oper:TensorFieldOperation
        a                owl:Class ;
        rdfs:subClassOf  oper:OperationOnFunction ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  func:TensorField ;
                           owl:onProperty     mrela:hasInput
                         ] .

oper:Fraction  a         owl:Class ;
        rdfs:subClassOf  oper:Division .

oper:Laplacian  a        owl:Class ;
        rdfs:subClassOf  oper:VectorFieldOperation .

oper:SaddlePoint  a      owl:Class ;
        rdfs:subClassOf  oper:Operation .

oper:Division  a             owl:Class ;
        rdfs:subClassOf      oper:BinaryOperation ;
        owl:equivalentClass  oper:Quotient .

oper:OperationOnFunction
        a                owl:Class ;
        rdfs:subClassOf  oper:Operation ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  func:Function ;
                           owl:onProperty     mrela:hasInput
                         ] .

oper:Operation  a        owl:Class ;
        rdfs:comment     "An operation is an action or procedure which produces a new value from one or more input values.  [Wikipedia]"@en ;
        rdfs:subClassOf  oper:MathematicalProcess .

oper:ScalarValuedOperation
        a                owl:Class ;
        rdfs:subClassOf  oper:OperationOnFunction ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  func:ScalarField ;
                           owl:onProperty     mrela:hasOutput
                         ] .

oper:Double  a           owl:Class ;
        rdfs:subClassOf  oper:UnaryOperation .

oper:Subtraction  a          owl:Class ;
        rdfs:subClassOf      oper:BinaryOperation ;
        owl:equivalentClass  oper:Difference .

oper:DotProduct  a       owl:Class ;
        rdfs:subClassOf  oper:ScalarValuedOperation , oper:VectorFieldOperation , oper:Product .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <proc> , <reprMathFunction> , <relaMath> , <reprMath> ;
        owl:versionInfo  "3.1.0" .

oper:InflectionPoint  a  owl:Class ;
        rdfs:subClassOf  oper:Operation .

oper:MathematicalProcess
        a                owl:Class ;
        rdfs:subClassOf  proc:Process .

oper:Integral  a             owl:Class ;
        rdfs:subClassOf      oper:OperationOnFunction ;
        rdfs:subClassOf      [ a                owl:Restriction ;
                               owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                               owl:onProperty   mrela:withRespectTo
                             ] ;
        rdfs:subClassOf      [ a                owl:Restriction ;
                               owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                               owl:onProperty   mrela:hasFunction
                             ] ;
        owl:equivalentClass  oper:Integration .

oper:Differentiation  a  owl:Class .

oper:Summation  a            owl:Class ;
        rdfs:subClassOf      oper:BinaryOperation ;
        owl:equivalentClass  oper:Sum .

oper:LineIntegral  a     owl:Class ;
        rdfs:subClassOf  oper:Integral .

oper:VolumeIntegral  a   owl:Class ;
        rdfs:subClassOf  oper:MultipleIntegral .

oper:VectorValuedOperation
        a                owl:Class ;
        rdfs:subClassOf  oper:OperationOnFunction ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  func:VectorField ;
                           owl:onProperty     mrela:hasOutput
                         ] .

oper:DefiniteIntegral
        a                owl:Class ;
        rdfs:subClassOf  oper:Integral ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   mrela:hasInterval
                         ] .

oper:Product  a  owl:Class .

oper:Square  a           owl:Class ;
        rdfs:subClassOf  oper:UnaryOperation .

oper:Slope  a            owl:Class ;
        rdfs:subClassOf  oper:Derivative .

oper:Logarithm  a        owl:Class ;
        rdfs:subClassOf  oper:UnaryOperation .

oper:VectorFieldOperation
        a                owl:Class ;
        rdfs:subClassOf  oper:OperationOnFunction ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  func:VectorField ;
                           owl:onProperty     mrela:hasInput
                         ] .

oper:Multiplication  a       owl:Class ;
        rdfs:subClassOf      oper:BinaryOperation ;
        owl:equivalentClass  oper:Product .

oper:UnaryOperation  a   owl:Class ;
        rdfs:subClassOf  oper:Operation ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  math:Scalar ;
                           owl:onProperty     mrela:hasInput
                         ] .
