###  http://sweetontology.net/phenPlanetClimate/Climate
:Climate rdf:type owl:Class ;
         owl:equivalentClass :ClimateAverage ,
                             :ClimaticAverage ,
                             :ClimatologicalAverage ;
         rdfs:subClassOf <http://sweetontology.net/phen/PlanetaryPhenomena> ,
                         <http://sweetontology.net/propFunction/TemporalAverage> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/relaSci/hasQuantity> ;
                           owl:someValuesFrom <http://sweetontology.net/propChemical/Moisture>
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/relaSci/hasQuantity> ;
                           owl:someValuesFrom <http://sweetontology.net/propTemperature/Temperature>
                         ] .


###  http://sweetontology.net/phenPlanetClimate/ClimateAnomaly
:ClimateAnomaly rdf:type owl:Class ;
                owl:equivalentClass :ClimaticAnomaly ;
                rdfs:subClassOf :ClimatePhenomena ,
                                <http://sweetontology.net/propDifference/Anomaly> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://sweetontology.net/relaMath/deviationFrom> ;
                                  owl:someValuesFrom :Climate
                                ] .


###  http://sweetontology.net/phenPlanetClimate/ClimateAverage
:ClimateAverage rdf:type owl:Class .


###  http://sweetontology.net/phenPlanetClimate/ClimateChange
:ClimateChange rdf:type owl:Class ;
               rdfs:subClassOf :GlobalChange .


###  http://sweetontology.net/phenPlanetClimate/ClimatePhenomena
:ClimatePhenomena rdf:type owl:Class ;
                  rdfs:subClassOf <http://sweetontology.net/phen/PlanetaryPhenomena> .


###  http://sweetontology.net/phenPlanetClimate/ClimaticAnomaly
:ClimaticAnomaly rdf:type owl:Class .


###  http://sweetontology.net/phenPlanetClimate/ClimaticAverage
:ClimaticAverage rdf:type owl:Class .


###  http://sweetontology.net/phenPlanetClimate/ClimatologicalAverage
:ClimatologicalAverage rdf:type owl:Class .


###  http://sweetontology.net/phenPlanetClimate/Drought
:Drought rdf:type owl:Class ;
         rdfs:subClassOf :ClimateAnomaly .


###  http://sweetontology.net/phenPlanetClimate/GlobalChange
:GlobalChange rdf:type owl:Class ;
              rdfs:subClassOf :ClimatePhenomena ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaSpace/hasSpatialScale> ;
                                owl:hasValue <http://sweetontology.net/stateSpaceScale/Global>
                              ] .


###  http://sweetontology.net/phenPlanetClimate/GlobalClimate
:GlobalClimate rdf:type owl:Class ;
               rdfs:subClassOf :Climate ,
                               <http://sweetontology.net/propFunction/Average> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaMath/averageOver> ;
                                 owl:someValuesFrom <http://sweetontology.net/reprSpace/Space>
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaSpace/hasSpatialScale> ;
                                 owl:hasValue <http://sweetontology.net/stateSpaceScale/Global>
                               ] .


###  http://sweetontology.net/phenPlanetClimate/GlobalWarming
:GlobalWarming rdf:type owl:Class ;
               rdfs:subClassOf :GlobalChange ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                 owl:allValuesFrom :GreenhouseEffect
                               ] .


###  http://sweetontology.net/phenPlanetClimate/GreenhouseEffect
:GreenhouseEffect rdf:type owl:Class ;
                  rdfs:subClassOf :GlobalWarming .


###  http://sweetontology.net/phenPlanetClimate/HeatIsland
:HeatIsland rdf:type owl:Class ;
            rdfs:subClassOf :ClimatePhenomena ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaSpace/hasSpatialScale> ;
                              owl:hasValue <http://sweetontology.net/stateSpaceScale/Microscale>
                            ] .


###  http://sweetontology.net/phenPlanetClimate/LocalClimate
:LocalClimate rdf:type owl:Class ;
              rdfs:subClassOf :Climate .


###  http://sweetontology.net/phenPlanetClimate/Mesoclimate
:Mesoclimate rdf:type owl:Class ;
             rdfs:subClassOf :Climate ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaSpace/hasSpatialScale> ;
                               owl:hasValue <http://sweetontology.net/stateSpaceScale/Mesoscale>
                             ] ;
             rdfs:comment "The climate of a natural region of small extent, for example, valley, forest, plantation, and park. Because of subtle differences in elevation and exposure, the climate may not be representative of the general climate of the region."@en .


###  http://sweetontology.net/phenPlanetClimate/Microclimate
:Microclimate rdf:type owl:Class ;
              rdfs:subClassOf :Climate ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaSpace/hasSpatialScale> ;
                                owl:hasValue <http://sweetontology.net/stateSpaceScale/Microscale>
                              ] .


###  http://sweetontology.net/phenPlanetClimate/MoistureClimate
:MoistureClimate rdf:type owl:Class ;
                 rdfs:subClassOf :Climate ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://sweetontology.net/relaSci/hasQuantity> ;
                                   owl:someValuesFrom <http://sweetontology.net/propChemical/Moisture>
                                 ] .


###  http://sweetontology.net/phenPlanetClimate/NuclearWinter
:NuclearWinter rdf:type owl:Class ;
               rdfs:subClassOf :ClimatePhenomena ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaSpace/hasSpatialScale> ;
                                 owl:hasValue <http://sweetontology.net/stateSpaceScale/Global>
                               ] .


###  http://sweetontology.net/phenPlanetClimate/Paleoclimate
:Paleoclimate rdf:type owl:Class ;
              rdfs:subClassOf :Climate ;
              rdfs:comment "Climate for periods prior to the development of measuring instruments, including historic and geologic time, for which only proxy climate records are available."@en .


###  http://sweetontology.net/phenPlanetClimate/RegionalClimate
:RegionalClimate rdf:type owl:Class ;
