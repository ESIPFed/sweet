@base          <http://sweetontology.net/phenSystemComplexity> .
@prefix comp:  <http://sweetontology.net/phenSystemComplexity/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix math:  <http://sweetontology.net/reprMathFunction/> .
@prefix sys:   <http://sweetontology.net/phenSystem/> .

comp:Transforming  a  owl:Class .

comp:SystemDynamics  a   owl:Class ;
        rdfs:comment     "System dynamics is an approach to understanding the behaviour of complex systems over time. It deals with internal feedback loops and time delays that affect the behaviour of the entire system. What makes using system dynamics different from other approaches to studying complex systems is the use of feedback loops and stocks and flows. These elements help describe how even seemingly simple systems display baffling nonlinearity" ;
        rdfs:subClassOf  sys:SystemPhenomena .

comp:EmergentPhenomenon
        a       owl:Class .

comp:Decouple  a         owl:Class ;
        rdfs:subClassOf  sys:SystemStateChange .

comp:Evolution  a        owl:Class ;
        rdfs:subClassOf  sys:SystemStateChange .

comp:Decay  a            owl:Class ;
        rdfs:subClassOf  sys:SystemStateChange .

comp:Recharge  a         owl:Class ;
        rdfs:subClassOf  sys:SystemStateChange .

comp:Maturation  a       owl:Class ;
        rdfs:subClassOf  sys:SystemStateChange .

comp:PositiveFeedback
        a                 owl:Class ;
        rdfs:subClassOf   comp:Feedback ;
        owl:disjointWith  comp:NegativeFeedback .

comp:Emergence  a  owl:Class .

comp:Signature  a        owl:Class ;
        rdfs:subClassOf  comp:Pattern .

comp:Feedback  a         owl:Class ;
        rdfs:subClassOf  comp:SelfRegulation .

comp:Cascade  a          owl:Class ;
        rdfs:subClassOf  sys:SystemPhenomena .

comp:Shock  a            owl:Class ;
        rdfs:subClassOf  math:Discontinuity , sys:SystemStateChange .

comp:Resilience  a       owl:Class ;
        rdfs:subClassOf  sys:SystemPhenomena .

comp:Synergy  a          owl:Class ;
        rdfs:subClassOf  sys:SystemPhenomena .

comp:Fluctuation  a      owl:Class ;
        rdfs:subClassOf  sys:Variation .

comp:Pattern  a          owl:Class ;
        rdfs:subClassOf  sys:Trend .

comp:Transformation  a       owl:Class ;
        rdfs:subClassOf      sys:SystemStateChange ;
        owl:equivalentClass  comp:Transforming .

comp:Collapse  a         owl:Class ;
        rdfs:subClassOf  sys:SystemStateChange .

comp:Emergent  a             owl:Class ;
        rdfs:subClassOf      sys:SystemStateChange ;
        owl:equivalentClass  comp:EmergentPhenomenon , comp:Emergence .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <phenSystem> , <reprMathFunction> ;
        owl:versionInfo  "3.1.0" .

comp:Development  a      owl:Class ;
        rdfs:subClassOf  comp:Evolution .

comp:SelfRegulation  a   owl:Class ;
        rdfs:subClassOf  sys:SystemPhenomena .

comp:ErrorPropagation
        a                owl:Class ;
        rdfs:subClassOf  sys:SystemStateChange .

comp:NegativeFeedback
        a                owl:Class ;
        rdfs:subClassOf  comp:Feedback .

comp:Dissipate  a        owl:Class ;
        rdfs:subClassOf  sys:SystemStateChange .
