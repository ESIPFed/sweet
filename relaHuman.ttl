@base          <http://sweetontology.net/relaHuman> .
@prefix rese:  <http://sweetontology.net/humanResearch/> .
@prefix comm:  <http://sweetontology.net/humanCommerce/> .
@prefix juri:  <http://sweetontology.net/humanJurisdiction/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix indu:  <http://sweetontology.net/matrIndustrial/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix know:  <http://sweetontology.net/humanKnowledgeDomain/> .
@prefix deci:  <http://sweetontology.net/humanDecision/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix hrela: <http://sweetontology.net/relaHuman/> .
@prefix equi:  <http://sweetontology.net/matrEquipment/> .

hrela:hasAttribute  a       owl:ObjectProperty ;
        rdfs:range          deci:Attribute ;
        rdfs:subPropertyOf  hrela:hasHumanActivity .

hrela:hasTRL  a     owl:DatatypeProperty ;
        rdfs:range  xsd:integer .

hrela:hasJurisdiction
        a                   owl:ObjectProperty ;
        rdfs:range          juri:Jurisdiction ;
        rdfs:subPropertyOf  hrela:hasHumanActivity .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <humanKnowledgeDomain> , <matrEquipment> , <humanCommerce> , <humanDecision> , <humanResearch> , <matrIndustrial> , <humanJurisdiction> ;
        owl:versionInfo  "3.1.0" .

hrela:hasLandUse  a  owl:ObjectProperty ;
        rdfs:range  comm:LandUse .

hrela:successIn  a  owl:ObjectProperty ;
        rdfs:range  rese:Research .

hrela:hasHumanActivity
        a       owl:ObjectProperty .

hrela:hasDecisionMaker
        a                   owl:ObjectProperty ;
        rdfs:range          deci:DecisionMaker ;
        rdfs:subPropertyOf  hrela:hasHumanActivity .

hrela:produces  a           owl:ObjectProperty ;
        rdfs:range          indu:Product ;
        rdfs:subPropertyOf  hrela:hasHumanActivity .

hrela:studyOf  a     owl:ObjectProperty ;
        rdfs:domain  know:KnowledgeDomain .

hrela:hasObjective  a       owl:ObjectProperty ;
        rdfs:range          deci:Objective ;
        rdfs:subPropertyOf  hrela:hasHumanActivity .

hrela:readinessOf  a  owl:ObjectProperty ;
        rdfs:range  equi:Infrastructure .
