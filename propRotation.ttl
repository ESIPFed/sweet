###  http://sweetontology.net/propRotation/AbsoluteVorticity
:AbsoluteVorticity rdf:type owl:Class ;
                   rdfs:subClassOf :Vorticity ;
                   rdfs:label "Absolute Vorticity" .


###  http://sweetontology.net/propRotation/AngularAcceleration
:AngularAcceleration rdf:type owl:Class ;
                     rdfs:subClassOf <http://sweetontology.net/prop/AngularMotionProperty> ,
                                     <http://sweetontology.net/propQuantity/VectorQuantity> ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                                       owl:hasValue <http://sweetontology.net/reprSciUnits/radianPerSecondSquared>
                                     ] .


###  http://sweetontology.net/propRotation/AngularMomentum
:AngularMomentum rdf:type owl:Class ;
                 rdfs:subClassOf <http://sweetontology.net/propSpeed/Momentum> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                                   owl:hasValue <http://sweetontology.net/reprSciUnits/kilogramRadianPerSecond>
                                 ] ;
                 rdfs:comment "In physics, the angular momentum of an object rotating about some reference point is the measure of the extent to which the object will continue to rotate about that point unless acted upon by an external torque. In particular, if a point mass rotates about an axis, then the angular momentum with respect to a point on the axis is related to the mass of the object, the velocity and the distance of the mass to the axis. While the motion associated with linear momentum has no absolute frame of reference, the rotation associated with angular momentum is sometimes spoken of as being measured relative to the fixed stars."@en .


###  http://sweetontology.net/propRotation/AngularVelocity
:AngularVelocity rdf:type owl:Class ;
                 rdfs:subClassOf <http://sweetontology.net/prop/AngularMotionProperty> ,
                                 <http://sweetontology.net/propQuantity/VectorQuantity> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                                   owl:hasValue <http://sweetontology.net/reprSciUnits/radianPerSecond>
                                 ] .


###  http://sweetontology.net/propRotation/Circulation
:Circulation rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/reprMathOperation/LineIntegral> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaMath/hasInput> ;
                               owl:allValuesFrom <http://sweetontology.net/reprMathFunction/VectorField>
                             ] .


###  http://sweetontology.net/propRotation/CoriolisParameter
:CoriolisParameter rdf:type owl:Class ;
                   rdfs:subClassOf <http://sweetontology.net/propQuantity/PhysicalConstant> ,
                                   <http://sweetontology.net/propTimeFrequency/Frequency> .


###  http://sweetontology.net/propRotation/Curl
:Curl rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/reprMathOperation/VectorFieldOperation> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/relaMath/hasOutput> ;
                        owl:allValuesFrom <http://sweetontology.net/reprMathFunction/VectorField>
                      ] .


###  http://sweetontology.net/propRotation/CyclotronFrequency
:CyclotronFrequency rdf:type owl:Class ;
                    owl:equivalentClass :Gyrofrequency .


###  http://sweetontology.net/propRotation/Enstrophy
:Enstrophy rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/prop/AngularMotionProperty> ,
                           <http://sweetontology.net/propQuantity/PhysicalQuantity> .


###  http://sweetontology.net/propRotation/ErtelPotentialVorticity
:ErtelPotentialVorticity rdf:type owl:Class ;
                         rdfs:subClassOf :PotentialVorticity ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                                           owl:hasValue <http://sweetontology.net/reprSciUnits/perKilogramMeterSquaredPerSecondKelvin>
                                         ] .


###  http://sweetontology.net/propRotation/Gyrofrequency
:Gyrofrequency rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/propTimeFrequency/Frequency> .


###  http://sweetontology.net/propRotation/Helicity
:Helicity rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/reprMathOperation/VectorFieldOperation> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                            owl:hasValue <http://sweetontology.net/reprSciUnits/meterPerSecondSquared>
                          ] .


###  http://sweetontology.net/propRotation/MomentOfInertia
:MomentOfInertia rdf:type owl:Class ;
                 rdfs:subClassOf <http://sweetontology.net/prop/AngularMotionProperty> ,
                                 <http://sweetontology.net/propQuantity/PhysicalQuantity> .


###  http://sweetontology.net/propRotation/PotentialVorticity
:PotentialVorticity rdf:type owl:Class ;
                    rdfs:subClassOf <http://sweetontology.net/prop/AngularMotionProperty> ,
                                    <http://sweetontology.net/propQuantity/PhysicalQuantity> ;
                    rdfs:comment "The vertically averaged vorticity of a layer divided by layer thickness."@en .


###  http://sweetontology.net/propRotation/RelativeVorticity
:RelativeVorticity rdf:type owl:Class ;
                   rdfs:subClassOf :Vorticity ;
                   rdfs:comment "The vorticity as measured in a system of coordinates fixed on the earth's surface.  Usually, only the vertical component of the vorticity is meant."@en .


###  http://sweetontology.net/propRotation/Spin
:Spin rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/prop/AngularMotionProperty> .


###  http://sweetontology.net/propRotation/Torque
:Torque rdf:type owl:Class ;
        rdfs:subClassOf <http://sweetontology.net/prop/AngularMotionProperty> ,
                        <http://sweetontology.net/propQuantity/VectorQuantity> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                          owl:hasValue <http://sweetontology.net/reprSciUnits/newtonMeter>
                        ] .


###  http://sweetontology.net/propRotation/Vorticity
:Vorticity rdf:type owl:Class ;
