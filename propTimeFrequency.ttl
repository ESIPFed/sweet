###  http://sweetontology.net/propTimeFrequency/Bandwidth
:Bandwidth rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/prop/WaveProperty> ,
                           :Frequency .


###  http://sweetontology.net/propTimeFrequency/BruntVaisalaFrequency
:BruntVaisalaFrequency rdf:type owl:Class ;
                       rdfs:subClassOf :Frequency ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://sweetontology.net/relaSci/measureOf> ;
                                         owl:allValuesFrom <http://sweetontology.net/phenFluidInstability/StaticStability>
                                       ] ;
                       rdfs:comment "The frequency at which a displaced parcel will oscillate when displaced vertically within a statically stable environment."@en .


###  http://sweetontology.net/propTimeFrequency/Cadence
:Cadence rdf:type owl:Class ;
         rdfs:subClassOf :Frequency .


###  http://sweetontology.net/propTimeFrequency/Convergence
:Convergence rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/reprMathOperation/VectorFieldOperation> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaMath/hasOutput> ;
                               owl:allValuesFrom <http://sweetontology.net/reprMathFunction/VectorField>
                             ] .


###  http://sweetontology.net/propTimeFrequency/Divergence
:Divergence rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/reprMathOperation/VectorFieldOperation> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaMath/hasInput> ;
                              owl:allValuesFrom <http://sweetontology.net/reprMathFunction/ScalarField>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaMath/hasOutput> ;
                              owl:allValuesFrom <http://sweetontology.net/reprMathFunction/VectorField>
                            ] ;
            rdfs:comment "The expansion or spreading out of a vector field; also, a precise measure thereof."@en .


###  http://sweetontology.net/propTimeFrequency/Fertility
:Fertility rdf:type owl:Class ;
           rdfs:subClassOf :Rate .


###  http://sweetontology.net/propTimeFrequency/Frequency
:Frequency rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/propQuantity/PhysicalQuantity> ,
                           :Rate ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                             owl:hasValue <http://sweetontology.net/reprSciUnits/hertz>
                           ] ;
           rdfs:comment "The rate of recurrence of any periodic phenomenon, often associated with waves of all kinds. Without qualification frequency often means temporal frequency, the rate of recurrence of a time-varying function, but could mean spatial frequency, the rate of recurrence of a space-varying function. Spatial frequency is the reciprocal of the repeat distance (sometimes the wavelength). The dimensions of (temporal) frequency are inverse time. A common unit for frequency is cycle per second, formerly abbreviated cps, but superseded by hertz, abbreviated as Hz."@en .


###  http://sweetontology.net/propTimeFrequency/FrequencyBand
:FrequencyBand rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/propQuantity/IntervalQuantity> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaSci/rangeOf> ;
                                 owl:allValuesFrom :Frequency
                               ] .


###  http://sweetontology.net/propTimeFrequency/FrequencyBand_MHz
:FrequencyBand_MHz rdf:type owl:Class ;
                   rdfs:subClassOf :FrequencyBand ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                                     owl:hasValue <http://sweetontology.net/reprSciUnits/MHz>
                                   ] .


###  http://sweetontology.net/propTimeFrequency/Incidence
:Incidence rdf:type owl:Class ;
           rdfs:subClassOf :Frequency .


###  http://sweetontology.net/propTimeFrequency/Rate
:Rate rdf:type owl:Class ;
      owl:equivalentClass :Tendency ;
      rdfs:subClassOf <http://sweetontology.net/reprMathOperation/PartialDerivative> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/relaMath/derivativeWithRespectTo> ;
                        owl:allValuesFrom <http://sweetontology.net/reprTime/Time>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                        owl:hasValue <http://sweetontology.net/reprSciUnits/perSecond>
                      ] .


###  http://sweetontology.net/propTimeFrequency/SpreadingRate
:SpreadingRate rdf:type owl:Class ;
               rdfs:subClassOf :Rate ;
               rdfs:comment "The spreading rate is a measure of how fast plate divergence is occurring. [Wikipedia]"@en .


###  http://sweetontology.net/propTimeFrequency/Tendency
:Tendency rdf:type owl:Class .


