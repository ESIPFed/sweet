@base          <http://sweetontology.net/matrAerosol> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix chem:  <http://sweetontology.net/matr/> .
@prefix solid: <http://sweetontology.net/phenSolid/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix pstate: <http://sweetontology.net/statePhysical/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mrela2: <http://sweetontology.net/relaMath/> .
@prefix aero:  <http://sweetontology.net/matrAerosol/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix land:  <http://sweetontology.net/realmLandform/> .
@prefix xten2: <http://sweetontology.net/propSpaceDistance/> .
@prefix xten:  <http://sweetontology.net/propSpaceDistance/> .
@prefix rela:  <http://sweetontology.net/rela/> .

aero:Pollen  a           owl:Class ;
        rdfs:subClassOf  aero:Particulate .

aero:Particulate  a      owl:Class ;
        rdfs:comment     "The term for solid or liquid particles found in a gas.   Some particles are large or dark enough to be seen as soot or smoke."@en ;
        rdfs:subClassOf  chem:SuspendedSubstance .

aero:FugitiveDust  a     owl:Class ;
        rdfs:subClassOf  aero:Dust .

aero:PM2point5  a        owl:Class ;
        rdfs:subClassOf  aero:Particulate ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    [ a                     xten:SizeRange_micron ;
                                             mrela2:hasUpperBound  "2.5"^^xsd:double
                                           ] ;
                           owl:onProperty  srela:diameter
                         ] .

aero:CloudCondensationNuclei
        a                    owl:Class ;
        rdfs:subClassOf      aero:CondensationNuclei ;
        owl:equivalentClass  aero:CCN .

aero:Soot  a                 owl:Class ;
        rdfs:subClassOf      aero:Particulate ;
        owl:equivalentClass  aero:BlackCarbon .

aero:ContinentalAerosol
        a                owl:Class ;
        rdfs:comment     "Aerosol having its origin over the continents with industrial, urban, agricultural, forest, and desert sources, with potential for high concentrations of hygroscopic aerosol."@en ;
        rdfs:subClassOf  aero:Aerosol ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  land:Land ;
                           owl:onProperty     rela:hasRealm
                         ] .

aero:Smoke  a            owl:Class ;
        rdfs:subClassOf  chem:SuspendedSubstance .

aero:PM10  a             owl:Class ;
        rdfs:subClassOf  aero:Particulate ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    [ a                     xten:SizeRange_micron ;
                                             mrela2:hasUpperBound  "10"^^xsd:double
                                           ] ;
                           owl:onProperty  srela:diameter
                         ] .

aero:PM1  a              owl:Class ;
        rdfs:subClassOf  aero:Particulate ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    [ a                     xten:SizeRange_micron ;
                                             mrela2:hasUpperBound  "1"^^xsd:double
                                           ] ;
                           owl:onProperty  srela:diameter
                         ] .

aero:BlackCarbon  a  owl:Class .

aero:Grain  a            owl:Class ;
        rdfs:subClassOf  chem:SolidSubstance , pstate:SubstanceForm .

aero:CCN  a     owl:Class .

aero:Ash  a              owl:Class ;
        rdfs:comment     "Fine particles of pulverized rock." ;
        rdfs:subClassOf  chem:SolidSubstance .

aero:Aerosol  a          owl:Class ;
        rdfs:subClassOf  chem:SuspendedSubstance .

aero:Spray  a            owl:Class ;
        rdfs:subClassOf  aero:Aerosol .

aero:Dust  a             owl:Class ;
        rdfs:subClassOf  chem:SolidSubstance .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <relaSpace> , <propSpaceDistance> , <statePhysical> , <realmLandform> , <matr> , <rela> , <phenSolid> , <relaMath> ;
        owl:versionInfo  "3.1.0" .

aero:CondensationNuclei
        a                owl:Class ;
        rdfs:subClassOf  chem:SolidSubstance ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  solid:Coalescence ;
                           owl:onProperty     rela:hasRole
                         ] .
