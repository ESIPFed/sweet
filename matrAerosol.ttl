@prefix : <http://sweetontology.net/matrAerosol/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/matrAerosol> .

<http://sweetontology.net/matrAerosol> rdf:type owl:Ontology ;
                                        owl:imports <http://sweetontology.net/matr> ,
                                                    <http://sweetontology.net/phenSolid> ,
                                                    <http://sweetontology.net/propSpaceDistance> ,
                                                    <http://sweetontology.net/realmLandform> ,
                                                    <http://sweetontology.net/rela> ,
                                                    <http://sweetontology.net/relaMath> ,
                                                    <http://sweetontology.net/relaSpace> ,
                                                    <http://sweetontology.net/statePhysical> ;
                                        rdfs:label "SWEET Ontology" ;
                                        owl:versionInfo "3.1.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://sweetontology.net/relaMath/hasUpperBound
<http://sweetontology.net/relaMath/hasUpperBound> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://sweetontology.net/rela/hasRealm
<http://sweetontology.net/rela/hasRealm> rdf:type owl:ObjectProperty .


###  http://sweetontology.net/relaSpace/diameter
<http://sweetontology.net/relaSpace/diameter> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/matrAerosol/Aerosol
:Aerosol rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/matr/SuspendedSubstance> .


###  http://sweetontology.net/matrAerosol/Ash
:Ash rdf:type owl:Class ;
     rdfs:subClassOf <http://sweetontology.net/matr/SolidSubstance> ;
     rdfs:comment "Fine particles of pulverized rock." .


###  http://sweetontology.net/matrAerosol/BlackCarbon
:BlackCarbon rdf:type owl:Class ;
             owl:equivalentClass :Soot .


###  http://sweetontology.net/matrAerosol/CCN
:CCN rdf:type owl:Class ;
     owl:equivalentClass :CloudCondensationNuclei .


###  http://sweetontology.net/matrAerosol/CloudCondensationNuclei
:CloudCondensationNuclei rdf:type owl:Class ;
                         rdfs:subClassOf :CondensationNuclei .


###  http://sweetontology.net/matrAerosol/CondensationNuclei
:CondensationNuclei rdf:type owl:Class ;
                    rdfs:subClassOf <http://sweetontology.net/matr/SolidSubstance> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://sweetontology.net/rela/hasRole> ;
                                      owl:allValuesFrom <http://sweetontology.net/phenSolid/Coalescence>
                                    ] .


###  http://sweetontology.net/matrAerosol/ContinentalAerosol
:ContinentalAerosol rdf:type owl:Class ;
                    rdfs:subClassOf :Aerosol ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://sweetontology.net/rela/hasRealm> ;
                                      owl:allValuesFrom <http://sweetontology.net/realmLandform/Land>
                                    ] ;
                    rdfs:comment "Aerosol having its origin over the continents with industrial, urban, agricultural, forest, and desert sources, with potential for high concentrations of hygroscopic aerosol."@en .


###  http://sweetontology.net/matrAerosol/Dust
:Dust rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/matr/SolidSubstance> .


###  http://sweetontology.net/matrAerosol/FugitiveDust
:FugitiveDust rdf:type owl:Class ;
              rdfs:subClassOf :Dust .


###  http://sweetontology.net/matrAerosol/Grain
:Grain rdf:type owl:Class ;
       rdfs:subClassOf <http://sweetontology.net/matr/SolidSubstance> ,
                       <http://sweetontology.net/statePhysical/SubstanceForm> .


###  http://sweetontology.net/matrAerosol/PM1
:PM1 rdf:type owl:Class ;
     rdfs:subClassOf :Particulate ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <http://sweetontology.net/relaSpace/diameter> ;
                       owl:hasValue _:genid4
                     ] .

_:genid4 rdf:type <http://sweetontology.net/propSpaceDistance/SizeRange_micron> ;
          <http://sweetontology.net/relaMath/hasUpperBound> "1.0"^^xsd:double .


###  http://sweetontology.net/matrAerosol/PM10
:PM10 rdf:type owl:Class ;
      rdfs:subClassOf :Particulate ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://sweetontology.net/relaSpace/diameter> ;
                        owl:hasValue _:genid6
                      ] .

_:genid6 rdf:type <http://sweetontology.net/propSpaceDistance/SizeRange_micron> ;
          <http://sweetontology.net/relaMath/hasUpperBound> "10.0"^^xsd:double .


###  http://sweetontology.net/matrAerosol/PM2point5
:PM2point5 rdf:type owl:Class ;
           rdfs:subClassOf :Particulate ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaSpace/diameter> ;
                             owl:hasValue _:genid8
                           ] .

_:genid8 rdf:type <http://sweetontology.net/propSpaceDistance/SizeRange_micron> ;
          <http://sweetontology.net/relaMath/hasUpperBound> "2.5"^^xsd:double .


###  http://sweetontology.net/matrAerosol/Particulate
:Particulate rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/matr/SuspendedSubstance> ;
             rdfs:comment "The term for solid or liquid particles found in a gas.   Some particles are large or dark enough to be seen as soot or smoke."@en .


###  http://sweetontology.net/matrAerosol/Pollen
:Pollen rdf:type owl:Class ;
        rdfs:subClassOf :Particulate .


###  http://sweetontology.net/matrAerosol/Smoke
:Smoke rdf:type owl:Class ;
       rdfs:subClassOf <http://sweetontology.net/matr/SuspendedSubstance> .


###  http://sweetontology.net/matrAerosol/Soot
:Soot rdf:type owl:Class ;
      rdfs:subClassOf :Particulate .


###  http://sweetontology.net/matrAerosol/Spray
:Spray rdf:type owl:Class ;
       rdfs:subClassOf :Aerosol .


###  http://sweetontology.net/phenSolid/Coalescence
<http://sweetontology.net/phenSolid/Coalescence> rdf:type owl:Class .


###  http://sweetontology.net/propSpaceDistance/SizeRange_micron
<http://sweetontology.net/propSpaceDistance/SizeRange_micron> rdf:type owl:Class .


###  http://sweetontology.net/realmLandform/Land
<http://sweetontology.net/realmLandform/Land> rdf:type owl:Class .


###  http://sweetontology.net/statePhysical/SubstanceForm
<http://sweetontology.net/statePhysical/SubstanceForm> rdf:type owl:Class .


