@base          <http://sweetontology.net/reprSciProvenance> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix res:   <http://sweetontology.net/humanResearch/> .
@prefix proc:  <http://sweetontology.net/proc/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix prop:  <http://sweetontology.net/prop/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pmlpbase: <http://inference-web.org/2.0/pml-provenance.owl#> .
@prefix prov:  <http://sweetontology.net/reprSciProvenance/> .
@prefix opm:   <http://openprovenance.org/model/opmo#> .
@prefix pmlj:  <http://inference-web.org/2.0/pml-justification.owl#> .

prov:ProvenanceElement
        a                owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

prov:DerivedRule  a      owl:Class ;
        rdfs:subClassOf  prov:InferenceRule .

prov:Genesis  a          owl:Class ;
        rdfs:subClassOf  prov:History .

prov:Record  a           owl:Class ;
        rdfs:subClassOf  res:Research .

prov:PrimitiveRule  a    owl:Class ;
        rdfs:subClassOf  prov:InferenceRule .

prov:InferenceRule  a    owl:Class ;
        rdfs:subClassOf  prov:ProvenanceElement .

prov:Provenance  a       owl:Class ;
        rdfs:subClassOf  prov:History .

prov:Background  a       owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

prov:Excavate  a         owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

prov:Process  a          owl:Class ;
        rdfs:subClassOf  proc:Process .

prov:Artifact  a         owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

prov:Reconstruction  a   owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

prov:Workflow  a         owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

prov:Pipeline  a         owl:Class ;
        rdfs:subClassOf  prov:Process .

prov:VariableMapping  a      owl:Class ;
        rdfs:subClassOf      prov:ProvenanceElement ;
        owl:equivalentClass  prov:Crosswalk .

prov:Predecessor  a      owl:Class ;
        rdfs:subClassOf  prov:ProvenanceElement .

prov:Agent  a            owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .

prov:JustificationElement
        a                owl:Class ;
        rdfs:subClassOf  prov:ProvenanceElement .

prov:Chronology  a       owl:Class ;
        rdfs:subClassOf  prov:History .

prov:Sequence  a         owl:Class ;
        rdfs:subClassOf  prov:History .

prov:Successor  a        owl:Class ;
        rdfs:subClassOf  prov:ProvenanceElement .

prov:History  a          owl:Class ;
        rdfs:subClassOf  prop:Property .

prov:MethodRule  a       owl:Class ;
        rdfs:subClassOf  prov:InferenceRule .

prov:Crosswalk  a  owl:Class .

prov:Metadata  a         owl:Class ;
        rdfs:subClassOf  repr:Representation .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <humanResearch> , <repr> , <proc> , <prop> ;
        owl:versionInfo  "3.1.0" .

prov:Documentation  a    owl:Class ;
        rdfs:subClassOf  res:ExperimentActivity .
