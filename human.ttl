@base          <http://sweetontology.net/human> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix know:  <http://sweetontology.net/repr/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix human: <http://sweetontology.net/human/> .

human:SocialActivity  a  owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:EnergyEndUse  a    owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:ConsumptiveUse  a  owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:HumanNeed  a       owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:CivilDisturbance
        a                owl:Class ;
        rdfs:subClassOf  human:SocialActivity .

human:Service  a         owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:HumanCapital  a    owl:Class ;
        rdfs:subClassOf  know:Knowledge , human:HumanActivity .

human:HumanActivity  a  owl:Class .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <repr> ;
        owl:versionInfo  "3.1.0" .

human:Recreation  a      owl:Class ;
        rdfs:subClassOf  human:SocialActivity .

human:Behavior  a        owl:Class ;
        rdfs:subClassOf  human:PsychologicalParameter .

human:WaterHeating  a    owl:Class ;
        rdfs:subClassOf  human:EnergyEndUse .

human:Manned  a          owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:PsychologicalParameter
        a                owl:Class ;
        rdfs:subClassOf  human:SocialActivity .

human:Census  a          owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:Colonization  a    owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:Mapping  a         owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:SpaceCooling  a    owl:Class ;
        rdfs:subClassOf  human:EnergyEndUse .

human:SpaceHeating  a    owl:Class ;
        rdfs:subClassOf  human:EnergyEndUse .

human:Profession  a      owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:Work  a            owl:Class ;
        rdfs:subClassOf  human:HumanActivity .

human:Unmanned  a         owl:Class ;
        rdfs:subClassOf   human:HumanActivity ;
        owl:disjointWith  human:Manned .

human:SocialBehavior  a  owl:Class ;
        rdfs:subClassOf  human:SocialActivity .
