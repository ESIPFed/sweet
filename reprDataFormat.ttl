@base          <http://sweetontology.net/reprDataFormat> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix :      <http://sweetontology.net/reprDataFormat/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mrela2: <http://sweetontology.net/relaMath/> .

:Binary  a      :Format .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <relaMath> , <repr> ;
        owl:versionInfo  "3.1.0" .

:LittleEndian  a  :ByteOrder .

:ByteOrder  a            owl:Class ;
        rdfs:subClassOf  repr:Representation .

:ARFF   a       :Format .

:MiddleEndian  a  :ByteOrder .

:Xls    a       :Format .

:FITS   a       :SelfDescribingFormat .

:Header  a               owl:Class ;
        rdfs:subClassOf  repr:Representation .

:NetCDF4  a           :SelfDescribingFormat ;
        rdfs:comment  "Shares data model with HDF5."@en .

:GRIB2  a       :Format .

:Format  a               owl:Class ;
        rdfs:subClassOf  repr:Representation .

:ASCII  a       :Format .

:HDFEOS2  a             :SelfDescribingFormat ;
        mrela2:basedOn  :HDF5 .

:File   a                owl:Class ;
        rdfs:subClassOf  repr:Representation .

:NetCDF3  a     :SelfDescribingFormat .

:NetworkEndian  a  :ByteOrder .

:GRIB1  a       :Format .

:HDFEOS  a              :SelfDescribingFormat ;
        mrela2:basedOn  :HDF4 .

:GRIB   a       :Format .

:SelfDescribingFormat
        a                owl:Class ;
        rdfs:subClassOf  :Format .

:HDF5   a       :SelfDescribingFormat .

:GeoTIFF  a     :Format .

:NetCDF  a      :SelfDescribingFormat .

:HDF4   a       :SelfDescribingFormat .

:HDF    a       :SelfDescribingFormat .

:BigEndian  a   :ByteOrder .
