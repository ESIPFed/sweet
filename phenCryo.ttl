###  http://sweetontology.net/phenCryo/Calving
:Calving rdf:type owl:Class ;
         owl:equivalentClass :IceCalving ;
         rdfs:subClassOf :GlacialProcess ,
                         <http://sweetontology.net/procStateChange/Melting> .


###  http://sweetontology.net/phenCryo/Englacial
:Englacial rdf:type owl:Class ;
           rdfs:subClassOf :GlacialProcess ;
           rdfs:comment "Embedded in, carried by, or running through a glacier"@en .


###  http://sweetontology.net/phenCryo/Glacial
:Glacial rdf:type owl:Class ;
         owl:equivalentClass :Glaciation .


###  http://sweetontology.net/phenCryo/GlacialProcess
:GlacialProcess rdf:type owl:Class ;
                rdfs:subClassOf <http://sweetontology.net/procPhysical/PhysicalProcess> .


###  http://sweetontology.net/phenCryo/Glaciation
:Glaciation rdf:type owl:Class ;
            rdfs:subClassOf :Accumulation ;
            owl:disjointWith :GlacierRetreat .


###  http://sweetontology.net/phenCryo/GlacierRetreat
:GlacierRetreat rdf:type owl:Class ;
                rdfs:subClassOf :GlacialProcess ,
                                <http://sweetontology.net/procStateChange/Melting> .


###  http://sweetontology.net/phenCryo/IceCalving
:IceCalving rdf:type owl:Class .


###  http://sweetontology.net/phenCryo/Periglacial
:Periglacial rdf:type owl:Class ;
             rdfs:subClassOf :GlacialProcess .


###  http://sweetontology.net/phenCryo/Periglaciation
:Periglaciation rdf:type owl:Class ;
                rdfs:subClassOf :Glaciation .


###  http://sweetontology.net/phenCryo/Proglacial
:Proglacial rdf:type owl:Class ;
            rdfs:subClassOf :GlacialProcess ;
            rdfs:comment "Of streams, deposits, and other features, being immediately in front of or just beyond the outer limits of a glacier or ice sheet, and formed by or derived from glacier ice."@en .


###  http://sweetontology.net/phenCryo/Subglacial
:Subglacial rdf:type owl:Class ;
            rdfs:subClassOf :GlacialProcess ;
            rdfs:comment "Pertaining to the area in or at the bottom of, or immediately beneath, a glacier."@en .


###  http://sweetontology.net/phenCryo/Supraglacial
:Supraglacial rdf:type owl:Class ;
              rdfs:subClassOf :GlacialProcess .


