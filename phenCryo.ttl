@base          <http://sweetontology.net/phenCryo> .
@prefix proc:  <http://sweetontology.net/procPhysical/> .
@prefix cryop: <http://sweetontology.net/phenCryo/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sys:   <http://sweetontology.net/phenSystem/> .
@prefix statec: <http://sweetontology.net/procStateChange/> .

cryop:Subglacial  a      owl:Class ;
        rdfs:comment     "Pertaining to the area in or at the bottom of, or immediately beneath, a glacier."@en ;
        rdfs:subClassOf  cryop:GlacialProcess .

cryop:Proglacial  a      owl:Class ;
        rdfs:comment     "Of streams, deposits, and other features, being immediately in front of or just beyond the outer limits of a glacier or ice sheet, and formed by or derived from glacier ice."@en ;
        rdfs:subClassOf  cryop:GlacialProcess .

cryop:GlacialProcess  a  owl:Class ;
        rdfs:subClassOf  proc:PhysicalProcess .

cryop:Calving  a             owl:Class ;
        rdfs:subClassOf      statec:Melting , cryop:GlacialProcess ;
        owl:equivalentClass  cryop:IceCalving .

cryop:Glaciation  a          owl:Class ;
        rdfs:subClassOf      cryop:Accumulation ;
        owl:equivalentClass  cryop:Glacial .

cryop:Englacial  a       owl:Class ;
        rdfs:comment     "Embedded in, carried by, or running through a glacier"@en ;
        rdfs:subClassOf  cryop:GlacialProcess .

cryop:Periglacial  a     owl:Class ;
        rdfs:subClassOf  cryop:GlacialProcess .

cryop:GlacierRetreat  a   owl:Class ;
        rdfs:subClassOf   statec:Melting , cryop:GlacialProcess ;
        owl:disjointWith  cryop:Glaciation .

cryop:IceCalving  a  owl:Class .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <procPhysical> , <procStateChange> , <phenSystem> ;
        owl:versionInfo  "3.1.0" .

cryop:Periglaciation  a  owl:Class ;
        rdfs:subClassOf  cryop:Glaciation .

cryop:Supraglacial  a    owl:Class ;
        rdfs:subClassOf  cryop:GlacialProcess .

cryop:Glacial  a  owl:Class .

cryop:Accumulation  a    owl:Class ;
        rdfs:subClassOf  sys:Accumulation , cryop:GlacialProcess .
