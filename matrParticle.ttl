#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/matrParticle/AnomalousEnergeticParticle
:AnomalousEnergeticParticle rdf:type owl:Class ;
                            rdfs:subClassOf :CosmicRay .


###  http://sweetontology.net/matrParticle/Atom
:Atom rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/statePhysical/SubstanceForm> .


###  http://sweetontology.net/matrParticle/CosmicRay
:CosmicRay rdf:type owl:Class ;
           rdfs:subClassOf :EnergeticParticle .


###  http://sweetontology.net/matrParticle/Electron
:Electron rdf:type owl:Class ;
          rdfs:subClassOf :SubatomicParticle .


###  http://sweetontology.net/matrParticle/EnergeticParticle
:EnergeticParticle rdf:type owl:Class ;
                   rdfs:subClassOf :SubatomicParticle .


###  http://sweetontology.net/matrParticle/GammaRay
:GammaRay rdf:type owl:Class ;
          rdfs:subClassOf :Photon ;
          rdfs:comment "Gamma rays are a form of electromagnetic radiation or light emission of frequencies produced by sub-atomic particle interactions, such as electron-positron annihilation or radioactive decay. Gamma rays are generally characterized as electromagnetic radiation having the highest frequency and energy, and also the shortest wavelength, within the electromagnetic spectrum, i.e. high energy photons. Due to their high energy content, they can cause serious damage when absorbed by living cells." .


###  http://sweetontology.net/matrParticle/HeavyNuclei
:HeavyNuclei rdf:type owl:Class ;
             rdfs:subClassOf :SubatomicParticle .


###  http://sweetontology.net/matrParticle/Molecule
:Molecule rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/statePhysical/SubstanceForm> .


###  http://sweetontology.net/matrParticle/Neutrino
:Neutrino rdf:type owl:Class ;
          rdfs:subClassOf :SubatomicParticle .


###  http://sweetontology.net/matrParticle/Neutron
:Neutron rdf:type owl:Class ;
         rdfs:subClassOf :SubatomicParticle .


###  http://sweetontology.net/matrParticle/Nucleon
:Nucleon rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:unionOf ( :Neutron
                                             :Proton
                                           )
                             ] ;
         rdfs:comment "A collective name for the two baryons: neutron and the proton." .


###  http://sweetontology.net/matrParticle/Particle
:Particle rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/matr/Substance> .


###  http://sweetontology.net/matrParticle/Photon
:Photon rdf:type owl:Class ;
        rdfs:subClassOf :SubatomicParticle .


###  http://sweetontology.net/matrParticle/Proton
:Proton rdf:type owl:Class ;
        rdfs:subClassOf :SubatomicParticle .


###  http://sweetontology.net/matrParticle/SolarEnergeticParticle
:SolarEnergeticParticle rdf:type owl:Class ;
                        rdfs:subClassOf :CosmicRay .


###  http://sweetontology.net/matrParticle/SubatomicParticle
:SubatomicParticle rdf:type owl:Class ;
                   rdfs:subClassOf <http://sweetontology.net/matr/Substance> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://sweetontology.net/relaSpace/hasSpatialScale> ;
                                     owl:hasValue <http://sweetontology.net/stateSpaceScale/SubatomicScale>
                                   ] .


###  http://sweetontology.net/statePhysical/SubstanceForm
<http://sweetontology.net/statePhysical/SubstanceForm> rdf:type owl:Class .


