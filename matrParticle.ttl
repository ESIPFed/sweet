@base          <http://sweetontology.net/matrParticle> .
@prefix chem:  <http://sweetontology.net/matr/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix part:  <http://sweetontology.net/matrParticle/> .
@prefix pstate: <http://sweetontology.net/statePhysical/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix scale: <http://sweetontology.net/stateSpaceScale/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ordi:  <http://sweetontology.net/relaSpace/> .

part:Neutrino  a         owl:Class ;
        rdfs:subClassOf  part:SubatomicParticle .

part:AnomalousEnergeticParticle
        a                owl:Class ;
        rdfs:subClassOf  part:CosmicRay .

part:Particle  a         owl:Class ;
        rdfs:subClassOf  chem:Substance .

part:Photon  a           owl:Class ;
        rdfs:subClassOf  part:SubatomicParticle .

part:CosmicRay  a        owl:Class ;
        rdfs:subClassOf  part:EnergeticParticle .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <stateSpaceScale> , <relaSpace> , <statePhysical> , <matr> ;
        owl:versionInfo  "3.1.0" .

part:SolarEnergeticParticle
        a                owl:Class ;
        rdfs:subClassOf  part:CosmicRay .

part:Neutron  a          owl:Class ;
        rdfs:subClassOf  part:SubatomicParticle .

part:Electron  a         owl:Class ;
        rdfs:subClassOf  part:SubatomicParticle .

part:Proton  a           owl:Class ;
        rdfs:subClassOf  part:SubatomicParticle .

part:EnergeticParticle
        a                owl:Class ;
        rdfs:subClassOf  part:SubatomicParticle .

part:Nucleon  a              owl:Class ;
        rdfs:comment         "A collective name for the two baryons: neutron and the proton." ;
        owl:equivalentClass  [ a            owl:Class ;
                               owl:unionOf  ( part:Neutron part:Proton )
                             ] .

part:HeavyNuclei  a      owl:Class ;
        rdfs:subClassOf  part:SubatomicParticle .

part:SubatomicParticle
        a                owl:Class ;
        rdfs:subClassOf  chem:Substance ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    scale:SubatomicScale ;
                           owl:onProperty  ordi:hasSpatialScale
                         ] .

part:GammaRay  a         owl:Class ;
        rdfs:comment     "Gamma rays are a form of electromagnetic radiation or light emission of frequencies produced by sub-atomic particle interactions, such as electron-positron annihilation or radioactive decay. Gamma rays are generally characterized as electromagnetic radiation having the highest frequency and energy, and also the shortest wavelength, within the electromagnetic spectrum, i.e. high energy photons. Due to their high energy content, they can cause serious damage when absorbed by living cells." ;
        rdfs:subClassOf  part:Photon .

part:Atom  a             owl:Class ;
        rdfs:subClassOf  pstate:SubstanceForm .

part:Molecule  a         owl:Class ;
        rdfs:subClassOf  pstate:SubstanceForm .
