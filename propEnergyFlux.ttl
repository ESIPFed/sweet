###  http://sweetontology.net/propEnergyFlux/AbsorbedSolarRadiation
:AbsorbedSolarRadiation rdf:type owl:Class ;
                        rdfs:subClassOf :SolarRadiation ;
                        rdfs:comment "Solar radiation absorbed by the atmosphere's constituent gases, suspended material, clouds, or by the earth's surface." ;
                        rdfs:label "Absorbed Solar Radiation" .


###  http://sweetontology.net/propEnergyFlux/AbsorptionCoefficient
:AbsorptionCoefficient rdf:type owl:Class ;
                       owl:equivalentClass :Opacity .


###  http://sweetontology.net/propEnergyFlux/Brightness
:Brightness rdf:type owl:Class ;
            owl:equivalentClass :SpecificIntensity ;
            rdfs:subClassOf :Luminance ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                              owl:hasValue <http://sweetontology.net/reprSciUnits/wattPerMeterSquaredPerSteradianPerWavelength>
                            ] .


###  http://sweetontology.net/propEnergyFlux/BulkHeatFlux
:BulkHeatFlux rdf:type owl:Class ;
              rdfs:subClassOf :ThermalFlux ;
              rdfs:comment "Heat flux averaged over a layer of air, such as the boundary layer; heat-flux divergence or difference between the top and bottom of a layer." .


###  http://sweetontology.net/propEnergyFlux/CloudForcing
:CloudForcing rdf:type owl:Class ;
              rdfs:subClassOf :RadiativeForcing .


###  http://sweetontology.net/propEnergyFlux/EarthquakeScale
:EarthquakeScale rdf:type owl:Class ;
                 rdfs:subClassOf <http://sweetontology.net/repr/LogarithmicScale> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                   owl:allValuesFrom <http://sweetontology.net/phenGeolSeismicity/Earthquake>
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                                   owl:hasValue <http://sweetontology.net/reprSciUnits/db>
                                 ] .


###  http://sweetontology.net/propEnergyFlux/EnergyFlux
:EnergyFlux rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/propQuantity/Flux> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/relaPhysical/transportOf> ;
                              owl:someValuesFrom <http://sweetontology.net/propEnergy/Energy>
                            ] .


###  http://sweetontology.net/propEnergyFlux/EnergyFlux_Wm2
:EnergyFlux_Wm2 rdf:type owl:Class ;
                rdfs:subClassOf :EnergyFlux ,
