@prefix : <http://sweetontology.net/stateBiological/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/stateBiological> .

<http://sweetontology.net/stateBiological> rdf:type owl:Ontology ;
                                            owl:imports <http://sweetontology.net/propCategorical> ,
                                                        <http://sweetontology.net/propOrdinal> ,
                                                        <http://sweetontology.net/relaSci> ,
                                                        <http://sweetontology.net/relaTime> ,
                                                        <http://sweetontology.net/reprSpaceDirection> ,
                                                        <http://sweetontology.net/state> ,
                                                        <http://sweetontology.net/stateTime> ;
                                            rdfs:label "SWEET Ontology" ;
                                            owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/stateBiological/BiologicalState
:BiologicalState rdf:type owl:Class ;
                 rdfs:subClassOf <http://sweetontology.net/state/State> .


#################################################################
#    Individuals
#################################################################

###  http://sweetontology.net/stateBiological/Abiotic
:Abiotic rdf:type owl:NamedIndividual ,
                  :BiologicalState .


###  http://sweetontology.net/stateBiological/Adult
:Adult rdf:type owl:NamedIndividual ,
                <http://sweetontology.net/propOrdinal/Age> ;
       <http://sweetontology.net/relaTime/olderThan> :Child .


###  http://sweetontology.net/stateBiological/Alive
:Alive rdf:type owl:NamedIndividual ,
                :BiologicalState ;
       owl:sameAs :Live .


###  http://sweetontology.net/stateBiological/Anadromous
:Anadromous rdf:type owl:NamedIndividual ,
                     :BiologicalState ;
            <http://sweetontology.net/relaSci/spawn> <http://sweetontology.net/reprSpaceDirection/Upstream> .


###  http://sweetontology.net/stateBiological/Anthropogenic
:Anthropogenic rdf:type owl:NamedIndividual ,
                        :BiologicalState .


###  http://sweetontology.net/stateBiological/Arable
:Arable rdf:type owl:NamedIndividual ,
                 :BiologicalState .


###  http://sweetontology.net/stateBiological/Assimilated
:Assimilated rdf:type owl:NamedIndividual ,
                      :BiologicalState .


###  http://sweetontology.net/stateBiological/Biodegradable
:Biodegradable rdf:type owl:NamedIndividual ,
                        :BiologicalState .


###  http://sweetontology.net/stateBiological/Biogenic
:Biogenic rdf:type owl:NamedIndividual ,
                   :BiologicalState .


###  http://sweetontology.net/stateBiological/Biotic
:Biotic rdf:type owl:NamedIndividual ,
                 :BiologicalState .


###  http://sweetontology.net/stateBiological/Catadromous
:Catadromous rdf:type owl:NamedIndividual ,
                      :BiologicalState ;
             <http://sweetontology.net/relaSci/spawn> <http://sweetontology.net/reprSpaceDirection/Downstream> .


###  http://sweetontology.net/stateBiological/Child
:Child rdf:type owl:NamedIndividual ,
                <http://sweetontology.net/propOrdinal/Age> .


###  http://sweetontology.net/stateBiological/Dead
:Dead rdf:type owl:NamedIndividual ,
               <http://sweetontology.net/state/BinaryState> ,
               :BiologicalState ;
      owl:sameAs :Deceased .


###  http://sweetontology.net/stateBiological/Deceased
:Deceased rdf:type owl:NamedIndividual ,
                   :BiologicalState .


###  http://sweetontology.net/stateBiological/Deciduous
:Deciduous rdf:type owl:NamedIndividual ,
                    :BiologicalState .


###  http://sweetontology.net/stateBiological/Decomposed
:Decomposed rdf:type owl:NamedIndividual ,
                     :BiologicalState .


###  http://sweetontology.net/stateBiological/Diseased
:Diseased rdf:type owl:NamedIndividual ,
                   :BiologicalState .


###  http://sweetontology.net/stateBiological/Diverse
:Diverse rdf:type owl:NamedIndividual ,
                  :BiologicalState .


###  http://sweetontology.net/stateBiological/Elderly
:Elderly rdf:type owl:NamedIndividual ,
                  <http://sweetontology.net/propOrdinal/Age> ;
         <http://sweetontology.net/relaTime/olderThan> :Youth .


###  http://sweetontology.net/stateBiological/Fertilized
:Fertilized rdf:type owl:NamedIndividual ,
                     :BiologicalState .


###  http://sweetontology.net/stateBiological/Healthy
:Healthy rdf:type owl:NamedIndividual ,
                  :BiologicalState .


###  http://sweetontology.net/stateBiological/Live
:Live rdf:type owl:NamedIndividual ,
               <http://sweetontology.net/state/BinaryState> ,
               :BiologicalState ;
      owl:sameAs :Living .


###  http://sweetontology.net/stateBiological/Living
:Living rdf:type owl:NamedIndividual ,
                 :BiologicalState .


###  http://sweetontology.net/stateBiological/Migrating
:Migrating rdf:type owl:NamedIndividual ,
                    :BiologicalState .


###  http://sweetontology.net/stateBiological/Nonliving
:Nonliving rdf:type owl:NamedIndividual ,
                    <http://sweetontology.net/state/BinaryState> ,
                    :BiologicalState .


###  http://sweetontology.net/stateBiological/NutrientRich
:NutrientRich rdf:type owl:NamedIndividual ,
                       :BiologicalState .


###  http://sweetontology.net/stateBiological/OldGrowth
:OldGrowth rdf:type owl:NamedIndividual ,
                    <http://sweetontology.net/propOrdinal/Age> .


###  http://sweetontology.net/stateBiological/Pollinated
:Pollinated rdf:type owl:NamedIndividual ,
                     :BiologicalState .


###  http://sweetontology.net/stateBiological/Youth
:Youth rdf:type owl:NamedIndividual ,
                <http://sweetontology.net/propOrdinal/Age> .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Abiotic
                        :Biotic
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Anthropogenic
                        :Biogenic
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Dead
                        :Live
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Live
                        :Nonliving
                      )
] .


