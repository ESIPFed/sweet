<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY mrela "http://sweetontology.net/relaMath.owl#">
  <!ENTITY screla "http://sweetontology.net/relaSci.owl#">
  <!ENTITY state "http://sweetontology.net/state.owl#">
  <!ENTITY repr "http://sweetontology.net/reprMath.owl#">
  <!ENTITY prela "http://sweetontology.net/relaPhysical.owl#">
  <!ENTITY rela "http://sweetontology.net/rela.owl#">
  <!ENTITY eflux "http://sweetontology.net/propEnergyFlux.owl#">
  <!ENTITY body "http://sweetontology.net/realmAstroBody.owl#">
  <!ENTITY realm "http://sweetontology.net/realm.owl#">
  <!ENTITY ener "http://sweetontology.net/phenEnergy.owl#">
  <!ENTITY tstate "http://sweetontology.net/stateThermodynamic.owl#">
  <!ENTITY ordi "http://sweetontology.net/propOrdinal.owl#">
  <!ENTITY velo "http://sweetontology.net/propSpeed.owl#">
  <!ENTITY prop "http://sweetontology.net/prop.owl#">
  <!ENTITY srela "http://sweetontology.net/relaSpace.owl#">
  <!ENTITY wave "http://sweetontology.net/phenWave.owl#">
  <!ENTITY tran "http://sweetontology.net/phenFluidTransport.owl#">
  <!ENTITY phen "http://sweetontology.net/phen.owl#">
  <!ENTITY quan "http://sweetontology.net/propQuantity.owl#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&prela;"
         xmlns:mrela="&mrela;"
         xmlns:screla="&screla;"
         xmlns:repr="&repr;"
         xmlns:body="&body;"
         xmlns:prela="&prela;"
         xmlns:eflux="&eflux;"
         xmlns:realm="&realm;"
         xmlns:srela="&srela;"
         xmlns:rela="&rela;"
         xmlns:prop="&prop;"
         xmlns:ordi="&ordi;"
         xmlns:quan="&quan;"
         xmlns:ener="&ener;"
         xmlns:wave="&wave;"
         xmlns:phen="&phen;"
         xmlns:state="&state;"
         xmlns:velo="&velo;"
         xmlns:tran="&tran;"
         xmlns:tstate="&tstate;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
>
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.4">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="http://sweetontology.net/reprMath.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/relaMath.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/relaSpace.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/state.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/relaSci.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/rela.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/prop.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/realm.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/phen.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/phenWave.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/phenEnergy.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/phenFluidTransport.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/propSpeed.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/propEnergyFlux.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/realmAstroBody.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/propOrdinal.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/propQuantity.owl"/>
     <owl:imports rdf:resource="http://sweetontology.net/stateThermodynamic.owl"/>
  </owl:Ontology>

  <owl:DatatypeProperty rdf:about="#hasCharge">
    <rdfs:range rdf:resource="&xsd;integer"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:ID="hasEnergyFlux">
    <rdfs:range rdf:resource="&eflux;EnergyFlux"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="hasTransport">
    <rdfs:range rdf:resource="&tran;Transport"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasEnergySource">
    <rdfs:range rdf:resource="&ener;EnergySource"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasEnergyProduct">
    <rdfs:range rdf:resource="&ener;EnergyForm"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasConstantQuantity">
    <rdfs:subPropertyOf rdf:resource="&mrela;hasConstant"/>
    <rdfs:subPropertyOf rdf:resource="&screla;hasQuantity"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#from">
    <rdfs:domain rdf:resource="&state;State"/>
    <rdfs:range rdf:resource="&state;State"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#to">
    <rdfs:domain rdf:resource="&state;State"/>
    <rdfs:range rdf:resource="&state;State"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#strongerThan">
    <rdfs:domain rdf:resource="&ordi;Impact"/>
    <rdfs:range rdf:resource="&ordi;Impact"/>
    <rdfs:subPropertyOf rdf:resource="&mrela;greaterThan"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#weakerThan">
    <owl:inverseOf rdf:resource="#strongerThan"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#transportOf">
    <rdfs:range rdf:resource="&quan;Quantity"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#revolvesAround">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasConservedQuantity">
    <rdfs:range rdf:resource="&quan;ConservedQuantity"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasAstronomicalBody">
    <rdfs:range rdf:resource="&body;AstronomicalBody"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasPlanetaryStructure">
    <rdfs:range rdf:resource="&realm;PlanetaryStructure"/>
  </owl:ObjectProperty>

<!-- Thermo -->
  <owl:ObjectProperty rdf:about="#warmerThan">
    <rdfs:domain rdf:resource="&tstate;ThermodynamicState"/>
    <rdfs:range rdf:resource="&tstate;ThermodynamicState"/>
    <rdfs:subPropertyOf rdf:resource="&mrela;greaterThan"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#colderThan">
    <owl:inverseOf rdf:resource="#warmerThan"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#dTdh">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#dTds">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasTemperatureRange">
    <rdfs:range rdf:resource="&ordi;TemperatureRange"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasStateChange">
    <rdfs:subPropertyOf rdf:resource="&rela;hasProcess"/>
  </owl:ObjectProperty>

<!-- waves -->
  <owl:ObjectProperty rdf:about="#hasRestoringForce">
    <rdfs:domain rdf:resource="&wave;Wave"/>
    <rdfs:range rdf:resource="&phen;Phenomena"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasOscillationDirection">
    <rdfs:subPropertyOf rdf:resource="&srela;hasDirection"/>
    <rdfs:domain rdf:resource="&wave;Wave"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasOscillationField">
    <rdfs:domain rdf:resource="&wave;Wave"/>
  </owl:ObjectProperty>

<!-- speed -->
  <owl:ObjectProperty rdf:about="#fasterThan">
    <rdfs:domain rdf:resource="&velo;Speed"/>
    <rdfs:range rdf:resource="&velo;Speed"/>
    <rdfs:subPropertyOf rdf:resource="&mrela;greaterThan"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#slowerThan">
    <owl:inverseOf rdf:resource="#fasterThan"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasSpeed">
  </owl:ObjectProperty>

<!-- electromagnetic -->
  <owl:ObjectProperty rdf:about="#moreVisibleThan">
    <rdfs:domain rdf:resource="&ordi;Visibility"/>
    <rdfs:range rdf:resource="&ordi;Visibility"/>
    <rdfs:subPropertyOf rdf:resource="&mrela;greaterThan"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasVisibility">
    <rdfs:range rdf:resource="&ordi;Visibility"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasSpectralBand">
    <rdfs:range rdf:resource="&ordi;SpectralBand"/>
  </owl:ObjectProperty>

</rdf:RDF>
