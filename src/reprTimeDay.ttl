###  http://sweetontology.net/reprTimeDay/Dawn
<http://sweetontology.net/reprTimeDay/Dawn> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://sweetontology.net/reprTimeDay/Twilight> ;
                                            rdfs:comment "time that marks the beginning of the twilight before sunrise"@en ;
                                            rdfs:label "dawn"@en .


###  http://sweetontology.net/reprTimeDay/Day
<http://sweetontology.net/reprTimeDay/Day> rdf:type owl:Class ;
                                           rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> ;
                                           rdfs:comment "unit of time lasting 24 hours, derived from the period of Earth's rotation about its axis"@en ;
                                           rdfs:label "day"@en .


###  http://sweetontology.net/reprTimeDay/Daytime
<http://sweetontology.net/reprTimeDay/Daytime> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> ;
                                               rdfs:comment "period on any given point of the planet's surface during which it experiences natural illumination from sunlight"@en ;
                                               rdfs:label "daytime"@en .


###  http://sweetontology.net/reprTimeDay/Dusk
<http://sweetontology.net/reprTimeDay/Dusk> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://sweetontology.net/reprTimeDay/Twilight> ;
                                            rdfs:comment "darkest stage of twilight, or at the very end of astronomical twilight after sunset and just before night"@en ;
                                            rdfs:label "dusk"@en .


###  http://sweetontology.net/reprTimeDay/Evening
<http://sweetontology.net/reprTimeDay/Evening> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://sweetontology.net/reprTimeDay/Nighttime> ;
                                               rdfs:comment "period of the day from 6:00 PM until nighttime"@en ;
                                               rdfs:label "evening"@en .


###  http://sweetontology.net/reprTimeDay/Midnight
<http://sweetontology.net/reprTimeDay/Midnight> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://www.w3.org/2006/time#Instant> ;
                                                rdfs:comment "12 o'clock at night; transition time period from one day to the next"@en ;
                                                rdfs:label "midnight"@en .


###  http://sweetontology.net/reprTimeDay/Morning
<http://sweetontology.net/reprTimeDay/Morning> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://sweetontology.net/reprTimeDay/Daytime> ;
                                               rdfs:comment "morning is the period of time from sunrise to noon."@en ;
                                               rdfs:label "morning"@en .


###  http://sweetontology.net/reprTimeDay/Nighttime
<http://sweetontology.net/reprTimeDay/Nighttime> rdf:type owl:Class ;
                                                 owl:equivalentClass <http://sweetontology.net/reprTimeDay/Nocturnal> ;
                                                 rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> ;
                                                 rdfs:label "nighttime"@en .


###  http://sweetontology.net/reprTimeDay/Nocturnal
<http://sweetontology.net/reprTimeDay/Nocturnal> rdf:type owl:Class ;
                                                 rdfs:comment "instrument used to determine the local time based on the relative positions of two or more stars in the night sky"@en ;
                                                 rdfs:label "nocturnal"@en .


###  http://sweetontology.net/reprTimeDay/Noon
<http://sweetontology.net/reprTimeDay/Noon> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://www.w3.org/2006/time#Instant> ;
                                            rdfs:comment "12 o'clock in the daytime"@en ;
                                            rdfs:label "noon"@en .


###  http://sweetontology.net/reprTimeDay/Sunrise
<http://sweetontology.net/reprTimeDay/Sunrise> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://www.w3.org/2006/time#Instant> ,
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <http://sweetontology.net/relaTime/startOf> ;
                                                                 owl:someValuesFrom <http://sweetontology.net/reprTimeDay/Morning>
                                                               ] ;
                                               rdfs:comment "instant at which the upper edge of the Sun appears over the eastern horizon in the morning"@en ;
                                               rdfs:label "sunrise"@en .


###  http://sweetontology.net/reprTimeDay/Sunset
<http://sweetontology.net/reprTimeDay/Sunset> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://www.w3.org/2006/time#Instant> ,
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty <http://sweetontology.net/relaTime/startOf> ;
                                                                owl:someValuesFrom <http://sweetontology.net/reprTimeDay/Evening>
                                                              ] ;
                                              rdfs:comment "daily disappearance of the Sun below the western half of the horizon"@en ;
                                              rdfs:label "sunset"@en .


###  http://sweetontology.net/reprTimeDay/Twilight
<http://sweetontology.net/reprTimeDay/Twilight> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://www.w3.org/2006/time#ProperInterval> ;
                                                rdfs:comment "illumination of the Earth's lower atmosphere when the Sun itself is not directly visible because it is below the horizon"@en ;
                                                rdfs:label "twilight"@en .


