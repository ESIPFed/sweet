<http://sweetontology.net/realmSoil> rdf:type owl:Ontology ;
                                      owl:imports <http://sweetontology.net/phenMixing> ,
                                                  <http://sweetontology.net/propCategorical> ,
                                                  <http://sweetontology.net/realm> ,
