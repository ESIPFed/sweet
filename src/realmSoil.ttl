###  http://sweetontology.net/realmSoil/Spodosol
sopropcat:Spodosol rdf:type owl:Class ;
              rdfs:subClassOf sopropcat:SoilOrder ;
              rdfs:label "spodosol"@en .


###  http://sweetontology.net/realmSoil/Subsoil
sopropcat:Subsoil rdf:type owl:Class ;
             rdfs:subClassOf sopropcat:SoilLayer ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty sorelsp:above ;
                               owl:someValuesFrom sopropcat:ParentRock
                             ] ;
             rdfs:label "subsoil"@en .


###  http://sweetontology.net/realmSoil/Substrata
sopropcat:Substrata rdf:type owl:Class ;
