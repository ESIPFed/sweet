#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( sostb:Abiotic
                        sostb:Biotic
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( sostb:Anthropogenic
                        sostb:Biogenic
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( sostb:Dead
                        sostb:Live
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( sostb:Live
                        sostb:Nonliving
                      )
] .


