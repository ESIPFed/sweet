                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty sorelm:hasSecondOperand ;
                                            owl:allValuesFrom sopropt:DewPointTemperature
                                          ] ;
                          rdfs:label "dew point depression"@en .


