somarocki:Kimberlite rdf:type owl:Class ;
                     rdfs:subClassOf somarocki:IgneousRock ;
                     rdfs:label "kimberlite"@en ;
                     skos:scopeNote "A porphyritic alkalic peridotite containing abundant phenocrysts of olivine and phlogopite, and possibly geikielite and chromian pyrope, in a fine-grained groundmass of calcite and second-generation olivine and phlogopite..."@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource somarocki:Kimberlite ;
   owl:annotatedProperty skos:scopeNote ;
   owl:annotatedTarget "A porphyritic alkalic peridotite containing abundant phenocrysts of olivine and phlogopite, and possibly geikielite and chromian pyrope, in a fine-grained groundmass of calcite and second-generation olivine and phlogopite..."@en ;
   dcterms:creator <https://orcid.org/0000-0002-0337-8610> ;
