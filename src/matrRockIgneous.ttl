#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/matrRockIgneous/Aa
somarocki:Aa rdf:type owl:Class ;
             rdfs:subClassOf somarocki:ExtrusiveRock ,
                             somarocki:Lava ;
             rdfs:label "aa"@en .


###  http://sweetontology.net/matrRockIgneous/Andesite
somarocki:Andesite rdf:type owl:Class ;
                   rdfs:subClassOf somarocki:VolcanicRock ;
                   rdfs:label "andesite"@en ;
                   skos:scopeNote "A volcanic rock defined modally by Q/(Q+A+P) < 20% or F/(F+A+P) < 10%, P/(A+P) > 90%, and M < 35"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource somarocki:Andesite ;
   owl:annotatedProperty skos:scopeNote ;
   owl:annotatedTarget "A volcanic rock defined modally by Q/(Q+A+P) < 20% or F/(F+A+P) < 10%, P/(A+P) > 90%, and M < 35"@en ;
   dcterms:creator <https://orcid.org/0000-0002-0337-8610> ;
   dcterms:source <https://apps.usgs.gov/thesaurus/term-simple.php?thcode=4&code=3.5.2> ,
                  "USGS Lithology terms--https://apps.usgs.gov/thesaurus/thesaurus-full.php?thcode=4--https://apps.usgs.gov/thesaurus/"@en
 ] .


###  http://sweetontology.net/matrRockIgneous/AnorogenicIgneousRock
somarocki:AnorogenicIgneousRock rdf:type owl:Class ;
                                rdfs:subClassOf somarocki:IntrusiveRock ;
                                rdfs:label "anorogenic igneous rock"@en .


###  http://sweetontology.net/matrRockIgneous/Anorthosite
somarocki:Anorthosite rdf:type owl:Class ;
                      rdfs:subClassOf somarocki:IgneousRock ;
                      rdfs:label "anorthosite"@en ;
                      skos:scopeNote "A plutonic rock defined in the QAPF diagram as having Q between 0 and 5, P/(A+P) greater than 90, and M less than 10. A group of monomineralogic plutonic igneous rocks composed almost entirely of plagioclase feldspar..."@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource somarocki:Anorthosite ;
   owl:annotatedProperty skos:scopeNote ;
   owl:annotatedTarget "A plutonic rock defined in the QAPF diagram as having Q between 0 and 5, P/(A+P) greater than 90, and M less than 10. A group of monomineralogic plutonic igneous rocks composed almost entirely of plagioclase feldspar..."@en ;
   dcterms:creator <https://orcid.org/0000-0002-0337-8610> ;
   dcterms:source <https://apps.usgs.gov/thesaurus/term-simple.php?thcode=4&code=4.5.5> ,
                  "USGS Lithology terms--https://apps.usgs.gov/thesaurus/thesaurus-full.php?thcode=4--https://apps.usgs.gov/thesaurus/"@en
 ] .


###  http://sweetontology.net/matrRockIgneous/Aphanite
somarocki:Aphanite rdf:type owl:Class ;
                   rdfs:subClassOf somarocki:IgneousRock ;
                   rdfs:label "aphanite"@en .


