                                 owl:onProperty sorelsp:hasGeometricalObject ;
                                 owl:allValuesFrom sorepsg3:Cone
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty sorelsc:hasDefaultUnit ;
                                 owl:hasValue sorepsu:steradian
                               ] ;
               rdfs:label "solid angle"@en .


