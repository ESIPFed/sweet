###  http://sweetontology.net/reprTimeSeason/ColdestMonth
:ColdestMonth rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/propTime/Time> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaMath/hasMinimum> ;
                                owl:someValuesFrom <http://sweetontology.net/propTemperature/Temperature>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaSci/hasUnit> ;
                                owl:hasValue <http://sweetontology.net/reprSciUnits/month>
                              ] ;
              rdfs:label "coldest month"@en .


###  http://sweetontology.net/reprTimeSeason/DrySeason
:DrySeason rdf:type owl:Class ;
           rdfs:subClassOf :Season ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/rela/hasState> ;
                             owl:hasValue <http://sweetontology.net/statePhysical/Dry>
                           ] ;
           rdfs:label "dry season"@en ;
           skos:definition _:genid4 .

