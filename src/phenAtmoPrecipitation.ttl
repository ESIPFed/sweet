                              owl:allValuesFrom somaw:Ice
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty sorelsc:hasImpact ;
                              owl:hasValue sostri:Heavy
                            ] ;
            rdfs:label "hail"@en .


