###  http://sweetontology.net/propOrdinal/TemperatureRange
<http://sweetontology.net/propOrdinal/TemperatureRange> rdf:type owl:Class .


###  http://sweetontology.net/stateThermodynamic/ThermodynamicState
:ThermodynamicState rdf:type owl:Class ;
                    rdfs:subClassOf <http://sweetontology.net/statePhysical/PhysicalState> ;
                    rdfs:label "thermodynamic state"@en ;
                    skos:definition _:genid1 .

_:genid1 dcterms:created "2020-07-23T09:24:50.29-07:00"^^xsd:dateTimeStamp ;
          dcterms:creator <https://orcid.org/0000-0003-2185-928X> ;
          dcterms:source <http://www.wikidata.org/entity/Q230875> ;
          rdfs:comment "state of a thermodynamic system at some point in time; variables/properties comprising the state"@en .


#################################################################
#    Individuals
#################################################################

###  http://sweetontology.net/stateThermodynamic/Adiabatic
:Adiabatic rdf:type owl:NamedIndividual ,
                    :ThermodynamicState ;
           rdfs:label "adiabatic"@en .


###  http://sweetontology.net/stateThermodynamic/BlackBody
:BlackBody rdf:type owl:NamedIndividual ,
                    :ThermodynamicState ;
           rdfs:label "black body"@en .


###  http://sweetontology.net/stateThermodynamic/BoilingHot
:BoilingHot rdf:type owl:NamedIndividual ,
                     <http://sweetontology.net/propOrdinal/TemperatureRange> ;
            <http://sweetontology.net/relaPhysical/warmerThan> :Warm ;
            rdfs:label "boiling hot"@en .


###  http://sweetontology.net/stateThermodynamic/Cold
:Cold rdf:type owl:NamedIndividual ,
               <http://sweetontology.net/propOrdinal/TemperatureRange> ;
      rdfs:label "cold"@en .


###  http://sweetontology.net/stateThermodynamic/Cool
:Cool rdf:type owl:NamedIndividual ,
               <http://sweetontology.net/propOrdinal/TemperatureRange> ;
      <http://sweetontology.net/relaPhysical/warmerThan> :Cold ,
                                                         :FreezingCold ;
      rdfs:label "cool"@en .


###  http://sweetontology.net/stateThermodynamic/Diabatic
:Diabatic rdf:type owl:NamedIndividual ,
                   :ThermodynamicState ;
          rdfs:label "diabatic"@en .


###  http://sweetontology.net/stateThermodynamic/DryAdiabat
:DryAdiabat rdf:type owl:NamedIndividual ,
                     :ThermodynamicState ;
            owl:sameAs :DryAdiabatic ;
            rdfs:label "dry adiabat"@en .


###  http://sweetontology.net/stateThermodynamic/DryAdiabatic
:DryAdiabatic rdf:type owl:NamedIndividual ,
                       :ThermodynamicState ;
              rdfs:label "dry adiabatic"@en .


###  http://sweetontology.net/stateThermodynamic/FreezingCold
:FreezingCold rdf:type owl:NamedIndividual ,
                       <http://sweetontology.net/propOrdinal/TemperatureRange> ;
              rdfs:label "freezing cold"@en .


###  http://sweetontology.net/stateThermodynamic/Hot
:Hot rdf:type owl:NamedIndividual ,
              <http://sweetontology.net/propOrdinal/TemperatureRange> ;
     <http://sweetontology.net/relaPhysical/warmerThan> :Warm ;
     rdfs:label "hot"@en .


###  http://sweetontology.net/stateThermodynamic/Inversion
:Inversion rdf:type owl:NamedIndividual ,
                    :ThermodynamicState ;
           rdfs:label "inversion"@en .


###  http://sweetontology.net/stateThermodynamic/Isentropic
:Isentropic rdf:type owl:NamedIndividual ,
                     :ThermodynamicState ;
            rdfs:label "isentropic"@en .


###  http://sweetontology.net/stateThermodynamic/Isothermal
:Isothermal rdf:type owl:NamedIndividual ,
                     :ThermodynamicState ;
            rdfs:label "isothermal"@en .


###  http://sweetontology.net/stateThermodynamic/LTE
:LTE rdf:type owl:NamedIndividual ,
              :ThermodynamicState ;
     owl:sameAs :LocalThermodynamicEquilibrium ;
     rdfs:label "lte"@en .


###  http://sweetontology.net/stateThermodynamic/LocalThermodynamicEquilibrium
:LocalThermodynamicEquilibrium rdf:type owl:NamedIndividual ,
                                        <http://sweetontology.net/stateSystem/EquilibriumState> ,
                                        :ThermodynamicState ;
                               rdfs:label "local thermodynamic equilibrium"@en .


###  http://sweetontology.net/stateThermodynamic/Lukewarm
:Lukewarm rdf:type owl:NamedIndividual ,
                   <http://sweetontology.net/propOrdinal/TemperatureRange> ;
          owl:sameAs :Tepid ;
          <http://sweetontology.net/relaPhysical/warmerThan> :Cool ;
          rdfs:label "lukewarm"@en .


###  http://sweetontology.net/stateThermodynamic/MaximumEntropy
:MaximumEntropy rdf:type owl:NamedIndividual ,
                         <http://sweetontology.net/stateSystem/EquilibriumState> ,
                         :ThermodynamicState ;
                rdfs:label "maximum entropy"@en .


###  http://sweetontology.net/stateThermodynamic/Pseudoadiabatic
:Pseudoadiabatic rdf:type owl:NamedIndividual ,
                          :ThermodynamicState ;
                 <http://sweetontology.net/relaMath/approximates> :Adiabatic ;
                 rdfs:label "pseudoadiabatic"@en .


###  http://sweetontology.net/stateThermodynamic/Superadiabatic
:Superadiabatic rdf:type owl:NamedIndividual ,
                         :ThermodynamicState ;
                rdfs:label "superadiabatic"@en .


###  http://sweetontology.net/stateThermodynamic/Tepid
:Tepid rdf:type owl:NamedIndividual ,
                <http://sweetontology.net/propOrdinal/TemperatureRange> ;
       rdfs:label "tepid"@en .


###  http://sweetontology.net/stateThermodynamic/ThermodynamicEquilibrium
:ThermodynamicEquilibrium rdf:type owl:NamedIndividual ,
                                   <http://sweetontology.net/stateSystem/EquilibriumState> ,
                                   :ThermodynamicState ;
                          rdfs:label "thermodynamic equilibrium"@en .


###  http://sweetontology.net/stateThermodynamic/Warm
:Warm rdf:type owl:NamedIndividual ,
               <http://sweetontology.net/propOrdinal/TemperatureRange> ;
      <http://sweetontology.net/relaPhysical/warmerThan> :Lukewarm ;
      rdfs:label "warm"@en .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Adiabatic
                        :Diabatic
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Adiabatic
                        :Superadiabatic
                      )
] .


