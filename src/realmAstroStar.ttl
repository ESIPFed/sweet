###  http://sweetontology.net/realmAstroStar/ActiveRegion
:ActiveRegion rdf:type owl:Class ;
              rdfs:subClassOf :StellarRealm ;
              rdfs:label "active region"@en .


###  http://sweetontology.net/realmAstroStar/Chromosphere
:Chromosphere rdf:type owl:Class ;
              rdfs:subClassOf :StellarAtmosphere ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                owl:hasValue <http://sweetontology.net/statePhysical/Plasma>
                              ] ;
              rdfs:label "chromosphere"@en ;
              skos:definition _:genid2 .

_:genid2 dcterms:created "2020-07-23T08:51:25.94-07:00"^^xsd:dateTimeStamp ;
          dcterms:creator <https://orcid.org/0000-0003-2185-928X> ;
          dcterms:source <http://www.wikidata.org/entity/Q190003> ;
          rdfs:comment "2nd of the 3 main layers in the Sun's atmosphere (above the photosphere, below the solar transition region); 3000â€“5000 km deep, with a rosy red color only apparent during eclipses"@en .


###  http://sweetontology.net/realmAstroStar/ConvectionZone
:ConvectionZone rdf:type owl:Class ;
                rdfs:subClassOf :StellarInterior ;
                owl:disjointWith :StellarCore ;
                rdfs:label "convection zone"@en ;
                skos:definition _:genid3 .

_:genid3 dcterms:created "2020-07-23T08:51:26.132-07:00"^^xsd:dateTimeStamp ;
          dcterms:creator <https://orcid.org/0000-0003-2185-928X> ;
          dcterms:source <http://www.wikidata.org/entity/Q128034> ;
          rdfs:comment "(of a star) layer which is unstable to convection. Energy is primarily or partially transported by convection in such a region"@en .


###  http://sweetontology.net/realmAstroStar/Corona
:Corona rdf:type owl:Class ;
        rdfs:subClassOf :StellarAtmosphere ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/rela/hasState> ;
                          owl:hasValue <http://sweetontology.net/statePhysical/Plasma>
                        ] ;
        rdfs:label "corona"@en ;
        skos:definition _:genid5 .

_:genid5 dcterms:created "2020-07-23T08:51:26.233-07:00"^^xsd:dateTimeStamp ;
          dcterms:creator <https://orcid.org/0000-0003-2185-928X> ;
          dcterms:source <http://www.wikidata.org/entity/Q170754> ;
          rdfs:comment "type of plasma \"atmosphere\" of the Sun or other celestial body"@en .


###  http://sweetontology.net/realmAstroStar/Photosphere
:Photosphere rdf:type owl:Class ;
             rdfs:subClassOf :StellarAtmosphere ;
             rdfs:label "photosphere"@en ;
