<http://sweetontology.net/reprSciModel> rdf:type owl:Ontology ;
                                         owl:imports <http://sweetontology.net/humanResearch> ,
                                                     <http://sweetontology.net/phenAtmo> ,
                                                     <http://sweetontology.net/rela> ,
                                                     <http://sweetontology.net/repr> ;
