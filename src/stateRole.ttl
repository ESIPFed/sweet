#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( sostro:Desirable
                        sostro:Undesirable
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( sostro:Negative
                        sostro:Positive
                      )
] .


