#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( sfreq:Cyclic
                        sfreq:Secular
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( sfreq:Interrupted
                        sfreq:Uninterrupted
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( sfreq:NonStationary
                        sfreq:Stationary
                      )
] .


