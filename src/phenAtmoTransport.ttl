###  http://sweetontology.net/phenAtmoTransport/AcidFog
:AcidFog rdf:type owl:Class ;
         owl:equivalentClass :AcidHaze ;
         rdfs:subClassOf <http://sweetontology.net/phenAtmoFog/Fog> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/relaChemical/hasSubstance> ;
                           owl:hasValue <http://sweetontology.net/stateChemical/Acid>
                         ] ;
         rdfs:label "acid fog"@en .


###  http://sweetontology.net/phenAtmoTransport/AcidHaze
:AcidHaze rdf:type owl:Class ;
          rdfs:label "acid haze"@en .


###  http://sweetontology.net/phenAtmoTransport/AcidRain
:AcidRain rdf:type owl:Class ;
          rdfs:subClassOf :WetDeposition ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaChemical/hasSubstance> ;
                            owl:hasValue <http://sweetontology.net/stateChemical/Acid>
                          ] ;
          rdfs:label "acid rain"@en ;
          skos:definition _:genid3 .

_:genid3 dcterms:created "2020-07-18T17:37:37.020"^^xsd:dateTime ;
          dcterms:creator <https://orcid.org/0000-0003-2185-928X> ;
          dcterms:source <http://www.wikidata.org/entity/Q40178> ;
          rdfs:comment "rain that is unusually acidic"@en .


###  http://sweetontology.net/phenAtmoTransport/MixedLayerVenting
:MixedLayerVenting rdf:type owl:Class ;
                   rdfs:subClassOf <http://sweetontology.net/phenSystem/Removal> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                     owl:allValuesFrom <http://sweetontology.net/realmAtmoBoundaryLayer/MixedLayer>
                                   ] ;
                   rdfs:label "mixed layer venting"@en .


###  http://sweetontology.net/phenAtmoTransport/NucleationScavenging
:NucleationScavenging rdf:type owl:Class ;
                      rdfs:subClassOf :ScavengingByPrecipitation ;
                      rdfs:label "nucleation scavenging"@en .


###  http://sweetontology.net/phenAtmoTransport/Rainout
:Rainout rdf:type owl:Class ;
         owl:equivalentClass :Washout ;
         rdfs:subClassOf :ScavengingByPrecipitation ;
         rdfs:label "rainout"@en ;
         skos:definition _:genid5 .

_:genid5 dcterms:created "2020-07-18T17:37:37.271"^^xsd:dateTime ;
          dcterms:creator <https://orcid.org/0000-0003-2185-928X> ;
          dcterms:source <http://www.wikidata.org/entity/Q7284937> ;
          rdfs:comment "process of precipitation causing the removal of radioactive particles from the atmosphere onto the ground"@en .


###  http://sweetontology.net/phenAtmoTransport/ScavengingByPrecipitation
:ScavengingByPrecipitation rdf:type owl:Class ;
                           rdfs:subClassOf :WetDeposition ;
                           rdfs:label "scavenging by precipitation"@en .


###  http://sweetontology.net/phenAtmoTransport/Washout
:Washout rdf:type owl:Class ;
         rdfs:label "washout"@en ;
         skos:definition _:genid6 .

_:genid6 dcterms:created "2020-07-18T17:37:37.460"^^xsd:dateTime ;
          dcterms:creator <https://orcid.org/0000-0003-2185-928X> ;
          dcterms:source <http://www.wikidata.org/entity/Q4896442> ;
          rdfs:comment "sudden erosion (usually by water) of either natural terrain or man-made objects (such as roads and railroads)"@en .


###  http://sweetontology.net/phenAtmoTransport/WetDeposition
:WetDeposition rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/phen/AtmosphereLandExchange> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                 owl:allValuesFrom <http://sweetontology.net/phen/Precipitation>
                               ] ;
               rdfs:label "wet deposition"@en .


