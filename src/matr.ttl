<http://sweetontology.net/matr> rdf:type owl:Ontology ;
                                 owl:imports <http://sweetontology.net/rela> ,
                                             <http://sweetontology.net/state> ,
                                             <http://sweetontology.net/statePhysical> ,
                                             <http://sweetontology.net/stateRole> ;
