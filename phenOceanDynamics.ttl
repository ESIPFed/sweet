#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/phenOceanDynamics/BoundaryCurrent
:BoundaryCurrent rdf:type owl:Class ;
                 rdfs:subClassOf :OceanCurrent .


###  http://sweetontology.net/phenOceanDynamics/CircumpolarCurrent
:CircumpolarCurrent rdf:type owl:Class ;
                    rdfs:subClassOf :OceanCurrent .


###  http://sweetontology.net/phenOceanDynamics/ColdCoreCell
:ColdCoreCell rdf:type owl:Class ;
              rdfs:subClassOf :OceanGyre ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaPhysical/hasTemperatureRange> ;
                                owl:hasValue <http://sweetontology.net/stateThermodynamic/Cool>
                              ] .


###  http://sweetontology.net/phenOceanDynamics/Downwelling
:Downwelling rdf:type owl:Class ;
             rdfs:subClassOf :OceanCirculation ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaSpace/hasDirection> ;
                               owl:hasValue <http://sweetontology.net/reprSpaceDirection/Down>
                             ] .


###  http://sweetontology.net/phenOceanDynamics/EasternBoundaryCurrent
:EasternBoundaryCurrent rdf:type owl:Class ;
                        rdfs:subClassOf :BoundaryCurrent .


###  http://sweetontology.net/phenOceanDynamics/LangmuirCirculation
:LangmuirCirculation rdf:type owl:Class ;
                     rdfs:subClassOf :OceanCirculation ;
                     rdfs:comment "Roll circulations approximately aligned with the surface stress vector that frequently occur in the upper boundary layer of oceans or lakes. Although similar in form to atmospheric longitudinal roll vortices, Langmuir circulations are thought to be driven by nonlinear interactions between the surface gravity wave field and the larger-scale turbulent motions within the mixed layer. They are sometimes called windrows because they form lines of surface debris or bubbles in their surface convergence zones. Their spatial scale is related to the depth of the mixed layer and their characteristic velocity is on the order of 8u*, where u* is the friction velocity in water. As a result of this scaling, Langmuir circulations generally require surface winds of at least 8 m s-1 in order to form. See coherent structures, longitudinal rolls."@en .


###  http://sweetontology.net/phenOceanDynamics/LittoralCurrent
:LittoralCurrent rdf:type owl:Class ;
                 owl:equivalentClass :LongshoreCurrent .


###  http://sweetontology.net/phenOceanDynamics/LongshoreCurrent
:LongshoreCurrent rdf:type owl:Class ;
                  rdfs:subClassOf :OceanCurrent ;
                  rdfs:comment "(Also called littoral current.) The resultant current produced by waves being deflected at an angle by the shore. In this case the current runs roughly parallel to the shoreline. The longshore current is capable of carrying a certain amount of material as long as its velocity remains fairly constant; however, any obstruction, such as a submarine rock ridge or a land point cutting across the path of the current, will cause loss of velocity and consequent loss of carrying power."@en .


###  http://sweetontology.net/phenOceanDynamics/LoopCurrent
:LoopCurrent rdf:type owl:Class ;
             rdfs:subClassOf :OceanCurrent .


###  http://sweetontology.net/phenOceanDynamics/MesoscaleEddy
:MesoscaleEddy rdf:type owl:Class ;
               owl:equivalentClass :ModeEddy ;
               rdfs:subClassOf <http://sweetontology.net/phenFluidDynamics/Eddy> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaSpace/hasSpatialScale> ;
                                 owl:hasValue <http://sweetontology.net/stateSpaceScale/Mesoscale>
                               ] ;
               rdfs:comment "In oceanography, densely packed, irregularly oval- shaped high and low pressure centers roughly 400 km (240 miles) in diameter in which current intensities are typically tenfold greater than the local means."@en .


###  http://sweetontology.net/phenOceanDynamics/ModeEddy
:ModeEddy rdf:type owl:Class .


###  http://sweetontology.net/phenOceanDynamics/OceanCirculation
:OceanCirculation rdf:type owl:Class ;
                  rdfs:subClassOf <http://sweetontology.net/phenFluidDynamics/Circulation> ,
                                  <http://sweetontology.net/phenOcean/OceanPhenomena> .


###  http://sweetontology.net/phenOceanDynamics/OceanConveyorBelt
:OceanConveyorBelt rdf:type owl:Class ;
                   rdfs:subClassOf :OceanCirculation ;
                   rdfs:comment "The global recirculation of water masses that determines today's climate."@en .


###  http://sweetontology.net/phenOceanDynamics/OceanCurrent
:OceanCurrent rdf:type owl:Class ;
              rdfs:subClassOf :OceanCirculation .


###  http://sweetontology.net/phenOceanDynamics/OceanGyre
:OceanGyre rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/phenFluidDynamics/Gyre> ,
                           <http://sweetontology.net/phenOcean/OceanPhenomena> .


###  http://sweetontology.net/phenOceanDynamics/OceanMixing
:OceanMixing rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/phenMixing/Mixing> ,
                             <http://sweetontology.net/phenOcean/OceanPhenomena> ;
             rdfs:comment "Any process or series of processes by which parcels of ocean water with different properties are brought into intimate small-scale contact, so that molecular diffusion erases the differences between them."@en .


###  http://sweetontology.net/phenOceanDynamics/SecondaryCirculation
:SecondaryCirculation rdf:type owl:Class ;
                      rdfs:subClassOf :OceanCirculation .


###  http://sweetontology.net/phenOceanDynamics/ThermohalineCirculation
:ThermohalineCirculation rdf:type owl:Class ;
                         rdfs:subClassOf :OceanCirculation .


###  http://sweetontology.net/phenOceanDynamics/TidalCurrent
:TidalCurrent rdf:type owl:Class ;
              rdfs:subClassOf :OceanCurrent ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                owl:allValuesFrom <http://sweetontology.net/phenOceanCoastal/OceanTide>
                              ] .


###  http://sweetontology.net/phenOceanDynamics/TidalWave
:TidalWave rdf:type owl:Class ;
           owl:equivalentClass :Tsunami .


###  http://sweetontology.net/phenOceanDynamics/Tsunami
:Tsunami rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/phenGeolSeismicity/SeismicWave> ,
                         <http://sweetontology.net/phenOcean/OceanPhenomena> ;
         rdfs:comment "Waves generated by seismic activity."@en .


###  http://sweetontology.net/phenOceanDynamics/TurbidityCurrent
:TurbidityCurrent rdf:type owl:Class ;
                  rdfs:subClassOf :OceanCurrent .


###  http://sweetontology.net/phenOceanDynamics/Upwelling
:Upwelling rdf:type owl:Class ;
           rdfs:subClassOf :OceanCirculation ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaSpace/hasDirection> ;
                             owl:hasValue <http://sweetontology.net/reprSpaceDirection/Up>
                           ] ;
           rdfs:comment "An ascending motion of subsurface water by which water from deeper layers is brought into the surface layer and is removed from the area of upwelling by divergent horizontal flow."@en .


###  http://sweetontology.net/phenOceanDynamics/WarmCoreCell
:WarmCoreCell rdf:type owl:Class ;
              rdfs:subClassOf :OceanGyre ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaPhysical/hasTemperatureRange> ;
                                owl:hasValue <http://sweetontology.net/stateThermodynamic/Warm>
                              ] .


###  http://sweetontology.net/phenOceanDynamics/WesternBoundaryCurrent
:WesternBoundaryCurrent rdf:type owl:Class ;
                        rdfs:subClassOf :BoundaryCurrent .


###  http://sweetontology.net/phenOceanDynamics/WesternIntensification
:WesternIntensification rdf:type owl:Class ;
                        rdfs:subClassOf :BoundaryCurrent .


###  http://sweetontology.net/phenOceanDynamics/WindDrivenCirculation
:WindDrivenCirculation rdf:type owl:Class ;
                       rdfs:subClassOf :OceanCirculation .


