@prefix : <http://sweetontology.net/reprMath/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/reprMath> .

<http://sweetontology.net/reprMath> rdf:type owl:Ontology ;
                                     owl:imports <http://sweetontology.net/relaMath> ,
                                                 <http://sweetontology.net/repr> ;
                                     rdfs:label "SWEET Ontology" ;
                                     owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/reprMath/Array
:Array rdf:type owl:Class ;
       rdfs:subClassOf :Seq .


###  http://sweetontology.net/reprMath/Bag
:Bag rdf:type owl:Class ;
     owl:equivalentClass :Multiset ;
     rdfs:subClassOf :Container .


###  http://sweetontology.net/reprMath/Collection
:Collection rdf:type owl:Class ;
            rdfs:subClassOf :NumericalEntity .


###  http://sweetontology.net/reprMath/Constant
:Constant rdf:type owl:Class ;
          rdfs:subClassOf :NumericalEntity .


###  http://sweetontology.net/reprMath/Container
:Container rdf:type owl:Class ;
           rdfs:subClassOf :NumericalEntity .


###  http://sweetontology.net/reprMath/Equation
:Equation rdf:type owl:Class ;
          rdfs:subClassOf :Relation ;
          rdfs:comment "An equation is a mathematical statement, in symbols, that two things are exactly the same (or equivalent). [Wikipedia]"@en .


###  http://sweetontology.net/reprMath/Exponent
:Exponent rdf:type owl:Class ;
          rdfs:subClassOf :NumericalEntity .


###  http://sweetontology.net/reprMath/FuzzyPoint
:FuzzyPoint rdf:type owl:Class ;
            rdfs:subClassOf :Neighborhood .


###  http://sweetontology.net/reprMath/FuzzySet
:FuzzySet rdf:type owl:Class ;
          rdfs:subClassOf :NumericalEntity .


###  http://sweetontology.net/reprMath/Inequality
:Inequality rdf:type owl:Class ;
            rdfs:subClassOf :Relation .


###  http://sweetontology.net/reprMath/Interval
:Interval rdf:type owl:Class ;
          rdfs:subClassOf :NumericalEntity ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaMath/hasDimension> ;
                            owl:hasValue 1
                          ] ;
          owl:disjointWith :Point .


###  http://sweetontology.net/reprMath/Matrix
:Matrix rdf:type owl:Class ;
        rdfs:subClassOf :Seq ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/relaMath/eachElementHasType> ;
                          owl:allValuesFrom :Array
                        ] .


###  http://sweetontology.net/reprMath/Multiset
:Multiset rdf:type owl:Class .


###  http://sweetontology.net/reprMath/Neighborhood
:Neighborhood rdf:type owl:Class ;
              rdfs:subClassOf :NumericalEntity .


###  http://sweetontology.net/reprMath/NumericalEntity
:NumericalEntity rdf:type owl:Class ;
                 rdfs:subClassOf <http://sweetontology.net/repr/Representation> .


###  http://sweetontology.net/reprMath/Order
:Order rdf:type owl:Class ;
       owl:equivalentClass :Seq .


###  http://sweetontology.net/reprMath/OrderedPair
:OrderedPair rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/repr/2D> ,
                             :Array ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaMath/hasFirstOperand> ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/relaMath/hasSecondOperand> ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] .


###  http://sweetontology.net/reprMath/OrderedTriple
:OrderedTriple rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/repr/3D> ,
                               :Array ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaMath/hasFirstOperand> ;
                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaMath/hasSecondOperand> ;
                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://sweetontology.net/relaMath/hasThirdOperand> ;
                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                               ] .


###  http://sweetontology.net/reprMath/Point
:Point rdf:type owl:Class ;
       rdfs:subClassOf :NumericalEntity ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://sweetontology.net/relaMath/hasDimension> ;
                         owl:hasValue 0
                       ] .


###  http://sweetontology.net/reprMath/Relation
:Relation rdf:type owl:Class ;
          rdfs:subClassOf :NumericalEntity ;
          rdfs:comment "In mathematics, the concept of a relation is a generalization of 2-place relations, such as the relation of equality, less than, greater than, etc.. [Wikipedia]"@en .


###  http://sweetontology.net/reprMath/Scalar
:Scalar rdf:type owl:Class ;
        rdfs:subClassOf :NumericalEntity ;
        owl:disjointWith :Vector .


###  http://sweetontology.net/reprMath/Seq
:Seq rdf:type owl:Class ;
     owl:equivalentClass :Sequence ;
     rdfs:subClassOf :Container .


###  http://sweetontology.net/reprMath/Sequence
:Sequence rdf:type owl:Class .


###  http://sweetontology.net/reprMath/Set
:Set rdf:type owl:Class ;
     rdfs:subClassOf :Bag .


###  http://sweetontology.net/reprMath/Tensor
:Tensor rdf:type owl:Class ;
        rdfs:subClassOf :Seq ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://sweetontology.net/relaMath/eachElementHasType> ;
                          owl:allValuesFrom :Matrix
                        ] .


###  http://sweetontology.net/reprMath/UnorderedPair
:UnorderedPair rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/repr/2D> ,
                               :Bag .


###  http://sweetontology.net/reprMath/Variable
:Variable rdf:type owl:Class ;
          rdfs:subClassOf :NumericalEntity .


###  http://sweetontology.net/reprMath/Vector
:Vector rdf:type owl:Class ;
        rdfs:subClassOf :Array ;
        rdfs:comment "Any quantity, such as force, velocity, or acceleration, that has both magnitude and direction at each point in space, as opposed to a scalar that has magnitude only."@en .


###  http://sweetontology.net/reprMath/Vector_3D
:Vector_3D rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/repr/3D> ,
                           :Vector .


###  http://sweetontology.net/reprMath/Window
:Window rdf:type owl:Class ;
        rdfs:subClassOf :Interval .


#################################################################
#    Individuals
#################################################################

###  http://sweetontology.net/reprMath/NormalizedRange
:NormalizedRange rdf:type owl:NamedIndividual ,
                          :Interval ;
                 <http://sweetontology.net/relaMath/hasLowerBound> "0.0"^^xsd:double ;
                 <http://sweetontology.net/relaMath/hasUpperBound> "1.0"^^xsd:double .


