#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/phenFluidInstability/AbsoluteInstability
:AbsoluteInstability rdf:type owl:Class ;
                     rdfs:subClassOf :FluidInstability ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://sweetontology.net/rela/hasState> ;
                                       owl:hasValue <http://sweetontology.net/stateThermodynamic/Superadiabatic>
                                     ] ;
                     rdfs:comment "state of a layer of atmosphere when it has superadiabatic lapse rate of temperature" ;
                     rdfs:label "Absolute Instability" .


###  http://sweetontology.net/phenFluidInstability/ConditionalInstability
:ConditionalInstability rdf:type owl:Class ;
                        rdfs:subClassOf :FluidInstability ;
                        rdfs:comment "The state of a layer of unsaturated air when its lapse rate of temperature is less than the dry-adiabatic lapse rate but greater than the moist-adiabatic lapse rate."@en .


###  http://sweetontology.net/phenFluidInstability/ConvectiveInstability
:ConvectiveInstability rdf:type owl:Class ;
                       rdfs:subClassOf :FluidInstability ;
                       rdfs:comment "An instability due to the buoyancy force of heavy fluid over light fluid overcoming the stabilizing influence of viscous forces."@en .


###  http://sweetontology.net/phenFluidInstability/FluidInstability
:FluidInstability rdf:type owl:Class ;
                  rdfs:subClassOf <http://sweetontology.net/phenFluidDynamics/FluidPhenomena> ,
                                  <http://sweetontology.net/stateSystem/Instability> .


###  http://sweetontology.net/phenFluidInstability/FreeInstability
:FreeInstability rdf:type owl:Class ;
                 rdfs:subClassOf :FluidInstability ;
                 rdfs:comment "The state of a layer of unsaturated air when its lapse rate of temperature is less than the dry-adiabatic lapse rate but greater than the moist-adiabatic lapse rate."@en .


###  http://sweetontology.net/phenFluidInstability/FrontalInversion
:FrontalInversion rdf:type owl:Class ;
                  rdfs:subClassOf :Inversion ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                                    owl:allValuesFrom <http://sweetontology.net/phenAtmoFront/Front>
                                  ] ;
                  rdfs:comment "A temperature inversion (temperature rising with height) in the atmosphere, encountered upon vertical ascent through a sloping front (or frontal zone)."@en .


###  http://sweetontology.net/phenFluidInstability/Inversion
:Inversion rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/phenFluidDynamics/FluidPhenomena> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/relaSci/hasQuantity> ;
                             owl:allValuesFrom <http://sweetontology.net/propTemperatureGradient/TemperatureGradient>
                           ] ;
           rdfs:comment "In meteorology, a departure from the usual decrease or increase with altitude of the value of an atmospheric quantity; also, the layer through which this departure occurs (the inversion layer), or the lowest altitude at which the departure is found (the base of the inversion)."@en .


###  http://sweetontology.net/phenFluidInstability/KelvinHelmholtzInstability
:KelvinHelmholtzInstability rdf:type owl:Class ;
                            rdfs:subClassOf :FluidInstability ;
                            rdfs:comment "An instability of the basic flow of an incompressible inviscid fluid in two parallel infinite streams of different velocities and densities."@en .


###  http://sweetontology.net/phenFluidInstability/LargeReynoldsNumberFlow
:LargeReynoldsNumberFlow rdf:type owl:Class ;
                         rdfs:subClassOf <http://sweetontology.net/phenFluidDynamics/Flow> ;
                         rdfs:comment "The behavior of a fluid with a Reynolds number typically greater than 1E4 to 1E6, which usually occurs within the atmosphere. The main property of such flows is a constant friction stress within the surface layer that depends only on relative roughness but not on the Reynolds number itself. Thus, molecular viscosity and qualities occurring in flow descriptions that are dependent on the Reynolds number may be totally ignored."@en .


###  http://sweetontology.net/phenFluidInstability/Lift
:Lift rdf:type owl:Class ;
      owl:equivalentClass :Lifting ;
      rdfs:subClassOf <http://sweetontology.net/phenFluidDynamics/FluidPhenomena> .


###  http://sweetontology.net/phenFluidInstability/Lifting
:Lifting rdf:type owl:Class .


###  http://sweetontology.net/phenFluidInstability/MechanicalTurbulence
:MechanicalTurbulence rdf:type owl:Class ;
                      rdfs:subClassOf :Turbulence ;
                      rdfs:comment "Turbulence produced by shear flow."@en .


###  http://sweetontology.net/phenFluidInstability/RayleighTaylorInstability
:RayleighTaylorInstability rdf:type owl:Class ;
                           rdfs:subClassOf :FluidInstability .


###  http://sweetontology.net/phenFluidInstability/StaticStability
:StaticStability rdf:type owl:Class ;
                 rdfs:subClassOf :FluidInstability ;
                 rdfs:comment "The ability of a fluid at rest to become turbulent or laminar due to the effects of buoyancy."@en .


###  http://sweetontology.net/phenFluidInstability/TearingMode
:TearingMode rdf:type owl:Class ;
             rdfs:subClassOf <http://sweetontology.net/stateSystem/Instability> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://sweetontology.net/rela/hasState> ;
                               owl:hasValue <http://sweetontology.net/statePhysical/Plasma>
                             ] .


###  http://sweetontology.net/phenFluidInstability/Turbulence
:Turbulence rdf:type owl:Class ;
            rdfs:subClassOf <http://sweetontology.net/phenFluidDynamics/FluidPhenomena> ,
                            <http://sweetontology.net/phenSystemComplexity/Fluctuation> ;
            rdfs:comment "Irregular fluctuations occurring in fluid motions."@en .


###  http://sweetontology.net/phenFluidInstability/Wake
:Wake rdf:type owl:Class ;
      rdfs:subClassOf :Turbulence ;
      rdfs:comment "The region of turbulence immediately to the rear of a solid body in motion relative to a fluid"@en .


###  http://sweetontology.net/phenFluidInstability/ZombieTurbulence
:ZombieTurbulence rdf:type owl:Class ;
                  rdfs:subClassOf :Turbulence ,
                                  <http://sweetontology.net/phenSystem/Perturbation> ;
                  rdfs:comment "The turbulent perturbations of temperature remaining in a stable boundary layer after all turbulent motions have died out."@en .


###  http://sweetontology.net/phenSystem/Perturbation
<http://sweetontology.net/phenSystem/Perturbation> rdf:type owl:Class .


