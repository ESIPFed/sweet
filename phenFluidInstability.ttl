@base          <http://sweetontology.net/phenFluidInstability> .
@prefix sysc:  <http://sweetontology.net/phenSystemComplexity/> .
@prefix tstate: <http://sweetontology.net/stateThermodynamic/> .
@prefix tempg: <http://sweetontology.net/propTemperatureGradient/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix pstate: <http://sweetontology.net/statePhysical/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix scale: <http://sweetontology.net/stateSpaceScale/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sys:   <http://sweetontology.net/phenSystem/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sstate: <http://sweetontology.net/stateSystem/> .
@prefix instab: <http://sweetontology.net/phenFluidInstability/> .
@prefix fluid: <http://sweetontology.net/phenFluidDynamics/> .
@prefix front: <http://sweetontology.net/phenAtmoFront/> .
@prefix screla: <http://sweetontology.net/relaSci/> .
@prefix rela:  <http://sweetontology.net/rela/> .

instab:ConditionalInstability
        a                owl:Class ;
        rdfs:comment     "The state of a layer of unsaturated air when its lapse rate of temperature is less than the dry-adiabatic lapse rate but greater than the moist-adiabatic lapse rate."@en ;
        rdfs:subClassOf  instab:FluidInstability .

instab:ConvectiveInstability
        a                owl:Class ;
        rdfs:comment     "An instability due to the buoyancy force of heavy fluid over light fluid overcoming the stabilizing influence of viscous forces."@en ;
        rdfs:subClassOf  instab:FluidInstability .

instab:Turbulence  a     owl:Class ;
        rdfs:comment     "Irregular fluctuations occurring in fluid motions."@en ;
        rdfs:subClassOf  fluid:FluidPhenomena , sysc:Fluctuation .

instab:AbsoluteInstability
        a                owl:Class ;
        rdfs:comment     "state of a layer of atmosphere when it has superadiabatic lapse rate of temperature" ;
        rdfs:label       "Absolute Instability" ;
        rdfs:subClassOf  instab:FluidInstability ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    tstate:Superadiabatic ;
                           owl:onProperty  rela:hasState
                         ] .

instab:FreeInstability
        a                owl:Class ;
        rdfs:comment     "The state of a layer of unsaturated air when its lapse rate of temperature is less than the dry-adiabatic lapse rate but greater than the moist-adiabatic lapse rate."@en ;
        rdfs:subClassOf  instab:FluidInstability .

instab:Lift  a               owl:Class ;
        rdfs:subClassOf      fluid:FluidPhenomena ;
        owl:equivalentClass  instab:Lifting .

instab:MechanicalTurbulence
        a                owl:Class ;
        rdfs:comment     "Turbulence produced by shear flow."@en ;
        rdfs:subClassOf  instab:Turbulence .

instab:KelvinHelmholtzInstability
        a                owl:Class ;
        rdfs:comment     "An instability of the basic flow of an incompressible inviscid fluid in two parallel infinite streams of different velocities and densities."@en ;
        rdfs:subClassOf  instab:FluidInstability .

instab:StaticStability
        a                owl:Class ;
        rdfs:comment     "The ability of a fluid at rest to become turbulent or laminar due to the effects of buoyancy."@en ;
        rdfs:subClassOf  instab:FluidInstability .

instab:Lifting  a  owl:Class .

instab:LargeReynoldsNumberFlow
        a                owl:Class ;
        rdfs:comment     "The behavior of a fluid with a Reynolds number typically greater than 1E4 to 1E6, which usually occurs within the atmosphere. The main property of such flows is a constant friction stress within the surface layer that depends only on relative roughness but not on the Reynolds number itself. Thus, molecular viscosity and qualities occurring in flow descriptions that are dependent on the Reynolds number may be totally ignored."@en ;
        rdfs:subClassOf  fluid:Flow .

instab:FrontalInversion
        a                owl:Class ;
        rdfs:comment     "A temperature inversion (temperature rising with height) in the atmosphere, encountered upon vertical ascent through a sloping front (or frontal zone)."@en ;
        rdfs:subClassOf  instab:Inversion ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  front:Front ;
                           owl:onProperty     rela:hasPhenomena
                         ] .

instab:TearingMode  a    owl:Class ;
        rdfs:subClassOf  sstate:Instability ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    pstate:Plasma ;
                           owl:onProperty  rela:hasState
                         ] .

instab:Wake  a           owl:Class ;
        rdfs:comment     "The region of turbulence immediately to the rear of a solid body in motion relative to a fluid"@en ;
        rdfs:subClassOf  instab:Turbulence .

instab:ZombieTurbulence
        a                owl:Class ;
        rdfs:comment     "The turbulent perturbations of temperature remaining in a stable boundary layer after all turbulent motions have died out."@en ;
        rdfs:subClassOf  instab:Turbulence , sys:Perturbation .

instab:RayleighTaylorInstability
        a                owl:Class ;
        rdfs:subClassOf  instab:FluidInstability .

instab:Inversion  a      owl:Class ;
        rdfs:comment     "In meteorology, a departure from the usual decrease or increase with altitude of the value of an atmospheric quantity; also, the layer through which this departure occurs (the inversion layer), or the lowest altitude at which the departure is found (the base of the inversion)."@en ;
        rdfs:subClassOf  fluid:FluidPhenomena ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  tempg:TemperatureGradient ;
                           owl:onProperty     screla:hasQuantity
                         ] .

instab:FluidInstability
        a                owl:Class ;
        rdfs:subClassOf  sstate:Instability , fluid:FluidPhenomena .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <phenSystemComplexity> , <phenFluidDynamics> , <stateSpaceScale> , <relaSci> , <phenAtmoFront> , <rela> , <stateSystem> , <stateThermodynamic> , <phenSystem> , <statePhysical> , <propTemperatureGradient> ;
        owl:versionInfo  "3.1.0" .
