@base          <http://sweetontology.net/reprSpace> .
@prefix repr:  <http://sweetontology.net/repr/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix screla: <http://sweetontology.net/relaSci/> .
@prefix space: <http://sweetontology.net/reprSpace/> .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <relaSci> , <reprSciUnits> , <repr> ;
        owl:versionInfo  "3.1.0" .

space:Space  a           owl:Class ;
        rdfs:comment     "This concept is of space as an independent variable"@en ;
        rdfs:subClassOf  repr:Dimension ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:meter ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .
