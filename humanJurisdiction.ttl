@base          <http://sweetontology.net/humanJurisdiction> .
@prefix hum:   <http://sweetontology.net/human/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix jur:   <http://sweetontology.net/humanJurisdiction/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix know:  <http://sweetontology.net/repr/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix hrela: <http://sweetontology.net/relaHuman/> .
@prefix space: <http://sweetontology.net/reprSpaceGeometry/> .

jur:SMSA  a     owl:Class .

jur:AdministrativeRegion
        a                owl:Class ;
        rdfs:subClassOf  jur:Jurisdiction , space:Region .

jur:Standard  a          owl:Class ;
        rdfs:subClassOf  jur:GoverningBodyProduct .

jur:State  a             owl:Class ;
        rdfs:subClassOf  jur:AdministrativeRegion ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  jur:Country ;
                           owl:onProperty     srela:inside
                         ] .

jur:Jurisdiction  a      owl:Class ;
        rdfs:subClassOf  hum:HumanActivity .

jur:SAR  a      owl:Class .

jur:County  a            owl:Class ;
        rdfs:subClassOf  jur:AdministrativeRegion ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  jur:State ;
                           owl:onProperty     srela:inside
                         ] .

jur:Military  a           owl:Class ;
        rdfs:subClassOf   jur:Jurisdiction ;
        owl:disjointWith  jur:Civil .

jur:GoverningBodyProduct
        a                owl:Class ;
        rdfs:subClassOf  hum:HumanActivity .

jur:GoverningBody  a     owl:Class ;
        rdfs:subClassOf  hum:HumanActivity .

jur:CombinedStatisticalArea
        a                    owl:Class ;
        rdfs:subClassOf      jur:MetropolitanArea ;
        owl:equivalentClass  jur:CSA .

jur:MSA  a      owl:Class .

jur:MetropolitanArea  a  owl:Class ;
        rdfs:subClassOf  jur:AdministrativeRegion ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  jur:Country ;
                           owl:onProperty     srela:inside
                         ] .

jur:Permit  a            owl:Class ;
        rdfs:subClassOf  jur:GovtActivity .

jur:Federal  a  owl:Class .

jur:SpecialAdministrationRegion
        a                    owl:Class ;
        rdfs:subClassOf      jur:AdministrativeRegion ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  jur:Country ;
                               owl:onProperty     srela:inside
                             ] ;
        owl:equivalentClass  jur:SAR .

jur:Treaty  a            owl:Class ;
        rdfs:subClassOf  jur:HumanLaw .

jur:Province  a          owl:Class ;
        rdfs:subClassOf  jur:AdministrativeRegion ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  jur:Country ;
                           owl:onProperty     srela:inside
                         ] .

jur:UrbanArea  a         owl:Class ;
        rdfs:comment     "An urban area is an area with an increased density of human-created structures in comparison to the areas surrounding it. This term is at one end of the spectrum of suburban and rural areas. An urban area is more frequently called a city or town."@en ;
        rdfs:subClassOf  jur:AdministrativeRegion .

jur:StandardsBody  a     owl:Class ;
        rdfs:subClassOf  jur:GoverningBody .

jur:Civilian  a  owl:Class .

jur:Regulation  a        owl:Class ;
        rdfs:subClassOf  jur:HumanLaw .

jur:MetropolitanStatisticalArea
        a                    owl:Class ;
        rdfs:subClassOf      jur:MetropolitanArea ;
        owl:equivalentClass  jur:MSA .

jur:CSA  a      owl:Class .

jur:Penalty  a           owl:Class ;
        rdfs:subClassOf  jur:Enforcement .

jur:Enforcement  a       owl:Class ;
        rdfs:subClassOf  jur:GovtActivity .

jur:GovtActivity  a      owl:Class ;
        rdfs:subClassOf  hum:HumanActivity .

jur:StandardMetropolitanStatisticalArea
        a                    owl:Class ;
        rdfs:subClassOf      jur:MetropolitanArea ;
        owl:equivalentClass  jur:SMSA .

jur:Continent  a         owl:Class ;
        rdfs:subClassOf  jur:AdministrativeRegion .

jur:HumanLaw  a          owl:Class ;
        rdfs:subClassOf  know:Knowledge , jur:GoverningBodyProduct .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <human> , <relaHuman> , <relaSpace> , <repr> , <reprSpaceGeometry> ;
        owl:versionInfo  "3.1.0" .

jur:City  a              owl:Class ;
        rdfs:subClassOf  jur:AdministrativeRegion ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  jur:State ;
                           owl:onProperty     srela:inside
                         ] .

jur:Country  a           owl:Class ;
        rdfs:subClassOf  jur:AdministrativeRegion .

jur:Policy  a            owl:Class ;
        rdfs:subClassOf  jur:GoverningBodyProduct .

jur:FederalGoverningBody
        a                    owl:Class ;
        rdfs:subClassOf      jur:GoverningBody ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  jur:Country ;
                               owl:onProperty     hrela:hasJurisdiction
                             ] ;
        owl:equivalentClass  jur:Federal .

jur:Government  a        owl:Class ;
        rdfs:subClassOf  hum:HumanActivity .

jur:Civil  a                 owl:Class ;
        rdfs:subClassOf      jur:Jurisdiction ;
        owl:equivalentClass  jur:Civilian .
