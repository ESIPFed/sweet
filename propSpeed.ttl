#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/propSpeed/Acceleration
:Acceleration rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/prop/MotionProperty> ,
                              <http://sweetontology.net/propQuantity/PhysicalQuantity> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                                owl:hasValue <http://sweetontology.net/reprSciUnits/meterPerSecondSquared>
                              ] ;
              rdfs:comment "The rate of change with time of the velocity vector of a particle." ;
              rdfs:label "Acceleration" .


###  http://sweetontology.net/propSpeed/Airspeed
:Airspeed rdf:type owl:Class ;
          rdfs:subClassOf :Speed ;
          rdfs:comment "Airspeed is the speed of an aircraft relative to the air. There are several different measures of airspeed: indicated airspeed, calibrated airspeed, equivalent airspeed and true airspeed." .


###  http://sweetontology.net/propSpeed/AlfvenVelocity
:AlfvenVelocity rdf:type owl:Class ;
                rdfs:subClassOf :Velocity .


###  http://sweetontology.net/propSpeed/DopplerVelocity
:DopplerVelocity rdf:type owl:Class ;
                 rdfs:subClassOf :Velocity .


###  http://sweetontology.net/propSpeed/Drift
:Drift rdf:type owl:Class ;
       rdfs:subClassOf :Velocity .


###  http://sweetontology.net/propSpeed/EscapeVelocity
:EscapeVelocity rdf:type owl:Class ;
                rdfs:subClassOf :Velocity .


###  http://sweetontology.net/propSpeed/GroundSpeed
:GroundSpeed rdf:type owl:Class ;
             rdfs:subClassOf :Speed ;
             rdfs:comment "Ground speed is the speed of an aircraft relative to the ground. It is the sum of the aircraft's true airspeed and the current wind and weather conditions; a headwind subtracts from the ground speed, while a tailwind adds to it. Winds at other angles to the heading will have components of either headwind or tailwind as well as a crosswind component." .


###  http://sweetontology.net/propSpeed/GroupVelocity
:GroupVelocity rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/prop/WaveProperty> ,
                               :Velocity .


###  http://sweetontology.net/propSpeed/Inertia
:Inertia rdf:type owl:Class ;
         rdfs:subClassOf :Momentum .


###  http://sweetontology.net/propSpeed/IonVelocity
:IonVelocity rdf:type owl:Class ;
             rdfs:subClassOf :Velocity .


###  http://sweetontology.net/propSpeed/MagnetiCurvatureDrift
:MagnetiCurvatureDrift rdf:type owl:Class ;
                       rdfs:subClassOf <http://sweetontology.net/propQuantity/ElectromagneticQuantity> ,
                                       :Drift .


###  http://sweetontology.net/propSpeed/Momentum
:Momentum rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/prop/MotionProperty> ,
                          <http://sweetontology.net/propQuantity/PhysicalQuantity> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                            owl:hasValue <http://sweetontology.net/reprSciUnits/kilogramMeterPerSecond>
                          ] .


###  http://sweetontology.net/propSpeed/NeutralVelocity
:NeutralVelocity rdf:type owl:Class ;
                 rdfs:subClassOf :Velocity .


###  http://sweetontology.net/propSpeed/Omega
:Omega rdf:type owl:Class ;
       rdfs:subClassOf :VerticalVelocity .


###  http://sweetontology.net/propSpeed/PhaseVelocity
:PhaseVelocity rdf:type owl:Class ;
               rdfs:subClassOf <http://sweetontology.net/prop/WaveProperty> ,
                               :Velocity .


###  http://sweetontology.net/propSpeed/ProperMotion
:ProperMotion rdf:type owl:Class ;
              rdfs:subClassOf :Speed .


###  http://sweetontology.net/propSpeed/RadialVelocity
:RadialVelocity rdf:type owl:Class ;
                rdfs:subClassOf :Velocity ;
                rdfs:comment "The component of a three-dimensional velocity vector oriented along the radial direction from the origin point or axis in polar, cylindrical, or spherical coordinates.  In connection with Doppler radar, the radial velocity component is called Doppler velocity."@en .


###  http://sweetontology.net/propSpeed/SeepageVelocity
:SeepageVelocity rdf:type owl:Class ;
                 rdfs:subClassOf <http://sweetontology.net/prop/HydrogeologicalProperty> ,
                                 :Velocity .


###  http://sweetontology.net/propSpeed/Speed
:Speed rdf:type owl:Class ;
       rdfs:subClassOf <http://sweetontology.net/prop/MotionProperty> ,
                       <http://sweetontology.net/propQuantity/PhysicalQuantity> ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                         owl:hasValue <http://sweetontology.net/reprSciUnits/meterPerSecond>
                       ] .


###  http://sweetontology.net/propSpeed/TangentialAcceleration
:TangentialAcceleration rdf:type owl:Class ;
                        rdfs:subClassOf :Acceleration ;
                        rdfs:comment "The component of the acceleration directed along the velocity vector (streamline), with magnitude equal to the rate of change of speed of the parcel dV/dt, where V is the speed."@en .


###  http://sweetontology.net/propSpeed/TerminalVelocity
:TerminalVelocity rdf:type owl:Class ;
                  rdfs:subClassOf :Velocity .


###  http://sweetontology.net/propSpeed/ThermalVelocity
:ThermalVelocity rdf:type owl:Class ;
                 rdfs:subClassOf :Velocity .


###  http://sweetontology.net/propSpeed/Velocity
:Velocity rdf:type owl:Class ;
          rdfs:subClassOf <http://sweetontology.net/prop/MotionProperty> ,
                          <http://sweetontology.net/propQuantity/PhysicalQuantity> ,
                          <http://sweetontology.net/reprMath/Vector> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaSci/hasDefaultUnit> ;
                            owl:hasValue <http://sweetontology.net/reprSciUnits/meterPerSecond>
                          ] ;
          rdfs:comment "The time rate of change of a position vector; that is, a change of position expressed in terms of speed and direction."@en .


###  http://sweetontology.net/propSpeed/VerticalVelocity
:VerticalVelocity rdf:type owl:Class ;
                  rdfs:subClassOf :Velocity ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://sweetontology.net/relaSpace/hasDirection> ;
                                    owl:hasValue <http://sweetontology.net/reprSpaceDirection/Vertical>
                                  ] .


###  http://sweetontology.net/propSpeed/WindShear
:WindShear rdf:type owl:Class ;
           rdfs:subClassOf <http://sweetontology.net/prop/FluidProperty> .


###  http://sweetontology.net/propSpeed/WindSpeed
:WindSpeed rdf:type owl:Class ;
           rdfs:subClassOf :Speed ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://sweetontology.net/rela/hasPhenomena> ;
                             owl:allValuesFrom <http://sweetontology.net/phenAtmoWind/Wind>
                           ] .


###  http://sweetontology.net/reprMath/Vector
<http://sweetontology.net/reprMath/Vector> rdf:type owl:Class .


