###  http://sweetontology.net/rela/hasProcess
<http://sweetontology.net/rela/hasProcess> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/procChemical/Absorption
:Absorption rdf:type owl:Class ;
            rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Acidification
:Acidification rdf:type owl:Class ;
               rdfs:subClassOf :ChemicalProcess ;
               rdfs:comment "Any process that decreases the pH value of a subtance."@en .


###  http://sweetontology.net/procChemical/Adsorption
:Adsorption rdf:type owl:Class ;
            rdfs:subClassOf :ChemicalProcess ;
            rdfs:comment "Adsorption is the accumulation of atoms or molecules on the surface of a material. This process creates a film of the adsorbate (the molecules or atoms being accumulated) on the adsorbent's surface. [Wikipedia]"@en .


###  http://sweetontology.net/procChemical/Bioluminescence
:Bioluminescence rdf:type owl:Class ;
                 rdfs:subClassOf :Luminescence .


###  http://sweetontology.net/procChemical/Bond
:Bond rdf:type owl:Class ;
      rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Burning
:Burning rdf:type owl:Class ;
         rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Calcification
:Calcification rdf:type owl:Class ;
               rdfs:subClassOf :ChemicalProcess ;
               rdfs:comment "Any process that increases the calcium concentration of a subtance."@en .


###  http://sweetontology.net/procChemical/Calcination
:Calcination rdf:type owl:Class ;
             rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/ChargeExchange
:ChargeExchange rdf:type owl:Class ;
                rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/ChemicalProcess
:ChemicalProcess rdf:type owl:Class ;
                 owl:equivalentClass :ChemicalReaction ,
                                     :Reaction ;
                 rdfs:subClassOf <http://sweetontology.net/proc/Process> ;
                 rdfs:comment "A chemical process is a method or means of somehow changing one or more chemicals or chemical compounds.  [Wikipedia]"@en .


###  http://sweetontology.net/procChemical/ChemicalReaction
:ChemicalReaction rdf:type owl:Class .


###  http://sweetontology.net/procChemical/Combustion
:Combustion rdf:type owl:Class ;
            rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Corrosion
:Corrosion rdf:type owl:Class ;
           rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Crystallization
:Crystallization rdf:type owl:Class ;
                 rdfs:subClassOf :ChemicalProcess ;
                 rdfs:comment "Crystallization is the (natural or artificial) process of formation of solid crystals precipitating from a solution, melt or more rarely deposited directly from a gas. [Wikipedia]"@en .


###  http://sweetontology.net/procChemical/Dehydration
:Dehydration rdf:type owl:Class ;
             rdfs:subClassOf :ChemicalProcess ;
             owl:disjointWith :Hydration .


###  http://sweetontology.net/procChemical/Desalinization
:Desalinization rdf:type owl:Class ;
                rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Dilution
:Dilution rdf:type owl:Class ;
          rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Dissolution
:Dissolution rdf:type owl:Class ;
             owl:equivalentClass :Solvation ;
             rdfs:subClassOf :ChemicalProcess ;
             rdfs:comment "Dissolution or solvation is the process of dissolving a solid substance into a solvent to yield a solution. [Wikipedia]"@en .


###  http://sweetontology.net/procChemical/Electrolysis
:Electrolysis rdf:type owl:Class ;
              rdfs:subClassOf :ChemicalProcess ;
              rdfs:comment "Electrolysis is a method of using an electric current to drive an otherwise non-spontaneous chemical reaction."@en .


###  http://sweetontology.net/procChemical/Enrichment
:Enrichment rdf:type owl:Class ;
            rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Fixation
:Fixation rdf:type owl:Class ;
          rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Fluorescence
:Fluorescence rdf:type owl:Class ;
              rdfs:subClassOf :Luminescence ;
              rdfs:comment "Fluorescence is a luminescence that is mostly found as an optical phenomenon in cold bodies, in which the molecular absorption of a photon triggers the emission of another photon with a longer wavelength. The energy difference between the absorbed and emitted photons ends up as molecular vibrations or heat. Usually the absorbed photon is in the ultraviolet range, and the emitted light is in the visible range, but this depends on the absorbance curve and Stokes shift of the particular fluorophore. Fluorescence is named after the mineral fluorite, composed of calcium fluoride, which often exhibits this phenomenon."@en .


###  http://sweetontology.net/procChemical/Hydration
:Hydration rdf:type owl:Class ;
           rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Hydrolysis
:Hydrolysis rdf:type owl:Class ;
            rdfs:subClassOf :ChemicalProcess ;
            rdfs:comment "Hydrolysis is a chemical reaction during which molecules of water (H2O) are split into hydrogen (H) and hydroxide anions (OHâˆ’) in the process of a chemical mechanism."@en .


###  http://sweetontology.net/procChemical/Leaching
:Leaching rdf:type owl:Class ;
          rdfs:subClassOf :ChemicalProcess ;
          rdfs:comment "In general, leaching is the extraction of certain materials from a carrier into a liquid (usually, but not always a solvent)."@en .


###  http://sweetontology.net/procChemical/Luminescence
:Luminescence rdf:type owl:Class ;
              rdfs:subClassOf <http://sweetontology.net/procPhysical/Radiation> .


###  http://sweetontology.net/procChemical/Neutralization
:Neutralization rdf:type owl:Class ;
                rdfs:subClassOf :ChemicalProcess ;
                rdfs:comment "In chemistry, neutralization is a chemical reaction (also called a water forming reaction since a water molecule is formed during the process) in which an acid and a base or alkali (soluble base) react to produce salt and water (H2O). During the process, hydrogen ions H+ (a bare proton) from the acid (proton donor) or a hydronium ion H3O+ and hydroxide ions OH_ or oxide ions O2_ from the base (proton acceptor) react together to form a water molecule H2O. [Wikipedia]"@en .


###  http://sweetontology.net/procChemical/Nitrification
:Nitrification rdf:type owl:Class ;
               rdfs:subClassOf :Reaction .


###  http://sweetontology.net/procChemical/NuclearFusion
:NuclearFusion rdf:type owl:Class ;
               rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Nucleation
:Nucleation rdf:type owl:Class ;
            rdfs:subClassOf :ChemicalProcess ;
            rdfs:comment "Nucleation is the extremely localized budding of a distinct thermodynamic phase. Some examples of phases that may form via nucleation in liquids are gaseous bubbles, crystals, or glassy regions."@en .


###  http://sweetontology.net/procChemical/Outgas
:Outgas rdf:type owl:Class ;
        rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Oxidation
:Oxidation rdf:type owl:Class ;
           rdfs:subClassOf :Reaction ;
           owl:disjointWith :Reduction ;
           rdfs:comment "Reaction of a substance with oxygen or incorporation of oxygen into a molecule."@en .


###  http://sweetontology.net/procChemical/Phosphorescence
:Phosphorescence rdf:type owl:Class ;
                 rdfs:subClassOf :Fluorescence ;
                 rdfs:comment "Continues to glow after light source is removed"@en .


###  http://sweetontology.net/procChemical/Photodecomposition
:Photodecomposition rdf:type owl:Class ;
                    owl:equivalentClass :Photolysis .


###  http://sweetontology.net/procChemical/Photodissociation
:Photodissociation rdf:type owl:Class ;
                   owl:equivalentClass :Photolysis .


###  http://sweetontology.net/procChemical/Photolysis
:Photolysis rdf:type owl:Class ;
            rdfs:subClassOf :ChemicalProcess ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://sweetontology.net/rela/hasProcess> ;
                              owl:allValuesFrom :Absorption
                            ] ;
            rdfs:comment "Photodissociation, photolysis, or photodecomposition is a chemical reaction in which a chemical compound is broken down by photons. It is defined as the interaction of one or more photons with one target molecule. [Wikipedia]"@en .


###  http://sweetontology.net/procChemical/Reaction
:Reaction rdf:type owl:Class .


###  http://sweetontology.net/procChemical/Recrystalization
:Recrystalization rdf:type owl:Class ;
                  rdfs:subClassOf :Crystallization .


###  http://sweetontology.net/procChemical/Reduction
:Reduction rdf:type owl:Class ;
           rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Solvation
:Solvation rdf:type owl:Class .


###  http://sweetontology.net/procChemical/Sorption
:Sorption rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( :Absorption
                                              :Adsorption
                                            )
                              ] .


###  http://sweetontology.net/procChemical/Thermoluminescence
:Thermoluminescence rdf:type owl:Class ;
                    rdfs:subClassOf :Luminescence ;
                    rdfs:comment "Glows when heated"@en .


###  http://sweetontology.net/procChemical/Titration
:Titration rdf:type owl:Class ;
           rdfs:subClassOf :ChemicalProcess ;
           rdfs:comment "Titration is a common laboratory method of quantitative chemical analysis that is used to determine the unknown concentration of a known reactant."@en .


###  http://sweetontology.net/procChemical/Transmutation
:Transmutation rdf:type owl:Class ;
               rdfs:subClassOf :ChemicalProcess .


###  http://sweetontology.net/procChemical/Triboluminescence
:Triboluminescence rdf:type owl:Class ;
                   rdfs:subClassOf :Luminescence ;
                   rdfs:comment "Glows when struck"@en .


###  http://sweetontology.net/procPhysical/Radiation
<http://sweetontology.net/procPhysical/Radiation> rdf:type owl:Class .


