@prefix : <http://sweetontology.net/reprSpaceCoordinate/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sweetontology.net/reprSpaceCoordinate> .

<http://sweetontology.net/reprSpaceCoordinate> rdf:type owl:Ontology ;
                                                owl:imports <http://sweetontology.net/propSpaceDirection> ,
                                                            <http://sweetontology.net/relaSpace> ,
                                                            <http://sweetontology.net/repr> ,
                                                            <http://sweetontology.net/reprSpaceDirection> ;
                                                rdfs:label "SWEET Ontology" ;
                                                owl:versionInfo "3.1.0" .

#################################################################
#    Classes
#################################################################

###  http://sweetontology.net/reprSpaceCoordinate/AngularCoordinate
:AngularCoordinate rdf:type owl:Class ;
                   rdfs:subClassOf <http://sweetontology.net/repr/Coordinate> .


###  http://sweetontology.net/reprSpaceCoordinate/Equator
:Equator rdf:type owl:Class ;
         rdfs:subClassOf <http://sweetontology.net/propSpaceDirection/Direction> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://sweetontology.net/relaSpace/perpendicularTo> ;
                           owl:allValuesFrom :Pole
                         ] .


###  http://sweetontology.net/reprSpaceCoordinate/HorizontalCoordinate
:HorizontalCoordinate rdf:type owl:Class ;
                      rdfs:subClassOf <http://sweetontology.net/repr/Coordinate> ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://sweetontology.net/relaSpace/orthogonalTo> ;
                                        owl:allValuesFrom :VerticalCoordinate
                                      ] .


###  http://sweetontology.net/reprSpaceCoordinate/MagneticPole
:MagneticPole rdf:type owl:Class ;
              rdfs:subClassOf :Pole .


###  http://sweetontology.net/reprSpaceCoordinate/Meridian
:Meridian rdf:type owl:Class ;
          rdfs:subClassOf :HorizontalCoordinate .


###  http://sweetontology.net/reprSpaceCoordinate/Pole
:Pole rdf:type owl:Class ;
      rdfs:subClassOf <http://sweetontology.net/propSpaceDirection/Direction> .


###  http://sweetontology.net/reprSpaceCoordinate/PrimeMeridian
:PrimeMeridian rdf:type owl:Class ;
               rdfs:subClassOf :Meridian .


###  http://sweetontology.net/reprSpaceCoordinate/Vertical
:Vertical rdf:type owl:Class ;
          owl:equivalentClass :VerticalCoordinate ;
          rdfs:subClassOf <http://sweetontology.net/repr/Coordinate> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://sweetontology.net/relaSpace/hasDirection> ;
                            owl:hasValue <http://sweetontology.net/reprSpaceDirection/Up>
                          ] .


###  http://sweetontology.net/reprSpaceCoordinate/VerticalCoordinate
:VerticalCoordinate rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://sweetontology.net/reprSpaceCoordinate/Colatitude
:Colatitude rdf:type owl:NamedIndividual ,
                     :HorizontalCoordinate ;
            <http://sweetontology.net/relaSpace/hasDirection> <http://sweetontology.net/reprSpaceDirection/South> .


###  http://sweetontology.net/reprSpaceCoordinate/Declination
:Declination rdf:type owl:NamedIndividual ,
                      :AngularCoordinate .


###  http://sweetontology.net/reprSpaceCoordinate/Depth
:Depth rdf:type owl:NamedIndividual ,
                :VerticalCoordinate ;
       <http://sweetontology.net/relaSpace/hasDirection> <http://sweetontology.net/reprSpaceDirection/Down> .


###  http://sweetontology.net/reprSpaceCoordinate/Elevation
:Elevation rdf:type owl:NamedIndividual ,
                    :VerticalCoordinate ;
           owl:sameAs :Height ;
           <http://sweetontology.net/relaSpace/hasDirection> <http://sweetontology.net/reprSpaceDirection/Up> .


###  http://sweetontology.net/reprSpaceCoordinate/Height
:Height rdf:type owl:NamedIndividual ,
                 :VerticalCoordinate .


