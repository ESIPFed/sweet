@base          <http://sweetontology.net/propQuantity> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix prop:  <http://sweetontology.net/prop/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix math:  <http://sweetontology.net/reprMath/> .
@prefix screla: <http://sweetontology.net/relaSci/> .

quan:Quantity  a             owl:Class ;
        rdfs:subClassOf      quan:ScientificQuantity ;
        rdfs:subClassOf      [ a                owl:Restriction ;
                               owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                               owl:onProperty   screla:hasNumericValue
                             ] ;
        owl:equivalentClass  quan:QuantitativeProperty .

quan:IntervalQuantity
        a                owl:Class ;
        rdfs:subClassOf  quan:ScientificQuantity ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   screla:hasInterval
                         ] .

quan:BudgetEquation  a       owl:Class ;
        rdfs:comment         "In general, an equation expressing a balance of quantities in the sense that the local or individual rates of change are zero."@en ;
        rdfs:subClassOf      math:Equation ;
        owl:equivalentClass  quan:Budget , quan:BalanceEquation .

quan:ElectromagneticQuantity
        a                owl:Class ;
        rdfs:subClassOf  quan:Quantity , prop:ElectromagneticRadiationProperty .

quan:ChemicalQuantity
        a                owl:Class ;
        rdfs:subClassOf  quan:Quantity , prop:ChemicalProperty .

quan:ElectromagneticRadiationQuantity
        a                owl:Class ;
        rdfs:subClassOf  quan:Quantity , prop:ElectromagneticRadiationProperty .

quan:RadiationMediumInteractionQuantity
        a                owl:Class ;
        rdfs:subClassOf  quan:Quantity , prop:ElectromagneticRadiationProperty .

quan:QuantitativeProperty
        a       owl:Class .

quan:ExtensiveProperty
        a                 owl:Class ;
        rdfs:subClassOf   quan:Quantity ;
        owl:disjointWith  quan:IntensiveProperty .

quan:ThermodynamicQuantity
        a                owl:Class ;
        rdfs:subClassOf  quan:Quantity , prop:ThermodynamicProperty .

quan:IntensiveProperty
        a                owl:Class ;
        rdfs:subClassOf  quan:Quantity .

quan:ScientificQuantity
        a                owl:Class ;
        rdfs:subClassOf  prop:Property ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   screla:hasUnit
                         ] .

quan:StokesParameters
        a                owl:Class ;
        rdfs:subClassOf  quan:VectorQuantity .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <reprSciUnits> , <relaSci> , <reprMath> , <prop> ;
        owl:versionInfo  "3.1.0" .

quan:Potential  a        owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity .

quan:PhysicalQuantity
        a                owl:Class ;
        rdfs:subClassOf  prop:PhysicalProperty , quan:Quantity .

quan:BalanceEquation  a  owl:Class .

quan:BiologicalQuantity
        a                owl:Class ;
        rdfs:subClassOf  quan:Quantity , prop:BiologicalProperty .

quan:VectorPotential  a  owl:Class ;
        rdfs:subClassOf  quan:Potential , quan:VectorQuantity .

quan:PhysicalConstant
        a                owl:Class ;
        rdfs:subClassOf  math:Constant .

quan:ConservedQuantity
        a                owl:Class ;
        rdfs:subClassOf  math:Constant .

quan:Force  a            owl:Class ;
        rdfs:subClassOf  quan:IntensiveProperty ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:newton ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

quan:FieldStrength  a    owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity .

quan:ScalarPotential  a  owl:Class ;
        rdfs:subClassOf  quan:Potential .

quan:Flux  a             owl:Class ;
        rdfs:comment     "The rate of flow of some quantity, often used in reference to the flow of some form of energy." ;
        rdfs:subClassOf  quan:VectorQuantity .

quan:VectorQuantity  a   owl:Class ;
        rdfs:subClassOf  quan:ScientificQuantity ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   screla:hasVector
                         ] .

quan:Budget  a  owl:Class .
