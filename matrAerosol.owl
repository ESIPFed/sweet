<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY land "http://sweetontology.net/realmLandform">
  <!ENTITY rela "http://sweetontology.net/rela">
  <!ENTITY chem "http://sweetontology.net/matr">
  <!ENTITY aero "http://sweetontology.net/matrAerosol">
  <!ENTITY pstate "http://sweetontology.net/statePhysical">
  <!ENTITY xten "http://sweetontology.net/propSpaceDistance">
  <!ENTITY xten2 "http://sweetontology.net/propSpaceDistance#">
  <!ENTITY srela "http://sweetontology.net/relaSpace">
  <!ENTITY mrela "http://sweetontology.net/relaMath">
  <!ENTITY mrela2 "http://sweetontology.net/relaMath#">
  <!ENTITY solid "http://sweetontology.net/phenSolid">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&aero;"
         xmlns:owl="&owl;"
         xmlns:land="&land;"
         xmlns:aero="&aero;"
         xmlns:rela="&rela;"
         xmlns:pstate="&pstate;"
         xmlns:solid="&solid;"
         xmlns:chem="&chem;"
         xmlns:xten="&xten;"
         xmlns:xten2="&xten2;"
         xmlns:srela="&srela;"
         xmlns:mrela="&mrela;"
         xmlns:mrela2="&mrela2;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information  -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.4">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&chem;"/>
     <owl:imports rdf:resource="&solid;"/>
     <owl:imports rdf:resource="&land;"/>
     <owl:imports rdf:resource="&mrela;"/>
     <owl:imports rdf:resource="&srela;"/>
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&xten;"/>
     <owl:imports rdf:resource="&pstate;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#Aerosol">
    <rdfs:subClassOf rdf:resource="&chem;#SuspendedSubstance"/>
  </owl:Class>

  <owl:Class rdf:about="#ContinentalAerosol">
    <rdfs:subClassOf rdf:resource="#Aerosol" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasRealm" />
        <owl:allValuesFrom rdf:resource="&land;#Land" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Aerosol having its origin over the continents with industrial, urban, agricultural, forest, and desert sources, with potential for high concentrations of hygroscopic aerosol.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#FugitiveDust">
    <rdfs:subClassOf rdf:resource="#Dust"/>
  </owl:Class>

  <owl:Class rdf:about="#PM1">
    <rdfs:subClassOf rdf:resource="#Particulate"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#diameter"/>
        <owl:hasValue>
          <xten2:SizeRange_micron>
            <mrela2:hasUpperBound rdf:datatype="&xsd;double">1</mrela2:hasUpperBound>
          </xten2:SizeRange_micron>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#PM2point5">
    <rdfs:subClassOf rdf:resource="#Particulate"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#diameter"/>
        <owl:hasValue>
          <xten2:SizeRange_micron>
            <mrela2:hasUpperBound rdf:datatype="&xsd;double">2.5</mrela2:hasUpperBound>
          </xten2:SizeRange_micron>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#PM10">
    <rdfs:subClassOf rdf:resource="#Particulate"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#diameter"/>
        <owl:hasValue>
          <xten2:SizeRange_micron>
            <mrela2:hasUpperBound rdf:datatype="&xsd;double">10</mrela2:hasUpperBound>
          </xten2:SizeRange_micron>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Particulate">
    <rdfs:subClassOf rdf:resource="&chem;#SuspendedSubstance"/>
    <rdfs:comment xml:lang="en">The term for solid or liquid particles found in a gas.   Some particles are large or dark enough to be seen as soot or smoke.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Pollen">
    <rdfs:subClassOf rdf:resource="#Particulate"/>
  </owl:Class>

  <owl:Class rdf:about="#Smoke">
    <rdfs:subClassOf rdf:resource="&chem;#SuspendedSubstance"/>
  </owl:Class>

  <owl:Class rdf:about="#Spray">
    <rdfs:subClassOf rdf:resource="#Aerosol"/>
  </owl:Class>

  <owl:Class rdf:about="#Soot">
    <rdfs:subClassOf rdf:resource="#Particulate"/>
    <owl:equivalentClass rdf:resource="#BlackCarbon" />
  </owl:Class>
  <owl:Class rdf:about="#BlackCarbon"/>

  <owl:Class rdf:about="#Ash">
    <rdfs:subClassOf rdf:resource="&chem;#SolidSubstance"/>
   <rdfs:comment>Fine particles of pulverized rock.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Dust">
    <rdfs:subClassOf rdf:resource="&chem;#SolidSubstance"/>
  </owl:Class>

  <owl:Class rdf:about="#CondensationNuclei">
    <rdfs:subClassOf rdf:resource="&chem;#SolidSubstance"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&solid;#Coalescence"/>
        <owl:onProperty rdf:resource="&rela;#hasRole"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#CloudCondensationNuclei">
    <rdfs:subClassOf rdf:resource="#CondensationNuclei"/>
    <owl:equivalentClass rdf:resource="#CCN" />
  </owl:Class>
  <owl:Class rdf:about="#CCN"/>

  <owl:Class rdf:about="#Grain">
    <rdfs:subClassOf rdf:resource="&pstate;#SubstanceForm"/>
    <rdfs:subClassOf rdf:resource="&chem;#SolidSubstance"/>
  </owl:Class>

</rdf:RDF>
