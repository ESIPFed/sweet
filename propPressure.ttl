@base          <http://sweetontology.net/propPressure> .
@prefix mrela: <http://sweetontology.net/relaMath/> .
@prefix proc:  <http://sweetontology.net/procPhysical/> .
@prefix mult:  <http://sweetontology.net/propSpaceMultidimensional/> .
@prefix pres:  <http://sweetontology.net/propPressure/> .
@prefix quan:  <http://sweetontology.net/propQuantity/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix mass:  <http://sweetontology.net/propMass/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix dist:  <http://sweetontology.net/propSpaceDistance/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://sweetontology.net/reprSciUnits/> .
@prefix sdir:  <http://sweetontology.net/reprSpaceDirection/> .
@prefix srela: <http://sweetontology.net/relaSpace/> .
@prefix rate:  <http://sweetontology.net/propTimeFrequency/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix prop:  <http://sweetontology.net/prop/> .
@prefix oper:  <http://sweetontology.net/reprMathOperation/> .
@prefix screla: <http://sweetontology.net/relaSci/> .
@prefix rela:  <http://sweetontology.net/rela/> .

pres:AirPressure  a  owl:Class .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <procPhysical> , <relaSpace> , <reprSciUnits> , <rela> , <reprMathOperation> , <propQuantity> , <relaSci> , <propSpaceDistance> , <propTimeFrequency> , <propMass> , <prop> , <propSpaceMultidimensional> , <reprSpaceDirection> , <relaMath> ;
        owl:versionInfo  "3.1.0" .

pres:TensileStrength  a  owl:Class ;
        rdfs:subClassOf  pres:Strength .

pres:CompressiveStrength
        a                owl:Class ;
        rdfs:subClassOf  pres:Strength .

pres:TotalPressure  a    owl:Class ;
        rdfs:subClassOf  pres:Pressure .

pres:PartialPressure  a  owl:Class ;
        rdfs:comment     "The pressure that a component of a gaseous mixture would have if it alone occupied the same volume at the same temperature as the mixture."@en ;
        rdfs:subClassOf  pres:Pressure .

pres:SurfaceTension  a   owl:Class ;
        rdfs:subClassOf  proc:Tension , quan:PhysicalQuantity , prop:FluidProperty ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:newtonPerMeter ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

pres:Stress  a           owl:Class ;
        rdfs:subClassOf  pres:Pressure .

pres:StaticPressure  a   owl:Class ;
        rdfs:subClassOf  pres:Pressure .

pres:ModulusOfElasticity
        a       owl:Class .

pres:Stiffness  a        owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity , mass:StrengthProperty ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:newtonPerMeter ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

pres:PressureGradient
        a                owl:Class ;
        rdfs:comment     "The derivative of pressure"@en ;
        rdfs:subClassOf  quan:PhysicalQuantity , oper:Gradient ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  dist:Distance ;
                           owl:onProperty     mrela:derivativeWithRespectTo
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  pres:Pressure ;
                           owl:onProperty     mrela:isDerivativeOf
                         ] .

pres:HydrostaticPressure
        a                owl:Class ;
        rdfs:subClassOf  pres:Pressure .

pres:Pressure  a         owl:Class ;
        rdfs:comment     "Force per unit area."@en ;
        rdfs:subClassOf  oper:Ratio , quan:IntensiveProperty ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  mult:Area ;
                           owl:onProperty     mrela:hasSecondOperand
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  quan:Force ;
                           owl:onProperty     mrela:hasFirstOperand
                         ] ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:pascal ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .

pres:ShearModulus  a     owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity , mass:MaterialProperty ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    sdir:Shear ;
                           owl:onProperty  srela:hasDirection
                         ] .

pres:TensileStress  a        owl:Class ;
        rdfs:subClassOf      pres:Stress ;
        rdfs:subClassOf      [ a               owl:Restriction ;
                               owl:hasValue    sdir:Perpendicular ;
                               owl:onProperty  srela:hasDirection
                             ] ;
        owl:disjointWith     pres:ShearStress ;
        owl:equivalentClass  pres:NormalStress .

pres:Strength  a         owl:Class ;
        rdfs:subClassOf  mass:StrengthProperty .

pres:Suction  a          owl:Class ;
        rdfs:subClassOf  pres:Pressure .

pres:BarometricTendency
        a                owl:Class ;
        rdfs:comment     "The character and amount of atmospheric pressure change during a specified period of time, often a three-hour period preceding an observation."@en ;
        rdfs:subClassOf  pres:PressureTendency .

pres:DynamicViscosity
        a       owl:Class .

pres:ExnerFunction  a    owl:Class ;
        rdfs:subClassOf  pres:Pressure .

pres:AtmosphericPressure
        a       owl:Class .

pres:SeaLevelPressure
        a                owl:Class ;
        rdfs:comment     "The atmospheric pressure at mean sea level, either directly measured or, most commonly, empirically determined from the observed station pressure."@en ;
        rdfs:subClassOf  pres:AtmosphericPressure .

pres:BulkModulus  a      owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity .

pres:BarometricPressure
        a                    owl:Class ;
        rdfs:comment         "The pressure exerted by the atmosphere as a consequence of gravitational attraction exerted upon the column of air lying directly above the point in question."@en ;
        rdfs:subClassOf      pres:Pressure ;
        owl:equivalentClass  pres:AtmosphericPressure , pres:AirPressure .

pres:NormalStress  a  owl:Class .

pres:YoungsModulus  a        owl:Class ;
        rdfs:subClassOf      quan:PhysicalQuantity , mass:MaterialProperty ;
        owl:equivalentClass  pres:ModulusOfElasticity .

pres:ShearStress  a      owl:Class ;
        rdfs:subClassOf  pres:Stress ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    sdir:Shear ;
                           owl:onProperty  srela:hasDirection
                         ] .

pres:DifferentialPressure
        a                owl:Class ;
        rdfs:subClassOf  pres:Pressure .

pres:PressureTendency
        a                owl:Class ;
        rdfs:comment     "The character and amount of atmospheric pressure change during a specified period of time, often a three-hour period preceding an observation."@en ;
        rdfs:subClassOf  rate:Tendency ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  pres:Pressure ;
                           owl:onProperty     rela:hasProperty
                         ] .

pres:MomentumFlux  a     owl:Class ;
        rdfs:subClassOf  quan:Flux , pres:Pressure .

pres:Viscosity  a            owl:Class ;
        rdfs:subClassOf      quan:PhysicalQuantity , prop:FluidProperty ;
        rdfs:subClassOf      [ a               owl:Restriction ;
                               owl:hasValue    units:pascalSecond ;
                               owl:onProperty  screla:hasDefaultUnit
                             ] ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  proc:ViscousForce ;
                               owl:onProperty     screla:measureOf
                             ] ;
        owl:equivalentClass  pres:DynamicViscosity .

pres:Compressibility  a  owl:Class ;
        rdfs:subClassOf  quan:PhysicalQuantity ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    units:perPascal ;
                           owl:onProperty  screla:hasDefaultUnit
                         ] .
