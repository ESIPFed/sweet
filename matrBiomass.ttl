@base          <http://sweetontology.net/matrBiomass> .
@prefix subs:  <http://sweetontology.net/matr/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

<matrBiomass/Species>
        a                owl:Class ;
        rdfs:subClassOf  <matrBiomass/Organism> .

<matrBiomass/Honey>  a   owl:Class ;
        rdfs:subClassOf  <matrBiomass/Food> .

<matrBiomass/AquaticOrganism>
        a                owl:Class ;
        rdfs:subClassOf  <matrBiomass/Organism> .

<matrBiomass/LivingEntity>
        a                owl:Class ;
        rdfs:subClassOf  subs:Substance .

<matrBiomass/Biomass>
        a                owl:Class ;
        rdfs:subClassOf  <matrBiomass/LivingEntity> .

<matrBiomass/IndicatorSpecies>
        a                    owl:Class ;
        rdfs:comment         "An indicator species is any biological species that defines a trait or characteristic of the environment. For example, a species may delineate an ecoregion or indicate an environmental condition such as a disease outbreak, pollution, species competition or climate change. Indicator species can be among the most sensitive species in a region, acting as an early warning to monitoring biologists." ;
        rdfs:subClassOf      <matrBiomass/Species> ;
        owl:equivalentClass  <matrBiomass/Bioindicator> .

<matrBiomass/Manure>  a  owl:Class ;
        rdfs:subClassOf  <matrBiomass/Biomass> .

<matrBiomass/Food>  a    owl:Class ;
        rdfs:subClassOf  <matrBiomass/Biomass> .

<matrBiomass/Litter>  a  owl:Class ;
        rdfs:subClassOf  <matrBiomass/Biomass> .

<matrBiomass/WoodDebris>
        a                owl:Class ;
        rdfs:subClassOf  <matrBiomass/Wood> .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <matr> ;
        owl:versionInfo  "3.1.0" .

<matrBiomass/AnimalWaste>
        a                owl:Class ;
        rdfs:subClassOf  <matrBiomass/Biomass> .

<matrBiomass/Fertilizer>
        a                owl:Class ;
        rdfs:subClassOf  <matrBiomass/Biomass> .

<matrBiomass/Gelbstoff>
        a                owl:Class ;
        rdfs:subClassOf  <matrBiomass/Biomass> .

<matrBiomass/Wood>  a    owl:Class ;
        rdfs:subClassOf  <matrBiomass/Biomass> .

<matrBiomass/Humus>  a   owl:Class ;
        rdfs:subClassOf  <matrBiomass/Biomass> .

<matrBiomass/Detritus>
        a                owl:Class ;
        rdfs:subClassOf  <matrBiomass/Biomass> .

<matrBiomass/Bioindicator>
        a       owl:Class .

<matrBiomass/Feed>  a    owl:Class ;
        rdfs:subClassOf  <matrBiomass/Food> .

<matrBiomass/Organism>
        a                owl:Class ;
        rdfs:subClassOf  <matrBiomass/LivingEntity> .
