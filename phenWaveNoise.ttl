@base          <http://sweetontology.net/phenWaveNoise> .
@prefix role:  <http://sweetontology.net/stateRole/> .
@prefix phen:  <http://sweetontology.net/phen/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix noise: <http://sweetontology.net/phenWaveNoise/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rela:  <http://sweetontology.net/rela/> .
@prefix wave:  <http://sweetontology.net/phenWave/> .

noise:Flicker  a         owl:Class ;
        rdfs:subClassOf  noise:Noise .

noise:Vibration  a       owl:Class ;
        rdfs:subClassOf  noise:Noise .

noise:WhiteNoise  a      owl:Class ;
        rdfs:subClassOf  noise:Noise .

noise:Interference  a    owl:Class ;
        rdfs:subClassOf  noise:Noise .

noise:Hiss  a            owl:Class ;
        rdfs:subClassOf  noise:Noise .

noise:SideLobe  a        owl:Class ;
        rdfs:subClassOf  noise:Noise .

noise:Resonance  a           owl:Class ;
        rdfs:subClassOf      wave:WavePhenomena ;
        owl:equivalentClass  noise:Resonant .

noise:Resonant  a  owl:Class .

noise:Scintillation  a   owl:Class ;
        rdfs:subClassOf  noise:Noise .

noise:Beat  a            owl:Class ;
        rdfs:subClassOf  noise:Interference .

noise:Noise  a           owl:Class ;
        rdfs:subClassOf  wave:WavePhenomena ;
        rdfs:subClassOf  [ a               owl:Restriction ;
                           owl:hasValue    role:Undesirable ;
                           owl:onProperty  rela:hasRole
                         ] .

noise:PulseSpreading  a  owl:Class ;
        rdfs:subClassOf  noise:Noise .

<>      a                owl:Ontology ;
        rdfs:label       "SWEET Ontology" ;
        owl:imports      <stateRole> , <phenWave> , <rela> , <phen> ;
        owl:versionInfo  "3.1.0" .

noise:Chorus  a          owl:Class ;
        rdfs:subClassOf  noise:Resonance .

noise:AmbientNoise  a    owl:Class ;
        rdfs:subClassOf  noise:Noise .

noise:Distortion  a      owl:Class ;
        rdfs:subClassOf  noise:Noise .
